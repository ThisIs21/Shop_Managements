{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport CashierLayouts from \"@/components/CashierLayouts.vue\";\nexport default {\n  __name: 'DashboardCashier',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const userName = ref(\"Kasir\");\n    const totalTransactions = ref(0);\n    const totalSalesValue = ref(0);\n    const salesHistory = ref([]);\n    const loadingHistory = ref(true);\n    const fetchDashboardData = async () => {\n      try {\n        const token = localStorage.getItem(\"token\");\n        const nameFromStorage = localStorage.getItem(\"userName\");\n        if (nameFromStorage) {\n          userName.value = nameFromStorage;\n        }\n        const today = new Date().toISOString().slice(0, 10);\n\n        // Gunakan URL yang terdeteksi di gambar ngrok Anda\n        const ngrokUrl = \"https://0275e9516a8a.ngrok-free.app\";\n        const response = await axios.get(`${ngrokUrl}/api/v1/history/sales`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          },\n          params: {\n            from: today,\n            to: today\n          }\n        });\n        const data = response.data;\n\n        // Perbarui state dengan data dari respons backend yang benar\n        if (data && data.data) {\n          salesHistory.value = data.data;\n          totalTransactions.value = data.total;\n\n          // Hitung total nilai penjualan dari data yang diterima\n          let sum = 0;\n          data.data.forEach(sale => {\n            sum += sale.total_amount || 0; // Gunakan operator || 0 untuk mencegah error jika total_amount null/undefined\n          });\n          totalSalesValue.value = sum;\n        } else {\n          console.warn(\"API response is missing 'data' or 'total' property.\");\n        }\n      } catch (error) {\n        console.error(\"Fai