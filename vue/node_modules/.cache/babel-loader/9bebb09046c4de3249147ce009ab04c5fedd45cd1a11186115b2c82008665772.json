   },\n      }\n    );\n\n    const data = response.data;\n    \n    // Perbarui state dengan data dari respons backend yang benar\n    if (data && data.data) {\n      salesHistory.value = data.data;\n      totalTransactions.value = data.total;\n\n      // Hitung total nilai penjualan dari data yang diterima\n      let sum = 0;\n      data.data.forEach((sale) => {\n        sum += sale.total_amount || 0; // Gunakan operator || 0 untuk mencegah error jika total_amount null/undefined\n      });\n      totalSalesValue.value = sum;\n    } else {\n      console.warn(\"API response is missing 'data' or 'total' property.\");\n    }\n  } catch (error) {\n    console.error(\"Failed to fetch dashboard data:\", error);\n    // Tambahkan logika untuk menangani error, misalnya redirect ke halaman login\n    if (error.response && error.response.status === 401) {\n      // Logic untuk redirect ke halaman login\n    }\n  } finally {\n    loadingHistory.value = false;\n  }\n};\n\nonMounted(() => {\n  fetchDashboardData();\n});\n</script>"],"mappings":";;AAuFA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,cAAc,MAAM,iCAAiC;;;;;;;IAE5D,MAAMC,QAAQ,GAAGJ,GAAG,CAAC,OAAO,CAAC;IAC7B,MAAMK,iBAAiB,GAAGL,GAAG,CAAC,CAAC,CAAC;IAChC,MAAMM,eAAe,GAAGN,GAAG,CAAC,CAAC,CAAC;IAC9B,MAAMO,YAAY,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMQ,cAAc,GAAGR,GAAG,CAAC,IAAI,CAAC;IAEhC,MAAMS,kBAAkB,GAAG,MAAAA,CAAA,KAAY;MACrC,IAAI;QACF,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,MAAMC,eAAe,GAAGF,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;QAExD,IAAIC,eAAe,EAAE;UACnBT,QAAQ,CAACU,KAAK,GAAGD,eAAe;QAClC;QAEA,MAAME,KAAK,GAAG,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC;;QAEnD;QACA,MAAMC,QAAQ,GAAG,qCAAqC;QAEtD,MAAMC,QAAQ,GAAG,MAAMlB,KAAK,CAACmB,GAAG,CAC9B,GAAGF,QAAQ,uBAAuB,EAClC;UACEG,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUb,KAAK;UAChC,CAAC;UACDc,MAAM,EAAE;YACNC,IAAI,EAAEV,KAAK;YACXW,EAAE,EAAEX;UACN;QACF,CACF,CAAC;QAED,MAAMY,IAAI,GAAGP,QAAQ,CAACO,IAAI;;QAE1B;QACA,IAAIA,IAAI,IAAIA,IAAI,