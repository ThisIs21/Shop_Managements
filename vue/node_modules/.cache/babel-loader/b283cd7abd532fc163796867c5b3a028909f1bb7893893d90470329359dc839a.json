    };\n        await axios({\n          method,\n          url,\n          data: payload,\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        fetchSuppliers();\n        closeModal();\n      } catch (err) {\n        console.error(\"Error submitting supplier:\", err.response?.data || err);\n      }\n    };\n    const confirmDelete = async id => {\n      if (!confirm(\"Are you sure you want to delete this supplier?\")) return;\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.delete(`http://localhost:8081/api/v1/owner/suppliers/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        fetchSuppliers();\n      } catch (err) {\n        console.error(\"Error deleting supplier:\", err.response?.data || err);\n      }\n    };\n    onMounted(() => {\n      fetchSuppliers();\n    });\n    const __returned__ = {\n      suppliers,\n      loading,\n      showModal,\n      isEditMode,\n      formData,\n      fetchSuppliers,\n      openCreateModal,\n      openEditModal,\n      closeModal,\n      submitForm,\n      confirmDelete,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      OwnerLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","OwnerLayouts","suppliers","loading","showModal","isEditMode","formData","id","name","contact","address","fetchSuppliers","token","localStorage","getItem","res","get","headers","Authorization","value","data","err","console","error","response","openCreateModal","openEditModal","supplier","closeModal","submitForm","url","method","payload","phone","confirmDelete","confirm","delete"],"sources":["C:/Users/Personal/Documents/PKL/PKL Shop Managements/vue/src/views/ManagementSupplierOwner.vue"],"sourcesConte