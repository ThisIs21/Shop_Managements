full rounded-lg border border-[#dbe0e6] p-2 text-sm\"\r\n                  placeholder=\"Enter email\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"mb-4\">\r\n                <label class=\"block text-sm font-medium text-[#111418] mb-1\"\r\n                  >Role</label\r\n                >\r\n                <select\r\n                  v-model=\"formData.role\"\r\n                  class=\"w-full rounded-lg border border-[#dbe0e6] p-2 text-sm\"\r\n                  required\r\n                >\r\n                  <option value=\"\" disabled>Select a role</option>\r\n                  <option\r\n                    v-for=\"role in roles\"\r\n                    :key=\"role.ID\"\r\n                    :value=\"role.Name\"\r\n                  >\r\n                    {{ role.Name }}\r\n                  </option>\r\n                </select>\r\n              </div>\r\n              <div class=\"mb-4\">\r\n                <label class=\"block text-sm font-medium text-[#111418] mb-1\"\r\n                  >Status</label\r\n                >\r\n                <select\r\n                  v-model=\"formData.status\"\r\n                  class=\"w-full rounded-lg border border-[#dbe0e6] p-2 text-sm\"\r\n                  required\r\n                >\r\n                  <option value=\"active\">Active</option>\r\n                  <option value=\"inactive\">Inactive</option>\r\n                </select>\r\n              </div>\r\n              <div class=\"flex justify-end gap-3\">\r\n                <button\r\n                  type=\"button\"\r\n                  @click=\"closeModal\"\r\n                  class=\"px-4 py-2 bg-gray-200 text-[#111418] rounded-lg text-sm font-medium\"\r\n                >\r\n                  Cancel\r\n                </button>\r\n                <button\r\n                  type=\"submit\"\r\n                  class=\"px-4 py-2 bg-[#f0f2f5] text-[#111418] rounded-lg text-s