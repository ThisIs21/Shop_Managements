{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport PurchasingLayouts from \"@/components/PurchasingLayouts.vue\";\nexport default {\n  __name: 'PurchaseReturn',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const userName = ref(\"Pembelian\");\n    const purchasesHistory = ref([]);\n    const purchaseReturns = ref([]);\n    const loadingHistory = ref(true);\n    const loadingReturns = ref(false);\n    const fromDate = ref(\"\");\n    const toDate = ref(\"\");\n    const returnFromDate = ref(\"\");\n    const returnToDate = ref(\"\");\n    const returnStatusFilter = ref(\"\");\n\n    // Modal states\n    const showModal = ref(false);\n    const selectedPurchase = ref(null);\n    const returnForm = ref({\n      items: [],\n      notes: \"\"\n    });\n    const submitting = ref(false);\n\n    // Computed untuk validasi items\n    const hasValidItems = computed(() => {\n      return returnForm.value.items.some(item => item.qty > 0 && item.reason.trim());\n    });\n    const api = axios.create({\n      baseURL: \"http://localhost:8081/api/v1\"\n    });\n    const savedToken = localStorage.getItem(\"token\");\n    if (savedToken) {\n      api.defaults.headers.common[\"Authorization\"] = `Bearer ${savedToken}`;\n    }\n    const fetchPurchasesHistory = async () => {\n      loadingHistory.value = true;\n      try {\n        const nameFromStorage = localStorage.getItem(\"userName\");\n        if (nameFromStorage) {\n          userName.value = nameFromStorage;\n        }\n        const params = {\n          page: 1,\n          size: 10,\n          status: \"APPROVED\" // Filter hanya APPROVED untuk retur\n        };\n        if (fromDate.value) {\n          params.start_date = fromDate.value; // Sesuaikan dengan backend (start_date/end_date)\n        }\n        if (toDate.value) {\n          params.end_date = toDate.value; // Sesuaikan dengan backend\n        }\n        console.log(\"Request Params:\", params);\n        const response = await api.get(\"/history/purchases\", {\n          params\n        });\n        console.log(\"API Response:\", response.data);\n        const data = response.data;\n        if (data && data.data && Array.isArray(data.data.data)) {\n          purchasesHistory.value = data.data.data.filter(p => p && (p.ID || p.id)); // Filter null/undefined items\n        } else if (data && Array.isArray(data)) {\n          purchasesHistory.value = data.filter(p => p && (p.ID || p.id));\n        } else {\n          purchasesHistory.value = [];\n          console.warn(\"No valid data from server.\");\n        }\n        console.log(\"Parsed purchases:\", purchasesHistory.value);\n      } catch (error) {\n        console.error(\"Error fetch:\", error);\n        if (error.response) {\n          console.error(\"Status:\", error.response.status);\n          console.error(\"Response data:\", error.response.data);\n          alert(`Error mengambil data: ${error.response.status} - ${error.response.data.error || error.message}`);\n        } else {\n          alert(\"Kesalahan jaringan atau server tidak tersedia.\");\n        }\n        purchasesHistory.value = [];\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n    const fetchPurchaseReturns = async () => {\n      loadingReturns.value = true;\n      try {\n        const params = {\n          page: 1,\n          size: 10\n        };\n        if (returnFromDate.value) {\n          params.start_date = returnFromDate.value; // Sesuaikan dengan backend\n        }\n        if (returnToDate.value) {\n          params.end_date = returnToDate.value; // Sesuaikan dengan backend\n        }\n        if (returnStatusFilter.value) {\n          params.status = returnStatusFilter.value;\n        }\n        console.log(\"Return Params:\", params);\n        const response = await api.get(\"/history/purchase-returns\", {\n          params\n        });\n        console.log(\"Returns Response:\", response.data);\n        let rawData = response.data.data || response.data || [];\n        purchaseReturns.value = Array.isArray(rawData) ? rawData.filter(ret => ret && ret.id) : [];\n        console.log(\"Parsed returns:\", purchaseReturns.value);\n      } catch (error) {\n        console.error(\"Error fetching returns:\", error.response ? error.response.data : error.message);\n        alert(`Gagal mengambil daftar retur: ${error.response?.data?.error || error.message}`);\n        purchaseReturns.value = [];\n      } finally {\n        loadingReturns.value = false;\n      }\n    };\n    const resetFilter = () => {\n      fromDate.value = \"\";\n      toDate.value = \"\";\n      fetchPurchasesHistory();\n    };\n    const resetReturnFilter = () => {\n      returnFromDate.value = \"\";\n      returnToDate.value = \"\";\n      returnStatusFilter.value = \"\";\n      fetchPurchaseReturns();\n    };\n    const openReturnModal = async purchaseId => {\n      if (!purchaseId) {\n        alert(\"ID pembelian tidak valid.\");\n        return;\n      }\n      try {\n        const response = await api.get(`/pembelian/purchases/${purchaseId}`);\n        console.log(\"Purchase Detail Response:\", response.data);\n        selectedPurchase.value = response.data.data || response.data;\n        if (!selectedPurchase.value) {\n          alert(\"Data pembelian tidak ditemukan.\");\n          return;\n        }\n        const items = selectedPurchase.value.Items || [];\n        if (!Array.isArray(items)) {\n          alert(\"Items pembelian tidak valid.\");\n          return;\n        }\n        returnForm.value.items = items.map(item => ({\n          product_id: item.ProductID || item.product_id,\n          qty: 0,\n          reason: \"\",\n          price: item.Price || item.price\n        }));\n        returnForm.value.notes = \"\";\n        showModal.value = true;\n      } catch (error) {\n        console.error(\"Error fetching purchase:\", error);\n        alert(\"Gagal memuat detail pembelian. Cek console.\");\n      }\n    };\n    const closeModal = () => {\n      showModal.value = false;\n      selectedPurchase.value = null;\n      returnForm.value = {\n        items: [],\n        notes: \"\"\n      };\n    };\n    const submitReturn = async () => {\n      if (!hasValidItems.value) {\n        alert(\"Pilih minimal satu item dengan qty > 0 dan alasan.\");\n        return;\n      }\n      submitting.value = true;\n      try {\n        const validItems = returnForm.value.items.filter(item => item.qty > 0 && item.reason.trim());\n        const dto = {\n          purchase_id: selectedPurchase.value.ID || selectedPurchase.value.id,\n          date: new Date().toISOString().split(\"T\")[0],\n          // Hanya tanggal, tanpa waktu\n          items: validItems,\n          notes: returnForm.value.notes\n        };\n        console.log(\"Return payload:\", dto);\n        const response = await api.post(\"/pembelian/purchase-returns\", dto);\n        console.log(\"Submit Response:\", response.data);\n        if (response.status === 201 || response.status === 200) {\n          alert(\"Retur pembelian berhasil dibuat (status: PENDING).\");\n          closeModal();\n          fetchPurchaseReturns();\n          fetchPurchasesHistory();\n        }\n      } catch (error) {\n        console.error(\"Error creating return:\", error);\n        alert(`Gagal membuat retur: ${error.response?.data?.error || error.message}`);\n      } finally {\n        submitting.value = false;\n      }\n    };\n    const formatPONumber = id => {\n      return `PO-${String(id || \"\").padStart(3, \"0\")}`;\n    };\n    const formatReturnNumber = id => {\n      return `2025-${String(id || \"\").padStart(3, \"0\")}`;\n    };\n    const getStatusClass = status => {\n      switch (status) {\n        case \"PENDING\":\n          return \"bg-yellow-100 text-yellow-800\";\n        case \"APPROVED\":\n          return \"bg-green-100 text-green-800\";\n        case \"REJECTED\":\n          return \"bg-red-100 text-red-800\";\n        default:\n          return \"bg-gray-100 text-gray-800\";\n      }\n    };\n    const getStatusText = status => {\n      switch (status) {\n        case \"PENDING\":\n          return \"Pending\";\n        case \"APPROVED\":\n          return \"Approved\";\n        case \"REJECTED\":\n          return \"Rejected\";\n        default:\n          return status || \"Unknown\";\n      }\n    };\n    const formatDate = dateStr => {\n      if (!dateStr) return \"N/A\";\n      const date = new Date(dateStr);\n      if (isNaN(date.getTime())) {\n        const sliced = dateStr.split(\"T\")[0];\n        return sliced || \"Invalid Date\";\n      }\n      return date.toLocaleDateString(\"id-ID\", {\n        day: \"2-digit\",\n        month: \"2-digit\",\n        year: \"numeric\"\n      });\n    };\n    const formatCurrency = amount => {\n      return amount !== undefined && amount !== null ? new Intl.NumberFormat(\"id-ID\", {\n        style: \"currency\",\n        currency: \"IDR\",\n        minimumFractionDigits: 0\n      }).format(amount) : \"Rp 0\";\n    };\n    onMounted(() => {\n      fetchPurchasesHistory();\n      fetchPurchaseReturns();\n    });\n    const __returned__ = {\n      userName,\n      purchasesHistory,\n      purchaseReturns,\n      loadingHistory,\n      loadingReturns,\n      fromDate,\n      toDate,\n      returnFromDate,\n      returnToDate,\n      returnStatusFilter,\n      showModal,\n      selectedPurchase,\n      returnForm,\n      submitting,\n      hasValidItems,\n      api,\n      savedToken,\n      fetchPurchasesHistory,\n      fetchPurchaseReturns,\n      resetFilter,\n      resetReturnFilter,\n      openReturnModal,\n      closeModal,\n      submitReturn,\n      formatPONumber,\n      formatReturnNumber,\n      getStatusClass,\n      getStatusText,\n      formatDate,\n      formatCurrency,\n      ref,\n      onMounted,\n      computed,\n      get axios() {\n        return axios;\n      },\n      PurchasingLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","axios","PurchasingLayouts","userName","purchasesHistory","purchaseReturns","loadingHistory","loadingReturns","fromDate","toDate","returnFromDate","returnToDate","returnStatusFilter","showModal","selectedPurchase","returnForm","items","notes","submitting","hasValidItems","value","some","item","qty","reason","trim","api","create","baseURL","savedToken","localStorage","getItem","defaults","headers","common","fetchPurchasesHistory","nameFromStorage","params","page","size","status","start_date","end_date","console","log","response","get","data","Array","isArray","filter","p","ID","id","warn","error","alert","message","fetchPurchaseReturns","rawData","ret","resetFilter","resetReturnFilter","openReturnModal","purchaseId","Items","map","product_id","ProductID","price","Price","closeModal","submitReturn","validItems","dto","purchase_id","date","Date","toISOString","split","post","formatPONumber","String","padStart","formatReturnNumber","getStatusClass","getStatusText","formatDate","dateStr","isNaN","getTime","sliced","toLocaleDateString","day","month","year","formatCurrency","amount","undefined","Intl","NumberFormat","style","currency","minimumFractionDigits","format"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/PurchaseReturn.vue"],"sourcesContent":["<template>\n  <PurchasingLayouts>\n    <div>\n      <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n        <p\n          class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\"\n        >\n          Retur Pembelian\n        </p>\n      </div>\n\n      <!-- Filter Tanggal untuk History Pembelian -->\n      <div class=\"p-4 bg-[#f0f2f5] rounded-lg\">\n        <div class=\"flex flex-wrap gap-4 items-end\">\n          <div>\n            <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n              >Dari Tanggal</label\n            >\n            <input\n              type=\"date\"\n              v-model=\"fromDate\"\n              class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n            />\n          </div>\n          <div>\n            <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n              >Sampai Tanggal</label\n            >\n            <input\n              type=\"date\"\n              v-model=\"toDate\"\n              class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n            />\n          </div>\n          <button\n            @click=\"fetchPurchasesHistory\"\n            :disabled=\"loadingHistory\"\n            class=\"px-4 py-2 bg-blue-500 text-white rounded-md hover:bg-blue-600 disabled:opacity-50\"\n          >\n            {{ loadingHistory ? \"Loading...\" : \"Filter\" }}\n          </button>\n          <button\n            @click=\"resetFilter\"\n            class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\"\n          >\n            Reset (10 Terakhir)\n          </button>\n        </div>\n        <p v-if=\"fromDate || toDate\" class=\"text-sm text-gray-600 mt-2\">\n          Filter aktif: {{ fromDate || \"N/A\" }} s/d {{ toDate || \"N/A\" }}\n        </p>\n      </div>\n\n      <hr class=\"my-4\" />\n\n      <div class=\"p-4\">\n        <p\n          class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\"\n        >\n          History Pembelian (untuk Retur) ({{\n            fromDate || toDate ? \"Filtered\" : \"10 Terakhir\"\n          }})\n        </p>\n        <div\n          class=\"mt-4 overflow-x-auto rounded-lg border border-[#dbe0e6] bg-white\"\n        >\n          <table class=\"min-w-full\">\n            <thead>\n              <tr class=\"bg-gray-100\">\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Tanggal\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Nomor PO\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Supplier\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Total\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Status\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Aksi\n                </th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-if=\"loadingHistory\">\n                <td colspan=\"6\" class=\"text-center py-4\">Loading history...</td>\n              </tr>\n              <tr v-else-if=\"purchasesHistory.length === 0\">\n                <td colspan=\"6\" class=\"text-center py-4 text-gray-500\">\n                  Tidak ada data pembelian. Pastikan ada PO dengan status\n                  APPROVED di database.\n                </td>\n              </tr>\n              <tr\n                v-else\n                v-for=\"purchase in purchasesHistory\"\n                :key=\"purchase?.ID || purchase?.id || Math.random()\"\n                class=\"border-t border-t-[#dbe0e6]\"\n              >\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{\n                    formatDate(purchase?.Date || purchase?.created_at) || \"N/A\"\n                  }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{ formatPONumber(purchase?.ID || purchase?.id) }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{\n                    purchase?.Supplier?.name || purchase?.supplier_name || \"N/A\"\n                  }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{\n                    formatCurrency(purchase?.Total || purchase?.total) || \"Rp 0\"\n                  }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm font-medium\">\n                  <span\n                    :class=\"\n                      getStatusClass(purchase?.Status || purchase?.status)\n                    \"\n                    class=\"px-2 py-1 rounded text-xs\"\n                  >\n                    {{ getStatusText(purchase?.Status || purchase?.status) }}\n                  </span>\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm\">\n                  <button\n                    @click=\"openReturnModal(purchase?.ID || purchase?.id)\"\n                    :disabled=\"\n                      (purchase?.Status || purchase?.status) !== 'APPROVED'\n                    \"\n                    class=\"px-3 py-1 bg-green-500 text-white rounded text-sm hover:bg-green-600 disabled:bg-gray-400 disabled:cursor-not-allowed\"\n                  >\n                    {{\n                      (purchase?.Status || purchase?.status) === \"APPROVED\"\n                        ? \"ADD\"\n                        : \"Tidak Bisa\"\n                    }}\n                  </button>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n\n      <!-- Daftar Retur yang Dibuat dengan Filter -->\n      <div class=\"p-4 mt-8\">\n        <p\n          class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\"\n        >\n          Daftar Retur Saya\n        </p>\n        <div class=\"p-4 bg-[#f0f2f5] rounded-lg mt-4\">\n          <div class=\"flex flex-wrap gap-4 items-end\">\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Dari Tanggal</label\n              >\n              <input\n                type=\"date\"\n                v-model=\"returnFromDate\"\n                class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n              />\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Sampai Tanggal</label\n              >\n              <input\n                type=\"date\"\n                v-model=\"returnToDate\"\n                class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n              />\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Status</label\n              >\n              <select\n                v-model=\"returnStatusFilter\"\n                class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n              >\n                <option value=\"\">Semua</option>\n                <option value=\"PENDING\">Pending</option>\n                <option value=\"APPROVED\">Approved</option>\n                <option value=\"REJECTED\">Rejected</option>\n              </select>\n            </div>\n            <button\n              @click=\"fetchPurchaseReturns\"\n              :disabled=\"loadingReturns\"\n              class=\"px-4 py-2 bg-blue-500 text-white rounded-md hover:bg-blue-600 disabled:opacity-50\"\n            >\n              {{ loadingReturns ? \"Loading...\" : \"Filter\" }}\n            </button>\n            <button\n              @click=\"resetReturnFilter\"\n              class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\"\n            >\n              Reset (10 Terakhir)\n            </button>\n          </div>\n          <p\n            v-if=\"returnFromDate || returnToDate || returnStatusFilter\"\n            class=\"text-sm text-gray-600 mt-2\"\n          >\n            Filter aktif: {{ returnFromDate || \"N/A\" }} s/d\n            {{ returnToDate || \"N/A\" }} (Status:\n            {{ returnStatusFilter || \"Semua\" }})\n          </p>\n        </div>\n        <div\n          class=\"mt-4 overflow-x-auto rounded-lg border border-[#dbe0e6] bg-white\"\n        >\n          <table class=\"min-w-full\">\n            <thead>\n              <tr class=\"bg-gray-100\">\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Return No.\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Nomor PO\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Tanggal\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Total\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Status\n                </th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-if=\"loadingReturns\">\n                <td colspan=\"5\" class=\"text-center py-4\">Loading returns...</td>\n              </tr>\n              <tr v-else-if=\"purchaseReturns.length === 0\">\n                <td colspan=\"5\" class=\"text-center py-4 text-gray-500\">\n                  Tidak ada retur yang dibuat.\n                </td>\n              </tr>\n              <tr\n                v-else\n                v-for=\"ret in purchaseReturns\"\n                :key=\"ret?.id || Math.random()\"\n                class=\"border-t border-t-[#dbe0e6]\"\n              >\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  RTN-{{ formatReturnNumber(ret?.id) }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{\n                    ret?.purchase_id ? formatPONumber(ret.purchase_id) : \"N/A\"\n                  }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{ formatDate(ret?.date || ret?.created_at) || \"N/A\" }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{ formatCurrency(ret?.total || ret?.Total) || \"Rp 0\" }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm font-medium\">\n                  <span\n                    :class=\"getStatusClass(ret?.status)\"\n                    class=\"px-2 py-1 rounded text-xs\"\n                  >\n                    {{ getStatusText(ret?.status) || \"Unknown\" }}\n                  </span>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n\n      <!-- Modal untuk Buat Retur -->\n      <div\n        v-if=\"showModal\"\n        class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50 p-4\"\n      >\n        <div\n          class=\"bg-white rounded-lg p-6 max-w-4xl w-full max-h-[90vh] overflow-y-auto\"\n        >\n          <h2 class=\"text-xl font-bold mb-4\">Buat Retur Pembelian</h2>\n\n          <div v-if=\"selectedPurchase\" class=\"mb-4 p-4 bg-gray-100 rounded\">\n            <p>\n              <strong>Nomor PO:</strong>\n              {{ formatPONumber(selectedPurchase.ID || selectedPurchase.id) }}\n            </p>\n            <p>\n              <strong>Tanggal:</strong>\n              {{\n                formatDate(selectedPurchase.Date || selectedPurchase.created_at)\n              }}\n            </p>\n            <p>\n              <strong>Supplier:</strong>\n              {{\n                selectedPurchase.Supplier?.name ||\n                selectedPurchase.supplier_name ||\n                \"N/A\"\n              }}\n            </p>\n            <p>\n              <strong>Total:</strong>\n              {{\n                formatCurrency(selectedPurchase.Total || selectedPurchase.total)\n              }}\n            </p>\n          </div>\n\n          <div class=\"mb-4\">\n            <h3 class=\"text-lg font-semibold mb-2\">Items untuk Diretur</h3>\n            <div\n              v-for=\"(retItem, index) in returnForm.items\"\n              :key=\"index\"\n              class=\"border-b py-3 last:border-b-0\"\n            >\n              <div class=\"flex flex-wrap gap-4 items-start\">\n                <div class=\"flex-1\">\n                  <p class=\"font-medium\">\n                    {{ selectedPurchase?.Items[index]?.Product?.name || \"N/A\" }}\n                  </p>\n                  <p class=\"text-sm text-gray-600\">\n                    Qty Original:\n                    {{ selectedPurchase?.Items[index]?.Qty || 0 }} | Harga:\n                    {{ formatCurrency(selectedPurchase?.Items[index]?.Price) }}\n                  </p>\n                </div>\n                <div class=\"w-32\">\n                  <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                    >Qty Retur</label\n                  >\n                  <input\n                    type=\"number\"\n                    v-model.number=\"retItem.qty\"\n                    min=\"0\"\n                    :max=\"selectedPurchase?.Items[index]?.Qty || 0\"\n                    class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n                  />\n                </div>\n                <div class=\"flex-1\">\n                  <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                    >Alasan</label\n                  >\n                  <textarea\n                    v-model=\"retItem.reason\"\n                    placeholder=\"Masukkan alasan retur item ini\"\n                    rows=\"2\"\n                    class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n                  ></textarea>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"mb-4\">\n            <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n              >Catatan Tambahan</label\n            >\n            <textarea\n              v-model=\"returnForm.notes\"\n              placeholder=\"Catatan opsional untuk retur ini\"\n              rows=\"3\"\n              class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n            ></textarea>\n          </div>\n\n          <div class=\"flex justify-end gap-2\">\n            <button\n              @click=\"closeModal\"\n              class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\"\n            >\n              Batal\n            </button>\n            <button\n              @click=\"submitReturn\"\n              :disabled=\"submitting || !hasValidItems\"\n              class=\"px-4 py-2 bg-green-500 text-white rounded-md hover:bg-green-600 disabled:opacity-50\"\n            >\n              {{ submitting ? \"Menyimpan...\" : \"Submit Retur\" }}\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </PurchasingLayouts>\n</template>\n\n<script setup>\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport PurchasingLayouts from \"@/components/PurchasingLayouts.vue\";\n\nconst userName = ref(\"Pembelian\");\nconst purchasesHistory = ref([]);\nconst purchaseReturns = ref([]);\nconst loadingHistory = ref(true);\nconst loadingReturns = ref(false);\nconst fromDate = ref(\"\");\nconst toDate = ref(\"\");\nconst returnFromDate = ref(\"\");\nconst returnToDate = ref(\"\");\nconst returnStatusFilter = ref(\"\");\n\n// Modal states\nconst showModal = ref(false);\nconst selectedPurchase = ref(null);\nconst returnForm = ref({\n  items: [],\n  notes: \"\",\n});\nconst submitting = ref(false);\n\n// Computed untuk validasi items\nconst hasValidItems = computed(() => {\n  return returnForm.value.items.some(\n    (item) => item.qty > 0 && item.reason.trim()\n  );\n});\n\nconst api = axios.create({ baseURL: \"http://localhost:8081/api/v1\" });\nconst savedToken = localStorage.getItem(\"token\");\nif (savedToken) {\n  api.defaults.headers.common[\"Authorization\"] = `Bearer ${savedToken}`;\n}\n\nconst fetchPurchasesHistory = async () => {\n  loadingHistory.value = true;\n  try {\n    const nameFromStorage = localStorage.getItem(\"userName\");\n    if (nameFromStorage) {\n      userName.value = nameFromStorage;\n    }\n\n    const params = {\n      page: 1,\n      size: 10,\n      status: \"APPROVED\", // Filter hanya APPROVED untuk retur\n    };\n\n    if (fromDate.value) {\n      params.start_date = fromDate.value; // Sesuaikan dengan backend (start_date/end_date)\n    }\n    if (toDate.value) {\n      params.end_date = toDate.value; // Sesuaikan dengan backend\n    }\n\n    console.log(\"Request Params:\", params);\n    const response = await api.get(\"/history/purchases\", { params });\n    console.log(\"API Response:\", response.data);\n\n    const data = response.data;\n    if (data && data.data && Array.isArray(data.data.data)) {\n      purchasesHistory.value = data.data.data.filter(\n        (p) => p && (p.ID || p.id)\n      ); // Filter null/undefined items\n    } else if (data && Array.isArray(data)) {\n      purchasesHistory.value = data.filter((p) => p && (p.ID || p.id));\n    } else {\n      purchasesHistory.value = [];\n      console.warn(\"No valid data from server.\");\n    }\n    console.log(\"Parsed purchases:\", purchasesHistory.value);\n  } catch (error) {\n    console.error(\"Error fetch:\", error);\n    if (error.response) {\n      console.error(\"Status:\", error.response.status);\n      console.error(\"Response data:\", error.response.data);\n      alert(\n        `Error mengambil data: ${error.response.status} - ${\n          error.response.data.error || error.message\n        }`\n      );\n    } else {\n      alert(\"Kesalahan jaringan atau server tidak tersedia.\");\n    }\n    purchasesHistory.value = [];\n  } finally {\n    loadingHistory.value = false;\n  }\n};\n\nconst fetchPurchaseReturns = async () => {\n  loadingReturns.value = true;\n  try {\n    const params = {\n      page: 1,\n      size: 10,\n    };\n\n    if (returnFromDate.value) {\n      params.start_date = returnFromDate.value; // Sesuaikan dengan backend\n    }\n    if (returnToDate.value) {\n      params.end_date = returnToDate.value; // Sesuaikan dengan backend\n    }\n    if (returnStatusFilter.value) {\n      params.status = returnStatusFilter.value;\n    }\n\n    console.log(\"Return Params:\", params);\n    const response = await api.get(\"/history/purchase-returns\", { params });\n    console.log(\"Returns Response:\", response.data);\n    let rawData = response.data.data || response.data || [];\n    purchaseReturns.value = Array.isArray(rawData)\n      ? rawData.filter((ret) => ret && ret.id)\n      : [];\n    console.log(\"Parsed returns:\", purchaseReturns.value);\n  } catch (error) {\n    console.error(\n      \"Error fetching returns:\",\n      error.response ? error.response.data : error.message\n    );\n    alert(\n      `Gagal mengambil daftar retur: ${\n        error.response?.data?.error || error.message\n      }`\n    );\n    purchaseReturns.value = [];\n  } finally {\n    loadingReturns.value = false;\n  }\n};\n\nconst resetFilter = () => {\n  fromDate.value = \"\";\n  toDate.value = \"\";\n  fetchPurchasesHistory();\n};\n\nconst resetReturnFilter = () => {\n  returnFromDate.value = \"\";\n  returnToDate.value = \"\";\n  returnStatusFilter.value = \"\";\n  fetchPurchaseReturns();\n};\n\nconst openReturnModal = async (purchaseId) => {\n  if (!purchaseId) {\n    alert(\"ID pembelian tidak valid.\");\n    return;\n  }\n  try {\n    const response = await api.get(`/pembelian/purchases/${purchaseId}`);\n    console.log(\"Purchase Detail Response:\", response.data);\n    selectedPurchase.value = response.data.data || response.data;\n\n    if (!selectedPurchase.value) {\n      alert(\"Data pembelian tidak ditemukan.\");\n      return;\n    }\n\n    const items = selectedPurchase.value.Items || [];\n    if (!Array.isArray(items)) {\n      alert(\"Items pembelian tidak valid.\");\n      return;\n    }\n\n    returnForm.value.items = items.map((item) => ({\n      product_id: item.ProductID || item.product_id,\n      qty: 0,\n      reason: \"\",\n      price: item.Price || item.price,\n    }));\n    returnForm.value.notes = \"\";\n\n    showModal.value = true;\n  } catch (error) {\n    console.error(\"Error fetching purchase:\", error);\n    alert(\"Gagal memua