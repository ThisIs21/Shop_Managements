 async () => {\n  console.log(\"Fetching categories...\");\n  loading.value = true;\n  try {\n    const token = localStorage.getItem(\"token\");\n    if (!token) {\n      console.error(\"No JWT token found in localStorage.\");\n      return;\n    }\n\n    const response = await axios.get(\n      \"http://localhost:8081/api/v1/owner/categories\",\n      {\n        headers: { Authorization: `Bearer ${token}` },\n      }\n    );\n\n    console.log(\"API Response:\", response.data);\n\n    if (response.data && response.data.data) {\n      categories.value = response.data.data;\n      console.log(\"Categories updated:\", categories.value);\n    } else {\n      categories.value = [];\n      console.log(\"API response has no data property.\");\n    }\n  } catch (error) {\n    console.error(\"Error fetching categories:\", error);\n  } finally {\n    loading.value = false;\n    console.log(\"Loading finished.\");\n  }\n};\n\n// Create\nconst createCategory = async () => {\n  if (!newCategory.value.trim()) {\n    console.warn(\"Category name is empty.\");\n    return;\n  }\n\n  try {\n    const token = localStorage.getItem(\"token\");\n    if (!token) {\n      console.error(\"No JWT token found for create operation.\");\n      return;\n    }\n\n    await axios.post(\n      \"http://localhost:8081/api/v1/owner/categories\",\n      { name: newCategory.value },\n      { headers: { Authorization: `Bearer ${token}` } }\n    );\n\n    newCategory.value = \"\";\n    fetchCategories();\n  } catch (error) {\n    console.error(\"Error creating category:\", error);\n  }\n};\n\n// Delete\nconst deleteCategory = async (id) => {\n  if (!confirm(\"Are you sure want to delete this category?\")) {\n    return;\n  }\n\n  try {\n    const token = localStorage.getItem(\"token\");\n    if (!token) {\n      console.error(\"No JWT token found for delete operation.\");\n      return;\n    }\n\n    await axios.delete(`http://localhost:8081/api/v1/owner/categories/${id}`, {\n      headers: { Authorization: `Bearer ${token}` },\n    });\n\n    console.log(`Category with ID ${id} deleted successfully.`);\n    fetchCategories();\n  } catch (error) {\n    console.error(\"Error deleting category:\", error);\n  }\n};\n\nonMounted(() => {\n  fetchCategories();\n});\n</script>\n\n<style scoped>\n/* Opsional: Sesuaikan gaya sesuai kebutuhan */\n</style>"],"mappings":"AA2GA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,YAAY,MAAM,gCAAgC;;;;;;;IAEzD,MAAMC,UAAU,GAAGJ,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMK,OAAO,GAAGL,GAAG,CAAC,IAAI,CAAC;IACzB,MAAMM,WAAW,GAAGN,GAAG,CAAC,EAAE,CAAC;;IAE3B;IACA,MAAMO,eAAe,GAAG,MAAAA,CAAA,KAAY;MAClCC,OAAO,CAACC,GAAG,CAAC,wBAAwB,CAAC;MACrCJ,OAAO,CAACK,KAAK,GAAG,IAAI;MACpB,IAAI;QACF,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,IAAI,CAACF,KAAK,EAAE;UACVH,OAAO,CAACM,KAAK,CAAC,qCAAqC,CAAC;UACpD;QACF;QAEA,MAAMC,QAAQ,GAAG,MAAMb,KAAK,CAACc,GAAG,CAC9B,+CAA+C,EAC/C;UACEC,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUP,KAAK;UAAG;QAC9C,CACF,CAAC;QAEDH,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEM,QAAQ,CAACI,IAAI,CAAC;QAE3C,IAAIJ,QAAQ,CAACI,IAAI,IAAIJ,QAAQ,CAACI,IAAI,CAACA,IAAI,EAAE;UACvCf,UAAU,CAACM,KAAK,GAAGK,QAAQ,CAACI,IAAI,CAACA,IAAI;UACrCX,OAAO,CAACC,GAAG,CAAC,qBAAqB,EAAEL,UAAU,CAACM,KAAK,CAAC;QACtD,CAAC,MAAM;UACLN,UAAU,CAACM,KAAK,GAAG,EAAE;UACrBF,OAAO,CAACC,GAAG,CAAC,oCAAoC,CAAC;QACnD;MACF,CAAC,CAAC,OAAOK,KAAK,EAAE;QACdN,OAAO,CAACM,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACpD,CAAC,SAAS;QACRT,OAAO,CAACK,KAAK,GAAG,KAAK;QACrBF,OAAO,CAACC,GAAG,CAAC,mBAAmB,CAAC;MAClC;IACF,CAAC;;IAED;IACA,MAAMW,cAAc,GAAG,MAAAA,CAAA,KAAY;MACjC,IAAI,CAACd,WAAW,CAACI,KAAK,CAACW,IAAI,CAAC,CAAC,EAAE;QAC7Bb,OAAO,CAACc,IAAI,CAAC,yBAAyB,CAAC;QACvC;MACF;MAEA,IAAI;QACF,MAAMX,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,IAAI,CAACF,KAAK,EAAE;UACVH,OAAO,CAACM,KAAK,CAAC,0CAA0C,CAAC;UACzD;QACF;QAEA,MAAMZ,KAAK,CAACqB,IAAI,CACd,+CAA+C,EAC/C;UAAEC,IAAI,EAAElB,WAAW,CAACI;QAAM,CAAC,EAC3B;UAAEO,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUP,KAAK;UAAG;QAAE,CAClD,CAAC;QAEDL,WAAW,CAACI,KAAK,GAAG,EAAE;QACtBH,eAAe,CAAC,CAA