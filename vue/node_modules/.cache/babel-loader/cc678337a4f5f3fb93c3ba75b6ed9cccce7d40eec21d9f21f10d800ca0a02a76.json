{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport OwnerLayouts from \"../components/OwnerLayouts.vue\";\nexport default {\n  __name: 'ManagementCategoryOwner',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const categories = ref([]);\n    const loading = ref(true);\n    const newCategory = ref(\"\");\n\n    // Fetch data\n    const fetchCategories = async () => {\n      console.log(\"Fetching categories...\");\n      loading.value = true;\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          console.error(\"No JWT token found in localStorage.\");\n          return;\n        }\n        const response = await axios.get(\"http://localhost:8081/api/v1/owner/categories\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        console.log(\"API Response:\", response.data);\n        if (response.data && response.data.data) {\n          categories.value = response.data.data;\n          console.log(\"Categories updated:\", categories.value);\n        } else {\n          categories.value = [];\n          console.log(\"API response has no data property.\");\n        }\n      } catch (error) {\n        console.error(\"Error fetching categories:\", error);\n      } finally {\n        loading.value = false;\n        console.log(\"Loading finished.\");\n      }\n    };\n\n    // Create\n    const createCategory = async () => {\n      if (!newCategory.value.trim()) {\n        console.warn(\"Category name is empty.\");\n        return;\n      }\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          console.error(\"No JWT token found for create operation.\");\n          return;\n        }\n        await axios.post(\"http://localhost:8081/api/v1/owner/categories\", {\n          name: newCategory.value\n        }, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        newCategory.value = \"\";\n        fetchCategories();\n      } catch (error) {\n        console.error(\"Error creating category:\", error);\n      }\n    };\n\n    // Delete\n    const deleteCategory = async id => {\n      if (!confirm(\"Are you sure want to delete this category?\")) {\n        return;\n      }\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          console.error(\"No JWT token found for delete operation.\");\n          return;\n        }\n        await axios.delete(`http://localhost:8081/api/v1/owner/categories/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        console.log(`Category with ID ${id} deleted successfully.`);\n        fetchCategories();\n      } catch (error) {\n        console.error(\"Error deleting category:\", error);\n      }\n    };\n    onMounted(() => {\n      fetchCategories();\n    });\n    const __returned__ = {\n      categories,\n      loading,\n      newCategory,\n      fetchCategories,\n      createCategory,\n      deleteCategory,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      OwnerLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","OwnerLayouts","categories","loading","newCategory","fetchCategories","console","log","value","token","localStorage","getItem","error","response","get","headers","Authorization","data","createCategory","trim","warn","post","name","deleteCategory","id","confirm","delete"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/ManagementCategoryOwner.vue"],"sourcesContent":["<template>\n  <OwnerLayouts>\n    <div class=\"flex flex-1 justify-center py-5\">\n      <div class=\"layout-content-container flex flex-col max-w-[960px] flex-1\">\n        <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n          <p\n            class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\"\n          >\n            Management Category\n          </p>\n        </div>\n\n        <div class=\"px-4 py-3\">\n          <div\n            class=\"flex flex-col gap-3 overflow-hidden rounded-lg border border-[#dbe0e6] bg-white p-5\"\n          >\n            <h2 class=\"text-lg font-semibold\">Add Category</h2>\n            <form @submit.prevent=\"createCategory\" class=\"flex gap-3\">\n              <input\n                v-model=\"newCategory\"\n                type=\"text\"\n                placeholder=\"Category name\"\n                class=\"flex-1 border border-[#dbe0e6] rounded-lg px-3 py-2 text-sm\"\n              />\n              <button\n                type=\"submit\"\n                class=\"h-10 px-4 rounded-lg bg-blue-500 text-white text-sm font-medium hover:bg-blue-600\"\n              >\n                Save\n              </button>\n            </form>\n          </div>\n        </div>\n\n        <div class=\"px-4 py-3\">\n          <div\n            class=\"flex overflow-hidden rounded-lg border border-[#dbe0e6] bg-white\"\n          >\n            <table class=\"flex-1\">\n              <thead>\n                <tr class=\"bg-white\">\n                  <th\n                    class=\"px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\"\n                  >\n                    ID\n                  </th>\n                  <th\n                    class=\"px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\"\n                  >\n                    Category Name\n                  </th>\n                  <th\n                    class=\"px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\"\n                  >\n                    Actions\n                  </th>\n                </t