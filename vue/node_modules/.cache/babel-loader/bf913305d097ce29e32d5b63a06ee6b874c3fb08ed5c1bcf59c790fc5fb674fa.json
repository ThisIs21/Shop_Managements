{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport AppSidebar from \"../components/CashierSidebar.vue\";\n\n// State definitions\n\nexport default {\n  __name: 'ReturnPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const sales = ref([]);\n    const selectedSaleId = ref(\"\");\n    const returnItems = ref([]);\n    const additionalNotes = ref(\"\");\n    const isSubmitting = ref(false);\n\n    // Axios instance\n    const api = axios.create({\n      baseURL: \"http://localhost:8081/api/v1\"\n    });\n    const token = localStorage.getItem(\"token\");\n    if (token) {\n      api.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n    }\n\n    // Load sales data\n    const loadSales = async () => {\n      try {\n        const res = await api.get(\"/kasir/sales\"); // Asumsi endpoint untuk daftar sales\n        sales.value = res.data.data || [];\n      } catch (err) {\n        console.error(\"Error loading sales:\", err);\n        alert(\"Gagal memuat daftar transaksi!\");\n      }\n    };\n\n    // Load sale items when transaction is selected\n    const loadSaleItems = async () => {\n      if (!selectedSaleId.value) {\n        returnItems.value = [];\n        return;\n      }\n      try {\n        const res = await api.get(`/kasir/sales/${selectedSaleId.value}/items`); // Asumsi endpoint untuk item sale\n        returnItems.value = res.data.map(item => ({\n          product: item.product,\n          qty: item.qty,\n          returnQty: 0,\n          reason: \"\"\n        }));\n      } catch (err) {\n        console.error(\"Error loading sale items:\", err);\n        alert(\"Gagal memuat item transaksi!\");\n      }\n    };\n\n    // Format currency\n    const formatCurrency = num => new Intl.NumberFormat(\"id-ID\", {\n      style: \"currency\",\n      currency: \"IDR\"\n    }).format(num);\n\n    // Format date\n    const formatDate = dateStr => {\n      return new Date(dateStr).toLocaleString(\"id-ID\", {\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\",\n        hour: \"2-digit\",\n        minute: \"2-digit\"\n      });\n    };\n\n    // Submit return\n    const submitReturn = async () => {\n      if (!selectedSaleId.value) {\n        alert(\"Pilih transaksi terlebih dahulu!\");\n        return;\n      }\n      const itemsToReturn = returnItems.value.filter(item => item.returnQty > 0 && item.reason);\n      if (itemsToReturn.length === 0) {\n        alert(\"Masukkan jumlah retur dan alasan untuk setidaknya satu item!\");\n        return;\n      }\n      isSubmitting.value = true;\n      try {\n        const payload = {\n          sale_id: selectedSaleId.value,\n          date: new Date().toISOString(),\n          items: itemsToReturn.map(item => ({\n            product_id: item.product.id,\n            qty: item.returnQty,\n            price: item.product.sell_price // Asumsi price dari product\n          })),\n          additional_notes: additionalNotes.value || null\n        };\n        const res = await api.post(\"/kasir/sale-returns\", payload);\n        alert(`Retur berhasil! ID: ${res.data.id}`);\n        selectedSaleId.value = \"\";\n        returnItems.value = [];\n        additionalNotes.value = \"\";\n      } catch (err) {\n        console.error(\"Error submitting return:\", err);\n        alert(\"Gagal mengirim retur: \" + (err.response?.data?.error || err.message));\n      } finally {\n        isSubmitting.value = false;\n      }\n    };\n\n    // Load data on mount\n    onMounted(() => {\n      loadSales();\n    });\n    const __returned__ = {\n      sales,\n      selectedSaleId,\n      returnItems,\n      additionalNotes,\n      isSubmitting,\n      api,\n      token,\n      loadSales,\n      loadSaleItems,\n      formatCurrency,\n      formatDate,\n      submitReturn,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      AppSidebar\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","AppSidebar","sales","selectedSaleId","returnItems","additionalNotes","isSubmitting","api","create","baseURL","token","localStorage","getItem","defaults","headers","common","loadSales","res","get","value","data","err","console","error","alert","loadSaleItems","map","item","product","qty","returnQty","reason","formatCurrency","num","Intl","NumberFormat","style","currency","format","formatDate","dateStr","Date","toLocaleString","year","month","day","hour","minute","submitReturn","itemsToReturn","filter","length","payload","sale_id","date","toISOString","items","product_id","id","price","sell_price","additional_notes","post","response","message"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/ReturnPage.vue"],"sourcesContent":["<template>\n  <div class=\"flex size-full min-h-screen\">\n    <AppSidebar />\n    <div\n      class=\"flex h-full grow flex-col bg-white p-6\"\n      style=\"font-family: Manrope, 'Noto Sans', sans-serif\"\n    >\n      <div class=\"layout-content-container flex flex-col flex-1 px-4 py-5\">\n        <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n          <div class=\"flex min-w-72 flex-col gap-3\">\n            <p\n              class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight\"\n            >\n              Sales Return\n            </p>\n            <p class=\"text-[#60758a] text-sm font-normal leading-normal\">\n              Create a return for a previous sale\n            </p>\n          </div>\n        </div>\n        <div class=\"flex max-w-[480px] flex-wrap items-end gap-4 px-4 py-3\">\n          <label class=\"flex flex-col min-w-40 flex-1\">\n            <select\n              v-model=\"selectedSaleId\"\n              class=\"form-input flex w-full min-w-0 flex-1 resize-none overflow-hidden rounded-lg text-[#111418] focus:outline-0 focus:ring-0 border border-[#dbe0e6] bg-white focus:border-[#dbe0e6] h-14 placeholder:text-[#60758a] p-[15px] text-base font-normal leading-normal\"\n              @change=\"loadSaleItems\"\n            >\n              <option value=\"\">Select Transaction</option>\n              <option v-for=\"sale in sales\" :key=\"sale.id\" :value=\"sale.id\">\n                {{ sale.id }} - {{ formatDate(sale.date) }} (Total:\n                {{ formatCurrency(sale.total) }})\n              </option>\n            </select>\n          </label>\n        </div>\n        <h3\n          class=\"text-[#111418] text-lg font-bold leading-tight tracking-[-0.015em] px-4 pb-2 pt-4\"\n        >\n          Return Items\n        </h3>\n        <div class=\"px-4 py-3 @container\">\n          <div\n            class=\"flex overflow-hidden rounded-lg border border-[#dbe0e6] bg-white\"\n          >\n            <table class=\"flex-1\">\n              <thead>\n                <tr class=\"bg-white\">\n                  <th\n                    class=\"table-3d241899-bed1-450b-a79f-66be1471b996-column-120 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                  >\n                    Product\n                  </th>\n                  <th\n                    class=\"table-3d241899-bed1-450b-a79f-66be1471b996-column-240 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                  >\n                    Quantity\n                  </th>\n                  <th\n                    class=\"table-3d241899-bed1-450b-a79f-66be1471b996-column-360 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                  >\n                    Return Quantity\n                  </th>\n                  <th\n                    class=\"table-3d241899-bed1-450b-a79f-66be1471b996-column-480 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                  >\n                    Reason\n                  </th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr\n                  v-for=\"(item, index) in returnItems\"\n                  :key=\"index\"\n                  class=\"border-t border-t-[#dbe0e6]\"\n                >\n                  <td\n                    class=\"table-3d241899-bed1-450b-a79f-66be1471b996-column-120 h-[72px] px-4 py-2 w-[400px] text-[#111418] text-sm font-normal leading-normal\"\n                  >\n                    {{ item.product.name }}\n                  </td>\n                  <td\n                    class=\"table-3d241899-bed1-450b-a79f-66be1471b996-column-240 h-[72px] px-4 py-2 w-[400px] text-[#60758a] text-sm font-normal leading-normal\"\n                  >\n                    {{ item.qty }}\n                  </td>\n                  <td\n                    class=\"table-3d241899-bed1-450b-a79f-66be1471b996-column-360 h-[72px] px-4 py-2 w-[400px] text-[#60758a] text-sm font-normal leading-normal\"\n                  >\n                    <input\n                      v-model.number=\"item.returnQty\"\n                      type=\"number\"\n                      min=\"0\"\n                      :max=\"item.qty\"\n                      class=\"w-full border rounded p-2\"\n                    />\n                  </td>\n                  <td\n                    class=\"table-3d241899-bed1-450b-a79f-66be1471b996-column-480 h-[72px] px-4 py-2 w-[400px] text-[#60758a] text-sm font-normal leading-normal\"\n                  >\n                    <input\n                      v-model=\"item.reason\"\n                      type=\"text\"\n                      placeholder=\"Enter reason\"\n                      class=\"w-full border rounded p-2\"\n                    />\n                  </td>\n                </tr>\n                <tr v-if=\"returnItems.length === 0\">\n                  <td colspan=\"4\" class=\"text-center text-[#60758a] py-4\">\n                    No items available for selected transaction\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n        <div class=\"flex max-w-[480px] flex-wrap items-end gap-4 px-4 py-3\">\n          <label class=\"flex flex-col min-w-40 flex-1\">\n            <textarea\n              v-model=\"additionalNotes\"\n              placeholder=\"Additional Notes\"\n              class=\"form-input flex w-full min-w-0 flex-1 resize-none overflow-hidden rounded-lg text-[#111418] focus:outline-0 focus:ring-0 border border-[#dbe0e6] bg-white focus:border-[#dbe0e6] min-h-36 placeholder:text-[#60758a] p-[15px] text-base font-normal leading-normal\"\n            ></textarea>\n          </label>\n        </div>\n        <div class=\"flex px-4 py-3 justify-end\">\n          <button\n            @click=\"submitReturn\"\n            :disabled=\"isSubmitting\"\n            class=\"flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-lg h-10 px-4 bg-[#0d80f2] text-white text-sm font-bold leading-normal tracking-[0.015em]\"\n          >\n            <span class=\"truncate\">{{\n              isSubmitting ? \"Submitting...\" : \"Submit Return\"\n            }}</span>\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport AppSidebar from \"../components/CashierSidebar.vue\";\n\n// State definitions\nconst sales = ref([]);\nconst selectedSaleId = ref(\"\");\nconst returnItems = ref([]);\nconst additionalNotes = ref(\"\");\nconst isSubmitting = ref(false);\n\n// Axios instance\nconst api = axios.create({ baseURL: \"http://localhost:8081/api/v1\" });\nconst token = localStorage.getItem(\"token\");\nif (token) {\n  api.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n}\n\n// Load sales data\nconst loadSales = async () => {\n  try {\n    const res = await api.get(\"/kasir/sales\"); // Asumsi endpoint untuk daftar sales\n    sales.value = res.data.data || [];\n  } catch (err) {\n    console.error(\"Error loading sales:\", err);\n    alert(\"Gagal memuat daftar transaksi!\");\n  }\n};\n\n// Load sale items when transaction is selected\nconst loadSaleItems = async () => {\n  if (!selectedSaleId.value) {\n    returnItems.value = [];\n    return;\n  }\n  try {\n    const res = await api.get(`/kasir/sales/${selectedSaleId.value}/items`); // Asumsi endpoint untuk item sale\n    returnItems.value = res.data.map((item) => ({\n      product: item.product,\n      qty: item.qty,\n      returnQty: 0,\n      reason: \"\",\n    }));\n  } catch (err) {\n    console.error(\"Error loading sale items:\", err);\n    alert(\"Gagal memuat item transaksi!\");\n  }\n};\n\n// Format currency\nconst formatCurrency = (num) =>\n  new Intl.NumberFormat(\"id-ID\", { style: \"currency\", currency: \"IDR\" }).format(\n    num\n  );\n\n// Format date\nconst formatDate = (dateStr) => {\n  return new Date(dateStr).toLocaleString(\"id-ID\", {\n    year: \"numeric\",\n    month: \"long\",\n    day: \"numeric\",\n    hour: \"2-digit\",\n    minute: \"2-digit\",\n  });\n};\n\n// Submit return\nconst submitReturn = async () => {\n  if (!selectedSaleId.value) {\n    alert(\"Pilih transaksi terlebih dahulu!\");\n    return;\n  }\n  const itemsToReturn = returnItems.value.filter(\n    (item) => item.returnQty > 0 && item.reason\n  );\n  if (itemsToReturn.length === 0) {\n    alert(\"Masukkan jumlah retur dan alasan untuk setidaknya satu item!\");\n    return;\n  }\n\n  isSubmitting.value = true;\n  try {\n    const payload = {\n      sale_id: selectedSaleId.value,\n      date: new Date().toISOString(),\n      items: itemsToReturn.map((item) => ({\n        product_id: item.product.id,\n        qty: item.returnQty,\n        price: item.product.sell_price, // Asumsi price dari product\n      })),\n      additional_notes: additionalNotes.value || null,\n    };\n    const res = await api.post(\"/kasir/sale-returns\", payload);\n    alert(`Retur berhasil! ID: ${res.data.id}`);\n    selectedSaleId.value = \"\";\n    returnItems.value = [];\n    additionalNotes.value = \"\";\n  } catch (err) {\n    console.error(\"Error submitting return:\", err);\n    alert(\n      \"Gagal mengirim retur: \" + (err.response?.data?.error || err.message)\n    );\n  } finally {\n    isSubmitting.value = false;\n  }\n};\n\n// Load data on mount\nonMounted(() => {\n  loadSales();\n});\n</script>\n\n<style>\n@container (max-width: 120px) {\n  .table-3d241899-bed1-450b-a79f-66be1471b996-column-120 {\n    display: none;\n  }\n}\n@container (max-width: 240px) {\n  .table-3d241899-bed1-450b-a79f-66be1471b996-column-240 {\n    display: none;\n  }\n}\n@container (max-width: 360px) {\n  .table-3d241899-bed1-450b-a79f-66be1471b996-column-360 {\n    display: none;\n  }\n}\n@container (max-width: 480px) {\n  .table-3d241899-bed1-450b-a79f-66be1471b996-column-480 {\n    display: none;\n  }\n}\n</style>\n"],"mappings":";;;AA8IA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,UAAU,MAAM,kCAAkC;;AAEzD;;;;;;;;IACA,MAAMC,KAAK,GAAGJ,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMK,cAAc,GAAGL,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMM,WAAW,GAAGN,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMO,eAAe,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC/B,MAAMQ,YAAY,GAAGR,GAAG,CAAC,KAAK,CAAC;;IAE/B;IACA,MAAMS,GAAG,GAAGP,KAAK,CAACQ,MAAM,CAAC;MAAEC,OAAO,EAAE;IAA+B,CAAC,CAAC;IACrE,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;IAC3C,IAAIF,KAAK,EAAE;MACTH,GAAG,CAACM,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,CAAC,GAAG,UAAUL,KAAK,EAAE;IAClE;;IAEA;IACA,MAAMM,SAAS,GAAG,MAAAA,CAAA,KAAY;MAC5B,IAAI;QACF,MAAMC,GAAG,GAAG,MAAMV,GAAG,CAACW,GAAG,CAAC,cAAc,CAAC,CAAC,CAAC;QAC3ChB,KAAK,CAACiB,KAAK,GAAG