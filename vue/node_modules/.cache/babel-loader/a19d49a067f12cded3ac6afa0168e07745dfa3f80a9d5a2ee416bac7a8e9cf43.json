{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport OwnerLayouts from \"../components/OwnerLayouts.vue\";\nexport default {\n  __name: 'ManagementUserOwner',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const users = ref([]);\n    const roles = ref([]);\n    const loading = ref(true);\n    const showModal = ref(false);\n    const isEditMode = ref(false);\n    const formData = ref({\n      name: \"\",\n      email: \"\",\n      roleId: null,\n      active: true,\n      id: null\n    });\n    const fetchUsers = async () => {\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          console.error(\"No JWT token found in localStorage.\");\n          loading.value = false;\n          return;\n        }\n        const apiUrl = \"http://localhost:8081/api/v1/owner/users\";\n        console.log(\"Fetching users from:\", apiUrl);\n        console.log(\"Using token:\", token);\n        const response = await axios.get(apiUrl, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        console.log(\"API Response status:\", response.status);\n        console.log(\"API Response data:\", response.data);\n        if (response.data && response.data.data) {\n          users.value = response.data.data;\n        } else {\n          console.error(\"Invalid API response format. Expected 'data' field.\");\n          users.value = [];\n        }\n      } catch (error) {\n        if (error.response) {\n          console.error(\"Error response from server:\", error.response.status, error.response.data);\n          if (error.response.status === 404) {\n            console.error(\"Endpoint Not Found. Check your API URL.\");\n          } else if (error.response.status === 401) {\n            console.error(\"Unauthorized. The token is invalid or expired.\");\n          }\n        } else {\n          console.error(\"Network or other error:\", error);\n        }\n      } finally {\n        loading.value = false;\n      }\n    };\n    const fetchRoles = async () => {\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          console.error(\"No JWT token found in localStorage.\");\n          return;\n        }\n        const response = await axios.get(\"http://localhost:8081/api/v1/owner/roles\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (response.data && response.data.data) {\n          roles.value = response.data.data;\n        } else {\n          console.error(\"Invalid API response format for roles.\");\n          roles.value = [];\n        }\n      } catch (error) {\n        console.error(\"Error fetching roles:\", error.response ? error.response.data : error);\n      }\n    };\n    const openEditModal = user => {\n      isEditMode.value = true;\n      formData.value = {\n        id: user.ID,\n        name: user.Name,\n        email: user.Email,\n        roleId: user.Role.ID,\n        active: user.Active\n      };\n      showModal.value = true;\n    };\n    const closeModal = () => {\n      showModal.value = false;\n      formData.value = {\n        name: \"\",\n        email: \"\",\n        roleId: null,\n        active: true,\n        id: null\n      };\n    };\n    const submitForm = async () => {\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          console.error(\"No JWT token found in localStorage.\");\n          return;\n        }\n        const url = isEditMode.value ? `http://localhost:8081/api/v1/owner/users/${formData.value.id}` : \"http://localhost:8081/api/v1/owner/users\";\n        const method = isEditMode.value ? \"put\" : \"post\";\n        const payload = {\n          name: formData.value.name,\n          email: formData.value.email,\n          role_id: formData.value.roleId,\n          active: formData.value.active\n        };\n        const response = await axios({\n          method,\n          url,\n          data: payload,\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (response.status === 200 || response.status === 201) {\n          fetchUsers();\n          closeModal();\n        }\n      } catch (error) {\n        console.error(\"Error submitting form:\", error.response ? error.response.data : error);\n      }\n    };\n    const confirmDelete = async id => {\n      if (!confirm(\"Are you sure you want to delete this user?\")) return;\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          console.error(\"No JWT token found in localStorage.\");\n          return;\n        }\n        await axios.delete(`http://localhost:8081/api/v1/owner/users/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        fetchUsers();\n      } catch (error) {\n        console.error(\"Error deleting user:\", error.response ? error.response.data : error);\n      }\n    };\n    onMounted(() => {\n      fetchUsers();\n      fetchRoles();\n    });\n    const __returned__ = {\n      users,\n      roles,\n      loading,\n      showModal,\n      isEditMode,\n      formData,\n      fetchUsers,\n      fetchRoles,\n      openEditModal,\n      closeModal,\n      submitForm,\n      confirmDelete,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      OwnerLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","OwnerLayouts","users","roles","loading","showModal","isEditMode","formData","name","email","roleId","active","id","fetchUsers","token","localStorage","getItem","console","error","value","apiUrl","log","response","get","headers","Authorization","sta