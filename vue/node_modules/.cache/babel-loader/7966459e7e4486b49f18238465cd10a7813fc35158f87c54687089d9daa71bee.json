ducts.value.map((p) => p.category?.name || \"Unknown\")),\n    ];\n  } catch (err) {\n    console.error(\"Load products error:\", err);\n    if (err.response?.status === 401) {\n      isLoggedIn.value = false;\n      localStorage.removeItem(\"token\");\n      localStorage.removeItem(\"userName\");\n    }\n  }\n};\n\nconst filteredProducts = computed(() => {\n  return products.value.filter((p) => {\n    const matchSearch = p.name\n      .toLowerCase()\n      .includes(search.value.toLowerCase());\n    const matchCategory =\n      selectedCategory.value === \"All\" ||\n      p.category?.name === selectedCategory.value;\n    return matchSearch && matchCategory;\n  });\n});\n\nconst addToCart = (product) => {\n  const existing = cart.value.find((i) => i.id === product.id);\n  if (existing) {\n    if (existing.qty < product.stock) existing.qty++;\n  } else {\n    cart.value.push({ ...product, qty: 1 });\n  }\n};\n\nconst removeFromCart = (index) => cart.value.splice(index, 1);\n\nconst totalAmount = computed(() =>\n  cart.value.reduce((sum, item) => sum + item.sell_price * item.qty, 0)\n);\nconst change = computed(() => amountReceived.value - totalAmount.value);\n\nconst checkout = async () => {\n  if (cart.value.length === 0) return alert(\"Cart masih kosong!\");\n  if (amountReceived.value < totalAmount.value) return alert(\"Uang kurang!\");\n\n  loadingCheckout.value = true;\n  try {\n    // Prepare data untuk backend: Sale + SaleItems\n    const saleData = {\n      customer_id: null, // Asumsi cash/no customer, atau tambah input customer\n      user_id: 1, // Ambil dari token/user context, sementara hardcode\n      voucher_id: null, // Kalau ada voucher\n      total: totalAmount.value,\n      paid: amountReceived.value,\n      change: change.value,\n      subtotal: totalAmount.value, // Asumsi no discount\n      discount: 0,\n      items: cart.value.map((item) => ({\n        product_id: item.id,\n        qty: item.qty,\n        price: item.sell_price, // Harga jual p