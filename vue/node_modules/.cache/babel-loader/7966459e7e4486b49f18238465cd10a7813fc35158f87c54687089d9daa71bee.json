{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport \"core-js/modules/es.set.difference.v2.js\";\nimport \"core-js/modules/es.set.intersection.v2.js\";\nimport \"core-js/modules/es.set.is-disjoint-from.v2.js\";\nimport \"core-js/modules/es.set.is-subset-of.v2.js\";\nimport \"core-js/modules/es.set.is-superset-of.v2.js\";\nimport \"core-js/modules/es.set.symmetric-difference.v2.js\";\nimport \"core-js/modules/es.set.union.v2.js\";\nimport { ref, computed, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport CashierSidebar from \"@/components/CashierSidebar.vue\";\nexport default {\n  __name: 'CashierPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const username = ref(\"\");\n    const password = ref(\"\");\n    const isLoggedIn = ref(!!localStorage.getItem(\"token\"));\n    const loadingCheckout = ref(false);\n    const api = axios.create({\n      baseURL: \"http://localhost:8081/api/v1\"\n    });\n    const savedToken = localStorage.getItem(\"token\");\n    if (savedToken) {\n      api.defaults.headers.common[\"Authorization\"] = `Bearer ${savedToken}`;\n    }\n    const products = ref([]);\n    const cart = ref([]);\n    const search = ref(\"\");\n    const selectedCategory = ref(\"All\");\n    const categories = ref([]);\n    const amountReceived = ref(0);\n    const login = async () => {\n      try {\n        const res = await axios.post(\"http://localhost:8081/api/v1/auth/login\", {\n          username: username.value,\n          password: password.value\n        });\n        const token = res.data.token;\n        localStorage.setItem(\"token\", token);\n        localStorage.setItem(\"userName\", res.data.user?.name || username.value); // Simpan nama user\n        isLoggedIn.value = true;\n        api.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n        await loadProducts();\n      } catch (err) {\n        alert(\"Login gagal: \" + (err.response?.data?.error || err.message));\n      }\n    };\n    const loadProducts = async () => {\n      try {\n        const res = await api.get(\"/products\");\n        products.value = res.data.data || [];\n        categories.value = [...new Set(products.value.map(p => p.category?.name || \"Unknown\"))];\n      } catch (err) {\n        console.error(\"Load products error:\", err);\n        if (err.response?.status === 401) {\n          isLoggedIn.value = false;\n          localStorage.removeItem(\"token\");\n          localStorage.removeItem(\"userName\");\n        }\n      }\n    };\n    const filteredProducts = computed(() => {\n      return products.value.filter(p => {\n        const matchSearch = p.name.toLowerCase().includes(search.value.toLowerCase());\n        const matchCategory = selectedCategory.value === \"All\" || p.category?.name === selectedCategory.value;\n        return matchSearch && matchCategory;\n      });\n    });\n    const addToCart = product => {\n      const existing = cart.value.find(i => i.id === product.id);\n      if (existing) {\n        if (existing.qty < product.stock) existing.qty++;\n      } else {\n        cart.value.push({\n          ...product,\n          qty: 1\n        });\n      }\n    };\n    const removeFromCart = index => cart.value.splice(index, 1);\n    const totalAmount = computed(() => cart.value.reduce((sum, item) => sum + item.sell_price * item.qty, 0));\n    const change = computed(() => amountReceived.value - totalAmount.value);\n    const checkout = async () => {\n      if (cart.value.length === 0) return alert(\"Cart masih kosong!\");\n      if (amountReceived.value < totalAmount.value) return alert(\"Uang kurang!\");\n      loadingCheckout.value = true;\n      try {\n        // Prepare data untuk backend: Sale + SaleItems\n        const saleData = {\n          customer_id: null,\n          // Asumsi cash/no customer, atau tambah input customer\n          user_id: 1,\n          // Ambil dari token/user context, sementara hardcode\n          voucher_id: null,\n          // Kalau ada voucher\n          total: totalAmount.value,\n          paid: amountReceived.value,\n          change: change.value,\n          subtotal: totalAmount.value,\n          // Asumsi no discount\n          discount: 0,\n          items: cart.value.map(item => ({\n            product_id: item.id,\n            qty: item.qty,\n            price: item.sell_price // Harga jual per unit\n          }))\n        };\n        const res = await api.post(\"/sales\", saleData); // POST ke /api/v1/sales\n        if (res.data.success) {\n          alert(\"Pembayaran berhasil! Transaksi # \" + res.data.transaction_number);\n          cart.value = [];\n          amountReceived.value = 0;\n          // Optional: Reload products untuk update stock di UI\n          await loadProducts();\n        } else {\n          alert(\"Checkout gagal: \" + (res.data.error || \"Unknown error\"));\n        }\n      } catch (err) {\n        console.error(\"Checkout error:\", err);\n        alert(\"Checkout gagal: \" + (err.response?.data?.error || err.message));\n      } finally {\n        loadingCheckout.value = false;\n      }\n    };\n    const handlePrintReceipt = () => {\n      // Simple print: Bisa di-extend dengan jsPDF atau window.print dengan CSS @media print\n      const receipt = `\n    RECEIPT - ${new Date().toLocaleString(\"id-ID\")}\n    =====================\n    Items:\n    ${cart.value.map(item => `${item.name} x${item.qty} - ${formatCurrency(item.sell_price * item.qty)}`).join(\"\\n\")}\n    Total: ${formatCurrency(totalAmount.value)}\n    Paid: ${formatCurrency(amountReceived.value)}\n    Change: ${formatCurrency(change.value)}\n    =====================\n    Thank you!\n  `;\n      const printWindow = window.