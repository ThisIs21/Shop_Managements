{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport AppSidebar from \"../components/CashierSidebar.vue\";\nimport axios from \"axios\";\n\n// Definisikan state reaktif\n\nexport default {\n  __name: 'ProductList',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const products = ref([]);\n    const loading = ref(true);\n\n    // Fungsi untuk mengambil data produk\n    const getProducts = async () => {\n      try {\n        const response = await axios.get(\"http://localhost:8081/api/v1/products\", {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem(\"token\")}`\n          }\n        });\n        products.value = response.data;\n      } catch (error) {\n        console.error(\"Failed to fetch products:\", error);\n        // Tambahkan logika penanganan error, misalnya menampilkan pesan ke pengguna\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // Panggil fungsi saat komponen dimuat\n    onMounted(() => {\n      getProducts();\n    });\n    const __returned__ = {\n      products,\n      loading,\n      getProducts,\n      ref,\n      onMounted,\n      AppSidebar,\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","AppSidebar","axios","products","loading","getProducts","response","get","headers","Authorization","localStorage","getItem","value","data","error","console"],"sources":["C:/Users/SPC STYLE 5/Documents/app-penjualan/aplikasi/aplikasi/src/views/ProductList.vue"],"sourcesContent":["<template>\n  <div class=\"flex size-full min-h-screen\">\n    <AppSidebar />\n    <div\n      class=\"flex h-full grow flex-col bg-white p-6\"\n      style=\"font-family: Manrope, 'Noto Sans', sans-serif\"\n    >\n      <div\n        class=\"layout-content-container flex flex-col max