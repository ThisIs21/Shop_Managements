ror);\n    // Tambahkan logika penanganan error, misalnya menampilkan pesan ke pengguna\n  } finally {\n    loading.value = false;\n  }\n};\n\n// Panggil fungsi saat komponen dimuat\nonMounted(() => {\n  getProducts();\n});\n</script>\n\n<style>\n@container (max-width: 120px) {\n  .table-75f3fd5d-fb3d-4782-ab8e-d54b0e1a052d-column-120 {\n    display: none;\n  }\n}\n@container (max-width: 240px) {\n  .table-75f3fd5d-fb3d-4782-ab8e-d54b0e1a052d-column-240 {\n    display: none;\n  }\n}\n@container (max-width: 360px) {\n  .table-75f3fd5d-fb3d-4782-ab8e-d54b0e1a052d-column-360 {\n    display: none;\n  }\n}\n</style>\n"],"mappings":"AAgIA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,UAAU,MAAM,kCAAkC;AACzD,OAAOC,KAAK,MAAM,OAAO;;AAEzB;;;;;;;;IACA,MAAMC,QAAQ,GAAGJ,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMK,OAAO,GAAGL,GAAG,CAAC,IAAI,CAAC;;IAEzB;IACA,MAAMM,WAAW,GAAG,MAAAA,CAAA,KAAY;MAC9B,IAAI;QACF,MAAMC,QAAQ,GAAG,MAAMJ,KAAK,CAACK,GAAG,CAAC,uCAAuC,EAAE;UACxEC,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;UACxD;QACF,CAAC,CAAC;QACFR,QAAQ,CAACS,KAAK,GAAGN,QAAQ,CAACO,IAAI;MAChC,CAAC,CAAC,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;QACjD;MACF,CAAC,SAAS;QACRV,OAAO,CAACQ,KAAK,GAAG,KAAK;MACvB;IACF,CAAC;;IAED;IACAZ,SAAS,CAAC,MAAM;MACdK,WAAW,CAAC,CAAC;IACf,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}