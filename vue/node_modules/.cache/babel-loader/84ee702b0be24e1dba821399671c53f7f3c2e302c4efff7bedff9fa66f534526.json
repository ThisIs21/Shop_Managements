\n  } finally {\n    loading.value = false;\n  }\n};\n\n// Create\nconst createCategory = async () => {\n  if (!newCategory.value.trim()) return;\n\n  try {\n    const token = localStorage.getItem(\"token\");\n    await axios.post(\n      \"http://localhost:8081/api/v1/owner/categories\",\n      { Name: newCategory.value },\n      { headers: { Authorization: `Bearer ${token}` } }\n    );\n\n    newCategory.value = \"\";\n    fetchCategories();\n  } catch (error) {\n    console.error(\"Error creating category:\", error);\n  }\n};\n\n// Delete\nconst deleteCategory = async (id) => {\n  if (!confirm(\"Are you sure want to delete this category?\")) return;\n\n  try {\n    const token = localStorage.getItem(\"token\");\n    await axios.delete(`http://localhost:8081/api/v1/owner/categories/${id}`, {\n      headers: { Authorization: `Bearer ${token}` },\n    });\n\n    fetchCategories();\n  } catch (error) {\n    console.error(\"Error deleting category:\", error);\n  }\n};\n\nonMounted(() => {\n  fetchCategories();\n});\n</script>\n"],"mappings":"AA8GA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,YAAY,MAAM,gCAAgC;;;;;;;IAEzD,MAAMC,UAAU,GAAGJ,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMK,OAAO,GAAGL,GAAG,CAAC,IAAI,CAAC;IACzB,MAAMM,WAAW,GAAGN,GAAG,CAAC,EAAE,CAAC;;IAE3B;IACA,MAAMO,eAAe,GAAG,MAAAA,CAAA,KAAY;MAClC,IAAI;QACF,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,IAAI,CAACF,KAAK,EAAE;UACVG,OAAO,CAACC,KAAK,CAAC,qCAAqC,CAAC;UACpDP,OAAO,CAACQ,KAAK,GAAG,KAAK;UACrB;QACF;QAEA,MAAMC,QAAQ,GAAG,MAAMZ,KAAK,CAACa,GAAG,CAC9B,+CAA+C,EAC/C;UACEC,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUT,KAAK;UAAG;QAC9C,CACF,CAAC;QAED,IAAIM,QAAQ,CAACI,IAAI,IAAIJ,QAAQ,CAACI,IAAI,CAACA,IAAI,EAAE;UACvCd,UAAU,CAACS,KAAK,GAAGC,QAAQ,CAACI,IAAI,CAACA,IAAI;QACvC,CAAC,MAAM;UACLd,UAAU,CAACS,KAAK,GAAG,EAAE;QACvB;MACF,CAAC,CAAC,OAAOD,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACpD,CAAC,SAAS;QACRP,OAAO,CAACQ,KAAK,GAAG,KAAK;MACvB;IACF,CAAC;;IAED;IACA,MAAMM,cAAc,GAAG,MAAAA,CAAA,KAAY;MACjC,IA