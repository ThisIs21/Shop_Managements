{"ast":null,"code":"import GudangSidebar from \"@/components/GudangSidebar.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"ProductListGudang\",\n  components: {\n    GudangSidebar\n  },\n  data() {\n    return {\n      products: [],\n      loading: false,\n      error: null,\n      search: \"\"\n    };\n  },\n  mounted() {\n    this.fetchProducts();\n  },\n  methods: {\n    async fetchProducts() {\n      this.loading = true;\n      try {\n        const res = await axios.get(\"http://localhost:8080/api/gudang/products\", {\n          params: {\n            search: this.search || \"\"\n          }\n        });\n        this.products = res.data.data || res.data;\n      } catch (err) {\n        console.error(\"Failed to fetch products:\", err);\n        this.error = \"Gagal memuat data produk\";\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["GudangSidebar","axios","name","components","data","products","loading","error","search","mounted","fetchProducts","methods","res","get","params","err","console"],"sources":["C:\\Users\\Personal\\Documents\\PKL\\aplikasi\\aplikasi\\src\\views\\ProductListGudang.vue"],"sourcesContent":["<template>\n  <v-app>\n    <!-- Sidebar Gudang -->\n    <GudangSidebar />\n\n    <!-- Kontainer utama -->\n    <div\n      class=\"flex h-full flex-col bg-white\"\n      style=\"font-family: Manrope, 'Noto Sans', sans-serif\"\n    >\n      <!-- Konten utama -->\n      <main class=\"mx-auto w-full max-w-7xl flex-1 px-4 py-5 sm:px-6 lg:px-8\">\n        <!-- Header -->\n        <div\n          class=\"flex flex-wrap items-center justify-between gap-3 border-b border-gray-200 pb-4\"\n        >\n          <h1 class=\"text-[#111418] tracking-tight text-3xl font-bold\">\n            Product List\n          </h1>\n          <v-btn\n            class=\"rounded-lg bg-blue-600 px-4 py-2 text-sm font-medium text-white transition-colors hover:bg-blue-700\"\n            flat\n          >\n            Add New Product\n          </v-btn>\n        </div>\n\n        <!-- Search & Filter -->\n        <div class=\"mt-6 flex flex-col gap-4 sm:flex-row sm:items-center\">\n          <label class=\"relative flex min-w-40 h-10 flex-1\">\n            <div\n              class=\"pointer-events-none absolute inset-y-0 left-0 flex items-center pl-3\"\n            >\n              <svg\n                xmlns=\"http://www.w3.org/2000/svg\"\n                width=\"20\"\n                height=\"20\"\n                fill=\"currentColor\"\n                viewBox=\"0 0 256 256\"\n                class=\"text-[#60758a]\"\n              >\n                <path\n                  d=\"M229.66,218.34l-50.07-50.06a88.11,88.11,0,1,0-11.31,11.31l50.06,50.07a8,8,0,0,0,11.32-11.32ZM40,112a72,72,0,1,1,72,72A72.08,72.08,0,0,1,40,112Z\"\n                ></path>\n              </svg>\n            </div>\n            <input\n              v-model=\"search\"\n              @input=\"fetchProducts\"\n              placeholder=\"Search for products\"\n              class=\"w-full rounded-lg border border-gray-300 bg-white py-2 pl-10 pr-4 text-base text-[#111418] focus:border-blue-500 focus:outline-none focus:ring-1 focus:ring-blue-500\"\n              type=\"text\"\n            />\n          </label>\n          <div class=\"flex gap-2\">\n            <v-btn\n              class=\"flex h-10 shrink-0 items-center justify-center gap-x-1 rounded-lg border border-gray-300 bg-[#f0f2f5] px-3 transition-colors hover:bg-gray-200\"\n              flat\n            >\n              <p class=\"text-sm font-medium leading-normal text-[#111418]\">\n                Category\n              </p>\n              <svg\n                xmlns=\"http://www.w3.org/2000/svg\"\n                width=\"20\"\n                height=\"20\"\n                fill=\"currentColor\"\n                viewBox=\"0 0 256 256\"\n                class=\"text-[#111418]\"\n              >\n                <path\n                  d=\"M213.66,101.66l-80,80a8,8,0,0,1-11.32,0l-80-80A8,8,0,0,1,53.66,90.34L128,164.69l74.34-74.35a8,8,0,0,1,11.32,11.32Z\"\n                ></path>\n              </svg>\n            </v-btn>\n            <v-btn\n              class=\"flex h-10 shrink-0 items-center justify-center gap-x-1 rounded-lg border border-gray-300 bg-[#f0f2f5] px-3 transition-colors hover:bg-gray-200\"\n              flat\n            >\n              <p class=\"text-sm font-medium leading-normal text-[#111418]\">\n                Stock Status\n              </p>\n              <svg\n                xmlns=\"http://www.w3.org/2000/svg\"\n                width=\"20\"\n                height=\"20\"\n                fill=\"currentColor\"\n                viewBox=\"0 0 256 256\"\n                class=\"text-[#111418]\"\n              >\n                <path\n                  d=\"M213.66,101.66l-80,80a8,8,0,0,1-11.32,0l-80-80A8,8,0,0,1,53.66,90.34L128,164.69l74.34-74.35a8,8,0,0,1,11.32,11.32Z\"\n                ></path>\n              </svg>\n            </v-btn>\n          </div>\n        </div>\n\n        <!-- Tabel Produk -->\n        <div class=\"mt-6 overflow-x-auto rounded-lg border border-[#dbe0e6]\">\n          <v-table class=\"min-w-full\">\n            <thead>\n              <tr class=\"bg-gray-50\">\n                <th class=\"px-4 py-3 text-left text-sm font-semibold text-gray-600\">\n                  Name\n                </th>\n                <th class=\"px-4 py-3 text-left text-sm font-semibold text-gray-600\">\n                  Code\n                </th>\n                <th class=\"px-4 py-3 text-left text-sm font-semibold text-gray-600\">\n                  Stock\n                </th>\n                <th class=\"px-4 py-3 text-left text-sm font-semibold text-gray-600\">\n                  Storage Location\n                </th>\n              </tr>\n            </thead>\n            <tbody class=\"divide-y divide-gray-200 bg-white\">\n              <tr\n                v-for=\"(product, index) in products\"\n                :key=\"index\"\n                class=\"hover:bg-gray-50\"\n              >\n                <td class=\"whitespace-nowrap px-4 py-4 text-sm text-gray-900\">\n                  {{ product.name }}\n                </td>\n                <td class=\"whitespace-nowrap px-4 py-4 text-sm text-gray-500\">\n                  {{ product.code }}\n                </td>\n                <td class=\"whitespace-nowrap px-4 py-4 text-sm text-gray-500\">\n                  {{ product.stock }}\n                </td>\n                <td class=\"whitespace-nowrap px-4 py-4 text-sm text-gray-500\">\n                  {{ product.location }}\n                </td>\n              </tr>\n              <tr v-if=\"loading\">\n                <td colspan=\"4\" class=\"text-center py-4 text-gray-500\">\n                  Loading products...\n                </td>\n              </tr>\n              <tr v-if=\"!loading && products.length === 0\">\n                <td colspan=\"4\" class=\"text-center py-4 text-gray-500\">\n                  No products found\n                </td>\n              </tr>\n            </tbody>\n          </v-table>\n        </div>\n      </main>\n    </div>\n  </v-app>\n</template>\n\n<script>\nimport GudangSidebar from \"@/components/GudangSidebar.vue\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"ProductListGudang\",\n  components: {\n    GudangSidebar,\n  },\n  data() {\n    return {\n      products: [],\n      loading: false,\n      error: null,\n      search: \"\",\n    };\n  },\n  mounted() {\n    this.fetchProducts();\n  },\n  methods: {\n    async fetchProducts() {\n      this.loading = true;\n      try {\n        const res = await axios.get(\"http://localhost:8080/api/gudang/products\", {\n          params: {\n            search: this.search || \"\",\n          },\n        });\n        this.products = res.data.data || res.data;\n      } catch (err) {\n        console.error(\"Failed to fetch products:\", err);\n        this.error = \"Gagal memuat data produk\";\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n"],"mappings":"AA2JA,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,KAAK;MACdC,KAAK,EAAE,IAAI;MACXC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,aAAaA,CAAA,EAAG;MACpB,IAAI,CAACJ,OAAM,GAAI,IAAI;MACnB,IAAI;QACF,MAAMM,GAAE,GAAI,MAAMX,KAAK,CAACY,GAAG,CAAC,2CAA2C,EAAE;UACvEC,MAAM,EAAE;YACNN,MAAM,EAAE,IAAI,CAACA,MAAK,IAAK;UACzB;QACF,CAAC,CAAC;QACF,IAAI,CAACH,QAAO,GAAIO,GAAG,CAACR,IAAI,CAACA,IAAG,IAAKQ,GAAG,CAACR,IAAI;MAC3C,EAAE,OAAOW,GAAG,EAAE;QACZC,OAAO,CAACT,KAAK,CAAC,2BAA2B,EAAEQ,GAAG,CAAC;QAC/C,IAAI,CAACR,KAAI,GAAI,0BAA0B;MACzC,UAAU;QACR,IAAI,CAACD,OAAM,GAAI,KAAK;MACtB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}