tomer_name: purchase.supplier?.nama || \"-\",\n      product_name: purchase.product?.nama_barang || \"-\",\n      quantity: purchase.quantity || 0,\n      price: purchase.price || 0,\n      total: purchase.total || 0,\n    }));\n  } catch (error) {\n    console.error(\"Failed to fetch report data:\", error);\n    if (error.response && error.response.status === 401) {\n      alert(\"Unauthorized. Please log in again.\");\n    } else {\n      alert(\"Failed to fetch report data: \" + error.message);\n    }\n  }\n};\n\n// Export\nconst exportReport = async (format) => {\n  const token = localStorage.getItem(\"token\");\n  if (!token) {\n    alert(\"Authentication token not found. Please log in again.\");\n    return;\n  }\n\n  try {\n    const params = {};\n    if (fromDate.value) params.from = fromDate.value;\n    if (toDate.value) params.to = toDate.value;\n    params.type = \"purchases\";\n    params.export = format;\n\n    const response = await axios.get(API_URL, {\n      headers: {\n        Authorization: `Bearer ${token}`,\n      },\n      params,\n      responseType: \"blob\",\n    });\n\n    const url = window.URL.createObjectURL(new Blob([response.data]));\n    const link = document.createElement(\"a\");\n    link.href = url;\n    link.setAttribute(\"download\", `purchase_report.${format}`);\n    document.body.appendChild(link);\n    link.click();\n    document.body.removeChild(link);\n  } catch (error) {\n    console.error(\"Export failed:\", error);\n    if (error.response && error.response.status === 401) {\n      alert(\"Unauthorized. Please log in again.\");\n    } else {\n      alert(\"Export failed: \" + error.message);\n    }\n  }\n};\n\n// Format Rupiah\nconst formatCurrency = (num) => {\n  if (num === null || num === undefined) return \"Rp 0\";\n  return new Intl.NumberFormat(\"id-ID\", {\n    style: \"currency\",\n    currency: \"IDR\",\n  }).format(num);\n};\n\nonMounted(fetchReport);\n</script>\n"],"mappings":";;;;;AAkJA,OAAOA,YAAY,MAAM,gCAAgC;AA