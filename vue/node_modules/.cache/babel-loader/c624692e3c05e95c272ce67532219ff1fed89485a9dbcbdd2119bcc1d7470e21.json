chases\n      .map((purchase) => {\n        if (!purchase.items || purchase.items.length === 0) {\n          return {\n            date: purchase.date,\n            customer_name: purchase.supplier?.nama || \"-\",\n            product_name: \"-\",\n            quantity: 0,\n            price: 0,\n            total: 0,\n          };\n        }\n\n        return purchase.items.map((it) => ({\n          date: purchase.date,\n          customer_name: purchase.supplier?.nama || \"-\",\n          product_name: it.product?.nama_barang || \"-\",\n          quantity: it.qty || 0,\n          price: it.price || 0,\n          total: it.subtotal || 0,\n        }));\n      })\n      .flat();\n  } catch (error) {\n    console.error(\"Failed to fetch report data:\", error);\n    if (error.response && error.response.status === 401) {\n      alert(\"Unauthorized. Please log in again.\");\n    } else {\n      alert(\"Failed to fetch report data: \" + error.message);\n    }\n  }\n};\n\n// Export\nconst exportReport = async (format) => {\n  const token = localStorage.getItem(\"token\");\n  if (!token) {\n    alert(\"Authentication token not found. Please log in again.\");\n    return;\n  }\n\n  try {\n    const params = {};\n    if (fromDate.value) params.from = fromDate.value;\n    if (toDate.value) params.to = toDate.value;\n    params.export = format;\n\n    const exportAPI_URL = \"http://localhost:8081/api/v1/history/purchases\";\n    const response = await axios.get(exportAPI_URL, {\n      headers: {\n        Authorization: `Bearer ${token}`,\n      },\n      params,\n      responseType: \"blob\",\n    });\n\n    const url = window.URL.createObjectURL(new Blob([response.data]));\n    const link = document.createElement(\"a\");\n    link.href = url;\n    link.setAttribute(\"download\", `purchase_report.${format}`);\n    document.body.appendChild(link);\n    link.click();\n    document.body.removeChild(link);\n  } catch (error) {\n    console.error(\"Export failed:\", error);\n    if (error.response && error.response.status === 401) {\n      alert(\"Unauthorized. Please log in again.\");\n    } else {\n      alert(\"Export failed: \" + error.message);\n    }\n  }\n};\n\n// Format Rupiah\nconst formatCurrency = (num) => {\n  if (num === null || num === undefined) return \"Rp 0\";\n  return new Intl.NumberFormat(\"id-ID\", {\n    style: \"currency\",\n    currency: \"IDR\",\n  }).format(num);\n};\n\nonMounted(fetchReport);\n</script>\n"],"mappings":";;;;;AAkJA,OAAOA,YAAY,MAAM,gCAAgC;AACzD,SAASC,UAAU,QAAQ,YAAY;AACvC,OAAOC,KAAK,MAAM,OAAO;AACzB,SAASC,GAAG,EAAEC,SAAS,QAAQ,KAAK;;AAEpC;AACA,MAAMC,OAAO,GAAG,wCAAwC;;;;;;;IAExD,MAAMC,UAAU,GAAGH,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMI,QAAQ,GAAGJ,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMK,MAAM,GAAGL,GAAG,CAAC,EAAE,CAAC;;IAEtB;IACA,MAAMM,WAAW,GAAG,MAAAA,CAAA,KAAY;MAC9B,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAC3C,IAAI,CAACF,KAAK,EAAE;QACVG,OAAO,CAACC,KAAK,CAAC,iCAAiC,CAAC;QAChDC,KAAK,CAAC,sDAAsD,CAAC;QAC7D;MACF;MAEA,IAAI;QACF,MAAMC,QAAQ,GAAG,MAAMd,KAAK,CAACe,GAAG,CAACZ,OAAO,EAAE;UACxCa,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUT,KAAK;UAChC;QACF,CAAC,CAAC;QAEFG,OAAO,CAACO,GAAG,CAAC,mBAAmB,EAAEJ,QAAQ,CAACK,IAAI,CAAC;QAE/C,MAAMC,SAAS,GAAGN,QAAQ,CAACK,IAAI,CAACA,IAAI,IAAI,EAAE;QAE1Cf,UAAU,CAACiB,KAAK,GAAGD,SAAQ,CACxBE,GAAG,CAAEC,QAAQ,IAAK;UACjB,IAAI,CAACA,QAAQ,CAACC,KAAK,IAAID,QAAQ,CAACC,KAAK,CAACC,MAAM,KAAK,CAAC,EAAE;YAClD,OAAO;cACLC,IAAI,EAAEH,QAAQ,CAACG,IAAI;cACnBC,aAAa,EAAEJ,QAAQ,CAACK,QAAQ,EAAEC,IAAI,IAAI,GAAG;cAC7CC,YAAY,EAAE,GAAG;cACjBC,QAAQ,EAAE,CAAC;cACXC,KAAK,EAAE,CAAC;cACRC,KAAK,EAAE;YACT,CAAC;UACH;UAEA,OAAOV,QAAQ,CAACC,KAAK,CAACF,GAAG,CAAEY,EAAE,KAAM;YACjCR,IAAI,EAAEH,QAAQ,CAACG,IAAI;YACnBC,aAAa,EAAEJ,QAAQ,CAACK,QAAQ,EAAEC,IAAI,IAAI,GAAG;YAC7CC,YAAY,EAAEI,EAAE,CAACC,OAAO,EAAEC,WAAW,IAAI,GAAG;YAC5CL,QAAQ,EAAEG,EAAE,CAACG,GAAG,IAAI,CAAC;YACrBL,KAAK,EAAEE,EAAE,CAACF,KAAK,IAAI,CAAC;YACpBC,KAAK,EAAEC,EAAE,CAACI,QAAQ,IAAI;UACxB,CAAC,CAAC,CAAC;QACL,CAAC,EACAC,IAAI,CAAC,CAAC;MACX,CAAC,CAAC,OAAO3B,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,C