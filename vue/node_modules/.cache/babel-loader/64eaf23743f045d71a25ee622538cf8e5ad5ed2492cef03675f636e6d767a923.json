err) {\n        console.error(\"Gagal mengambil data kategori:\", err);\n      }\n    };\n\n    // Filter produk berdasarkan pencarian dan kategori\n    const filteredProducts = computed(() => {\n      let filtered = products.value;\n      if (searchQuery.value) {\n        filtered = filtered.filter(product => product.name.toLowerCase().includes(searchQuery.value.toLowerCase()));\n      }\n      if (selectedCategory.value) {\n        filtered = filtered.filter(product => product.category === selectedCategory.value);\n      }\n      return filtered;\n    });\n\n    // Navigasi ke halaman tambah produk\n    const goToAddProduct = () => {\n      router.push(\"/management-product\");\n    };\n\n    // Inisialisasi data saat komponen dimuat\n    onMounted(() => {\n      fetchProducts();\n      fetchCategories();\n    });\n    const __returned__ = {\n      router,\n      products,\n      searchQuery,\n      selectedCategory,\n      categories,\n      fetchProducts,\n      fetchCategories,\n      filteredProducts,\n      goToAddProduct,\n      ref,\n      onMounted,\n      computed,\n      get useRouter() {\n        return useRouter;\n      },\n      PurchasingSidebar,\n      get api() {\n        return api;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","useRouter","PurchasingSidebar","api","router","products","searchQuery","selectedCategory","categories","fetchProducts","response","get","value","data","map","product","id","name","code","toString","padStart","purchasePrice","cost_price","toLocaleString","sellingPrice","sell_price","stock","category","err","console","error","alert","fetchCategories","filteredProducts","filtered","filter","toLowerCase","includes","goToAddProduct","push"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/ProductListPurchase.vue"],"sourcesConten