ms.value.push({\n          ...product,\n          quantity: 1\n        });\n      }\n    };\n\n    // Function to change product quantity\n    const changeQuantity = (index, delta) => {\n      orderItems.value[index].quantity += delta;\n    };\n\n    // Function to remove a product from the order\n    const removeItem = index => {\n      orderItems.value.splice(index, 1);\n    };\n\n    // Function to show a custom modal message\n    const showMessage = (title, message, type) => {\n      modalTitle.value = title;\n      modalMessage.value = message;\n      modalType.value = type;\n      showModal.value = true;\n    };\n\n    // Function to close the custom modal\n    const closeModal = () => {\n      showModal.value = false;\n      // Reset state after successful payment\n      if (modalType.value === 'success') {\n        customerName.value = '';\n        orderItems.value = [];\n        amountReceived.value = 0;\n      }\n    };\n\n    // Function to handle \"Pay Now\" action\n    const handlePayNow = () => {\n      if (orderItems.value.length === 0) {\n        showMessage('Gagal', 'Silakan tambahkan produk ke pesanan terlebih dahulu.', 'error');\n        return;\n      }\n      if (change.value < 0) {\n        showMessage('Gagal', 'Uang yang diterima kurang dari total.', 'error');\n        return;\n      }\n\n      // Simulate successful payment\n      const transactionData = {\n        customer: customerName.value || 'Umum',\n        items: orderItems.value.map(item => ({\n          name: item.name,\n          quantity: item.quantity,\n          price: item.price\n        })),\n        total: total.value,\n        paid: amountReceived.value,\n        change: change.value\n      };\n      console.log('Transaksi berhasil:', transactionData);\n      showMessage('Pembayaran Berhasil!', `Kembalian: ${formatCurrency(change.value)}. Transaksi selesai!`, 'success');\n    };\n\n    // Function to handle \"Print Receipt\" action\n    const handlePrintReceipt = () => {\n   