r) {\n    console.error(\n      \"Gagal mengambil laporan:\",\n      err.response?.data || err.message\n    );\n    reportData.value = [];\n    topProducts.value = [];\n    bottomProducts.value = [];\n  } finally {\n    loading.value = false;\n  }\n};\n\nconst exportReport = async (format) => {\n  try {\n    loading.value = true;\n    const token = localStorage.getItem(\"token\");\n    const res = await axios.get(API_URL, {\n      params: { type: \"sales\", by: filterBy.value, export: format },\n      headers: { Authorization: `Bearer ${token}` },\n      responseType: \"blob\",\n    });\n\n    const url = window.URL.createObjectURL(new Blob([res.data]));\n    const link = document.createElement(\"a\");\n    link.href = url;\n    link.setAttribute(\"download\", `sales_report.${format}`);\n    document.body.appendChild(link);\n    link.click();\n    document.body.removeChild(link);\n  } catch (err) {\n    console.error(\"Ekspor gagal:\", err.response?.data || err.message);\n  } finally {\n    loading.value = false;\n  }\n};\n\nconst formatCurrency = (num) => {\n  if (!num) return \"Rp 0\";\n  return new Intl.NumberFormat(\"id-ID\", {\n    style: \"currency\",\n    currency: \"IDR\",\n  }).format(num);\n};\n\nonMounted(fetchReport);\n</script>\n"],"mappings":";;;AA2MA,OAAOA,YAAY,MAAM,gCAAgC;AACzD,SAASC,UAAU,QAAQ,YAAY;AACvC,OAAOC,KAAK,MAAM,OAAO;AACzB,SAASC,GAAG,EAAEC,SAAS,QAAQ,KAAK;AAEpC,MAAMC,OAAO,GAAG,4CAA4C;;;;;;;IAE5D,MAAMC,UAAU,GAAGH,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMI,WAAW,GAAGJ,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMK,cAAc,GAAGL,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMM,QAAQ,GAAGN,GAAG,CAAC,OAAO,CAAC;IAC7B,MAAMO,OAAO,GAAGP,GAAG,CAAC,KAAK,CAAC;IAE1B,MAAMQ,WAAW,GAAG,MAAAA,CAAA,KAAY;MAC9B,IAAI;QACFD,OAAO,CAACE,KAAK,GAAG,IAAI;QACpB,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;;QAE3C;QACA,MAAMC,QAAQ,GAAG,MAAMd,KAAK,CAACe,GAAG,CAACZ,OAAO,EAAE;UACxCa,MAAM,EAAE;YAAEC,IAAI,EAAE,OAAO;YAAEC,EAAE,EAAEX,QAAQ,CAACG;UAAM,CAAC;UAC7CS,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUT,KAAK;UAAG;QAC9C,CAAC,CAAC;QACFP,U