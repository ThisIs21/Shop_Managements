{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.flat-map.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport AppSidebar from \"../components/CashierSidebar.vue\";\n\n// State definitions\n\nexport default {\n  __name: 'ReturnPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const salesHistory = ref([]);\n    const loadingHistory = ref(true);\n    const selectedSaleItems = ref([]);\n    const fromDate = ref(\"\");\n    const toDate = ref(\"\");\n    const returnItems = ref([]);\n    const additionalNotes = ref(\"\");\n    const isSubmitting = ref(false);\n\n    // Axios instance\n    const api = axios.create({\n      baseURL: \"http://localhost:8081/api/v1\"\n    });\n    const token = localStorage.getItem(\"token\");\n    if (token) {\n      api.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n    }\n\n    // Computed property to check if all items are selected\n    const isAllSelected = computed(() => {\n      const allItemsCount = salesHistory.value.reduce((total, sale) => total + sale.items.length, 0);\n      return selectedSaleItems.value.length > 0 && selectedSaleItems.value.length === allItemsCount;\n    });\n\n    // Load sales history data with date filters\n    const loadSalesHistory = async (isFiltered = false) => {\n      loadingHistory.value = true;\n      selectedSaleItems.value = []; // Reset selections\n      try {\n        const params = {\n          page: 1,\n          size: 20,\n          // Load 20 transaksi terakhir secara default\n          orderBy: \"date DESC\"\n        };\n        if (isFiltered) {\n          if (fromDate.value) {\n            params.start_date = fromDate.value;\n          }\n          if (toDate.value) {\n            params.end_date = toDate.value;\n          }\n        }\n        const res = await api.get(\"/history/sales\", {\n          params\n        });\n        salesHistory.value = res.data.data || [];\n      } catch (err) {\n        console.error(\"Error loading sales history:\", err);\n        alert(\"Gagal memuat riwayat transaksi!\");\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n\n    // Reset filter tanggal\n    const resetFilter = () => {\n      fromDate.value = \"\";\n      toDate.value = \"\";\n      loadSalesHistory();\n    };\n\n    // Select/Deselect all items\n    const selectAllItems = event => {\n      if (event.target.checked) {\n        selectedSaleItems.value = salesHistory.value.flatMap(sale => sale.items);\n      } else {\n        selectedSaleItems.value = [];\n      }\n    };\n\n    // Add selected items to the return table\n    const addToReturnTable = () => {\n      selectedSaleItems.value.forEach(selectedItem => {\n        // Cek apakah item sudah ada di tabel retur\n        const existingItem = returnItems.value.find(item => item.product.id === selectedItem.product.id);\n        if (!existingItem) {\n          returnItems.value.push({\n            ...selectedItem,\n            returnQty: 0,\n            reason: \"\"\n          });\n        }\n      });\n      selectedSaleItems.value = []; // Clear selection after adding\n    };\n\n    // Remove item from return table\n    const removeItem = index => {\n      returnItems.value.splice(index, 1);\n    };\n\n    // Format currency\n    const formatCurrency = num => new Intl.NumberFormat(\"id-ID\", {\n      style: \"currency\",\n      currency: \"IDR\"\n    }).format(num);\n\n    // Format date\n    const formatDate = dateStr => {\n      return new Date(dateStr).toLocaleString(\"id-ID\", {\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\",\n        hour: \"2-digit\",\n        minute: \"2-digit\"\n      });\n    };\n\n    // Submit return\n    const submitReturn = async () => {\n      const itemsToReturn = returnItems.value.filter(item => item.returnQty > 0 && item.reason);\n      if (itemsToReturn.length === 0) {\n        alert(\"Masukkan jumlah retur dan alasan untuk setidaknya satu item!\");\n        return;\n      }\n      isSubmitting.value = true;\n      try {\n        const payload = {\n          date: new Date().toISOString(),\n          notes: additionalNotes.value || null,\n          items: itemsToReturn.map(item => ({\n            product_id: item.product.id,\n            qty: item.returnQty,\n            reason: item.reason\n          }))\n        };\n        const res = await api.post(\"/kasir/sale-returns\", payload);\n        alert(`Retur berhasil! ID: ${res.data.id}`);\n\n        // Reset form setelah berhasil\n        returnItems.value = [];\n        additionalNotes.value = \"\";\n        loadSalesHistory(); // Refresh daftar riwayat penjualan\n      } catch (err) {\n        console.error(\"Error submitting return:\", err);\n        alert(\"Gagal mengirim retur: \" + (err.response?.data?.error || err.message));\n      } finally {\n        isSubmitting.value = false;\n      }\n    };\n\n    // Load data on mount\n    onMounted(() => {\n      loadSalesHistory();\n    });\n    const __returned__ = {\n      salesHistory,\n      loadingHistory,\n      selectedSaleItems,\n      fromDate,\n      toDate,\n      returnItems,\n      additionalNotes,\n      isSubmitting,\n      api,\n      token,\n      isAllSelected,\n      loadSalesHistory,\n      resetFilter,\n      selectAllItems,\n      addToReturnTable,\n      removeItem,\n      formatCurrency,\n      formatDate,\n      submitReturn,\n      ref,\n      onMounted,\n      computed,\n      get axios() {\n        return axios;\n      },\n      AppSidebar\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","axios","AppSidebar","salesHistory","loadingHistory","selectedSaleItems","fromDate","toDate","returnItems","additionalNotes","isSubmitting","api","create","baseURL","token","localStorage","getItem","defaults","headers","common","isAllSelected","allItemsCount","value","reduce","total","sale","items","length","loadSalesHistory","isFiltered","params","page","size","orderBy","start_date","end_date","res","get","data","err","console","error","alert","resetFilter","selectAllItems","event","target","checked","flatMap","addToReturnTable","forEach","selectedItem","existingItem","find","item","product","id","push","returnQty","reason","removeItem","index","splice","formatCurrency","num","Intl","NumberFormat","style","currency","format","formatDate","dateStr","Date","toLocaleString","year","month","day","hour","minute","submitReturn","itemsToReturn","filter","payload","date","toISOString","notes","map","product_id","qty","post","response","message"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/ReturnPage.vue"],"sourcesContent":["<template>\n  <div class=\"flex size-full min-h-screen\">\n    <AppSidebar />\n    <div\n      class=\"flex h-full grow flex-col bg-white p-6\"\n      style=\"font-family: Manrope, 'Noto Sans', sans-serif\"\n    >\n      <div class=\"layout-content-container flex flex-col flex-1 px-4 py-5\">\n        <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n          <div class=\"flex min-w-72 flex-col gap-3\">\n            <p\n              class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight\"\n            >\n              Sales Return\n            </p>\n            <p class=\"text-[#60758a] text-sm font-normal leading-normal\">\n              Create a return for a previous sale\n            </p>\n          </div>\n        </div>\n\n        <div class=\"p-4 bg-[#f0f2f5] rounded-lg mb-4\">\n          <h3\n            class=\"text-[#111418] text-lg font-bold leading-tight tracking-[-0.015em] mb-4\"\n          >\n            Filter Sales History\n          </h3>\n          <div class=\"flex flex-wrap gap-4 items-end\">\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Dari Tanggal</label\n              >\n              <input\n                type=\"date\"\n                v-model=\"fromDate\"\n                class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n              />\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Sampai Tanggal</label\n              >\n              <input\n                type=\"date\"\n                v-model=\"toDate\"\n                class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n              />\n            </div>\n            <button\n              @click=\"loadSalesHistory(true)\"\n              :disabled=\"loadingHistory\"\n              class=\"px-4 py-2 bg-blue-500 text-white rounded-md hover:bg-blue-600 disabled:opacity-50\"\n            >\n              {{ loadingHistory ? \"Loading...\" : \"Filter\" }}\n            </button>\n            <button\n              @click=\"resetFilter\"\n              class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\"\n            >\n              Reset\n            </button>\n          </div>\n        </div>\n\n        <h3\n          class=\"text-[#111418] text-lg font-bold leading-tight tracking-[-0.015em] px-4 pb-2 pt-4\"\n        >\n          Select Items from Sales History\n        </h3>\n        <div class=\"px-4 py-3\">\n          <div\n            class=\"mt-4 overflow-x-auto rounded-lg border border-[#dbe0e6] bg-white\"\n          >\n            <table class=\"min-w-full\">\n              <thead>\n                <tr class=\"bg-gray-100\">\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    <input\n                      type=\"checkbox\"\n                      @change=\"selectAllItems\"\n                      :checked=\"isAllSelected\"\n                      class=\"form-checkbox\"\n                    />\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Tanggal\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Nomor Transaksi\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Produk\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Qty\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Total\n                  </th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-if=\"loadingHistory\">\n                  <td colspan=\"6\" class=\"text-center py-4\">\n                    Loading history...\n                  </td>\n                </tr>\n                <tr v-else-if=\"salesHistory.length === 0\">\n                  <td colspan=\"6\" class=\"text-center py-4 text-gray-500\">\n                    Tidak ada data penjualan.\n                  </td>\n                </tr>\n                <template v-else>\n                  <tr\n                    v-for=\"sale in salesHistory\"\n                    :key=\"sale.ID\"\n                    class=\"border-t border-t-[#dbe0e6]\"\n                  >\n                    <td colspan=\"6\" class=\"p-0\">\n                      <table class=\"w-full\">\n                        <tr\n                          v-for=\"(item, itemIndex) in sale.items\"\n                          :key=\"itemIndex\"\n                        >\n                          <td class=\"px-6 py-4 whitespace-nowrap w-[3%]\">\n                            <input\n                              type=\"checkbox\"\n                              :value=\"item\"\n                              v-model=\"selectedSaleItems\"\n                              class=\"form-checkbox\"\n                              :data-sale-id=\"sale.ID\"\n                            />\n                          </td>\n                          <td class=\"px-6 py-4 whitespace-nowrap w-[15%]\">\n                            {{ formatDate(sale.Date) || \"N/A\" }}\n                          </td>\n                          <td class=\"px-6 py-4 whitespace-nowrap w-[15%]\">\n                            {{\n                              sale.transaction_number ||\n                              `TRX-${sale.ID || \"N/A\"}`\n                            }}\n                          </td>\n                          <td class=\"px-6 py-4 whitespace-nowrap w-[30%]\">\n                            {{ item.product.name }}\n                          </td>\n                          <td class=\"px-6 py-4 whitespace-nowrap w-[10%]\">\n                            {{ item.qty }}\n                          </td>\n                          <td class=\"px-6 py-4 whitespace-nowrap w-[20%]\">\n                            {{\n                              formatCurrency(item.qty * item.product.sell_price)\n                            }}\n                          </td>\n                        </tr>\n                      </table>\n                    </td>\n                  </tr>\n                </template>\n              </tbody>\n            </table>\n          </div>\n          <div class=\"flex justify-end mt-4\">\n            <button\n              @click=\"addToReturnTable\"\n              :disabled=\"selectedSaleItems.length === 0\"\n              class=\"px-4 py-2 bg-blue-500 text-white rounded-md hover:bg-blue-600 disabled:opacity-50\"\n            >\n              Add Selected to Return\n            </button>\n          </div>\n        </div>\n\n        <hr class=\"my-4\" />\n\n        <h3\n          class=\"text-[#111418] text-lg font-bold leading-tight tracking-[-0.015em] px-4 pb-2 pt-4\"\n        >\n          Return Items\n        </h3>\n        <div class=\"px-4 py-3 @container\">\n          <div\n            class=\"flex overflow-hidden rounded-lg border border-[#dbe0e6] bg-white\"\n          >\n            <table class=\"flex-1\">\n              <thead>\n                <tr class=\"bg-white\">\n                  <th\n                    class=\"table-column px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\"\n                  >\n                    Product\n                  </th>\n                  <th\n                    class=\"table-column px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\"\n                  >\n                    Quantity\n                  </th>\n                  <th\n                    class=\"table-column px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\"\n                  >\n                    Return Quantity\n                  </th>\n                  <th\n                    class=\"table-column px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\"\n                  >\n                    Reason\n                  </th>\n                  <th\n                    class=\"table-column px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\"\n                  >\n                    Aksi\n                  </th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-if=\"returnItems.length === 0\">\n                  <td colspan=\"5\" class=\"text-center text-[#60758a] py-4\">\n                    No items available for return\n                  </td>\n                </tr>\n                <tr\n                  v-else\n                  v-for=\"(item, index) in returnItems\"\n                  :key=\"index\"\n                  class=\"border-t border-t-[#dbe0e6]\"\n                >\n                  <td\n                    class=\"table-column h-[72px] px-4 py-2 text-[#111418] text-sm font-normal leading-normal\"\n                  >\n                    {{ item.product.name }}\n                  </td>\n                  <td\n                    class=\"table-column h-[72px] px-4 py-2 text-[#60758a] text-sm font-normal leading-normal\"\n                  >\n                    {{ item.qty }}\n                  </td>\n                  <td\n                    class=\"table-column h-[72px] px-4 py-2 text-[#60758a] text-sm font-normal leading-normal\"\n                  >\n                    <input\n                      v-model.number=\"item.returnQty\"\n                      type=\"number\"\n                      min=\"0\"\n                      :max=\"item.qty\"\n                      class=\"w-full border rounded p-2\"\n                    />\n                  </td>\n                  <td\n                    class=\"table-column h-[72px] px-4 py-2 text-[#60758a] text-sm font-normal leading-normal\"\n                  >\n                    <input\n                      v-model=\"item.reason\"\n                      type=\"text\"\n                      placeholder=\"Enter reason\"\n                      class=\"w-full border rounded p-2\"\n                    />\n                  </td>\n                  <td\n                    class=\"table-column h-[72px] px-4 py-2 text-red-500 cursor-pointer\"\n                    @click=\"removeItem(index)\"\n                  >\n                    Remove\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n        <div class=\"flex max-w-[480px] flex-wrap items-end gap-4 px-4 py-3\">\n          <label class=\"flex flex-col min-w-40 flex-1\">\n            <textarea\n              v-model=\"additionalNotes\"\n              placeholder=\"Additional Notes\"\n              class=\"form-input flex w-full min-w-0 flex-1 resize-none overflow-hidden rounded-lg text-[#111418] focus:outline-0 focus:ring-0 border border-[#dbe0e6] bg-white focus:border-[#dbe0e6] min-h-36 placeholder:text-[#60758a] p-[15px] text-base font-normal leading-normal\"\n            ></textarea>\n          </label>\n        </div>\n        <div class=\"flex px-4 py-3 justify-end\">\n          <button\n            @click=\"submitReturn\"\n            :disabled=\"isSubmitting || returnItems.length === 0\"\n            class=\"flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-lg h-10 px-4 bg-[#0d80f2] text-white text-sm font-bold leading-normal tracking-[0.015em]\"\n          >\n            <span class=\"truncate\">{{\n              isSubmitting ? \"Submitting...\" : \"Submit Return\"\n            }}</span>\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport AppSidebar from \"../components/CashierSidebar.vue\";\n\n// State definitions\nconst salesHistory = ref([]);\nconst loadingHistory = ref(true);\nconst selectedSaleItems = ref([]);\nconst fromDate = ref(\"\");\nconst toDate = ref(\"\");\n\nconst returnItems = ref([]);\nconst additionalNotes = ref(\"\");\nconst isSubmitting = ref(false);\n\n// Axios instance\nconst api = axios.create({ baseURL: \"http://localhost:8081/api/v1\" });\nconst token = localStorage.getItem(\"token\");\nif (token) {\n  api.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n}\n\n// Computed property to check if all items are selected\nconst isAllSelected = computed(() => {\n  const allItemsCount = salesHistory.value.reduce(\n    (total, sale) => total + sale.items.length,\n    0\n  );\n  return (\n    selectedSaleItems.value.length > 0 &&\n    selectedSaleItems.value.length === allItemsCount\n  );\n});\n\n// Load sales history data with date filters\nconst loadSalesHistory = async (isFiltered = false) => {\n  loadingHistory.value = true;\n  selectedSaleItems.value = []; // Reset selections\n  try {\n    const params = {\n      page: 1,\n      size: 20, // Load 20 transaksi terakhir secara default\n      orderBy: \"date DESC\",\n    };\n\n    if (isFiltered) {\n      if (fromDate.value) {\n        params.start_date = fromDate.value;\n      }\n      if (toDate.value) {\n        params.end_date = toDate.value;\n      }\n    }\n\n    const res = await api.get(\"/history/sales\", { params });\n    salesHistory.value = res.data.data || [];\n  } catch (err) {\n    console.error(\"Error loading sales history:\", err);\n    alert(\"Gagal memuat riwayat transaksi!\");\n  } finally {\n    loadingHistory.value = false;\n  }\n};\n\n// Reset filter tanggal\nconst resetFilter = () => {\n  fromDate.value = \"\";\n  toDate.value = \"\";\n  loadSalesHistory();\n};\n\n// Select/Deselect all items\nconst selectAllItems = (event) => {\n  if (event.target.checked) {\n    selectedSaleItems.value = salesHistory.value.flatMap((sale) => sale.items);\n  } else {\n    selectedSaleItems.value = [];\n  }\n};\n\n// Add selected items to the return table\nconst addToReturnTable = () => {\n  selectedSaleItems.value.forEach((selectedItem) => {\n    // Cek apakah item sudah ada di tabel retur\n    const existingItem = returnItems.value.find(\n      (item) => item.product.id === selectedItem.product.id\n    );\n    if (!existingItem) {\n      returnItems.value.push({\n        ...selectedItem,\n        returnQty: 0,\n        reason: \"\",\n      });\n    }\n  });\n  selectedSaleItems.value = []; // Clear selection after adding\n};\n\n// Remove item from return table\nconst removeItem = (index) => {\n  returnItems.value.splice(index, 1);\n};\n\n// Format currency\nconst formatCurrency = (num) =>\n  new Intl.NumberFormat(\"id-ID\", { style: \"currency\", currency: \"IDR\" }).format(\n    num\n  );\n\n// Format date\nconst formatDate = (dateStr) => {\n  return new Date(dateStr).toLocaleString(\"id-ID\", {\n    year: \"numeric\",\n    month: \"long\",\n    day: \"numeric\",\n    hour: \"2-digit\",\n    minute: \"2-digit\",\n  });\n};\n\n// Submit return\nconst submitReturn = async () => {\n  const itemsToReturn = returnItems.value.filter(\n    (item) => item.returnQty > 0 && item.reason\n  );\n  if (itemsToReturn.length === 0) {\n    alert(\"Masukkan jumlah retur dan alasan untuk setidaknya satu item!\");\n    return;\n  }\n\n  isSubmitting.value = true;\n  try {\n    const payload = {\n      date: new Date().toISOString(),\n      notes: additionalNotes.value || null,\n      items: itemsToReturn.map((item) => ({\n        product_id: item.product.id,\n        qty: item.returnQty,\n        reason: item.reason,\n      })),\n    };\n\n    const res = await api.post(\"/kasir/sale-returns\", payload);\n    alert(`Retur berhasil! ID: ${res.data.id}`);\n\n    // Reset form setelah berhasil\n    returnItems.value = [];\n    additionalNotes.value = \"\";\n    loadSalesHistory(); // Refresh daftar riwayat penjualan\n  } catch (err) {\n    console.error(\"Error submitting return:\", err);\n    alert(\n      \"Gagal mengirim retur: \" + (err.response?.data?.error || err.message)\n    );\n  } finally {\n    isSubmitting.value = false;\n  }\n};\n\n// Load data on mount\nonMounted(() => 