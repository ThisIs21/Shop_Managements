{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport GudangSidebar from \"../components/GudangSidebar.vue\"; // Sesuaikan path\n\nconst API_BASE = \"http://localhost:8081/api/v1\"; // ganti sesuai backend\n\nexport default {\n  __name: 'StockOpnameHistory',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const token = localStorage.getItem(\"token\");\n    const stockOpnames = ref([]);\n    const loading = ref(false);\n    const totalItems = ref(0);\n    const searchQuery = ref(\"\");\n    const startDate = ref(null);\n    const endDate = ref(null);\n\n    // Modal state\n    const selectedSO = ref(null);\n    const isModalOpen = ref(false);\n    const page = ref(1);\n    const size = ref(10);\n    const formatDate = dateString => {\n      if (!dateString) return \"-\";\n      return new Date(dateString).toLocaleDateString(\"id-ID\", {\n        day: \"2-digit\",\n        month: \"2-digit\",\n        year: \"numeric\"\n      });\n    };\n    const closeModal = () => {\n      isModalOpen.value = false;\n      selectedSO.value = null;\n    };\n\n    // === API Calls ===\n    const loadStockOpnames = async () => {\n      loading.value = true;\n      stockOpnames.value = [];\n      try {\n        const res = await axios.get(`${API_BASE}/history/stock-opnames`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          },\n          params: {\n            page: page.value,\n            size: size.value,\n            q: searchQuery.value,\n            start_date: startDate.value,\n            end_date: endDate.value\n          }\n        });\n\n        // backend di Go return { data: [...], total: n }\n        const data = res.data.data || [];\n        stockOpnames.value = data;\n        totalItems.value = res.data.total || data.length;\n      } catch (err) {\n        console.error(\"Error loading stock opnames:\", err.response?.data || err);\n        alert(\"Gagal memuat daftar Riwayat Stock Opname.\");\n      } finally {\n        loading.value = false;\n      }\n    };\n    const showDetail = async id => {\n      try {\n        const res = await axios.get(`${API_BASE}/history/stock-opnames/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n\n        // Pastikan backend return { data: {...} }\n        selectedSO.value = res.data.data;\n        isModalOpen.value = true;\n      } catch (err) {\n        console.error(\"Error loading SO detail:\", err.response?.data || err);\n        alert(`Gagal memuat detail Stock Opname #${id}`);\n      }\n    };\n    onMounted(() => {\n      loadStockOpnames();\n    });\n    const __returned__ = {\n      API_BASE,\n      token,\n      stockOpnames,\n      loading,\n      totalItems,\n      searchQuery,\n      startDate,\n      endDate,\n      selectedSO,\n      isModalOpen,\n      page,\n      size,\n      formatDate,\n      closeModal,\n      loadStockOpnames,\n      showDetail,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      GudangSidebar\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","GudangSidebar","API_BASE","token","localStorage","getItem","stockOpnames","loading","totalItems","searchQuery","startDate","endDate","selectedSO","isModalOpen","page","size","formatDate","dateString","Date","toLocaleDateString","day","month","year","closeModal","value","loadStockOpnames","res","get","headers","Authorization","params","q","start_date","end_date","data","total","length","err","console","error","response","alert","showDetail","id"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/StockOpnameHistory.vue"],"sourcesContent":["<template>\n  <div class=\"flex\">\n    <GudangSidebar />\n\n    <main class=\"flex-1 ml-64 p-6\">\n      <h1 class=\"text-2xl font-bold mb-6\">Riwayat Stock Opname</h1>\n\n      <div class=\"flex justify-between items-center mb-4\">\n        <div class=\"flex gap-4\">\n          <label class=\"text-sm font-medium pt-2\">Dari Tanggal:</label>\n          <input\n            type=\"date\"\n            v-model=\"startDate\"\n            @change=\"loadStockOpnames\"\n            class=\"border rounded p-2 text-sm\"\n          />\n          <label class=\"text-sm font-medium pt-2\">Sampai Tanggal:</label>\n          <input\n            type=\"date\"\n            v-model=\"endDate\"\n            @change=\"loadStockOpnames\"\n            class=\"border rounded p-2 text-sm\"\n          />\n        </div>\n        <input\n          type=\"text\"\n          v-model=\"searchQuery\"\n          @keyup.enter=\"loadStockOpnames\"\n          placeholder=\"Cari ID atau Oleh...\"\n          class=\"border rounded p-2 text-sm w-64\"\n        />\n      </div>\n\n      <div class=\"bg-white shadow-md rounded-lg overflow-hidden\">\n        <table class=\"min-w-full leading-normal\">\n          <thead>\n            <tr\n              class=\"bg-gray-100 text-gray-600 uppercase text-sm leading-normal\"\n            >\n              <th class=\"py-3 px-6 text-left\">ID SO</th>\n              <th class=\"py-3 px-6 text-left\">Tanggal</th>\n              <th class=\"py-3 px-6 text-left\">Dibuat Oleh</th>\n              <th class=\"py-3 px-6 text-right\">Total Item Dicatat</th>\n              <th class=\"py-3 px-6 text-center\">Aksi</th>\n            </tr>\n          </thead>\n          <tbody class=\"text-gray-600 text-sm font-light\">\n            <tr\n              v-for=\"so in stockOpnames\"\n              :key=\"so.ID\"\n              class=\"border-b border-gray-200 hover:bg-gray-50\"\n            >\n              <td class=\"py-3 px-6 text-left whitespace-nowrap\">{{ so.ID }}</td>\n              <td class=\"py-3 px-6 text-left\">\n                {{ formatDate(so.Date) }}\n              </td>\n              <td class=\"py-3 px-6 text-left\">\n                {{ so.User?.name || \"N/A\" }}\n              </td>\n              <td class=\"py-3 px-6 text-right\">\n                {{ so.TotalItems || so.Items?.length || 0 }}\n              </td>\n              <td class=\"py-3 px-6 text-center\">\n                <button\n                  @click=\"showDetail(so.ID)\"\n                  class=\"text-blue-500 hover:text-blue-700 text-xs font-medium\"\n                >\n                  Detail\n                </button>\n              </td>\n            </tr>\n            <tr v-if=\"!stockOpnames.length && !loading\">\n              <td colspan=\"5\" class=\"py-4 text-center text-gray-500\">\n                Tidak ada riwayat Stock Opname ditemukan.\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n\n      <div class=\"flex justify-between items-center mt-4\">\n        <p class=\"text-sm text-gray-600\">Total: {{ totalItems }}</p>\n        <div v-if=\"loading\" class=\"text-blue-500\">Memuat...</div>\n      </div>\n    </main>\n\n    <div\n      v-if=\"isModalOpen\"\n      class=\"fixed inset-0 bg-gray-600 bg-opacity-50 overflow-y-auto h-full w-full z-50 flex justify-center items-center\"\n      @click=\"closeModal\"\n    >\n      <div\n        class=\"bg-white p-6 rounded-lg shadow-xl max-w-6xl w-full mx-4\"\n        @click.stop\n      >\n        <div class=\"flex justify-between items-center border-b pb-3 mb-4\">\n          <h3 class=\"text-xl font-bold\">\n            Detail Stock Opname #{{ selectedSO?.ID }}\n          </h3>\n          <button\n            @click=\"closeModal\"\n            class=\"text-gray-500 hover:text-gray-800 text-2xl leading-none\"\n          >\n            &times;\n          </button>\n        </div>\n\n        <div v-if=\"selectedSO\">\n          <div class=\"grid grid-cols-2 gap-4 text-sm mb-4\">\n            <div>\n              <p><strong>ID SO:</strong> {{ selectedSO.ID }}</p>\n              <p><strong>Tanggal:</strong> {{ formatDate(selectedSO.Date) }}</p>\n            </div>\n            <div>\n              <p>\n                <strong>Dibuat Oleh:</strong>\n                {{ selectedSO.User?.name || \"N/A\" }}\n              </p>\n              <p>\n                <strong>Keterangan:</strong>\n                {{ selectedSO.Notes || \"-\" }}\n              </p>\n            </div>\n          </div>\n\n          <h4 class=\"text-lg font-semibold mb-2\">Item Perubahan Stok</h4>\n          <div class=\"overflow-x-auto border rounded\">\n            <table class=\"min-w-full text-sm\">\n              <thead>\n                <tr class=\"bg-gray-50\">\n                  <th class=\"py-2 px-4 text-left\">Produk</th>\n                  <th class=\"py-2 px-4 text-left\">Kategori</th>\n                  <th class=\"py-2 px-4 text-left\">Unit</th>\n                  <th class=\"py-2 px-4 text-right\">Stok Fisik</th>\n                  <th class=\"py-2 px-4 text-right\">Stok Sistem</th>\n                  <th class=\"py-2 px-4 text-right\">Selisih</th>\n                </tr>\n              </thead>\n              <tbody>\n  <tr\n    v-for=\"item in selectedSO.Items\"\n    :key=\"item.ID\"\n    class=\"border-t\"\n  >\n    <td class=\"py-2 px-4\">{{ item.Product?.name || \"Produk Hilang\" }}</td>\n    <td class=\"py-2 px-4\">{{ item.Product?.Category?.name || \"-\" }}</td>\n    <td class=\"py-2 px-4\">{{ item.Product?.Unit?.name || \"-\" }}</td>\n    <td class=\"py-2 px-4 text-right\">{{ item.QtyPhysical }}</td>\n    <td class=\"py-2 px-4 text-right\">{{ item.QtySystem }}</td>\n    <td\n      class=\"py-2 px-4 text-right\"\n      :class=\"{\n        'text-red-600 font-semibold': item.QtyPhysical !== item.QtySystem\n      }\"\n    >\n      {{ item.QtyPhysical - item.QtySystem }}\n    </td>\n  </tr>\n</tbody>\n\n            </table>\n          </div>\n        </div>\n\n        <div class=\"mt-4 flex justify-end\">\n          <button\n            @click=\"closeModal\"\n            class=\"bg-gray-300 hover:bg-gray-400 text-gray-800 font-bold py-2 px-4 rounded\"\n          >\n            Tutup\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport GudangSidebar from \"../components/GudangSidebar.vue\"; // Sesuaikan path\n\nconst API_BASE = \"http://localhost:8081/api/v1\"; // ganti sesuai backend\nconst token = localStorage.getItem(\"token\");\n\nconst stockOpnames = ref([]);\nconst loading = ref(false);\nconst totalItems = ref(0);\nconst searchQuery = ref(\"\");\nconst startDate = ref(null);\nconst endDate = ref(null);\n\n// Modal state\nconst selectedSO = ref(null);\nconst isModalOpen = ref(false);\n\nconst page = ref(1);\nconst size = ref(10);\n\nconst formatDate = (dateString) => {\n  if (!dateString) return \"-\";\n  return new Date(dateString).toLocaleDateString(\"id-ID\", {\n    day: \"2-digit\",\n    month: \"2-digit\",\n    year: \"numeric\",\n  });\n};\n\nconst closeModal = () => {\n  isModalOpen.value = false;\n  selectedSO.value = null;\n};\n\n// === API Calls ===\nconst loadStockOpnames = async () => {\n  loading.value = true;\n  stockOpnames.value = [];\n  try {\n    const res = await axios.get(`${API_BASE}/history/stock-opnames`, {\n      headers: { Authorization: `Bearer ${token}` },\n      params: {\n        page: page.value,\n        size: size.value,\n        q: searchQuery.value,\n        start_date: startDate.value,\n        end_date: endDate.value,\n      },\n    });\n\n    // backend di Go return { data: [...], total: n }\n    const data = res.data.data || [];\n    stockOpnames.value = data;\n    totalItems.value = res.data.total || data.length;\n  } catch (err) {\n    console.error(\"Error loading stock opnames:\", err.response?.data || err);\n    alert(\"Gagal memuat daftar Riwayat Stock Opname.\");\n  } finally {\n    loading.value = false;\n  }\n};\n\nconst showDetail = async (id) => {\n  try {\n    const res = await axios.get(`${API_BASE}/history/stock-opnames/${id}`, {\n      headers: { Authorization: `Bearer ${token}` },\n    });\n\n    // Pastikan backend return { data: {...} }\n    selectedSO.value = res.data.data;\n    isModalOpen.value = true;\n  } catch (err) {\n    console.error(\"Error loading SO detail:\", err.response?.data || err);\n    alert(`Gagal memuat detail Stock Opname #${id}`);\n  }\n};\n\nonMounted(() => {\n  loadStockOpnames();\n});\n</script>\n\n"],"mappings":"AAkLA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,aAAa,MAAM,iCAAiC,CAAC,CAAC;;AAE7D,MAAMC,QAAQ,GAAG,8BAA8B,CAAC,CAAC;;;;;;;;IACjD,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;IAE3C,MAAMC,YAAY,GAAGR,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMS,OAAO,GAAGT,GAAG,CAAC,KAAK,CAAC;IAC1B,MAAMU,UAAU,GAAGV,GAAG,CAAC,CAAC,CAAC;IACzB,MAAMW,WAAW,GAAGX,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMY,SAAS,GAAGZ,GAAG,CAAC,IAAI,CAAC;IAC3B,MAAMa,OAAO,GAAGb,GAAG,CAAC,IAAI,CAAC;;IAEzB;IACA,MAAMc,UAAU,GAAGd,GAAG,CAAC,IAAI,CAAC;IAC5B,MAAMe,WAAW,GAAGf,GAAG,CAAC,KAAK,CAAC;IAE9B,MAAMgB,IAAI,GAAGhB,GAAG,CAAC,CAAC,CAAC;IACnB,MAAMiB,IAAI,GAAGjB,GAAG,CAAC,EAAE,CAAC;IAEpB,MAAMkB,UAAU,GAAIC,UAAU,IAAK;MACjC,IAAI,CAACA,UAAU,EAAE,OAAO,GAAG;MAC3B,OAAO,IAAIC,IAAI,CAACD,UAAU,CAAC,CAACE,kBAAkB,CAAC,OAAO,EAAE;QACtDC,GAAG,EAAE,SAAS;QACdC,KAAK,EAAE,SAAS;QAChBC,IAAI,EAAE;MACR,CAAC,CAAC;IACJ,CAAC;IAED,MAAMC,UAAU,GAAGA,CAAA,KAAM;MACvBV,WAAW,CAACW,KAAK,GAAG,KAAK;MACzBZ,UAAU,CAACY,KAAK,GAAG,IAAI;IACzB,CAAC;;IAED;IACA,MAAMC,gBAAgB,GAAG,MAAAA,CAAA,KAAY;MACnClB,OAAO,CAACiB,KAAK,GAAG,IAAI;MACpBlB,YAAY,CAACkB,KAAK,GAAG,EAAE;MACvB,IAAI;QACF,MAAME,GAAG,GAAG,MAAM1B,KAAK,CAAC2B,GAAG,CAAC,GAAGzB,QAAQ,wBAAwB,EAAE;UAC/D0B,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAU1B,KAAK;UAAG,CAAC;UAC7C2B,MAAM,EAAE;YACNhB,IAAI,EAAEA,IAAI,CAACU,KAAK;YAChBT,IAAI,EAAEA,IAAI,CAACS,KAAK;YAChBO,CAAC,EAAEtB,WAAW,CAACe,KAAK;YACpBQ,UAAU,EAAEtB,SAAS,CAACc,KAAK;YAC3BS,QAAQ,EAAEtB,OAAO,CAACa;UACpB;QACF,CAAC,CAAC;;QAEF;QACA,MAAMU,IAAI,GAAGR,GAA