{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport PurchasingSidebar from \"@/components/PurchasingSidebar.vue\";\nexport default {\n  __name: 'ReturnPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const purchaseHistory = ref([]);\n    const purchaseReturns = ref([]);\n    const loadingHistory = ref(true);\n    const loadingReturns = ref(false);\n    const fromDate = ref(\"\");\n    const toDate = ref(\"\");\n    const returnFromDate = ref(\"\");\n    const returnToDate = ref(\"\");\n    const returnStatusFilter = ref(\"\");\n    const showModal = ref(false);\n    const selectedPurchase = ref(null);\n    const returnForm = ref({\n      items: [],\n      notes: \"\"\n    });\n    const submitting = ref(false);\n    const token = localStorage.getItem(\"token\");\n    const api = axios.create({\n      baseURL: \"http://localhost:8081/api/v1\"\n    });\n    if (token) api.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n\n    // Format functions\n    const formatPONumber = id => `PO-${String(id).padStart(3, \"0\")}`;\n    const formatReturnNumber = id => `RTN-${String(id).padStart(3, \"0\")}`;\n    const formatDate = dateStr => {\n      if (!dateStr) return \"N/A\";\n      return new Date(dateStr).toLocaleDateString(\"id-ID\", {\n        day: \"2-digit\",\n        month: \"2-digit\",\n        year: \"numeric\"\n      });\n    };\n    const formatCurrency = amount => amount !== undefined && amount !== null ? new Intl.NumberFormat(\"id-ID\", {\n      style: \"currency\",\n      currency: \"IDR\",\n      minimumFractionDigits: 0\n    }).format(amount) : \"Rp 0\";\n    const getStatusClass = status => {\n      switch (status) {\n        case \"PENDING\":\n          return \"bg-yellow-100 text-yellow-800\";\n        case \"APPROVED\":\n          return \"bg-green-100 text-green-800\";\n        case \"REJECTED\":\n          return \"bg-red-100 text-red-800\";\n        default:\n          return \"bg-gray-100 text-gray-800\";\n      }\n    };\n    const getStatusText = status => {\n      switch (status) {\n        case \"PENDING\":\n          return \"Pending\";\n        case \"APPROVED\":\n          return \"Approved\";\n        case \"REJECTED\":\n          return \"Rejected\";\n        default:\n          return status;\n      }\n    };\n\n    // Computed untuk validasi items\n    const hasValidItems = computed(() => returnForm.value.items.some(item => item.qty > 0 && item.reason.trim()));\n    const fetchPurchaseHistory = async () => {\n      loadingHistory.value = true;\n      try {\n        const params = {\n          page: 1,\n          size: 10,\n          status: \"APPROVED\"\n        };\n        if (fromDate.value) params.from = fromDate.value;\n        if (toDate.value) params.to = toDate.value;\n        const response = await api.get(\"/pembelian/purchases\", {\n          params\n        });\n        purchaseHistory.value = response.data.data.data || [];\n      } catch (error) {\n        console.error(\"Error fetching purchase history:\", error);\n        purchaseHistory.value = [];\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n    const fetchPurchaseReturns = async () => {\n      loadingReturns.value = true;\n      try {\n        const params = {\n          page: 1,\n          size: 10\n        };\n        if (returnFromDate.value) params.start_date = returnFromDate.value;\n        if (returnToDate.value) params.end_date = returnToDate.value;\n        if (returnStatusFilter.value) params.status = returnStatusFilter.value;\n        const response = await api.get(\"/pembelian/purchase-returns\", {\n          params\n        });\n        purchaseReturns.value = response.data.data || [];\n      } catch (error) {\n        console.error(\"Error fetching purchase returns:\", error);\n        purchaseReturns.value = [];\n      } finally {\n        loadingReturns.value = false;\n      }\n    };\n    const resetFilter = () => {\n      fromDate.value = \"\";\n      toDate.value = \"\";\n      fetchPurchaseHistory();\n    };\n    const resetReturnFilter = () => {\n      returnFromDate.value = \"\";\n      returnToDate.value = \"\";\n      returnStatusFilter.value = \"\";\n      fetchPurchaseReturns();\n    };\n    const openReturnModal = async purchaseId => {\n      try {\n        const response = await api.get(`/pembelian/purchases/${purchaseId}`);\n        selectedPurchase.value = response.data.data;\n        returnForm.value.items = selectedPurchase.value.Items.map(item => ({\n          product_id: item.ProductID,\n          qty: 0,\n          reason: \"\"\n        }));\n        returnForm.value.notes = \"\";\n        showModal.value = true;\n      } catch (error) {\n        console.error(\"Error fetching purchase:\", error);\n        alert(\"Gagal memuat detail pembelian.\");\n      }\n    };\n    const closeModal = () => {\n      showModal.value = false;\n      selectedPurchase.value = null;\n      returnForm.value = {\n        items: [],\n        notes: \"\"\n      };\n    };\n    const submitReturn = async () => {\n      if (!hasValidItems.value) {\n        alert(\"Pilih minimal satu item dengan qty > 0 dan alasan.\");\n        return;\n      }\n      submitting.value = true;\n      try {\n        const validItems = returnForm.value.items.filter(item => item.qty > 0 && item.reason.trim());\n        const dto = {\n          purchase_id: selectedPurchase.value.ID,\n          date: new Date().toISOString(),\n          items: validItems.map(item => ({\n            product_id: item.product_id,\n            qty: item.qty,\n            price: selectedPurchase.value.Items.find(i => i.ProductID === item.product_id)?.Price || 0\n          })),\n          notes: returnForm.value.notes\n        };\n        const response = await api.post(\"/pembelian/purchase-returns\", dto);\n        if (response.status === 201) {\n          alert(\"Retur pembelian berhasil dibuat (status: PENDING).\");\n          closeModal();\n          fetchPurchaseReturns();\n        }\n      } catch (error) {\n        console.error(\"Error creating return:\", error);\n        alert(`Gagal membuat retur: ${error.response?.data?.error || error.message}`);\n      } finally {\n        submitting.value = false;\n      }\n    };\n    onMounted(() => {\n      fetchPurchaseHistory();\n      fetchPurchaseReturns();\n    });\n    const __returned__ = {\n      purchaseHistory,\n      purchaseReturns,\n      loadingHistory,\n      loadingReturns,\n      fromDate,\n      toDate,\n      returnFromDate,\n      returnToDate,\n      returnStatusFilter,\n      showModal,\n      selectedPurchase,\n      returnForm,\n      submitting,\n      token,\n      api,\n      formatPONumber,\n      formatReturnNumber,\n      formatDate,\n      formatCurrency,\n      getStatusClass,\n      getStatusText,\n      hasValidItems,\n      fetchPurchaseHistory,\n      fetchPurchaseReturns,\n      resetFilter,\n      resetReturnFilter,\n      openReturnModal,\n      closeModal,\n      submitReturn,\n      ref,\n      onMounted,\n      computed,\n      get axios() {\n        return axios;\n      },\n      PurchasingSidebar\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","axios","PurchasingSidebar","purchaseHistory","purchaseReturns","loadingHistory","loadingReturns","fromDate","toDate","returnFromDate","returnToDate","returnStatusFilter","showModal","selectedPurchase","returnForm","items","notes","submitting","token","localStorage","getItem","api","create","baseURL","defaults","headers","common","formatPONumber","id","String","padStart","formatReturnNumber","formatDate","dateStr","Date","toLocaleDateString","day","month","year","formatCurrency","amount","undefined","Intl","NumberFormat","style","currency","minimumFractionDigits","format","getStatusClass","status","getStatusText","hasValidItems","value","some","item","qty","reason","trim","fetchPurchaseHistory","params","page","size","from","to","response","get","data","error","console","fetchPurchaseReturns","start_date","end_date","resetFilter","resetReturnFilter","openReturnModal","purchaseId","Items","map","product_id","ProductID","alert","closeModal","submitReturn","validItems","filter","dto","purchase_id","ID","date","toISOString","price","find","i","Price","post","message"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/ReturnPage.vue"],"sourcesContent":["<template>\n  <div\n    class=\"relative flex size-full min-h-screen flex-col bg-white group/design-root overflow-x-hidden\"\n  >\n    <div class=\"layout-container flex h-full grow\">\n      <PurchasingSidebar />\n\n      <div class=\"layout-content-container flex flex-col flex-1\">\n        <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n          <p\n            class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\"\n          >\n            Retur Pembelian\n          </p>\n        </div>\n\n        <!-- Filter Tanggal untuk History Pembelian -->\n        <div class=\"p-4 bg-[#f0f2f5] rounded-lg\">\n          <div class=\"flex flex-wrap gap-4 items-end\">\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Dari Tanggal</label\n              >\n              <input\n                type=\"date\"\n                v-model=\"fromDate\"\n                class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n                @change=\"fetchPurchaseHistory\"\n              />\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Sampai Tanggal</label\n              >\n              <input\n                type=\"date\"\n                v-model=\"toDate\"\n                class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n                @change=\"fetchPurchaseHistory\"\n              />\n            </div>\n            <button\n              @click=\"fetchPurchaseHistory\"\n              :disabled=\"loadingHistory\"\n              class=\"px-4 py-2 bg-blue-500 text-white rounded-md hover:bg-blue-600 disabled:opacity-50\"\n            >\n              {{ loadingHistory ? \"Loading...\" : \"Filter\" }}\n            </button>\n            <button\n              @click=\"resetFilter\"\n              class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\"\n            >\n              Reset (10 Terakhir)\n            </button>\n          </div>\n          <p v-if=\"fromDate || toDate\" class=\"text-sm text-gray-600 mt-2\">\n            Filter aktif: {{ fromDate || \"N/A\" }} s/d {{ toDate || \"N/A\" }}\n          </p>\n        </div>\n\n        <hr class=\"my-4\" />\n\n        <!-- Tabel History Pembelian -->\n        <div class=\"p-4\">\n          <p\n            class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\"\n          >\n            History Pembelian (untuk Retur) ({{\n              fromDate || toDate ? \"Filtered\" : \"10 Terakhir\"\n            }})\n          </p>\n          <div\n            class=\"mt-4 overflow-x-auto rounded-lg border border-[#dbe0e6] bg-white\"\n          >\n            <table class=\"min-w-full\">\n              <thead>\n                <tr class=\"bg-gray-100\">\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Tanggal\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Nomor PO\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Total\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Aksi\n                  </th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-if=\"loadingHistory\">\n                  <td colspan=\"4\" class=\"text-center py-4\">\n                    Loading history...\n                  </td>\n                </tr>\n                <tr v-else-if=\"purchaseHistory.length === 0\">\n                  <td colspan=\"4\" class=\"text-center py-4 text-gray-500\">\n                    Tidak ada data pembelian.\n                  </td>\n                </tr>\n                <tr\n                  v-else\n                  v-for=\"purchase in purchaseHistory\"\n                  :key=\"purchase.ID\"\n                  class=\"border-t border-t-[#dbe0e6]\"\n                >\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                    {{ formatDate(purchase.Date) || \"N/A\" }}\n                  </td>\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                    {{ formatPONumber(purchase.ID) }}\n                  </td>\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                    {{ formatCurrency(purchase.Total) || \"Rp 0\" }}\n                  </td>\n                  <td\n                    class=\"px-6 py-4 whitespace-nowrap text-sm text-green-500 hover:text-green-700 cursor-pointer\"\n                  >\n                    <button\n                      @click=\"openReturnModal(purchase.ID)\"\n                      class=\"underline\"\n                    >\n                      Pilih\n                    </button>\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n\n        <!-- Daftar Retur yang Dibuat -->\n        <div class=\"p-4 mt-8\">\n          <p\n            class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\"\n          >\n            Daftar Retur Pembelian Saya\n          </p>\n          <div class=\"p-4 bg-[#f0f2f5] rounded-lg mt-4\">\n            <div class=\"flex flex-wrap gap-4 items-end\">\n              <div>\n                <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                  >Dari Tanggal</label\n                >\n                <input\n                  type=\"date\"\n                  v-model=\"returnFromDate\"\n                  class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n                  @change=\"fetchPurchaseReturns\"\n                />\n              </div>\n              <div>\n                <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                  >Sampai Tanggal</label\n                >\n                <input\n                  type=\"date\"\n                  v-model=\"returnToDate\"\n                  class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n                  @change=\"fetchPurchaseReturns\"\n                />\n              </div>\n              <div>\n                <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                  >Status</label\n                >\n                <select\n                  v-model=\"returnStatusFilter\"\n                  class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n                  @change=\"fetchPurchaseReturns\"\n                >\n                  <option value=\"\">Semua</option>\n                  <option value=\"PENDING\">Pending</option>\n                  <option value=\"APPROVED\">Approved</option>\n                  <option value=\"REJECTED\">Rejected</option>\n                </select>\n              </div>\n              <button\n                @click=\"fetchPurchaseReturns\"\n                :disabled=\"loadingReturns\"\n                class=\"px-4 py-2 bg-blue-500 text-white rounded-md hover:bg-blue-600 disabled:opacity-50\"\n              >\n                {{ loadingReturns ? \"Loading...\" : \"Filter\" }}\n              </button>\n              <button\n                @click=\"resetReturnFilter\"\n                class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\"\n              >\n                Reset (10 Terakhir)\n              </button>\n            </div>\n            <p\n              v-if=\"returnFromDate || returnToDate || returnStatusFilter\"\n              class=\"text-sm text-gray-600 mt-2\"\n            >\n              Filter aktif: {{ returnFromDate || \"N/A\" }} s/d\n              {{ returnToDate || \"N/A\" }} (Status:\n              {{ returnStatusFilter || \"Semua\" }})\n            </p>\n          </div>\n          <div\n            class=\"mt-4 overflow-x-auto rounded-lg border border-[#dbe0e6] bg-white\"\n          >\n            <table class=\"min-w-full\">\n              <thead>\n                <tr class=\"bg-gray-100\">\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Return No.\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Nomor PO\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Tanggal\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Total\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                  >\n                    Status\n                  </th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-if=\"loadingReturns\">\n                  <td colspan=\"5\" class=\"text-center py-4\">\n                    Loading returns...\n                  </td>\n                </tr>\n                <tr v-else-if=\"purchaseReturns.length === 0\">\n                  <td colspan=\"5\" class=\"text-center py-4 text-gray-500\">\n                    Tidak ada retur yang dibuat.\n                  </td>\n                </tr>\n                <tr\n                  v-else\n                  v-for=\"ret in purchaseReturns\"\n                  :key=\"ret.id\"\n                  class=\"border-t border-t-[#dbe0e6]\"\n                >\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                    RTN-{{ formatReturnNumber(ret.id) }}\n                  </td>\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                    {{\n                      ret.purchase_id\n                        ? `PO-${String(ret.purchase_id).padStart(3, \"0\")}`\n                        : \"N/A\"\n                    }}\n                  </td>\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                    {{ formatDate(ret.created_at) || \"N/A\" }}\n                  </td>\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                    {{ formatCurrency(ret.total) || \"Rp 0\" }}\n                  </td>\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm font-medium\">\n                    <span\n                      :class=\"getStatusClass(ret.status)\"\n                      class=\"px-2 py-1 rounded text-xs\"\n                    >\n                      {{ getStatusText(ret.status) }}\n                    </span>\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n\n        <!-- Modal untuk Buat Retur -->\n        <div\n          v-if=\"showModal\"\n          class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50 p-4\"\n        >\n          <div\n            class=\"bg-white rounded-lg p-6 max-w-4xl w-full max-h-[90vh] overflow-y-auto\"\n          >\n            <h2 class=\"text-xl font-bold mb-4\">Buat Retur Pembelian</h2>\n\n            <div v-if=\"selectedPurchase\" class=\"mb-4 p-4 bg-gray-100 rounded\">\n              <p>\n                <strong>Nomor PO:</strong>\n                {{ formatPONumber(selectedPurchase.ID) }}\n              </p>\n              <p>\n                <strong>Tanggal:</strong>\n                {{ formatDate(selectedPurchase.Date) }}\n              </p>\n              <p>\n                <strong>Total:</strong>\n                {{ formatCurrency(selectedPurchase.Total) }}\n              </p>\n              <p>\n                <strong>Supplier:</strong>\n                {{ selectedPurchase.Supplier?.name || \"N/A\" }}\n              </p>\n            </div>\n\n            <div class=\"mb-4\">\n              <h3 class=\"text-lg font-semibold mb-2\">Items untuk Diretur</h3>\n              <div\n                v-for=\"(retItem, index) in returnForm.items\"\n                :key=\"index\"\n                class=\"border-b py-3 last:border-b-0\"\n              >\n                <div class=\"flex flex-wrap gap-4 items-start\">\n                  <div class=\"flex-1\">\n                    <p class=\"font-medium\">\n                      {{\n                        selectedPurchase?.Items[index]?.Product?.name || \"N/A\"\n                      }}\n                    </p>\n                    <p class=\"text-sm text-gray-600\">\n                      Qty Original:\n                      {{ selectedPurchase?.Items[index]?.Qty || 0 }} | Harga:\n                      {{\n                        formatCurrency(selectedPurchase?.Items[index]?.Price)\n                      }}\n                    </p>\n                  </div>\n                  <div class=\"w-32\">\n                    <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                      >Qty Retur</label\n                    >\n                    <input\n                      type=\"number\"\n                      v-model.number=\"retItem.qty\"\n                      min=\"0\"\n                      :max=\"selectedPurchase?.Items[index]?.Qty || 0\"\n                      class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n                    />\n                  </div>\n                  <div class=\"flex-1\">\n                    <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                      >Alasan</label\n                    >\n                    <textarea\n                      v-model=\"retItem.reason\"\n                      placeholder=\"Masukkan alasan retur item ini\"\n                      rows=\"2\"\n                      class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n                    ></textarea>\n                  </div>\n                </div>\n              </div>\n            </div>\n\n            <div class=\"mb-4\">\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Catatan Tambahan</label\n              >\n              <textarea\n                v-model=\"returnForm.notes\"\n                placeholder=\"Catatan opsional untuk retur ini\"\n                rows=\"3\"\n                class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n              ></textarea>\n            </div>\n\n            <div class=\"flex justify-end gap-2\">\n              <button\n                @click=\"closeModal\"\n                class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\"\n              >\n                Batal\n              </button>\n              <button\n                @click=\"submitReturn\"\n                :disabled=\"submitting || !hasValidItems\"\n                class=\"px-4 py-2 bg-green-500 text-white rounded-md hover:bg-green-600 disabled:opacity-50\"\n              >\n                {{ submitting ? \"Menyimpan...\" : \"Submit Retur\" }}\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport PurchasingSidebar from \"@/components/PurchasingSidebar.vue\";\n\nconst purchaseHistory = ref([]);\nconst purchaseReturns = ref([]);\nconst loadingHistory = ref(true);\nconst loadingReturns = ref(false);\nconst fromDate = ref(\"\");\nconst toDate = ref(\"\");\nconst returnFromDate = ref(\"\");\nconst returnToDate = ref(\"\");\nconst returnStatusFilter = ref(\"\");\nconst showModal = ref(false);\nconst selectedPurchase = ref(null);\nconst returnForm = ref({ items: [], notes: \"\" });\nconst submitting = ref(false);\nconst token = localStorage.getItem(\"token\");\n\nconst api = axios.create({ baseURL: \"http://localhost:8081/api/v1\" });\nif (token) api.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n\n// Format functions\nconst formatPONumber = (id) => `PO-${String(id).padStart(3, \"0\")}`;\nconst formatReturnNumber = (id) => `RTN-${String(id).padStart(3, \"0\")}`;\nconst formatDate = (dateStr) => {\n  if (!dateStr) return \"N/A\";\n  return new Date(dateStr).toLocaleDateString(\"id-ID\", {\n    day: \"2-digit\",\n    month: \"2-digit\",\n    year: \"numeric\",\n  });\n};\nconst formatCurrency = (amount) =>\n  amount !== undefined && amount !== null\n    ? new Intl.NumberFormat(\"id-ID\", {\n        style: \"currency\",\n        currency: \"IDR\",\n        minimumFractionDigits: 0,\n      }).format(amount)\n    : \"Rp 0\";\nconst getStatusClass = (status) => {\n  switch (status) {\n    case \"PENDING\":\n      return \"bg-yellow-100 text-yellow-800\";\n    case \"APPROVED\":\n      return \"bg-green-100 text-green-800\";\n    case \"REJECTED\":\n      return \"bg-red-100 text-red-800\";\n    default:\n      return \"bg-gray-100 text-gray-800\";\n  }\n};\nconst getStatusText = (status) => {\n  switch (status) {\n    case \"PENDING\":\n      return \"Pending\";\n    case \"APPROVED\":\n      return \"Approved\";\n    case \"REJECTED\":\n      return \"Rejected\";\n    default:\n      return status;\n  }\n};\n\n// Computed untuk validasi items\nconst hasValidItems = computed(() =>\n  returnForm.value.items.some((item) => item.qty > 0 && item.reason.trim())\n);\n\nconst fetchPurchaseHistory = async () => {\n  loadingHistory.value = true;\n  try {\n    const params = { page: 1, size: 10, status: \"APPROVED\" };\n    if (fromDate.value) params.from = fromDate.value;\n    if (toDate.value) params.to = toDate.value;\n\n    const response = await api.get(\"/pembelian/purchases\", { params });\n    purchaseHistory.value = response.data.data.data || [];\n  } catch (error) {\n    console.error(\"Error fetching purchase history:\", error);\n    purchaseHistory.value = [];\n  } finally {\n    loadingHistory.value = false;\n  }\n};\n\nconst fetchPurchaseReturns = async () => {\n  loadingReturns.value = true;\n  try {\n    const params = { page: 1, size: 10 };\n    if (returnFromDate.value) params.start_date = returnFromDate.value;\n    if (returnToDate.value) params.end_date = returnToDate.value;\n    if (returnStatusFilter.value) params.status = returnStatusFilter.value;\n\n    const response = await api.get(\"/pembelian/purchase-returns\", { params });\n    purchaseReturns.value = response.data.data || [];\n  } catch (error) {\n    console.error(\"Error fetching purchase returns:\", error);\n    purchaseReturns.value = [];\n  } finally {\n    loadingReturns.value = false;\n  }\n};\n\nconst resetFilter = () => {\n  fromDate.value = \"\";\n  toDate.value = \"\";\n  fetchPurchaseHistory();\n};\n\nconst resetReturnFilter = () => {\n  returnFromDate.value = \"\";\n  returnToDate.value = \"\";\n  returnStatusFilter.value = \"\";\n  fetchPurchaseReturns();\n};\n\nconst openReturnModal = async (purchaseId) => {\n  try {\n    const response = await api.get(`/pembelian/purchases/${purchaseId}`);\n    selectedPurchase.value = response.data.data;\n\n    returnForm.value.items = selectedPurchase.value.Items.map((item) => ({\n      product_id: item.ProductID,\n      qty: 0,\n      reason: \"\",\n    }));\n    returnForm.value.notes = \"\";\n\n    showModal.value = true;\n  } catch (error) {\n    console.error(\"Error fetching purchase:\", error);\n    alert(\"Gagal memuat detail pembelian.\");\n  }\n};\n\nconst closeModal = () => {\n  showModal.value = false;\n  selectedPurchase.value = null;\n  returnForm.value = { items: [], notes: \"\" };\n};\n\nconst submitReturn = async () => {\n  if (!hasValidItems.value) {\n    alert(\"Pilih minimal satu item dengan qty > 0 dan alasan.\");\n    return;\n  }\n\n  submitting.value = true;\n  try {\n    const validItems = returnForm.value.items.filter(\n      (item) => item.qty > 0 && item.reason.trim()\n    );\n    const dto = {\n      purchase_id: selectedPurchase.value.ID,\n      date: new Date().toISOString(),\n      items: validItems.map((item) => ({\n        product_id: item.product_id,\n        qty: item.qty,\n        price:\n          selectedPurchase.value.Items.find(\n            (i) => i.ProductID === item.product_id\n          )?.Price || 0,\n      })),\n      notes: returnForm.value.notes,\n    };\n\n    const response = await api.post(\"/pembelian/purchase-returns\", dto);\n    if (response.status === 201) {\n      alert(\"Retur pembelian berhasil dibuat (status: PENDING).\");\n      closeModal();\n      fetchPurchaseReturns();\n    }\n  } catch (error) {\n    console.error(\"Error creating return:\", error);\n    alert(\n      `Gagal membuat retur: ${error.response?.data?.error || error.message}`\n    );\n  } finally {\n    submitting.value = false;\n  }\n};\n\nonMounted(() => {\n  fetchPurchaseHistory();\n  fetchPurchaseReturns();\n});\n</script>\n\n<style scoped>\n.layout-content-container {\n  max-width: 960px;\n  width: 100%;\n}\n</style>\n"],"mappings":";;;;;AAmZA,SAASA,GAAG,EAAEC,SAAS,EAAEC,QAAQ,QAAQ,KAAK;AAC9C,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,iBAAiB,MAAM,oCAAoC;;;;;;;IAElE,MAAMC,eAAe,GAAGL,GAAG,CAAC,EAAE,CAAC;IAC/B,MAAMM,eAAe,GAAGN,GAAG,CAAC,EAAE,CAAC;IAC/B,MAAMO,cAAc,GAAGP,GAAG,CAAC,IAAI,CAAC;IAChC,MAAMQ,cAAc,GAAGR,GAAG,CAAC,KAAK,CAAC;IACjC,MAAMS,QAAQ,GAAGT,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMU,MAAM,GAAGV,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMW,cAAc,GAAGX,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMY,YAAY,GAAGZ,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMa,kBAAkB,GAAGb,GAAG,CAAC,EAAE,CAAC;IAClC,MAAMc,SAAS,GAAGd,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMe,gBAAgB,GAAGf,GAAG,CAAC,IAAI,CAAC;IAClC,MAAMgB,UAAU,GAAGhB,GAAG,CAAC;MAAEiB,KAAK,EAAE,EAAE;MAAEC,KAAK,EAAE;IAAG,CAAC,CAAC;IAChD,MAAMC,UAAU,GAAGnB,GAAG,CAAC,KAAK,CAAC;IAC7B,MAAMoB,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;IAE3C,MAAMC,GAAG,GAAGpB,KAAK,CAACqB,MAAM,CAAC;MAAEC,OAAO,EAAE;IAA+B,CAAC,CAAC;IACrE,IAAIL,KAAK,EAAEG,GAAG,CAACG,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,CAAC,GAAG,UAAUR,KAAK,EAAE;;IAE3E;IACA