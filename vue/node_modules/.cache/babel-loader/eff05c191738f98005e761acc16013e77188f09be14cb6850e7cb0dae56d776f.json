{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.every.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport GudangSidebar from \"../components/GudangSidebar.vue\";\nconst API_BASE = \"http://localhost:8081/api/v1\";\nexport default {\n  __name: 'GoodsReceipt',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const token = localStorage.getItem(\"token\");\n    const purchases = ref([]);\n    const loading = ref(false);\n    const totalItems = ref(0);\n    const searchQuery = ref(\"\");\n    const currentStatus = ref(\"APPROVED\");\n    const loadingStates = ref({});\n\n    // State untuk Modal Detail (Original)\n    const selectedPurchase = ref(null);\n    const isModalOpen = ref(false);\n\n    // State untuk Modal Penerimaan/QC (NEW)\n    const isReceiveModalOpen = ref(false);\n    const submitting = ref(false);\n    const receiveError = ref(\"\");\n    const receiveForm = ref({\n      purchase_id: null,\n      supplierName: \"\",\n      purchaseDate: \"\",\n      items: []\n    });\n    const isReceiveFormValid = computed(() => {\n      return receiveForm.value.items.length > 0 && receiveForm.value.items.every(item => item.qty_received !== null && item.qty_received >= 0 && item.qty_received <= item.qty_ordered // QC Qty tidak boleh melebihi Qty PO\n      );\n    });\n    const page = ref(1);\n    const size = ref(10);\n\n    // --- Helper Functions ---\n    const formatDate = dateString => {\n      if (!dateString) return \"-\";\n      const date = new Date(dateString);\n      const day = String(date.getDate()).padStart(2, \"0\");\n      const month = String(date.getMonth() + 1).padStart(2, \"0\");\n      const year = date.getFullYear();\n      return `${day}/${month}/${year}`;\n    };\n    const formatCurrency = amount => {\n      const numericAmount = Number(amount) || 0;\n      return new Intl.NumberFormat(\"id-ID\", {\n        style: \"currency\",\n        currency: \"IDR\",\n        minimumFractionDigits: 0\n      }).format(numericAmount);\n    };\n    const getStatusClass = status => {\n      switch (status) {\n        case \"APPROVED\":\n          return \"bg-yellow-200 text-yellow-800 py-1 px-3 rounded-full text-xs font-medium\";\n        case \"RECEIVED\":\n          return \"bg-green-200 text-green-800 py-1 px-3 rounded-full text-xs font-medium\";\n        case \"REJECTED\":\n          return \"bg-red-200 text-red-800 py-1 px-3 rounded-full text-xs font-medium\";\n        default:\n          return \"bg-gray-200 text-gray-800 py-1 px-3 rounded-full text-xs font-medium\";\n      }\n    };\n    const closeModal = () => {\n      isModalOpen.value = false;\n      selectedPurchase.value = null;\n    };\n    const closeReceiveModal = () => {\n      isReceiveModalOpen.value = false;\n      receiveError.value = \"\";\n      // Reset form\n      receiveForm.value = {\n        purchase_id: null,\n        supplierName: \"\",\n        purchaseDate: \"\",\n        items: []\n      };\n    };\n\n    // Fungsi untuk memuat detail PO dan menampilkan Modal Detail\n    const showDetail = async id => {\n      try {\n        const res = await axios.get(`${API_BASE}/history/purchases/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        selectedPurchase.value = res.data.data;\n        isModalOpen.value = true;\n      } catch (err) {\n        console.error(\"Error loading purchase detail:\", err.response?.data || err.message);\n        alert(`Gagal memuat detail Purchase Order #${id}.`);\n      }\n    };\n\n    // --- API Calls ---\n    const loadPurchases = async () => {\n      loading.value = true;\n      purchases.value = [];\n      try {\n        const res = await axios.get(`${API_BASE}/history/purchases`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          },\n          params: {\n            page: page.value,\n            size: size.value,\n            q: searchQuery.value,\n            status: currentStatus.value\n          }\n        });\n\n        // Sesuaikan jika data list ada di res.data.data.data\n        const data = res.data.data?.data || res.data.data || [];\n        totalItems.value = res.data.data?.total || 0;\n        purchases.value = data;\n      } catch (err) {\n        console.error(\"Error load purchases:\", err.response?.data || err.message);\n        alert(\"Gagal memuat daftar Purchase Order.\");\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // Fungsi untuk membuka Modal QC dan memuat data PO\n    const openReceiveModal = async id => {\n      try {\n        const res = await axios.get(`${API_BASE}/history/purchases/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        const po = res.data.data;\n        if (!po || po.Status !== 'APPROVED') {\n          alert(\"PO ini tidak bisa diterima karena statusnya bukan APPROVED.\");\n          return;\n        }\n\n        // Siapkan data untuk form penerimaan\n        receiveForm.value.purchase_id = po.ID;\n        receiveForm.value.supplierName = po.Supplier?.name || \"N/A\";\n        receiveForm.value.purchaseDate = po.Date;\n        receiveForm.value.items = po.Items.map(item => ({\n          purchase_item_id: item.ID,\n          product_id: item.Product_ID,\n          product_name: item.Product?.name || \"Produk Hilang\",\n          qty_ordered: item.Qty,\n          qty_received: item.Qty,\n          // Default: Qty diterima sama dengan Qty dipesan\n          note: \"\"\n        }));\n        isReceiveModalOpen.value = true;\n      } catch (err) {\n        console.error(\"Error loading PO for receipt:\", err.response?.data || err.message);\n        alert(`Gagal memuat detail PO #${id} untuk penerimaan.`);\n      }\n    };\n\n    // Fungsi untuk submit Good Receipt (QC)\n    const submitGoodReceipt = async () => {\n      if (!isReceiveFormValid.value) {\n        receiveError.value = \"Kuantitas yang diterima harus valid (tidak melebihi Qty dipesan).\";\n        return;\n      }\n      submitting.value = true;\n      receiveError.value = \"\";\n\n      // Format payload untuk API backend:\n      // Kita hanya mengirim data item yang relevan untuk proses penerimaan/QC\n      const payloadItems = receiveForm.value.items.map(item => ({\n        purchase_item_id: item.purchase_item_id,\n        // ID item PO\n        qty_received: item.qty_received,\n        // Kuantitas yang Diterima (QC Qty)\n        note: item.note // Catatan (misal: 5 rusak)\n      }));\n      const payload = {\n        items: payloadItems\n        // Jika backend membutuhkan Purchase ID di body, tambahkan:\n        // purchase_id: receiveForm.value.purchase_id, \n      };\n      const id = receiveForm.value.purchase_id;\n      try {\n        // ASUMSI: Endpoint untuk memproses Good Receipt dengan QC adalah:\n        // POST /gudang/purchases/:id/receive\n        const res = await axios.post(`${API_BASE}/gudang/purchases/${id}/receive`, payload, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        alert(`Purchase Order #${id} berhasil DITERIMA. Stok telah diperbarui. Catatan: Jika ada selisih, barang dianggap retur/kurang.`);\n        closeReceiveModal();\n        loadPurchases(); // Muat ulang data\n      } catch (err) {\n        console.error(\"Error submitting Good Receipt:\", err.response?.data || err.message);\n        receiveError.value = `Gagal menyelesaikan penerimaan: ${err.response?.data?.error || \"Terjadi kesalahan pada server.\"} Pastikan backend Anda menerima payload dengan field 'qty_received' dan mampu mengurus stok serta retur otomatis/pencatatan selisih.`;\n      } finally {\n        submitting.value = false;\n      }\n    };\n    onMounted(() => {\n      loadPurchases();\n    });\n    const __returned__ = {\n      API_BASE,\n      token,\n      purchases,\n      loading,\n      totalItems,\n      searchQuery,\n      currentStatus,\n      loadingStates,\n      selectedPurchase,\n      isModalOpen,\n      isReceiveModalOpen,\n      submitting,\n      receiveError,\n      receiveForm,\n      isReceiveFormValid,\n      page,\n      size,\n      formatDate,\n      formatCurrency,\n      getStatusClass,\n      closeModal,\n      closeReceiveModal,\n      showDetail,\n      loadPurchases,\n      openReceiveModal,\n      submitGoodReceipt,\n      ref,\n      onMounted,\n      computed,\n      get axios() {\n        return axios;\n      },\n      GudangSidebar\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","axios","GudangSidebar","API_BASE","token","localStorage","getItem","purchases","loading","totalItems","searchQuery","currentStatus","loadingStates","selectedPurchase","isModalOpen","isReceiveModalOpen","submitting","receiveError","receiveForm","purchase_id","supplierName","purchaseDate","items","isReceiveFormValid","value","length","every","item","qty_received","qty_ordered","page","size","formatDate","dateString","date","Date","day","String","getDate","padStart","month","getMonth","year","getFullYear","formatCurrency","amount","numericAmount","Number","Intl","NumberFormat","style","currency","minimumFractionDigits","format","getStatusClass","status","closeModal","closeReceiveModal","showDetail","id","res","get","headers","Authorization","data","err","console","error","response","message","alert","loadPurchases","params","q","total","openReceiveModal","po","Status","ID","Supplier","name","Items","map","purchase_item_id","product_id","Product_ID","product_name","Product","Qty","note","submitGoodReceipt","payloadItems","payload","post"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/GoodsReceipt.vue"],"sourcesContent":["<template>\n  <div class=\"flex\">\n    <GudangSidebar />\n\n    <main class=\"flex-1 ml-64 p-6\">\n      <h1 class=\"text-2xl font-bold mb-6\">Penerimaan Barang (Goods Receipt)</h1>\n\n      <div class=\"flex justify-between items-center mb-4\">\n        <div class=\"flex gap-2\">\n          <label class=\"text-sm font-medium pt-2\">Filter Status:</label>\n          <select\n            v-model=\"currentStatus\"\n            @change=\"loadPurchases\"\n            class=\"border rounded p-2 text-sm\"\n          >\n            <option value=\"APPROVED\">Siap Diterima (APPROVED)</option>\n            <option value=\"RECEIVED\">Sudah Diterima (RECEIVED)</option>\n            <option value=\"\">Semua</option>\n          </select>\n        </div>\n        <input\n          type=\"text\"\n          v-model=\"searchQuery\"\n          @keyup.enter=\"loadPurchases\"\n          placeholder=\"Cari PO ID atau Supplier...\"\n          class=\"border rounded p-2 text-sm w-64\"\n        />\n      </div>\n\n      <div class=\"bg-white shadow-md rounded-lg overflow-hidden\">\n        <table class=\"min-w-full leading-normal\">\n          <thead>\n            <tr\n              class=\"bg-gray-100 text-gray-600 uppercase text-sm leading-normal\"\n            >\n              <th class=\"py-3 px-6 text-left\">ID</th>\n              <th class=\"py-3 px-6 text-left\">Tanggal</th>\n              <th class=\"py-3 px-6 text-left\">Supplier</th>\n              <th class=\"py-3 px-6 text-right\">Total</th>\n              <th class=\"py-3 px-6 text-center\">Status</th>\n              <th class=\"py-3 px-6 text-center\">Aksi</th>\n            </tr>\n          </thead>\n          <tbody class=\"text-gray-600 text-sm font-light\">\n            <tr\n              v-for=\"po in purchases\"\n              :key=\"po.ID\"\n              class=\"border-b border-gray-200 hover:bg-gray-50\"\n            >\n              <td class=\"py-3 px-6 text-left whitespace-nowrap\">{{ po.ID }}</td>\n              <td class=\"py-3 px-6 text-left\">\n                {{ formatDate(po.Date) }}\n              </td>\n              <td class=\"py-3 px-6 text-left\">\n                {{ po.Supplier?.name || \"N/A\" }}\n              </td>\n              <td class=\"py-3 px-6 text-right\">\n                {{ formatCurrency(po.Total) }}\n              </td>\n              <td class=\"py-3 px-6 text-center\">\n                <span :class=\"getStatusClass(po.Status)\">\n                  {{ po.Status }}\n                </span>\n              </td>\n              <td class=\"py-3 px-6 text-center flex justify-center items-center\">\n                <button\n                  v-if=\"po.Status === 'APPROVED'\"\n                  @click=\"openReceiveModal(po.ID)\"\n                  class=\"bg-green-500 hover:bg-green-600 text-white text-xs font-bold py-1 px-3 rounded disabled:opacity-50\"\n                >\n                  Terima & QC\n                </button>\n                <span\n                  v-else-if=\"po.Status === 'RECEIVED'\"\n                  class=\"text-green-500 font-bold text-xs\"\n                >\n                  âœ… Diterima\n                </span>\n                <button\n                  @click=\"showDetail(po.ID)\"\n                  class=\"ml-2 text-blue-500 hover:text-blue-700 text-xs\"\n                >\n                  Detail\n                </button>\n              </td>\n            </tr>\n            <tr v-if=\"!purchases.length && !loading\">\n              <td colspan=\"6\" class=\"py-4 text-center text-gray-500\">\n                Tidak ada Purchase Order yang ditemukan.\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n\n      <div class=\"flex justify-between items-center mt-4\">\n        <p class=\"text-sm text-gray-600\">Total: {{ totalItems }}</p>\n        <div v-if=\"loading\" class=\"text-blue-500\">Memuat...</div>\n      </div>\n    </main>\n\n    <div\n      v-if=\"isModalOpen\"\n      class=\"fixed inset-0 bg-gray-600 bg-opacity-50 overflow-y-auto h-full w-full z-50 flex justify-center items-center\"\n      @click=\"closeModal\"\n    >\n      <div\n        class=\"bg-white p-6 rounded-lg shadow-xl max-w-4xl w-full mx-4\"\n        @click.stop\n      >\n        <div class=\"flex justify-between items-center border-b pb-3 mb-4\">\n          <h3 class=\"text-xl font-bold\">\n            Detail Purchase Order #{{ selectedPurchase?.ID }}\n          </h3>\n          <button\n            @click=\"closeModal\"\n            class=\"text-gray-500 hover:text-gray-800 text-2xl leading-none\"\n          >\n            &times;\n          </button>\n        </div>\n\n        <div v-if=\"selectedPurchase\">\n          <div class=\"grid grid-cols-2 gap-4 text-sm mb-4\">\n            <div>\n              <p><strong>ID PO:</strong> {{ selectedPurchase.ID }}</p>\n              <p>\n                <strong>Tanggal:</strong>\n                {{ formatDate(selectedPurchase.Date) }}\n              </p>\n              <p>\n                <strong>Status:</strong>\n                <span :class=\"getStatusClass(selectedPurchase.Status)\">{{\n                  selectedPurchase.Status\n                }}</span>\n              </p>\n            </div>\n            <div>\n              <p>\n                <strong>Supplier:</strong>\n                {{ selectedPurchase.Supplier?.name || \"N/A\" }}\n              </p>\n              <p>\n                <strong>Total Pembelian:</strong>\n                {{ formatCurrency(selectedPurchase.Total) }}\n              </p>\n              <p>\n                <strong>Approved Oleh:</strong>\n                {{ selectedPurchase.Approver?.Name || \"N/A\" }}\n              </p>\n            </div>\n          </div>\n\n          <h4 class=\"text-lg font-semibold mb-2\">Item Pembelian</h4>\n          <div class=\"overflow-x-auto border rounded\">\n            <table class=\"min-w-full text-sm\">\n              <thead>\n                <tr class=\"bg-gray-50\">\n                  <th class=\"py-2 px-4 text-left\">Produk</th>\n                  <th class=\"py-2 px-4 text-right\">Harga Beli</th>\n                  <th class=\"py-2 px-4 text-right\">Qty PO</th>\n                  <th class=\"py-2 px-4 text-right\">Subtotal</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr\n                  v-for=\"item in selectedPurchase.Items\"\n                  :key=\"item.ID\"\n                  class=\"border-t\"\n                >\n                  <td class=\"py-2 px-4\">\n                    {{ item.Product?.name || \"Produk Hilang\" }}\n                  </td>\n                  <td class=\"py-2 px-4 text-right\">\n                    {{ formatCurrency(item.Price) }}\n                  </td>\n                  <td class=\"py-2 px-4 text-right\">{{ item.Qty }}</td>\n                  <td class=\"py-2 px-4 text-right\">\n                    {{ formatCurrency(item.Subtotal) }}\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n\n        <div class=\"mt-4 flex justify-end\">\n          <button\n            @click=\"closeModal\"\n            class=\"bg-gray-300 hover:bg-gray-400 text-gray-800 font-bold py-2 px-4 rounded\"\n          >\n            Tutup\n          </button>\n        </div>\n      </div>\n    </div>\n\n    <div\n      v-if=\"isReceiveModalOpen\"\n      class=\"fixed inset-0 bg-gray-600 bg-opacity-50 overflow-y-auto h-full w-full z-50 flex justify-center items-center\"\n      @click=\"closeReceiveModal\"\n    >\n      <div\n        class=\"bg-white p-6 rounded-lg shadow-xl max-w-5xl w-full mx-4\"\n        @click.stop\n      >\n        <div class=\"flex justify-between items-center border-b pb-3 mb-4\">\n          <h3 class=\"text-xl font-bold text-green-700\">\n            Penerimaan & QC (Purchase #{{ receiveForm.purchase_id }})\n          </h3>\n          <button\n            @click=\"closeReceiveModal\"\n            class=\"text-gray-500 hover:text-gray-800 text-2xl leading-none\"\n            :disabled=\"submitting\"\n          >\n            &times;\n          </button>\n        </div>\n\n        <form @submit.prevent=\"submitGoodReceipt\">\n          <div class=\"mb-4\">\n            <p class=\"text-sm font-medium\">\n              Supplier:\n              <span class=\"font-bold\">{{ receiveForm.supplierName }}</span>\n            </p>\n            <p class=\"text-sm font-medium\">\n              Tanggal PO:\n              <span class=\"font-bold\">{{\n                formatDate(receiveForm.purchaseDate)\n              }}</span>\n            </p>\n          </div>\n\n          <h4 class=\"text-base font-semibold mb-3\">\n            Cek Kuantitas Diterima (QC)\n          </h4>\n          <div class=\"overflow-x-auto border rounded max-h-80\">\n            <table class=\"min-w-full text-sm\">\n              <thead>\n                <tr class=\"bg-gray-50\">\n                  <th class=\"py-2 px-4 text-left w-1/3\">Produk</th>\n                  <th class=\"py-2 px-4 text-right w-1/6\">Qty Dipesan (PO)</th>\n                  <th class=\"py-2 px-4 text-center w-1/6\">Qty Diterima (QC)</th>\n                  <th class=\"py-2 px-4 text-right w-1/6\">Selisih</th>\n                  <th class=\"py-2 px-4 text-left w-1/6\">Catatan (Rusak/Kurang)</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr\n                  v-for=\"(item, index) in receiveForm.items\"\n                  :key=\"index\"\n                  class=\"border-t\"\n                >\n                  <td class=\"py-2 px-4 font-medium\">\n                    {{ item.product_name }}\n                  </td>\n                  <td class=\"py-2 px-4 text-right font-semibold\">\n                    {{ item.qty_ordered }}\n                  </td>\n                  <td class=\"py-2 px-4 text-center\">\n                    <input\n                      v-model.number=\"item.qty_received\"\n                      type=\"number\"\n                      min=\"0\"\n                      :max=\"item.qty_ordered\"\n                      required\n                      class=\"w-full border-gray-300 rounded-lg text-sm p-1.5 text-center\"\n                      :class=\"{\n                        'border-red-500 ring-red-500':\n                          item.qty_received > item.qty_ordered,\n                      }\"\n                    />\n                    <p\n                      v-if=\"item.qty_received > item.qty_ordered\"\n                      class=\"text-red-500 text-xs mt-1\"\n                    >\n                      Max Qty: {{ item.qty_ordered }}\n                    </p>\n                  </td>\n                  <td\n                    class=\"py-2 px-4 text-right font-bold\"\n                    :class=\"{\n                      'text-red-500': item.qty_ordered - item.qty_received > 0,\n                    }\"\n                  >\n                    {{ item.qty_received - item.qty_ordered }}\n                  </td>\n                  <td class=\"py-2 px-4\">\n                    <input\n                      v-model=\"item.note\"\n                      type=\"text\"\n                      class=\"w-full border-gray-300 rounded-lg text-sm p-1.5\"\n                      placeholder=\"e.g. 5 Pcs Rusak\"\n                    />\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n          \n          <p v-if=\"receiveError\" class=\"text-red-500 text-sm mt-4 p-2 bg-red-50 border border-red-200 rounded\">\n            **Error:** {{ receiveError }}\n          </p>\n\n          <div class=\"mt-6 flex justify-end\">\n            <button\n              type=\"button\"\n              @click=\"closeReceiveModal\"\n              class=\"px-6 py-2 border border-gray-300 text-gray-700 rounded-lg shadow-sm hover:bg-gray-100 mr-3\"\n              :disabled=\"submitting\"\n            >\n              Batal\n            </button>\n            <button\n              type=\"submit\"\n              :disabled=\"submitting || !isReceiveFormValid\"\n              class=\"px-6 py-2 bg-green-600 text-white font-medium rounded-lg shadow-md hover:bg-green-700 disabled:opacity-50 transition-colors\"\n            >\n              {{ submitting ? \"Memproses...\" : \"Selesaikan Penerimaan\" }}\n            </button>\n          </div>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport GudangSidebar from \"../components/GudangSidebar.vue\";\n\nconst API_BASE = \"http://localhost:8081/api/v1\";\nconst token = localStorage.getItem(\"token\");\n\nconst purchases = ref([]);\nconst loading = ref(false);\nconst totalItems = ref(0);\nconst searchQuery = ref(\"\");\nconst currentStatus = ref(\"APPROVED\");\nconst loadingStates = ref({});\n\n// State untuk Modal Detail (Original)\nconst selectedPurchase = ref(null);\nconst isModalOpen = ref(false);\n\n// State untuk Modal Penerimaan/QC (NEW)\nconst isReceiveModalOpen = ref(false);\nconst submitting = ref(false);\nconst receiveError = ref(\"\");\nconst receiveForm = ref({\n  purchase_id: null,\n  supplierName: \"\",\n  purchaseDate: \"\",\n  items: [],\n});\n\nconst isReceiveFormValid = computed(() => {\n  return (\n    receiveForm.value.items.length > 0 &&\n    receiveForm.value.items.every(\n      (item) =>\n        item.qty_received !== null &&\n        item.qty_received >= 0 &&\n        item.qty_received <= item.qty_ordered // QC Qty tidak boleh melebihi Qty PO\n    )\n  );\n});\n\nconst page = ref(1);\nconst size = ref(10);\n\n// --- Helper Functions ---\nconst formatDate = (dateString) => {\n  if (!dateString) return \"-\";\n  const date = new Date(dateString);\n  const day = String(date.getDate()).padStart(2, \"0\");\n  const month = String(date.getMonth() + 1).padStart(2, \"0\");\n  const year = date.getFullYear();\n  return `${day}/${month}/${year}`;\n};\n\nconst formatCurrency = (amount) => {\n  const numericAmount = Number(amount) || 0;\n\n  return new Intl.NumberFormat(\"id-ID\", {\n    style: \"currency\",\n    currency: \"IDR\",\n    minimumFractionDigits: 0,\n  }).format(numericAmount);\n};\n\nconst getStatusClass = (status) => {\n  switch (status) {\n    case \"APPROVED\":\n      return \"bg-yellow-200 text-yellow-800 py-1 px-3 rounded-full text-xs font-medium\";\n    case \"RECEIVED\":\n      return \"bg-green-200 text-green-800 py-1 px-3 rounded-full text-xs font-medium\";\n    case \"REJECTED\":\n      return \"bg-red-200 text-red-800 py-1 px-3 rounded-full text-xs font-medium\";\n    default:\n      return \"bg-gray-200 text-gray-800 py-1 px-3 rounded-full text-xs font-medium\";\n  }\n};\n\nconst closeModal = () => {\n  isModalOpen.value = false;\n  selectedPurchase.value = null;\n};\n\nconst closeReceiveModal = () => {\n  isReceiveModalOpen.value = false;\n  receiveError.value = \"\";\n  // Reset form\n  receiveForm.value = {\n    purchase_id: null,\n    supplierName: \"\",\n    purchaseDate: \"\",\n    items: [],\n  };\n};\n\n// Fungsi untuk memuat detail PO dan menampilkan Modal Detail\nconst showDetail = async (id) => {\n  try {\n    const res = await axios.get(`${API_BASE}/history/purchases/${id}`, {\n      headers: { Authorization: `Bearer ${token}` },\n    });\n\n    selectedPurchase.value = res.data.data;\n    isModalOpen.value = true;\n  } catch (err) {\n    console.error(\n      \"Error loading purchase detail:\",\n      err.response?.data || err.message\n    );\n    alert(`Gagal memuat detail Purchase Order #${id}.`);\n  }\n};\n\n// --- API Calls ---\nconst loadPurchases = async () => {\n  loading.value = true;\n  purchases.value = [];\n  try {\n    const res = await axios.get(`${API_BASE}/history/purchases`, {\n      headers: { Authorization: `Bearer ${token}` },\n      params: {\n        page: page.value,\n        size: size.value,\n        q: searchQuery.value,\n        status: currentStatus.value,\n      },\n    });\n\n    // Sesuaikan jika data list ada di res.data.data.data\n    const data = res.data.data?.data || res.data.data || [];\n    totalItems.value = res.data.data?.total || 0;\n    purchases.value = data;\n  } catch (err) {\n    console.error(\"Error load purchases:\", err.response?.data || err.message);\n    alert(\"Gagal memuat daftar Purchase Order.\");\n  } finally {\n    loading.value = false;\n  }\n};\n\n// Fungsi untuk membuka Modal QC dan memuat data PO\nconst openReceiveModal = async (id) => {\n  try {\n    const res = await axios.get(`${API_BASE}/history/purchases/${id}`, {\n      headers: { Authorization: `Bearer ${token}` },\n    });\n    \n    const po = res.data.data;\n\n    if (!po || po.Status !== 'APPROVED') {\n        alert(\"PO ini tidak bisa diterima karena statusnya bukan APPROVED.\");\n        return;\n    }\n\n    // Siapkan data untuk form penerimaan\n    receiveForm.value.purchase_id = po.ID;\n    receiveForm.value.supplierName = po.Supplier?.name || \"N/A\";\n    receiveForm.value.purchaseDate = po.Date;\n    \n    receiveForm.value.items = po.Items.map((item) => ({\n      purchase_item_id: item.ID,\n      product_id: item.Product_ID,\n      product_name: item.Product?.name || \"Produk Hilang\",\n      qty_ordered: item.Qty,\n      qty_received: item.Qty, // Default: Qty diterima sama dengan Qty dipesan\n      note: \"\",\n    }));\n\n    isReceiveModalOpen.value = true;\n  } catch (err) {\n    console.error(\n      \"Error loading PO for receipt:\",\n      err.response?.data || err.message\n    );\n    alert(`Gagal memuat detail PO #${id} untuk penerimaan.`);\n  }\n};\n\n// Fungsi untuk submit Good Receipt (QC)\nconst submitGoodReceipt = async () => {\n  if (!isReceiveFormValid.value) {\n    receiveError.value = \"Kuantitas yang diterima harus valid (tidak melebihi Qty dipesan).\";\n    return;\n  }\n\n  submitting.value = true;\n  receiveError.value = \"\";\n  \n  // Format payload untuk API backend:\n  // Kita hanya mengirim data item yang relevan untuk proses penerimaan/QC\n  const payloadItems = receiveForm.value.items.map(item => ({\n    purchase_item_id: item.purchase_item_id, // ID item PO\n    qty_received: item.qty_received,         // Kuantitas yang Diterima (QC Qty)\n    note: item.note,                         // Catatan (misal: 5 rusak)\n  }));\n  \n  const payload = {\n    items: payloadItems,\n    // Jika backend membutuhkan Purchase ID di body, tambahkan:\n    // purchase_id: receiveForm.value.purchase_id, \n  };\n  \n  const id = receiveForm.value.purchase_id;\n\n  try {\n    // ASUMSI: Endpoint untuk memproses Good Receipt dengan QC adalah:\n    // POST /gudang/purchases/:id/receive\n    const res = await axios.post(\n      `${API_BASE}/gudang/purchases/${id}/receive`,\n      payload,\n      { headers: { Authorization: `Bearer ${token}` } }\n    );\n\n    alert(`Purchase Order #${id} berhasil DITERIMA. Stok telah diperbarui. Catatan: Jika ada selisih, barang dianggap retur/kurang.`);\n\n    closeReceiveModal();\n    loadPurchases(); // Muat ulang data\n  } catch (err) {\n    console.error(\n      \"Error submitting Good Receipt:\",\n      err.response?.data || err.message\n    );\n    receiveError.value = `Gagal menyelesaikan penerimaan: ${\n      err.response?.data?.error || \"Terjadi kesalahan pada server.\"\n    } Pastikan backend Anda menerima payload dengan field 'qty_received' dan mampu mengurus stok serta retur otomatis/pencatatan selisih.`;\n  } finally {\n    submitting.value = false;\n  }\n};\n\nonMounted(() => {\n  loadPurchases();\n});\n</script>"],"mappings":";;;AAwUA,SAASA,GAAG,EAAEC,SAAS,EAAEC,QAAQ,QAAQ,KAAK;AAC9C,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,aAAa,MAAM,iCAAiC;AAE3D,MAAMC,QAAQ,GAAG,8BAA8B;;;;;;;IAC/C,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;IAE3C,MAAMC,SAAS,GAAGT,GAAG,CAAC,EAAE,CAAC