Items,\n      api,\n      savedToken,\n      fetchSalesHistory,\n      resetFilter,\n      openReturnModal,\n      closeModal,\n      submitReturn,\n      formatDate,\n      formatCurrency,\n      ref,\n      onMounted,\n      computed,\n      get axios() {\n        return axios;\n      },\n      CashierLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","axios","CashierLayouts","userName","totalTransactions","totalSalesValue","salesHistory","loadingHistory","fromDate","toDate","showModal","selectedSale","returnForm","items","notes","submitting","hasValidItems","value","some","item","qty","reason","trim","api","create","baseURL","savedToken","localStorage","getItem","defaults","headers","common","fetchSalesHistory","nameFromStorage","params","page","size","orderBy","start_date","end_date","response","get","data","total","length","total_value","reduce","sum","sale","Total","error","console","alert","status","message","resetFilter","openReturnModal","saleId","Items","map","product_id","ProductID","closeModal","submitReturn","validItems","filter","dto","sale_id","ID","date","Date","toISOString","post","formatDate","dateStr","isNaN","getTime","sliced","split","toLocaleDateString","day","month","year","formatCurrency","amount","undefined","Intl","NumberFormat","style","currency","minimumFractionDigits","format"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/ReturnPage.vue"],"sourcesContent":["<template>\n  <CashierLayouts>\n    <div>\n      <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n        <p\n          class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\"\n        >\n          Retur Penjualan\n        </p>\n      </div>\n\n      <!-- Filter Tanggal -->\n      <div class=\"p-4 bg-[#f0f2f5] rounded-lg\">\n        <div class=\"flex