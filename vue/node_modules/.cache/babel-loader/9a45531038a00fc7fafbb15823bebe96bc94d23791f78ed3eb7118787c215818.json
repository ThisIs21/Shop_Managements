\"/history/sales\", {\n          params\n        });\n        const data = response.data;\n        if (data && data.data) {\n          salesHistory.value = data.data;\n          totalTransactions.value = data.total || data.data.length;\n          totalSalesValue.value = data.total_value || data.data.reduce((sum, sale) => sum + (sale.Total || 0), 0);\n        } else {\n          salesHistory.value = [];\n          totalTransactions.value = 0;\n          totalSalesValue.value = 0;\n        }\n      } catch (error) {\n        console.error(\"Error fetch:\", error);\n        if (error.response) {\n          alert(`Error mengambil data: ${error.response.status} - ${error.response.data.error || error.message}`);\n        } else {\n          alert(\"Kesalahan jaringan atau server tidak tersedia.\");\n        }\n        salesHistory.value = [];\n        totalTransactions.value = 0;\n        totalSalesValue.value = 0;\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n    const resetFilter = () => {\n      fromDate.value = \"\";\n      toDate.value = \"\";\n      fetchSalesHistory();\n    };\n    const openReturnModal = async saleId => {\n      try {\n        const response = await api.get(`/kasir/sales/${saleId}`);\n        selectedSale.value = response.data;\n\n        // Initialize return form items based on sale items\n        returnForm.value.items = selectedSale.value.Items.map(item => ({\n          product_id: item.ProductID,\n          qty: 0,\n          reason: ''\n        }));\n        returnForm.value.notes = '';\n        showModal.value = true;\n      } catch (error) {\n        console.error(\"Error fetching sale:\", error);\n        alert(\"Gagal memuat detail transaksi.\");\n      }\n    };\n    const closeModal = () => {\n      showModal.value = false;\n      selectedSale.value = null;\n      returnForm.value = {\n        items: [],\n        notes: ''\n      };\n    };\n    const submitReturn = async () => {\n      if (!hasValidItems.v