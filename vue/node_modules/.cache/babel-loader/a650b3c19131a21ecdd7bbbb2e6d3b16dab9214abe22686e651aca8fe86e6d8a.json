Id.value = saleId;\n      returnItems.value = [];\n      try {\n        const res = await api.get(`/kasir/sales/${saleId}`);\n        console.log(\"API response for /kasir/sales/${saleId}:\", res.data);\n        const items = res.data.Items || [];\n        if (items.length === 0) {\n          console.warn(\"No items found for sale ID:\", saleId);\n          alert(\"Transaksi ini tidak memiliki item untuk diretur.\");\n          selectedSaleId.value = null;\n          return;\n        }\n        returnItems.value = items.map(item => ({\n          ID: item.ID,\n          Product: item.Product || {\n            name: \"Unknown Product\"\n          },\n          Qty: item.Qty || 0,\n          returnQty: 0,\n          reason: \"\"\n        }));\n        console.log(\"Mapped return items:\", returnItems.value);\n      } catch (err) {\n        console.error(\"Error loading sale items:\", err.response?.data || err.message);\n        alert(\"Gagal memuat item transaksi! Periksa konsol.\");\n        selectedSaleId.value = null;\n      }\n    };\n\n    // Validate return quantity\n    const validateReturnQty = item => {\n      if (item.returnQty < 0) item.returnQty = 0;\n      if (item.returnQty > item.Qty) item.returnQty = item.Qty;\n    };\n\n    // Format currency\n    const formatCurrency = num => new Intl.NumberFormat(\"id-ID\", {\n      style: \"currency\",\n      currency: \"IDR\"\n    }).format(num);\n\n    // Format date\n    const formatDate = dateStr => {\n      if (!dateStr) return \"N/A\";\n      const date = new Date(dateStr);\n      return !isNaN(date.getTime()) ? date.toLocaleString(\"id-ID\", {\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\",\n        hour: \"2-digit\",\n        minute: \"2-digit\"\n      }) : \"Invalid Date\";\n    };\n\n    // Submit return\n    const submitReturn = async () => {\n      if (!selectedSaleId.value) {\n        alert(\"Pilih transaksi terlebih dahulu!\");\n        return;\n      }\n      const itemsToReturn = returnItems.value.filter(item => item.returnQty > 0 && item.reason.trim());\n      if (itemsToReturn.length === 0) {\n        alert(\"Masukkan jumlah retur dan alasan untuk setidaknya satu item!\");\n        return;\n      }\n      isSubmitting.value = true;\n      try {\n        const payload = {\n          sale_id: selectedSaleId.value,\n          date: new Date().toISOString(),\n          items: itemsToReturn.map(item => ({\n            product_id: item.Product.id,\n            qty: item.returnQty,\n            price: item.Product.sell_price || 0,\n            reason: item.reason.trim()\n          })),\n          notes: additionalNotes.value.trim() || null\n        };\n        console.log(\"Submitting payload:\", payload);\n        const res = await api.post(\"/kasir/sale-returns\", payload);\n        alert(`Retur berhasil! ID: ${res.data.id || res.data.ID}`);\n        selectedSaleId.value = null;\n        returnItems.value = [];\n        additionalNotes.value = \"\";\n        await loadSalesHistory();\n      } catch (err) {\n        console.error(\"Error submitting return:\", err.response?.data || err.message);\n        alert(\"Gagal mengirim retur! Periksa konsol.\");\n      } finally {\n        isSubmitting.value = false;\n      }\n    };\n    onMounted(() => {\n      loadSalesHistory();\n    });\n    const __returned__ = {\n      salesHistory,\n      loadingHistory,\n      selectedSaleId,\n      returnItems,\n      additionalNotes,\n      isSubmitting,\n      isFormValid,\n      api,\n      token,\n      loadSalesHistory,\n      loadSaleItems,\n      validateReturnQty,\n      formatCurrency,\n      formatDate,\n      submitReturn,\n      ref,\n      onMounted,\n      computed,\n      get axios() {\n        return axios;\n      },\n      AppSidebar\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMount