e = [];\n          totalTransactions.value = 0;\n          totalSalesValue.value = 0;\n        }\n      } catch (error) {\n        console.error(\"Failed to fetch dashboard data:\", error);\n        if (error.response) {\n          console.error(\"Status:\", error.response.status);\n        }\n        salesHistory.value = [];\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n    const resetFilter = () => {\n      fromDate.value = \"\";\n      toDate.value = \"\";\n      fetchDashboardData(); // Fetch 10 terakhir lagi\n    };\n    onMounted(() => {\n      fetchDashboardData(); // Default: 10 terakhir\n    });\n    const __returned__ = {\n      userName,\n      totalTransactions,\n      totalSalesValue,\n      salesHistory,\n      loadingHistory,\n      fromDate,\n      toDate,\n      fetchDashboardData,\n      resetFilter,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      CashierLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","CashierLayouts","userName","totalTransactions","totalSalesValue","salesHistory","loadingHistory","fromDate","toDate","fetchDashboardData","value","token","localStorage","getItem","nameFromStorage","params","limit","orderBy","from","to","response","get","headers","Authorization","data","total","length","sum","forEach","sale","total_amount","console","warn","error","status","resetFilter"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/DashboardCashier.vue"],"sourcesContent":["<template>\n  <CashierLayouts>\n    <div>\n      <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n        <p class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\">\n          Selamat datang, {{ userName }}\n        </p>\n      </div>\n\n      <!-- Filter Tanggal -->\n      <