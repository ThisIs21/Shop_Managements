ns.value = data.total || data.data.length;\n\n      let sum = 0;\n      data.data.forEach((sale) => {\n        sum += sale.total_amount || 0;\n      });\n      totalSalesValue.value = sum;\n    } else {\n      console.warn(\"API response is missing 'data' or 'total' property.\");\n      salesHistory.value = [];\n      totalTransactions.value = 0;\n      totalSalesValue.value = 0;\n    }\n  } catch (error) {\n    console.error(\"Failed to fetch dashboard data:\", error);\n    if (error.response) {\n      console.error(\"Status:\", error.response.status);\n    }\n    salesHistory.value = [];\n  } finally {\n    loadingHistory.value = false;\n  }\n};\n\nconst resetFilter = () => {\n  fromDate.value = \"\";\n  toDate.value = \"\";\n  fetchDashboardData();  // Fetch 10 terakhir lagi\n};\n\nonMounted(() => {\n  fetchDashboardData();  // Default: 10 terakhir\n});\n</script>"],"mappings":";;AAiHA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,cAAc,MAAM,iCAAiC;;;;;;;IAE5D,MAAMC,QAAQ,GAAGJ,GAAG,CAAC,OAAO,CAAC;IAC7B,MAAMK,iBAAiB,GAAGL,GAAG,CAAC,CAAC,CAAC;IAChC,MAAMM,eAAe,GAAGN,GAAG,CAAC,CAAC,CAAC;IAC9B,MAAMO,YAAY,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMQ,cAAc,GAAGR,GAAG,CAAC,IAAI,CAAC;;IAEhC;IACA,MAAMS,QAAQ,GAAGT,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMU,MAAM,GAAGV,GAAG,CAAC,EAAE,CAAC;IAEtB,MAAMW,kBAAkB,GAAG,MAAAA,CAAA,KAAY;MACrCH,cAAc,CAACI,KAAK,GAAG,IAAI;MAC3B,IAAI;QACF,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,MAAMC,eAAe,GAAGF,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;QAExD,IAAIC,eAAe,EAAE;UACnBZ,QAAQ,CAACQ,KAAK,GAAGI,eAAe;QAClC;;QAEA;QACA,MAAMC,MAAM,GAAG;UACbC,KAAK,EAAE,EAAE;UAAG;UACZC,OAAO,EAAE,MAAM,CAAG;QACpB,CAAC;;QAED;QACA,IAAIV,QAAQ,CAACG,KAAK,EAAEK,MAAM,CAACG,IAAI,GAAGX,QAAQ,CAACG,KAAK;QAChD,IAAIF,MAAM,CAACE,KAAK,EAAEK,MAAM,CAACI,EAAE,GAAGX,MAAM,CAACE,KAAK;QAE1C,MAAMU,QAAQ,GAAG,MAAMpB,KAAK,CAACqB,GAAG,CAAC,4CAA4C,EAAE;UAC7EC,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUZ,KAAK;UAAG,CAAC;UAC7CI;QACF,CAAC,CAAC;QAEF,MAAMS,IAAI,GAAGJ,QAAQ,CAACI,IAAI;QAE1B,IAAIA,IAAI,IAAIA,IAA