        }\n        const res = await api.get(\"/history/sales\", {\n          params\n        });\n        salesHistory.value = (res.data.data || []).map(sale => ({\n          ...sale,\n          items: sale.items || []\n        }));\n      } catch (err) {\n        console.error(\"Error loading sales history:\", err);\n        alert(\"Failed to load sales history!\");\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n\n    // Reset filter\n    const resetFilter = () => {\n      fromDate.value = \"\";\n      toDate.value = \"\";\n      loadSalesHistory();\n    };\n\n    // Select/Deselect all sales\n    const selectAllItems = event => {\n      if (event.target.checked) {\n        selectedSales.value = [...salesHistory.value];\n      } else {\n        selectedSales.value = [];\n      }\n    };\n\n    // Add selected sales items to the return table\n    const addToReturnTable = () => {\n      selectedSales.value.forEach(sale => {\n        sale.items.forEach(item => {\n          const existingItem = returnItems.value.find(returnItem => returnItem.product.id === item.product.id);\n          if (!existingItem) {\n            returnItems.value.push({\n              product: item.product,\n              qty: item.qty,\n              returnQty: 0,\n              reason: \"\"\n            });\n          }\n        });\n      });\n      selectedSales.value = [];\n    };\n\n    // Remove item from return table\n    const removeItem = index => {\n      returnItems.value.splice(index, 1);\n    };\n\n    // Format currency\n    const formatCurrency = amount => {\n      return amount !== undefined && amount !== null ? new Intl.NumberFormat(\"id-ID\", {\n        style: \"currency\",\n        currency: \"IDR\",\n        minimumFractionDigits: 0\n      }).format(amount) : \"Rp 0\";\n    };\n\n    // Format date\n    const formatDate = dateStr => {\n      if (!dateStr) return \"N/A\";\n      const date = new Date(dateStr);\n      return !isNaN(date.getTime()) ? date.toLocaleString(\"id-ID\", {\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\",\n        hour: \"2-digit\",\n        minute: \"2-digit\"\n      }) : \"Invalid Date\";\n    };\n\n    // Submit return\n    const submitReturn = async () => {\n      const itemsToReturn = returnItems.value.filter(item => item.returnQty > 0 && item.reason);\n      if (itemsToReturn.length === 0) {\n        alert(\"Please enter return quantity and reason for at least one item!\");\n        return;\n      }\n      isSubmitting.value = true;\n      try {\n        const payload = {\n          date: new Date().toISOString(),\n          notes: additionalNotes.value || null,\n          items: itemsToReturn.map(item => ({\n            product_id: item.product.id,\n            qty: item.returnQty,\n            reason: item.reason\n          }))\n        };\n        const res = await api.post(\"/kasir/sale-returns\", payload);\n        alert(`Return successful! ID: ${res.data.id}`);\n\n        // Reset form after successful submission\n        returnItems.value = [];\n        additionalNotes.value = \"\";\n        loadSalesHistory();\n      } catch (err) {\n        console.error(\"Error submitting return:\", err);\n        alert(\"Failed to submit return: \" + (err.response?.data?.error || err.message));\n      } finally {\n        isSubmitting.value = false;\n      }\n    };\n\n    // Load data on mount\n    onMounted(() => {\n      if (!token) {\n        alert(\"Please login again!\");\n        return;\n      }\n      loadSalesHistory();\n    });\n    const __returned__ = {\n      salesHistory,\n      loadingHistory,\n      selectedSales,\n      fromDate,\n      toDate,\n      returnItems,\n      additionalNotes,\n      isSubmitting,\n      api,\n      token,\n      isAllSelected,\n      loadSalesHistory,\n      resetFilter,\n      selectAllItems,\n      addToReturnTable,\n      removeItem,\n      formatCurrency,\n      formatDate,\n      submitReturn,\n      r