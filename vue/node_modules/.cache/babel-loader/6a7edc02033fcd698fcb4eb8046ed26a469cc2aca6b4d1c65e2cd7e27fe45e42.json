ner/categories/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        fetchCategories();\n      } catch (error) {\n        console.error(\"Error deleting category:\", error);\n      }\n    };\n    onMounted(() => {\n      fetchCategories();\n    });\n    const __returned__ = {\n      categories,\n      loading,\n      newCategory,\n      fetchCategories,\n      createCategory,\n      deleteCategory,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      OwnerLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","OwnerLayouts","categories","loading","newCategory","fetchCategories","token","localStorage","getItem","console","error","value","response","get","headers","Authorization","data","createCategory","trim","post","Name","deleteCategory","id","confirm","delete"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/ManagementCategoryOwner.vue"],"sourcesContent":["<template>\r\n  <OwnerLayouts>\r\n    <div class=\"flex flex-1 justify-center py-5\">\r\n      <div class=\"layout-content-container flex flex-col max-w-[960px] flex-1\">\r\n        <!-- Title -->\r\n        <div class=\"flex flex-wrap justify-between gap-3 p-4\">\r\n          <p\r\n            class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\"\r\n          >\r\n            Management Category\r\n          </p>\r\n        </div>\r\n\r\n        <!-- Form Create -->\r\n        <div class=\"px-4 py-3\">\r\n          <div\r\n            class=\"flex flex-col gap-3 overflow-hidden rounded-lg border border-[#dbe0e6] bg-white p-5\"\r\n          >\r\n            <h2 class=\"text-lg font-semibold\">Add Category</h2>\r\n            <form @submit.prevent=\"createCategory\" class=\"flex gap-3\">\r\n         