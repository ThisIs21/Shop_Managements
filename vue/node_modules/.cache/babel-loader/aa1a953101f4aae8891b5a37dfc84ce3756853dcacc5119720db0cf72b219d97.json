{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport OwnerLayouts from \"../components/OwnerLayouts.vue\";\nimport { useRouter } from \"vue-router\";\nexport default {\n  __name: 'ManagementProductOwner',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const products = ref([]);\n    const loading = ref(true);\n    const router = useRouter();\n    const fetchProducts = async () => {\n      loading.value = true;\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) return;\n        const res = await axios.get(\"http://192.168.100.111:8081/api/v1/products\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        console.log(\"Response data:\", res.data); // Tambahkan log ini\n        products.value = res.data.data || [];\n      } catch (err) {\n        console.error(\"Error fetching products:\", err);\n        products.value = [];\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // Format angka menjadi mata uang\n    const formatCurrency = value => {\n      return new Intl.NumberFormat(\"id-ID\", {\n        style: \"currency\",\n        currency: \"IDR\"\n      }).format(value);\n    };\n\n    // Delete product\n    const deleteProduct = async id => {\n      const confirmed = confirm(\"Are you sure to delete this product?\");\n      if (!confirmed) return;\n      try {\n        const token = localStorage.getItem(\"token\");\n        await axios.delete(`http://192.168.100.111:8081/api/v1/products/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        products.value = products.value.filter(p => p.id !== id);\n        alert(\"Product deleted successfully\");\n      } catch (err) {\n        console.error(err);\n        alert(\"Failed to delete product\");\n      }\n    };\n\n    // Edit product\n    const editProduct = id => {\n      router.push(`/edit-product/${id}`);\n    };\n    onMounted(fetchProducts);\n    const __returned__ = {\n      products,\n      loading,\n      router,\n      fetchProducts,\n      formatCurrency,\n      deleteProduct,\n      editProduct,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      OwnerLayouts,\n      get useRouter() {\n        return useRouter;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","OwnerLayouts","useRouter","products","loading","router","fetchProducts","value","token","localStorage","getItem","res","get","headers","Authorization","console","log","data","err","error","formatCurrency","Intl","NumberFormat","style","currency","format","deleteProduct","id","confirmed","confirm","delete","filter","p","alert","editProduct","push"],"sources":["C:/Users/SPC STYLE 5/aplikasi/src/views/ManagementProductOwner.vue"],"sourcesContent":["<template>\n  <OwnerLayouts>\n    <div class=\"flex flex-1 flex-col py-5 p-8\">\n      <div class=\"layout-content-container flex flex-col max-w-[960px] flex-1\">\n        <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n          <p\n            class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\"\n          >\n            Product List\n          </p>\n          <router-link\n            to=\"/add-product\"\n            class=\"flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-lg h-8 px-4 bg-[#f0f2f5] text-[#111418] text-sm font-medium leading-normal\"\n          >\n            <span class=\"truncate\">Add Product</span>\n          </router-link>\n        </div>\n\n        <div clas