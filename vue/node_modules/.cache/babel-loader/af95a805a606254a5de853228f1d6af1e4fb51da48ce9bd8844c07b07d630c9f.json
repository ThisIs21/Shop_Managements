{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport CashierLayouts from \"@/components/CashierLayouts.vue\";\n\n// Definisikan state reaktif\n\nexport default {\n  __name: 'DashboardCashier',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const userName = ref(\"Kasir\"); // Default value\n    const totalTransactions = ref(0);\n    const totalSalesValue = ref(0);\n    const salesHistory = ref([]);\n    const loadingHistory = ref(true);\n\n    // Fungsi untuk mengambil data dashboard\n    const fetchDashboardData = async () => {\n      try {\n        // Ambil token dan nama pengguna dari localStorage\n        const token = localStorage.getItem(\"token\");\n        const nameFromStorage = localStorage.getItem(\"userName\");\n\n        // Perbarui nama pengguna dari localStorage\n        if (nameFromStorage) {\n          userName.value = nameFromStorage;\n        }\n\n        // PENTING: Ganti URL ini dengan URL API Anda yang sebenarnya\n        const response = await axios.get(\"http://localhost:8081/api/v1/history/sales\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        const data = response.data;\n        totalTransactions.value = data.totalTransactions;\n        totalSalesValue.value = data.totalSalesValue;\n        salesHistory.value = data.salesHistory;\n      } catch (error) {\n        console.error(\"Failed to fetch dashboard data:\", error);\n        // Tambahkan logika penanganan error, misalnya mengarahkan ke halaman login jika token tidak valid\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n\n    // Panggil fungsi saat komponen dimuat\n    onMounted(() => {\n      fetchDashboardData();\n    });\n    const __returned__ = {\n      userName,\n      totalTransactions,\n      totalSalesValue,\n      salesHistory,\n      loadingHistory,\n      fetchDashboardData,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      CashierLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","CashierLayouts","userName","totalTransactions","totalSalesValue","salesHistory","loadingHistory","fetchDashboardData","token","localStorage","getItem","nameFromStorage","value","response","get","headers","Authorization","data","error","console"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/DashboardCashier.vue"],"sourcesContent":["<template>\n  <CashierLayouts>\n    <div>\n      <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n        <p\n          class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\"\n        >\n          Selamat datang, {{ userName }}\n        </p>\n      </div>\n\n      <div class=\"flex flex-wrap gap-4 p-4\">\n        <div\n          class=\"flex min-w-[158px] flex-1 flex-col gap-2 rounded-lg p-6 bg-[#f0f2f5]\"\n        >\n          <p class=\"text-[#111418] text-base font-medium leading-normal\">\n            Total Transaksi\n          </p>\n          <p\n            class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\"\n          >\n            {{ totalTransactions }}\n          </p>\n        </div>\n        <div\n          class=\"flex min-w-[158px] flex-1 flex-col gap-2 rounded-lg p-6 bg-[#f0f2f5]\"\n        >\n          <p class=\"text-[#111418] text-base font-medium leading-normal\">\n            Nilai Penjualan\n          </p>\n          <p\n            class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\"\n          >\n            ${{ totalSalesValue.toLocaleString() }}\n          </p>\n        </div>\n      </div>\n\n      <div class=\"p-4\">\n        <p\n          class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\"\n        >\n          History Penjualan Hari Ini\n        </p>\n        <div\n          class=\"mt-4 overflow-x-auto rounded-lg border border-[#dbe0e6] bg-white\"\n        >\n          <table class=\"min-w-full\">\n            <thead>\n              <tr class=\"bg-gray-100\">\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Tanggal\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Nomor Transaksi\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Total\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Detail\n                </th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-if=\"loadingHistory\">\n                <td colspan=\"4\" class=\"text-center py-4\">Loading history...</td>\n              </tr>\n              <tr v-else-if=\"salesHistory.length === 0\">\n                <td colspan=\"4\" class=\"text-center py-4 text-gray-500\">\n                  Tidak ada data penjualan hari ini.\n                </td>\n              </tr>\n              <tr\n                v-else\n                v-for=\"sale in salesHistory\"\n                :key=\"sale.id\"\n                class=\"border-t border-t-[#dbe0e6]\"\n              >\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{ new Date(sale.date).toLocaleDateString() }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{ sale.transaction_number }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  ${{ sale.total_amount.toLocaleString() }}\n                </td>\n                <td\n                  class=\"px-6 py-4 whitespace-nowrap text-sm text-blue-500 hover:text-blue-700 cursor-pointer\"\n                >\n                  Lihat Detail\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  </CashierLayouts>\n</template>\n\n<script setup>\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport CashierLayouts from \"@/components/CashierLayouts.vue\";\n\n// Definisikan state reaktif\nconst userName = ref(\"Kasir\"); // Default value\nconst totalTransactions = ref(0);\nconst totalSalesValue = ref(0);\nconst salesHistory = ref([]);\nconst loadingHistory = ref(true);\n\n// Fungsi untuk mengambil data dashboard\nconst fetchDashboardData = async () => {\n  try {\n    // Ambil token dan nama pengguna dari localStorage\n    const token = localStorage.getItem(\"token\");\n    const nameFromStorage = localStorage.getItem(\"userName\");\n\n    // Perbarui nama pengguna dari localStorage\n    if (nameFromStorage) {\n      userName.value = nameFromStorage;\n    }\n\n    // PENTING: Ganti URL ini dengan URL API Anda yang sebenarnya\n    const response = await axios.get(\"http://localhost:8081/api/v1/history/sales\", {\n      headers: {\n        Authorization: `Bearer ${token}`,\n      },\n    });\n\n    const data = response.data;\n    totalTransactions.value = data.totalTransactions;\n    totalSalesValue.value = data.totalSalesValue;\n    salesHistory.value = data.salesHistory;\n  } catch (error) {\n    console.error(\"Failed to fetch dashboard data:\", error);\n    // Tambahkan logika penanganan error, misalnya mengarahkan ke halaman login jika token tidak valid\n  } finally {\n    loadingHistory.value = false;\n  }\n};\n\n// Panggil fungsi saat komponen dimuat\nonMounted(() => {\n  fetchDashboardData();\n});\n</script>\n\n<style scoped>\n/* Tambahkan style jika diperlukan */\n</style>\n"],"mappings":"AA+GA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,cAAc,MAAM,iCAAiC;;AAE5D;;;;;;;;IACA,MAAMC,QAAQ,GAAGJ,GAAG,CAAC,OAAO,CAAC,CAAC,CAAC;IAC/B,MAAMK,iBAAiB,GAAGL,GAAG,CAAC,CAAC,CAAC;IAChC,MAAMM,eAAe,GAAGN,GAAG,CAAC,CAAC,CAAC;IAC9B,MAAMO,YAAY,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMQ,cAAc,GAAGR,GAAG,CAAC,IAAI,CAAC;;IAEhC;IACA,MAAMS,kBAAkB,GAAG,MAAAA,CAAA,KAAY;MACrC,IAAI;QACF;QACA,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,MAAMC,eAAe,GAAGF,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;;QAExD;QACA,IAAIC,eAAe,EAAE;UACnBT,QAAQ,CAACU,KAAK,GAAGD,eAAe;QAClC;;QAEA;QACA,MAAME,QAAQ,GAAG,MAAMb,KAAK,CAACc,GAAG,CAAC,4CAA4C,EAAE;UAC7EC,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUR,KAAK;UAChC;QACF,CAAC,CAAC;QAEF,MAAMS,IAAI,GAAGJ,QAAQ,CAACI,IAAI;QAC1Bd,iBAAiB,CAACS,KAAK,GAAGK,IAAI,CAACd,iBAAiB;QAChDC,eAAe,CAACQ,KAAK,GAAGK,IAAI,CAACb,eAAe;QAC5CC,YAAY,CAACO,KAAK,GAAGK,IAAI,CAACZ,YAAY;MACxC,CAAC,CAAC,OAAOa,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;QACvD;MACF,CAAC,SAAS;QACRZ,cAAc,CAACM,KAAK,GAAG,KAAK;MAC9B;IACF,CAAC;;IAED;IACAb,SAAS,CAAC,MAAM;MACdQ,kBAAkB,CAAC,CAAC;IACtB,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}