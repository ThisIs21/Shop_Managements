 \" + token;\n        await loadProducts();\n      } catch (err) {\n        alert(\"Login gagal: \" + (err.response?.data?.error || err.message));\n        console.error(\"Error login:\", err); // Debugging\n      }\n    };\n\n    // load produk\n    const loadProducts = async () => {\n      try {\n        console.log(\"Memulai loadProducts\");\n        const res = await api.get(\"/api/v1/products\");\n        console.log(\"Produk dari API:\", res.data);\n        products.value = res.data.data || [];\n        categories.value = [...new Set(products.value.map(p => p.category?.name || \"Unknown\"))];\n      } catch (err) {\n        console.error(\"Gagal load produk:\", err.response?.data || err.message);\n        if (err.response?.status === 401) {\n          isLoggedIn.value = false; // token invalid â†’ balik ke login\n        }\n      }\n    };\n    onMounted(() => {\n      if (isLoggedIn.value) loadProducts();\n    });\n    const filteredProducts = computed(() => products.value.filter(p => {\n      const matchCategory = selectedCategory.value === \"All\" || p.category?.name === selectedCategory.value;\n      const matchSearch = p.name.toLowerCase().includes(search.value.toLowerCase());\n      return matchCategory && matchSearch;\n    }));\n    const addToCart = product => {\n      const exist = cart.value.find(item => item.id === product.id);\n      if (exist) {\n        if (exist.qty < product.stock) {\n          exist.qty++;\n        }\n      } else {\n        cart.value.push({\n          ...product,\n          qty: 1\n        });\n      }\n    };\n    const removeFromCart = i => cart.value.splice(i, 1);\n    const formatCurrency = num => new Intl.NumberFormat(\"id-ID\", {\n      style: \"currency\",\n      currency: \"IDR\"\n    }).format(num);\n    const totalAmount = computed(() => cart.value.reduce((sum, item) => sum + item.sell_price * item.qty, 0));\n    const change = computed(() => amountReceived.value - totalAmount.value);\n    const checkout = async () => {\n      if (cart.value.length === 0) return alert(\"Cart kosong\");\n      try {\n        const payload = {\n          customer_id: 1,\n          date: new Date(),\n          items: cart.value.map(item => ({\n            product_id: item.id,\n            qty: item.qty,\n            price: item.sell_price\n          }))\n        };\n        console.log(\"Payload checkout:\", payload); // Debugging\n        const res = await api.post(\"/api/v1/kasir/sales\", payload);\n        console.log(\"Respon checkout:\", res.data);\n        alert(\"Transaksi berhasil dibuat!\");\n        cart.value = [];\n        amountReceived.value = 0;\n      } catch (err) {\n        console.error(\"Checkout gagal:\", err.response?.data || err.message);\n        alert(\"Checkout gagal: \" + (err.response?.data?.error || err.message));\n      }\n    };\n    const handlePrintReceipt = () => {\n      window.print();\n    };\n    const __returned__ = {\n      username,\n      password,\n      isLoggedIn,\n      api,\n      products,\n      cart,\n      search,\n      selectedCategory,\n      categories,\n      amountReceived,\n      login,\n      loadProducts,\n      filteredProducts,\n      addToCart,\n      removeFromCart,\n      formatCurrency,\n      totalAmount,\n      change,\n      checkout,\n      handlePrintReceipt,\n      ref,\n      computed,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      CashierSidebar\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","axios","CashierSidebar","username","password","isLoggedIn","localStorage","getItem","api","create","baseURL","products","cart","search","selectedCategory","categories","amountReceived","login","res","post","value","token","data","setItem","console","log","defaults","headers","loadProducts","err","alert","response","error","message","get","Set","map","p","category","name","status","filteredProducts","filter","matchCategory","matchSearch","toLowerCase","includes","addToCart","product","exist","find","item","id","qty","stock","push","removeFromCart","i","splice","formatCurrency","num","Intl","NumberFormat","style","currency","format","totalAmount","reduce","sum","sell_price","change","checkout","length","payload","customer_id","date","Date","items","product_id","price","handlePrintReceipt","window","print"],"sources":["C:/Users/SPC STYLE 5/aplikasi/src/views/CashierPage.vue"],"sourcesContent":["<template>\n  <div class=\"min-h-screen flex bg-white\">\n    <!-- Sidebar -->\n    <CashierSidebar v-if=\"isLoggedIn\" />\n\n    <!-- Main -->\n    <main class=\"flex-1 p-6 max-w-7xl mx-auto w-full\">\n      <h2 class=\"text-2xl font-bold text-center mb-6\">Cashier - Order</h2>\n\n      <!-- ðŸ”‘ LOGIN FORM -->\n      <div\n        v-if=\"!isLoggedIn\"\n        class=\"max-w-sm mx-auto border rounded p-6 shadow\"\n      >\n        <h3 class=\"text-lg font-semibold mb-4\">Login Kasir</h3>\n        <input\n          v-model=\"username\"\n          type=\"text\"\n          placeholder=\"Username\"\n          class=\"w-full border rounded p-2 mb-3 text-sm\"\n        />\n        <input\n          v-model=\"password\"\n          type=\"password\"\n          placeholder=\"Password\"\n          class=\"w-full border rounded p-2 mb-3 text-sm\"\n        />\n        <button\n          @click=\"login\"\n          class=\"w-full bg-blue-600 text-white py-2 rounded hover:bg-blue-700\"\n        >\n          Login\n        </button>\n      </div>\n\n      <!-- ðŸ”¹ HALAMAN KASIR -->\n      <div v-else class=\"grid grid-cols-1 md:grid-cols-3 gap-6\">\n        <!-- LEFT: Products -->\n        <section class=\"md:col-span-2\">\n          <h3 class=\"text-lg font-semibold mb-4\">Available Products</h3>\n\n          <!-- Search + Filter -->\n          <div\n            class=\"flex flex-col md:flex-row md:items-center md:justify-between gap-3 mb-4\"\n          >\n            <input\n              v-model=\"search\"\n              type=\"text\"\n              placeholder=\"Cari produk...\"\n              class=\"w-full md:w-1/2 border rounded p-2 text-sm\"\n            />\n            <select\n              v-model=\"selectedCategory\"\n              class=\"border rounded p-2 text-sm w-full md:w-48\"\n            >\n              <option value=\"All\">Semua Kategori</option>\n              <option v-for=\"cat in categories\" :key=\"cat\" :value=\"cat\">\n                {{ cat }}\n              </option>\n            </select>\n          </div>\n\n          <!-- Product Grid -->\n          <div class=\"grid grid-cols-2 gap-4\">\n            <div\n              v-for=\"p in filteredProducts\"\n              :key=\"p.id\"\n              class=\"border rounded-lg p-4 bg-white shadow flex flex-col justify-between\"\n            >\n              <div>\n                <h4 class=\"font-semibold\">{{ p.name }}</h4>\n                <p class=\"text-sm text-gray-600\">\n                  {{ formatCurrency(p.sell_price) }}\n                </p>\n                <p class=\"text-xs text-green-600\">Stock: {{ p.stock }}</p>\n              </div>\n              <button\n                @click=\"addToCart(p)\"\n                class=\"mt-3 bg-blue-500 text-white text-sm py-1 rounded hover:bg-blue-600\"\n              >\n                Add\n              </button>\n            </div>\n          </div>\n        </section>\n\n        <!-- RIGHT: Cart -->\n        <section class=\"md:col-span-1 flex flex-col\">\n          <h3 class=\"text-lg font-semibold mb-4\">Cart Order</h3>\n\n          <!-- Cart Table -->\n          <div\n            class=\"border rounded-lg p-3 bg-white shadow max-h-64 overflow-y-auto\"\n          >\n            <table class=\"w-full text-sm\">\n              <thead class=\"sticky top-0 bg-gray-100\">\n                <tr>\n                  <th class=\"p-2\">Item</th>\n                  <th class=\"p-2\">Qty</th>\n                  <th class=\"p-2\">Subtotal</th>\n                  <th class=\"p-2\">X</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-for=\"(item, i) in cart\" :key=\"item.id\" class=\"border-t\">\n                  <td class=\"p-2\">{{ item.name }}</td>\n                  <td class=\"p-2\">\n                    <input\n                      type=\"number\"\n                      v-model.number=\"item.qty\"\n                      min=\"1\"\n                      :max=\"item.stock\"\n                      class=\"w-14 border rounded p-1\"\n                    />\n                  </td>\n                  <td class=\"p-2\">\n                    {{ formatCurrency(item.sell_price * item.qty) }}\n                  </td>\n                  <td class=\"p-2\">\n                    <button @click=\"removeFromCart(i)\" class=\"text-red-500\">\n                      âœ•\n                    </button>\n                  </td>\n                </tr>\n                <tr v-if=\"cart.length === 0\">\n                  <td colspan=\"4\" class=\"text-center text-gray-500 py-4\">\n                    Cart kosong\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n\n          <!-- Order Summary -->\n          <div class=\"mt-6 border rounded-lg p-3 bg-white shadow\">\n            <h4 class=\"font-semibold mb-2\">Order Summary</h4>\n            <div class=\"flex justify-between text-sm\">\n              <span>Total</span>\n              <span>{{ formatCurrency(totalAmount) }}</span>\n            </div>\n\n            <!-- Payment -->\n            <div class=\"mt-4\">\n              <label class=\"block mb-2 text-sm\">Amount Received</label>\n              <input\n                v-model.number=\"amountReceived\"\n                type=\"number\"\n                placeholder=\"Masukkan nominal\"\n                class=\"w-full border rounded p-2 text-sm\"\n              />\n              <p class=\"text-sm mt-1\">\n                Change:\n                <span class=\"font-semibold\">{{ formatCurrency(change) }}</span>\n              </p>\n            </div>\n\n            <!-- Buttons -->\n            <div class=\"flex flex-col gap-3 mt-6\">\n              <button\n                @click=\"checkout\"\n                class=\"w-full bg-blue-600 text-white py-2 rounded hover:bg-blue-700\"\n              >\n                Pay Now\n              </button>\n              <button\n                @click=\"handlePrintReceipt\"\n                class=\"w-full bg-gray-100 py-2 rounded hover:bg-gray-200\"\n              >\n                Print Receipt\n              </button>\n            </div>\n          </div>\n        </section>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport CashierSidebar from \"@/components/CashierSidebar.vue\";\n\n// state login\nconst username = ref(\"\");\nconst password = ref(\"\");\nconst isLoggedIn = ref(!!localStorage.getItem(\"token\"));\n\n// Inisialisasi axios tanpa header Authorization awal\nconst api = axios.create({\n  baseURL: \"http://localhost:8081\",\n});\n\nconst products = ref([]);\nconst cart = ref([]);\nconst search = ref(\"\");\nconst selectedCategory = ref(\"All\");\nconst categories = ref([]);\nconst amountReceived = ref(0);\n\n// login\nconst login = async () => {\n  try {\n    const res = await axios.post(\"http://localhost:8081/api/v1/auth/login\", {\n      username: username.value,\n      password: password.value,\n    });\n    const token = res.data.token;\n    localStorage.setItem(\"token\", token);\n    console.log(\"Token disimpan:\", token); // Debugging\n    isLoggedIn.value = true;\n    // Set header Authorization setelah token diterima\n    api.defaults.headers[\"Authorization\"] = \"Bearer \" + token;\n    await loadProducts();\n  } catch (err) {\n    alert(\"Login gagal: \" + (err.response?.data?.error || err.message));\n    console.error(\"Error login:\", err); // Debugging\n  }\n};\n\n// load produk\nconst loadProducts = async () => {\n  try {\n    console.log(\"Memulai loadProducts\");\n    const res = await api.get(\"/api/v1/products\");\n    console.log(\"Produk dari API:\", res.data);\n    products.value = res.data.data || [];\n    categories.value = [\n      ...new Set(products.value.map((p) => p.category?.name || \"Unknown\")),\n    ];\n  } catch (err) {\n    console.error(\"Gagal load produk:\", err.response?.data || err.message);\n    if (err.response?.status === 401) {\n      isLoggedIn.value = false; // token invalid â†’ balik ke login\n    }\n  }\n};\n\nonMounted(() => {\n  if (isLoggedIn.value) loadProducts();\n});\n\nconst filteredProducts = computed(() =>\n  products.value.filter((p) => {\n    const matchCategory =\n      selectedCategory.value === \"All\" ||\n      p.category?.name === selectedCategory.value;\n    const matchSearch = p.name\n      .toLowerCase()\n      .includes(search.value.toLowerCase());\n    return matchCategory && matchSearch;\n  })\n);\n\nconst addToCart = (product) => {\n  const exist = cart.value.find((item) => item.id === product.id);\n  if (exist) {\n    if (exist.qty < product.stock) {\n      exist.qty++;\n    }\n  } else {\n    cart.value.push({ ...product, qty: 1 });\n  }\n};\n\nconst removeFromCart = (i) => cart.value.splice(i, 1);\n\nconst formatCurrency = (num) =>\n  new Intl.NumberFormat(\"id-ID\", { style: \"currency\", currency: \"IDR\" }).format(\n    num\n  );\n\nconst totalAmount = computed(() =>\n  cart.value.reduce((sum, item) => sum + item.sell_price * item.qty, 0)\n);\n\nconst change = computed(() => amountReceived.value - totalAmount.value);\n\nconst checkout = async () => {\n  if (cart.value.length === 0) return alert(\"Cart kosong\");\n\n  try {\n    const payload = {\n      customer_id: 1,\n      date: new Date(),\n      items: cart.value.map((item) => ({\n        product_id: item.id,\n        qty: item.qty,\n        price: item.sell_price,\n      })),\n    };\n\n    console.log(\"Payload checkout:\", payload); // Debugging\n    const res = await api.post(\"/api/v1/kasir/sales\", payload);\n    console.log(\"Respon checkout:\", res.data);\n    alert(\"Transaksi berhasil dibuat!\");\n    cart.value = [];\n    amountReceived.value = 0;\n  } catch (err) {\n    console.error(\"Checkout gagal:\", err.response?.data || err.message);\n    alert(\"Checkout gagal: \" + (err.response?.data?.error || err.message));\n  }\n};\n\nconst handlePrintReceipt = () => {\n  window.print();\n};\n</script>\n"],"mappings":";;;;;;;;;;;;;AAmLA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,QAAQ,KAAK;AAC9C,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,cAAc,MAAM,iCAAiC;;AAE5D;;;;;;;;IACA,MAAMC,QAAQ,GAAGL,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMM,QAAQ,GAAGN,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMO,UAAU,GAAGP,GAAG,CAAC,CAAC,CAACQ,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,CAAC;;IAEvD;IACA,MAAMC,GAAG,GAAGP,KAAK,CAACQ,MAAM,CAAC;MACvBC,OAAO,EAAE;IACX,CAAC,CAAC;IAEF,MAAMC,QAAQ,GAAGb,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMc,IAAI,GAAGd,GAAG,CAAC,EAAE,CAAC;IACpB,MAAMe,MAAM,GAAGf,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMgB,gBAAgB,GAAGhB,GAAG,CAAC,KAAK,CAAC;IACnC,MAAMiB,UAAU,GAAGjB,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMkB,cAAc,GAAGlB,GAAG,CAAC,CAAC,CAAC;;IAE7B;IACA,MAAMmB,KAAK,GAAG,MAAAA,CAAA,KAAY;MACxB,IAAI;QACF,MAAMC,GAAG,GAAG,MAAMjB,KAAK,CAACkB,IAAI,CAAC,yCAAyC,EAAE;UACtEhB,QAAQ,EAAEA,QAAQ,CAACiB,KAAK;UACxBhB,QAAQ,EAAEA,QAAQ,CAACgB;QACrB,CAAC,CAAC;QACF,MAAMC,KAAK,GAAGH,GAAG,CAACI,IAAI,CAACD,KAAK;QAC5Bf,YAAY,CAACiB,OAAO,CAAC,OAAO,EAAEF,KAAK,CAAC;QACpCG,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAEJ,KAAK,CAAC,CAAC,CAAC;QACvChB,UAAU,CAACe,KAAK,GAAG,IAAI;QACvB;QACAZ,GAAG,CAACkB,QAAQ,CAACC,OAAO,CAAC,eAAe,CAAC,GAAG,SAAS,GAAGN,KAAK;QACzD,MAAMO,YAAY,CAAC,CAAC;MACtB,CAAC,CAAC,OAAOC,GAAG,EAAE;QACZC,KAAK,CAAC,eAAe,IAAID,GAAG,CAACE,QAAQ,EAAET,IAAI,EAAEU,KAAK,IAAIH,GAAG,CAACI,OAAO,CAAC,CAAC;QACnET,OAAO,CAACQ,KAAK,CAAC,cAAc,EAAEH,GAAG,CAAC,CAAC,CA