{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport GudangLayouts from \"@/components/GudangLayouts.vue\";\n\n// --------------------------------------------------------\n// 1. STATE MANAGEMENT (Tidak Berubah)\n// --------------------------------------------------------\n\nexport default {\n  __name: 'PurchaseReturn',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const purchaseReturns = ref([]);\n    const loadingReturns = ref(false);\n    const returnFromDate = ref(\"\");\n    const returnToDate = ref(\"\");\n    const returnStatusFilter = ref(\"\");\n    const purchasesHistory = ref([]);\n    const loadingHistory = ref(false);\n    const showSelectPOModal = ref(false);\n    const fromDate = ref(\"\");\n    const toDate = ref(\"\");\n    const showModal = ref(false);\n    const selectedPurchase = ref(null);\n    const returnForm = ref({\n      items: [],\n      notes: \"\"\n    });\n    const submitting = ref(false);\n\n    // --------------------------------------------------------\n    // 2. COMPUTED PROPERTIES (Tidak Berubah)\n    // --------------------------------------------------------\n    const hasValidItems = computed(() => {\n      return returnForm.value.items.some(item => item.qty > 0 && item.reason.trim().length > 3);\n    });\n    const validReturnItemsCount = computed(() => {\n      return returnForm.value.items.filter(item => item.qty > 0 && item.reason.trim().length > 3).length;\n    });\n\n    // --------------------------------------------------------\n    // 3. PENGATURAN API (Tidak Berubah)\n    // --------------------------------------------------------\n    const api = axios.create({\n      baseURL: \"http://localhost:8081/api/v1\"\n    });\n    const savedToken = localStorage.getItem(\"token\");\n    if (savedToken) {\n      api.defaults.headers.common[\"Authorization\"] = `Bearer ${savedToken}`;\n    }\n\n    // --------------------------------------------------------\n    // 4. FUNGSI UNTUK DAFTAR RETUR UTAMA (Tidak Berubah)\n    // --------------------------------------------------------\n    const fetchPurchaseReturns = async () => {\n      loadingReturns.value = true;\n      try {\n        const params = {\n          page: 1,\n          size: 10\n        };\n        if (returnFromDate.value) params.start_date = returnFromDate.value;\n        if (returnToDate.value) params.end_date = returnToDate.value;\n        if (returnStatusFilter.value) params.status = returnStatusFilter.value;\n        const response = await api.get(\"/history/purchase-returns\", {\n          params\n        });\n        const rawData = response.data.data || response.data || [];\n        purchaseReturns.value = Array.isArray(rawData) ? rawData.filter(ret => ret && ret.id) : [];\n      } catch (error) {\n        console.error(\"Error fetching returns:\", error.response?.data || error.message);\n        alert(`Gagal mengambil daftar retur: ${error.response?.data?.error || \"Cek otorisasi.\"}`);\n        purchaseReturns.value = [];\n      } finally {\n        loadingReturns.value = false;\n      }\n    };\n    const resetReturnFilter = () => {\n      returnFromDate.value = \"\";\n      returnToDate.value = \"\";\n      returnStatusFilter.value = \"\";\n      fetchPurchaseReturns();\n    };\n\n    // --------------------------------------------------------\n    // 5. FUNGSI UNTUK MODAL PILIH PO (Tidak Berubah)\n    // --------------------------------------------------------\n    const fetchPurchasesHistory = async (status = \"RECEIVED\") => {\n      loadingHistory.value = true;\n      try {\n        const params = {\n          page: 1,\n          size: 10,\n          status: status\n        };\n        if (fromDate.value) params.start_date = fromDate.value;\n        if (toDate.value) params.end_date = toDate.value;\n        const response = await api.get(\"/history/purchases\", {\n          params\n        });\n        const data = response.data;\n        let rawData = [];\n        if (data && data.data && Array.isArray(data.data.data)) rawData = data.data.data;else if (data && Array.isArray(data.data)) rawData = data.data;else if (data && Array.isArray(data)) rawData = data;\n        purchasesHistory.value = rawData.filter(p => p && (p.ID || p.id));\n      } catch (error) {\n        console.error(\"Error fetch purchases for manual return:\", error);\n        alert(`Error memuat PO: ${error.response?.data?.error || \"Kesalahan jaringan.\"}`);\n        purchasesHistory.value = [];\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n    const openSelectPOModal = () => {\n      fetchPurchasesHistory(\"RECEIVED\");\n      showSelectPOModal.value = true;\n    };\n    const closeSelectPOModal = () => {\n      showSelectPOModal.value = false;\n      purchasesHistory.value = [];\n    };\n    const resetFilter = () => {\n      fromDate.value = \"\";\n      toDate.value = \"\";\n      fetchPurchasesHistory(\"RECEIVED\");\n    };\n\n    // --------------------------------------------------------\n    // 6. FUNGSI UNTUK MODAL FORM RETUR (Tidak Berubah)\n    // --------------------------------------------------------\n    const openReturnModal = async purchaseId => {\n      closeSelectPOModal();\n      if (!purchaseId) {\n        alert(\"ID pembelian tidak valid.\");\n        return;\n      }\n      try {\n        const response = await api.get(`/gudang/purchases/${purchaseId}`);\n        selectedPurchase.value = response.data.data || response.data;\n        const items = selectedPurchase.value.Items || [];\n        returnForm.value.items = items.map(item => ({\n          purchase_item_id: item.ID || item.id,\n          product_id: item.ProductID || item.product_id,\n          qty: 0,\n          reason: \"\",\n          price: item.Price || item.price\n        }));\n        returnForm.value.notes = \"\";\n        showModal.value = true;\n      } catch (error) {\n        console.error(\"Error fetching purchase:\", error);\n        alert(`Gagal memuat detail pembelian: ${error.response?.data?.error || error.message}`);\n      }\n    };\n    const closeModal = () => {\n      showModal.value = false;\n      selectedPurchase.value = null;\n      returnForm.value = {\n        items: [],\n        notes: \"\"\n      };\n    };\n    const submitReturn = async () => {\n      if (!hasValidItems.value) {\n        alert(\"Pilih minimal satu item dengan Qty > 0 dan Alasan yang detail (min. 4 karakter).\");\n        return;\n      }\n      submitting.value = true;\n      try {\n        const validItems = returnForm.value.items.filter(item => item.qty > 0 && item.reason.trim().length > 3).map(item => ({\n          product_id: item.product_id,\n          purchase_item_id: item.purchase_item_id,\n          qty: item.qty,\n          price: item.price,\n          reason: item.reason\n        }));\n        const dto = {\n          purchase_id: selectedPurchase.value.ID || selectedPurchase.value.id,\n          date: new Date().toISOString().split(\"T\")[0],\n          items: validItems,\n          notes: returnForm.value.notes\n        };\n        const response = await api.post(\"/gudang/purchase-returns\", dto);\n        if (response.status === 201 || response.status === 200) {\n          alert(\"Retur pembelian manual berhasil dibuat (status: PENDING).\");\n          closeModal();\n          fetchPurchaseReturns();\n        }\n      } catch (error) {\n        console.error(\"Error creating return:\", error.response?.data || error);\n        alert(`Gagal membuat retur: ${error.response?.data?.error || error.response?.data?.message || \"Kesalahan tidak diketahui.\"}`);\n      } finally {\n        submitting.value = false;\n      }\n    };\n\n    // --------------------------------------------------------\n    // 7. UTILITY FUNCTIONS (FORMATTING DAN SKEMA WARNA BARU)\n    // --------------------------------------------------------\n    const formatPONumber = id => `PO-${String(id || \"\").padStart(3, \"0\")}`;\n    const formatReturnNumber = id => String(id || \"\").padStart(4, \"0\");\n    const getStatusClass = status => {\n      switch (status) {\n        // PENDING: Background #BBE1FA, Teks #0F4C75, Border #3282B8\n        case \"PENDING\":\n          return \"bg-[#BBE1FA] text-[#0F4C75] border border-[#3282B8]\";\n        // APPROVED: Tetap Hijau (Selesai/Sukses)\n        case \"APPROVED\":\n          return \"bg-green-100 text-green-800 border border-green-300\";\n        // REJECTED: Merah yang lebih gelap untuk Rejected (Tetap menyala tapi beda dengan Retur)\n        case \"REJECTED\":\n          return \"bg-red-100 text-red-800 border border-red-300\";\n        // QC_DEFECT: Merah yang lebih gelap untuk QC Defect (Peringatan Kerusakan)\n        case \"QC_DEFECT\":\n          return \"bg-red-200 text-red-900 border border-red-500 font-bold\";\n        // Default\n        default:\n          return \"bg-gray-100 text-gray-800 border border-gray-300\";\n      }\n    };\n    const getStatusText = status => {\n      switch (status) {\n        case \"PENDING\":\n          return \"Pending (Retur Manual)\";\n        case \"APPROVED\":\n          return \"Approved\";\n        case \"REJECTED\":\n          return \"Rejected\";\n        case \"QC_DEFECT\":\n          return \"Laporan QC Defect\";\n        default:\n          return status || \"Unknown\";\n      }\n    };\n    const formatDate = dateStr => {\n      if (!dateStr) return \"N/A\";\n      const date = new Date(dateStr);\n      if (isNaN(date.getTime())) return dateStr.split(\"T\")[0] || \"Invalid Date\";\n      return date.toLocaleDateString(\"id-ID\", {\n        day: \"2-digit\",\n        month: \"2-digit\",\n        year: \"numeric\"\n      });\n    };\n    const formatCurrency = amount => {\n      if (amount === null || amount === undefined) return \"Rp 0\";\n      return new Intl.NumberFormat(\"id-ID\", {\n        style: \"currency\",\n        currency: \"IDR\",\n        minimumFractionDigits: 0,\n        maximumFractionDigits: 0\n      }).format(amount);\n    };\n\n    // --------------------------------------------------------\n    // 8. LIFECYCLE HOOKS\n    // --------------------------------------------------------\n    onMounted(() => {\n      fetchPurchaseReturns();\n    });\n    const __returned__ = {\n      purchaseReturns,\n      loadingReturns,\n      returnFromDate,\n      returnToDate,\n      returnStatusFilter,\n      purchasesHistory,\n      loadingHistory,\n      showSelectPOModal,\n      fromDate,\n      toDate,\n      showModal,\n      selectedPurchase,\n      returnForm,\n      submitting,\n      hasValidItems,\n      validReturnItemsCount,\n      api,\n      savedToken,\n      fetchPurchaseReturns,\n      resetReturnFilter,\n      fetchPurchasesHistory,\n      openSelectPOModal,\n      closeSelectPOModal,\n      resetFilter,\n      openReturnModal,\n      closeModal,\n      submitReturn,\n      formatPONumber,\n      formatReturnNumber,\n      getStatusClass,\n      getStatusText,\n      formatDate,\n      formatCurrency,\n      ref,\n      onMounted,\n      computed,\n      get axios() {\n        return axios;\n      },\n      GudangLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","axios","GudangLayouts","purchaseReturns","loadingReturns","returnFromDate","returnToDate","returnStatusFilter","purchasesHistory","loadingHistory","showSelectPOModal","fromDate","toDate","showModal","selectedPurchase","returnForm","items","notes","submitting","hasValidItems","value","some","item","qty","reason","trim","length","validReturnItemsCount","filter","api","create","baseURL","savedToken","localStorage","getItem","defaults","headers","common","fetchPurchaseReturns","params","page","size","start_date","end_date","status","response","get","rawData","data","Array","isArray","ret","id","error","console","message","alert","resetReturnFilter","fetchPurchasesHistory","p","ID","openSelectPOModal","closeSelectPOModal","resetFilter","openReturnModal","purchaseId","Items","map","purchase_item_id","product_id","ProductID","price","Price","closeModal","submitReturn","validItems","dto","purchase_id","date","Date","toISOString","split","post","formatPONumber","String","padStart","formatReturnNumber","getStatusClass","getStatusText","formatDate","dateStr","isNaN","getTime","toLocaleDateString","day","month","year","formatCurrency","amount","undefined","Intl","NumberFormat","style","currency","minimumFractionDigits","maximumFractionDigits","format"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/PurchaseReturn.vue"],"sourcesContent":["<template>\n  <GudangLayouts>\n    <div class=\"p-4 sm:p-6 bg-gray-100 min-h-screen\">\n      <header class=\"mb-8\">\n        <h1\n          class=\"text-3xl sm:text-4xl font-extrabold text-[#1B262C] tracking-tight border-b pb-2\"\n        >\n          Laporan Retur Pembelian & Defect QC\n        </h1>\n        <p class=\"text-gray-600 mt-2\">\n          Memantau semua pengajuan Retur Manual (Pending) dan Laporan Barang\n          Rusak (QC Defect).\n        </p>\n      </header>\n\n      <section\n        class=\"bg-white shadow-xl rounded-xl p-4 sm:p-6 border border-gray-200\"\n      >\n        <h2\n          class=\"text-xl sm:text-2xl font-bold text-[#1B262C] mb-6 flex flex-col sm:flex-row items-start sm:items-center\"\n        >\n          Daftar Laporan Retur & Defect\n          <span\n            class=\"text-sm font-normal ml-0 sm:ml-3 mt-1 sm:mt-0 text-[#0F4C75] bg-[#BBE1FA] px-3 py-1 rounded-full\"\n          >\n            Status Default: PENDING & QC_DEFECT.\n          </span>\n        </h2>\n\n        <div\n          class=\"p-5 bg-gray-100 rounded-lg mb-8 border border-gray-300 space-y-4\"\n        >\n          <div\n            class=\"grid grid-cols-1 md:grid-cols-4 lg:grid-cols-6 gap-6 items-end\"\n          >\n            <div class=\"md:col-span-2 flex flex-col space-y-4\">\n              <div>\n                <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                  >Dari Tanggal</label\n                >\n                <input\n                  type=\"date\"\n                  v-model=\"returnFromDate\"\n                  :class=\"[\n                    'w-full px-3 py-2 border border-gray-300 rounded-lg shadow-sm',\n                    'focus:ring-[#3282B8] focus:border-[#3282B8]',\n                  ]\"\n                />\n              </div>\n              <div>\n                <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                  >Sampai Tanggal</label\n                >\n                <input\n                  type=\"date\"\n                  v-model=\"returnToDate\"\n                  :class=\"[\n                    'w-full px-3 py-2 border border-gray-300 rounded-lg shadow-sm',\n                    'focus:ring-[#3282B8] focus:border-[#3282B8]',\n                  ]\"\n                />\n              </div>\n            </div>\n\n            <div class=\"md:col-span-2\">\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Status</label\n              >\n              <select\n                v-model=\"returnStatusFilter\"\n                :class=\"[\n                  'w-full px-3 py-2 border border-gray-300 rounded-lg shadow-sm',\n                  'focus:ring-[#3282B8] focus:border-[#3282B8]',\n                ]\"\n              >\n                <option value=\"\">Pending & QC Defect (Default)</option>\n                <option value=\"PENDING\">Pending (Retur Manual)</option>\n                <option value=\"QC_DEFECT\">QC Defect (Laporan Rusak)</option>\n                <option value=\"APPROVED\">Approved (Siap Retur)</option>\n                <option value=\"REJECTED\">Rejected</option>\n              </select>\n            </div>\n\n            <div class=\"col-span-1 flex flex-col space-y-4\">\n              <button\n                @click=\"fetchPurchaseReturns\"\n                :disabled=\"loadingReturns\"\n                class=\"w-full px-4 py-2.5 bg-[#0F4C75] text-white font-semibold rounded-lg shadow-md hover:bg-[#3282B8] disabled:opacity-50 transition\"\n              >\n                {{ loadingReturns ? \"Memuat...\" : \"Terapkan Filter\" }}\n              </button>\n              <button\n                @click=\"resetReturnFilter\"\n                class=\"w-full px-4 py-2.5 bg-gray-600 text-white font-semibold rounded-lg shadow-md hover:bg-gray-700 transition\"\n              >\n                Reset\n              </button>\n            </div>\n\n            <div class=\"col-span-1 flex items-end\">\n              <button\n                @click=\"openSelectPOModal\"\n                class=\"w-full px-5 py-2.5 bg-[#9C3046] text-white font-semibold rounded-lg shadow-lg hover:bg-[#B1405A] transition\"\n                title=\"Membuat retur untuk barang yang sudah ada di gudang\"\n              >\n                + Retur Manual Baru\n              </button>\n            </div>\n          </div>\n\n          <p\n            v-if=\"returnFromDate || returnToDate || returnStatusFilter\"\n            class=\"text-sm text-gray-600 pt-3 font-medium border-t border-gray-200 mt-4\"\n          >\n            Filter aktif: **{{ returnFromDate || \"N/A\" }}** s/d **{{\n              returnToDate || \"N/A\"\n            }}** (Status: **{{\n              getStatusText(returnStatusFilter) || \"Default\"\n            }}**)\n          </p>\n        </div>\n\n        <div class=\"overflow-x-auto rounded-lg border border-gray-300\">\n          <table class=\"min-w-full divide-y divide-gray-200\">\n            <thead class=\"bg-gray-50\">\n              <tr>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-semibold text-gray-500 uppercase tracking-wider\"\n                >\n                  Return No.\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-semibold text-gray-500 uppercase tracking-wider\"\n                >\n                  Nomor PO Asal\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-semibold text-gray-500 uppercase tracking-wider\"\n                >\n                  Tanggal Retur\n                </th>\n                <th\n                  class=\"px-6 py-3 text-right text-xs font-semibold text-gray-500 uppercase tracking-wider\"\n                >\n                  Total Retur\n                </th>\n                <th\n                  class=\"px-6 py-3 text-center text-xs font-semibold text-gray-500 uppercase tracking-wider\"\n                >\n                  Status\n                </th>\n              </tr>\n            </thead>\n            <tbody class=\"bg-white divide-y divide-gray-200\">\n              <tr v-if=\"loadingReturns\">\n                <td colspan=\"5\" class=\"text-center py-8 text-lg text-gray-500\">\n                  <div\n                    class=\"animate-pulse flex flex-col items-center justify-center\"\n                  >\n                    <svg\n                      class=\"animate-spin h-5 w-5 text-[#3282B8] mb-2\"\n                      xmlns=\"http://www.w3.org/2000/svg\"\n                      fill=\"none\"\n                      viewBox=\"0 0 24 24\"\n                    >\n                      <circle\n                        class=\"opacity-25\"\n                        cx=\"12\"\n                        cy=\"12\"\n                        r=\"10\"\n                        stroke=\"currentColor\"\n                        stroke-width=\"4\"\n                      ></circle>\n                      <path\n                        class=\"opacity-75\"\n                        fill=\"currentColor\"\n                        d=\"M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z\"\n                      ></path>\n                    </svg>\n                    Memuat Daftar Retur...\n                  </div>\n                </td>\n              </tr>\n              <tr v-else-if=\"purchaseReturns.length === 0\">\n                <td colspan=\"5\" class=\"text-center py-8 text-gray-500 italic\">\n                  Tidak ada retur/defect yang ditemukan dalam rentang filter.\n                </td>\n              </tr>\n              <tr\n                v-else\n                v-for=\"ret in purchaseReturns\"\n                :key=\"ret?.id || Math.random()\"\n                class=\"hover:bg-gray-50 transition\"\n              >\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm font-medium\">\n                  <span\n                    :class=\"\n                      ret?.status === 'QC_DEFECT'\n                        ? 'text-red-600 font-bold'\n                        : 'text-[#3282B8]'\n                    \"\n                  >\n                    {{ ret?.status === \"QC_DEFECT\" ? \"QC-DEFECT\" : \"RTN\" }}-{{\n                      formatReturnNumber(ret?.id)\n                    }}\n                  </span>\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-600\">\n                  {{\n                    ret?.purchase_id ? formatPONumber(ret.purchase_id) : \"N/A\"\n                  }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-600\">\n                  {{ formatDate(ret?.date || ret?.created_at) || \"N/A\" }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-right\">\n                  <span class=\"font-semibold text-gray-800\">\n                    {{ formatCurrency(ret?.total || ret?.Total) || \"Rp 0\" }}\n                  </span>\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-center\">\n                  <span\n                    :class=\"getStatusClass(ret?.status)\"\n                    class=\"px-3 py-1 inline-flex text-xs leading-5 font-semibold rounded-full\"\n                  >\n                    {{ getStatusText(ret?.status) || \"Unknown\" }}\n                  </span>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </section>\n\n      <div\n        v-if=\"showSelectPOModal\"\n        class=\"fixed inset-0 bg-gray-900 bg-opacity-75 backdrop-blur-sm flex items-center justify-center z-50 p-4 transition-opacity duration-300\"\n      >\n        <div\n          class=\"bg-white rounded-xl shadow-2xl p-6 sm:p-8 max-w-lg md:max-w-xl w-full max-h-[90vh] overflow-y-auto\"\n        >\n          <h2 class=\"text-2xl font-bold text-red-700 mb-4 border-b pb-2\">\n            Pilih PO untuk Retur Manual\n          </h2>\n          <p class=\"mb-4 text-sm text-gray-600\">\n            Pilih PO yang sudah di-**RECEIVE** (stok sudah masuk) untuk membuat\n            retur manual.\n          </p>\n\n          <div class=\"flex flex-wrap gap-3 mb-4\">\n            <input\n              type=\"date\"\n              v-model=\"fromDate\"\n              placeholder=\"Dari Tanggal\"\n              class=\"flex-1 px-3 py-2 border border-gray-300 rounded-lg text-sm focus:ring-[#3282B8] focus:border-[#3282B8]\"\n            />\n            <input\n              type=\"date\"\n              v-model=\"toDate\"\n              placeholder=\"Sampai Tanggal\"\n              class=\"flex-1 px-3 py-2 border border-gray-300 rounded-lg text-sm focus:ring-[#3282B8] focus:border-[#3282B8]\"\n            />\n            <button\n              @click=\"fetchPurchasesHistory('RECEIVED')\"\n              :disabled=\"loadingHistory\"\n              class=\"px-4 py-2 bg-[#0F4C75] text-white text-sm font-semibold rounded-lg shadow hover:bg-[#3282B8] disabled:opacity-50\"\n            >\n              Filter\n            </button>\n            <button\n              @click=\"resetFilter\"\n              class=\"px-4 py-2 bg-gray-600 text-white text-sm font-semibold rounded-lg shadow hover:bg-gray-700\"\n            >\n              Reset\n            </button>\n          </div>\n\n          <div class=\"flex justify-end pt-4 border-t border-gray-200 mt-4\">\n            <button\n              @click=\"closeSelectPOModal\"\n              class=\"px-6 py-2 bg-gray-500 text-white font-semibold rounded-lg shadow-md hover:bg-gray-600\"\n            >\n              Tutup\n            </button>\n          </div>\n        </div>\n      </div>\n\n      <div\n        v-if=\"showModal\"\n        class=\"fixed inset-0 bg-gray-900 bg-opacity-75 backdrop-blur-sm flex items-center justify-center z-50 p-4 transition-opacity duration-300\"\n      >\n        <div\n          class=\"bg-white rounded-xl shadow-2xl p-6 sm:p-8 max-w-3xl lg:max-w-4xl w-full max-h-[90vh] overflow-y-auto transform transition-all duration-300 scale-100\"\n        >\n          <h2 class=\"text-2xl font-bold text-[#9C3046] mb-6 border-b pb-2\">\n            Form Retur Pembelian Manual\n          </h2>\n\n          <div\n            v-if=\"selectedPurchase\"\n            class=\"mb-6 p-4 bg-red-50 border-l-4 border-red-400 text-red-800 rounded-lg grid grid-cols-1 sm:grid-cols-2 gap-x-6 gap-y-2\"\n          ></div>\n\n          <div class=\"mb-6\">\n            <h3 class=\"text-xl font-bold text-[#1B262C] mb-3\">\n              Pilih Item yang Akan Dikembalikan (Qty > 0 & Alasan Wajib)\n            </h3>\n            <div class=\"space-y-4 max-h-80 overflow-y-auto p-1 pr-3\">\n              <div\n                v-for=\"(retItem, index) in returnForm.items\"\n                :key=\"index\"\n                class=\"p-4 border border-gray-200 rounded-lg bg-white shadow-sm hover:border-red-300 transition\"\n              >\n                <div class=\"grid grid-cols-1 lg:grid-cols-12 gap-4 items-start\">\n                  <div class=\"lg:col-span-4\">\n                    <p class=\"font-bold text-[#1B262C] text-base\">\n                      {{\n                        selectedPurchase?.Items[index]?.Product?.name || \"N/A\"\n                      }}\n                    </p>\n                    <p class=\"text-sm text-gray-500 mt-0.5\">\n                      Dipesan:\n                      <span class=\"font-semibold\">{{\n                        selectedPurchase?.Items[index]?.Qty || 0\n                      }}</span>\n                      | Harga:\n                      <span class=\"font-semibold\">{{\n                        formatCurrency(selectedPurchase?.Items[index]?.Price)\n                      }}</span>\n                    </p>\n                  </div>\n\n                  <div class=\"lg:col-span-3\">\n                    <label class=\"block text-xs font-medium text-gray-700 mb-1\"\n                      >Qty Retur</label\n                    >\n                    <input\n                      type=\"number\"\n                      v-model.number=\"retItem.qty\"\n                      min=\"0\"\n                      :max=\"selectedPurchase?.Items[index]?.Qty || 0\"\n                      class=\"w-full px-3 py-2 border border-gray-300 rounded-lg shadow-sm focus:ring-red-500 focus:border-red-500 text-center font-bold\"\n                    />\n                  </div>\n\n                  <div class=\"lg:col-span-5\">\n                    <label class=\"block text-xs font-medium text-gray-700 mb-1\"\n                      >Alasan Retur</label\n                    >\n                    <textarea\n                      v-model=\"retItem.reason\"\n                      placeholder=\"Contoh: Barang cacat/ru