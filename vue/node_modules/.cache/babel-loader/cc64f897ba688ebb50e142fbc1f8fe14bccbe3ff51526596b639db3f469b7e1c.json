{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport OwnerLayouts from \"../components/OwnerLayouts.vue\";\nexport default {\n  __name: 'ManagementProductOwner',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n\n    // Bagian script tidak diubah karena logikanya sudah benar, hanya menyesuaikan beberapa hal minor.\n    const products = ref([]);\n    const categories = ref([]);\n    const units = ref([]);\n    const suppliers = ref([]);\n    const loading = ref(true);\n    const showModal = ref(false);\n    const formData = ref({\n      id: null,\n      name: \"\",\n      category_id: null,\n      unit_id: null,\n      supplier_id: null,\n      cost_price: 0,\n      sell_price: 0,\n      stock: 0\n    });\n    const fetchProducts = async () => {\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          alert(\"Harap login dulu!\");\n          loading.value = false;\n          return;\n        }\n        const apiUrl = \"http://localhost:8081/api/v1/owner/products\";\n        const response = await axios.get(apiUrl, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        // Mengubah produk.cost_price dan sell_price ke number agar sesuai dengan input type=number\n        products.value = (response.data.data || []).map(p => ({\n          ...p,\n          cost_price: Number(p.cost_price),\n          sell_price: Number(p.sell_price),\n          stock: Number(p.stock)\n        }));\n      } catch (error) {\n        console.error(\"Error fetching products:\", error.response?.data || error.message);\n        alert(`Gagal ambil data produk: ${error.response?.data?.message || error.message}`);\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // ... (fetchCategories, fetchUnits, fetchSuppliers tetap sama)\n    const fetchCategories = async () => {\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) return;\n        const apiUrl = \"http://localhost:8081/api/v1/owner/categories\";\n        const response = await axios.get(apiUrl, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        categories.value = response.data.data || [];\n      } catch (error) {\n        console.error(\"Error fetching categories:\", error.response?.data || error.message);\n      }\n    };\n    const fetchUnits = async () => {\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) return;\n        const apiUrl = \"http://localhost:8081/api/v1/owner/units\";\n        const response = await axios.get(apiUrl, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        units.value = response.data.data || [];\n      } catch (error) {\n        console.error(\"Error fetching units:\", error.response?.data || error.message);\n      }\n    };\n    const fetchSuppliers = async () => {\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) return;\n        const apiUrl = \"http://localhost:8081/api/v1/owner/suppliers\";\n        const response = await axios.get(apiUrl, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        suppliers.value = response.data.data || [];\n      } catch (error) {\n        console.error(\"Error fetching suppliers:\", error.response?.data || error.message);\n      }\n    };\n    // ... (Akhir dari fetch functions)\n\n    const formatCurrency = value => {\n      return new Intl.NumberFormat(\"id-ID\", {\n        style: \"currency\",\n        currency: \"IDR\",\n        minimumFractionDigits: 0 // Opsional: hapus desimal jika harga biasanya bulat\n      }).format(value);\n    };\n    const openEditModal = product => {\n      formData.value = {\n        id: product.id,\n        name: product.name,\n        category_id: Number(product.category_id) || null,\n        unit_id: Number(product.unit_id) || null,\n        supplier_id: Number(product.supplier_id) || null,\n        cost_price: Number(product.cost_price),\n        sell_price: Number(product.sell_price),\n        stock: Number(product.stock)\n      };\n      showModal.value = true;\n    };\n    const closeModal = () => {\n      showModal.value = false;\n      formData.value = {\n        id: null,\n        name: \"\",\n        category_id: null,\n        unit_id: null,\n        supplier_id: null,\n        cost_price: 0,\n        sell_price: 0,\n        stock: 0\n      };\n    };\n    const submitForm = async () => {\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          alert(\"Harap login dulu!\");\n          return;\n        }\n        // Validasi sederhana sebelum kirim\n        if (!formData.value.name || !formData.value.category_id || !formData.value.unit_id || !formData.value.supplier_id || formData.value.cost_price <= 0 || formData.value.sell_price <= 0) {\n          alert(\"Pastikan semua field terisi dengan benar (harga harus lebih dari 0)!\");\n          return;\n        }\n        const url = `http://localhost:8081/api/v1/owner/products/${formData.value.id}`;\n        const payload = {\n          name: formData.value.name,\n          category_id: Number(formData.value.category_id),\n          unit_id: Number(formData.value.unit_id),\n          supplier_id: Number(formData.value.supplier_id),\n          cost_price: Number(formData.value.cost_price),\n          sell_price: Number(formData.value.sell_price),\n          stock: Number(formData.value.stock)\n        };\n        const response = await axios.put(url, payload, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (response.data.success) {\n          await fetchProducts();\n          closeModal();\n          alert(\"Produk berhasil diupdate!\");\n        }\n      } catch (error) {\n        console.error(\"Error updating product:\", error.response?.data || error.message);\n        alert(`Gagal update produk: ${error.response?.data?.message || error.message}`);\n      }\n    };\n    const confirmDelete = async id => {\n      if (!confirm(\"Yakin hapus produk ini? Aksi ini tidak bisa dibatalkan.\")) return;\n      try {\n        const token = localStorage.getItem(\"token\");\n        if (!token) {\n          alert(\"Harap login dulu!\");\n          return;\n        }\n        const response = await axios.delete(`http://localhost:8081/api/v1/owner/products/${id}`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        if (response.data.success) {\n          await fetchProducts();\n          alert(\"Produk berhasil dihapus!\");\n        }\n      } catch (error) {\n        console.error(\"Error deleting product:\", error.response?.data || error.message);\n        alert(`Gagal hapus produk: ${error.response?.data?.message || error.message}`);\n      }\n    };\n    onMounted(() => {\n      fetchProducts();\n      fetchCategories();\n      fetchUnits();\n      fetchSuppliers();\n    });\n    const __returned__ = {\n      products,\n      categories,\n      units,\n      suppliers,\n      loading,\n      showModal,\n      formData,\n      fetchProducts,\n      fetchCategories,\n      fetchUnits,\n      fetchSuppliers,\n      formatCurrency,\n      openEditModal,\n      closeModal,\n      submitForm,\n      confirmDelete,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      OwnerLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","OwnerLayouts","products","categories","units","suppliers","loading","showModal","formData","id","name","category_id","unit_id","supplier_id","cost_price","sell_price","stock","fetchProducts","token","localStorage","getItem","alert","value","apiUrl","response","get","headers","Authorization","data","map","p","Number","error","console","message","fetchCategories","fetchUnits","fetchSuppliers","formatCurrency","Intl","NumberFormat","style","currency","minimumFractionDigits","format","openEditModal","product","closeModal","submitForm","url","payload","put","success","confirmDelete","confirm","delete"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/ManagementProductOwner.vue"],"sourcesContent":["<template>\n  <OwnerLayouts>\n    <div class=\"flex flex-1 flex-col py-8 px-6 sm:px-8\">\n      <div class=\"layout-content-container flex flex-col max-w-[1200px] w-full mx-auto flex-1\">\n        \n        <div class=\"flex flex-wrap justify-between items-center gap-4 pb-6\">\n          <p\n            class=\"text-[#111418] text-3xl font-bold leading-tight\"\n          >\n            Daftar Produk\n          </p>\n          <router-link\n            to=\"/add-product\"\n            class=\"flex items-center justify-center rounded-lg h-10 px-6 bg-blue-600 hover:bg-blue-700 text-white text-base font-medium transition-colors shadow-md\"\n          >\n            <span class=\"truncate\">Tambah Produk Baru</span>\n          </router-link>\n        </div>\n\n        <div class=\"bg-white rounded-xl shadow-lg border border-gray-200 overflow-hidden\">\n          <div class=\"overflow-x-auto\">\n            <table class=\"min-w-full divide-y divide-gray-200\">\n              <thead class=\"bg-gray-50\">\n                <tr>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-semibold text-gray-700 uppercase tracking-wider min-w-[250px]\"\n                  >\n                    Nama Produk\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-semibold text-gray-700 uppercase tracking-wider w-[120px]\"\n                  >\n                    Kategori\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-semibold text-gray-700 uppercase tracking-wider w-[100px]\"\n                  >\n                    Satuan\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-left text-xs font-semibold text-gray-700 uppercase tracking-wider w-[120px]\"\n                  >\n                    Supplier\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-right text-xs font-semibold text-gray-700 uppercase tracking-wider w-[120px]\"\n                  >\n                    Harga Modal\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-right text-xs font-semibold text-gray-700 uppercase tracking-wider w-[120px]\"\n                  >\n                    Harga Jual\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-right text-xs font-semibold text-gray-700 uppercase tracking-wider w-[80px]\"\n                  >\n                    Stok\n                  </th>\n                  <th\n                    class=\"px-6 py-3 text-center text-xs font-semibold text-gray-700 uppercase tracking-wider w-[120px]\"\n                  >\n                    Aksi\n                  </th>\n                </tr>\n              </thead>\n              <tbody class=\"divide-y divide-gray-100\">\n                <tr v-if=\"loading\">\n                  <td\n                    colspan=\"8\"\n                    class=\"h-20 px-6 py-4 text-center text-gray-500 text-sm italic\"\n                  >\n                    Memuat data...\n                  </td>\n                </tr>\n                <tr v-else-if=\"products.length === 0\">\n                  <td\n                    colspan=\"8\"\n                    class=\"h-20 px-6 py-4 text-center text-gray-500 text-sm italic\"\n                  >\n                    Tidak ada produk ditemukan.\n                  </td>\n                </tr>\n                <tr\n                  v-else\n                  v-for=\"product in products\"\n                  :key=\"product.id\"\n                  class=\"hover:bg-gray-50 transition-colors\"\n                >\n                  <td\n                    class=\"px-6 py-3 text-sm font-medium text-gray-900\"\n                  >\n                    {{ product.name }}\n                  </td>\n                  <td\n                    class=\"px-6 py-3 text-sm text-gray-600\"\n                  >\n                    {{ product.category?.name || \"-\" }}\n                  </td>\n                  <td\n                    class=\"px-6 py-3 text-sm text-gray-600\"\n                  >\n                    {{ product.unit?.name || \"-\" }}\n                  </td>\n                  <td\n                    class=\"px-6 py-3 text-sm text-gray-600\"\n                  >\n                    {{ product.supplier?.name || \"-\" }}\n                  </td>\n                  <td\n                    class=\"px-6 py-3 text-sm text-gray-600 text-right font-mono\"\n                  >\n                    {{ formatCurrency(product.cost_price) }}\n                  </td>\n                  <td\n                    class=\"px-6 py-3 text-sm text-gray-600 text-right font-mono\"\n                  >\n                    {{ formatCurrency(product.sell_price) }}\n                  </td>\n                  <td\n                    class=\"px-6 py-3 text-sm text-gray-600 text-right\"\n                  >\n                    {{ product.stock }}\n                  </td>\n                  <td\n                    class=\"px-6 py-3 text-sm text-center font-medium\"\n                  >\n                    <button\n                      class=\"text-blue-600 hover:text-blue-800 transition-colors mr-2 font-medium\"\n                      @click=\"openEditModal(product)\"\n                    >Edit</button>\n                    <button\n                      class=\"text-red-600 hover:text-red-800 transition-colors font-medium\"\n                      @click=\"confirmDelete(product.id)\"\n                    >Hapus</button>\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </div>\n    </div>\n  </OwnerLayouts>\n\n    <div\n      v-if=\"showModal\"\n      class=\"fixed inset-0 bg-black bg-opacity-60 flex items-center justify-center z-50 p-4\"\n      @click.self=\"closeModal\"\n    >\n      <div class=\"bg-white rounded-xl shadow-2xl p-6 w-full max-w-lg transform transition-all duration-300\">\n        <h2 class=\"text-2xl font-bold text-gray-900 mb-6 border-b pb-3\">Ubah Produk</h2>\n        <form @submit.prevent=\"submitForm\">\n          \n          <div class=\"grid grid-cols-1 md:grid-cols-2 gap-4\">\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Nama Produk</label>\n              <input\n                v-model=\"formData.name\"\n                type=\"text\"\n                class=\"w-full border border-gray-300 focus:border-blue-500 focus:ring-1 focus:ring-blue-500 rounded-lg p-2.5 text-sm transition-shadow\"\n                required\n              />\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Kategori</label>\n              <select\n                v-model.number=\"formData.category_id\"\n                class=\"w-full border border-gray-300 focus:border-blue-500 focus:ring-1 focus:ring-blue-500 rounded-lg p-2.5 text-sm bg-white\"\n                required\n              >\n                <option :value=\"null\" disabled>Pilih kategori</option>\n                <option\n                  v-for=\"category in categories\"\n                  :key=\"category.id\"\n                  :value=\"category.id\"\n                >\n                  {{ category.name }}\n                </option>\n              </select>\n            </div>\n\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Satuan</label>\n              <select\n                v-model.number=\"formData.unit_id\"\n                class=\"w-full border border-gray-300 focus:border-blue-500 focus:ring-1 focus:ring-blue-500 rounded-lg p-2.5 text-sm bg-white\"\n                required\n              >\n                <option :value=\"null\" disabled>Pilih satuan</option>\n                <option v-for=\"unit in units\" :key=\"unit.id\" :value=\"unit.id\">\n                  {{ unit.name }}\n                </option>\n              </select>\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Supplier</label>\n              <select\n                v-model.number=\"formData.supplier_id\"\n                class=\"w-full border border-gray-300 focus:border-blue-500 focus:ring-1 focus:ring-blue-500 rounded-lg p-2.5 text-sm bg-white\"\n                required\n              >\n                <option :value=\"null\" disabled>Pilih supplier</option>\n                <option\n                  v-for=\"supplier in suppliers\"\n                  :key=\"supplier.id\"\n                  :value=\"supplier.id\"\n                >\n                  {{ supplier.name }}\n                </option>\n              </select>\n            </div>\n\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Harga Modal (Rp)</label>\n              <input\n                v-model.number=\"formData.cost_price\"\n                type=\"number\"\n                step=\"0.01\"\n                class=\"w-full border border-gray-300 focus:border-blue-500 focus:ring-1 focus:ring-blue-500 rounded-lg p-2.5 text-sm text-right font-mono\"\n                required\n              />\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Harga Jual (Rp)</label>\n              <input\n                v-model.number=\"formData.sell_price\"\n                type=\"number\"\n                step=\"0.01\"\n                class=\"w-full border border-gray-300 focus:border-blue-500 focus:ring-1 focus:ring-blue-500 rounded-lg p-2.5 text-sm text-right font-mono\"\n                required\n              />\n            </div>\n\n            <div class=\"md:col-span-2\">\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Stok Saat Ini</label>\n              <input\n                v-model.number=\"formData.stock\"\n                type=\"number\"\n                class=\"w-full border border-gray-300 focus:border-blue-500 focus:ring-1 focus:ring-blue-500 rounded-lg p-2.5 text-sm\"\n                required\n              />\n            </div>\n          </div>\n          \n          <div class=\"flex justify-end gap-3 mt-6 pt-4 border-t\">\n            <button\n              type=\"button\"\n              @click=\"closeModal\"\n              class=\"px-4 py-2 bg-gray-200 hover:bg-gray-300 text-gray-700 rounded-lg text-sm font-medium transition-colors\"\n            >\n              Batal\n            </button>\n            <button\n              type=\"submit\"\n              class=\"px-4 py-2 bg-blue-600 hover:bg-blue-700 text-white rounded-lg text-sm font-medium transition-colors shadow-md\"\n            >\n              Simpan Perubahan\n            </button>\n          </div>\n        </form>\n      </div>\n    </div>\n</template>\n\n<script setup>\n// Bagian script tidak diubah karena logikanya sudah benar, hanya menyesuaikan beberapa hal minor.\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport OwnerLayouts from \"../components/OwnerLayouts.vue\";\n\nconst products = ref([]);\nconst ca