{"ast":null,"code":"import OwnerLayouts from \"../components/OwnerLayouts.vue\";\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  __name: 'StockSnapshotOwners',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const api = axios.create({\n      baseURL: \"http://localhost:8081/api/v1\"\n    });\n    const savedToken = localStorage.getItem(\"token\");\n    if (savedToken) {\n      api.defaults.headers.common[\"Authorization\"] = `Bearer ${savedToken}`;\n    }\n    const tableData = ref([]);\n    const loading = ref(false);\n    const fetchStock = async () => {\n      loading.value = true;\n      try {\n        const res = await api.get(\"/owner/products\"); // Pastikan endpoint ini benar\n        tableData.value = res.data.data || [];\n      } catch (err) {\n        console.error(\"Failed to fetch stock\", err);\n      }\n      loading.value = false;\n    };\n    const formatRupiah = value => new Intl.NumberFormat(\"id-ID\").format(value);\n    onMounted(fetchStock);\n    const __returned__ = {\n      api,\n      savedToken,\n      tableData,\n      loading,\n      fetchStock,\n      formatRupiah,\n      OwnerLayouts,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["OwnerLayouts","ref","onMounted","axios","api","create","baseURL","savedToken","localStorage","getItem","defaults","headers","common","tableData","loading","fetchStock","value","res","get","data","err","console","error","formatRupiah","Intl","NumberFormat","format"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/StockSnapshotOwners.vue"],"sourcesContent":["<template>\n  <OwnerLayouts>\n    <div class=\"flex flex-1 flex-col py-5 p-8\">\n      <div class=\"layout-content-container flex flex-col flex-1\">\n        <h1 class=\"text-2xl font-bold mb-4 text-white\">Stock Snapshot</h1>\n\n        <div class=\"px-4 py-3\">\n          <div class=\"flex overflow-hidden rounded-lg border border-[#333] bg-[#1f1f1f]\">\n            <table class=\"flex-1 text-white\">\n              <thead>\n                <tr>\n                  <th class=\"px-4 py-3 text-left\">Product Name</th>\n                  <th class=\"px-4 py-3 text-left\">Product Code</th>\n                  <th class=\"px-4 py-3 text-left\">Category</th>\n                  <th class=\"px-4 py-3 text-left\">Purchase Price</th>\n                  <th class=\"px-4 py-3 text-left\">Selling Price</th>\n                  <th class=\"px-4 py-3 text-left\">Stock Quantity</th>\n                  <th class=\"px-4 py-3 text-left\">Location</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-if=\"loading\">\n                  <td colspan=\"7\" class=\"text-center py-4\">Loading...</td>\n                </tr>\n                <tr v-for=\"(item, index) in tableData\" :key=\"index\" class=\"border-t border-[#333]\">\n                  <td class=\"px-4 py-2\">{{ item.name }}</td>\n                  <td class=\"px-4 py-2\">-</td>\n                  <td class=\"px-4 py-2\">{{ item.category?.name || \"-\" }}</td>\n                  <td class=\"px-4 py-2\">Rp {{ formatRupiah(item.cost_price) }}</td>\n                  <td class=\"px-4 py-2\">Rp {{ formatRupiah(item.sell_price) }}</td>\n                  <td class=\"px-4 py-2\">{{ item.stock }}</td>\n                  <td class=\"px-4 py-2\">{{ item.supplier?.address || \"-\" }}</td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n\n      </div>\n    </div>\n  </OwnerLayouts>\n</template>\n\n<script setup>\nimport OwnerLayouts from \"../components/OwnerLayouts.vue\";\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\n\nconst api = axios.create({ baseURL: \"http://localhost:8081/api/v1\" });\nconst savedToken = localStorage.getItem(\"token\");\n\nif (savedToken) {\n  api.defaults.headers.common[\"Authorization\"] = `Bearer ${savedToken}`;\n}\n\nconst tableData = ref([]);\nconst loading = ref(false);\n\nconst fetchStock = async () => {\n  loading.value = true;\n  try {\n    const res = await api.get(\"/owner/products\"); // Pastikan endpoint ini benar\n    tableData.value = res.data.data || [];\n  } catch (err) {\n    console.error(\"Failed to fetch stock\", err);\n  }\n  loading.value = false;\n};\n\nconst formatRupiah = (value) => new Intl.NumberFormat(\"id-ID\").format(value);\n\nonMounted(fetchStock);\n</script>\n"],"mappings":"AA4CA,OAAOA,YAAY,MAAM,gCAAgC;AACzD,SAASC,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;;;;;;;IAEzB,MAAMC,GAAG,GAAGD,KAAK,CAACE,MAAM,CAAC;MAAEC,OAAO,EAAE;IAA+B,CAAC,CAAC;IACrE,MAAMC,UAAU,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;IAEhD,IAAIF,UAAU,EAAE;MACdH,GAAG,CAACM,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,CAAC,GAAG,UAAUL,UAAU,EAAE;IACvE;IAEA,MAAMM,SAAS,GAAGZ,GAAG,CAAC,EAAE,CAAC;IACzB,MAAMa,OAAO,GAAGb,GAAG,CAAC,KAAK,CAAC;IAE1B,MAAMc,UAAU,GAAG,MAAAA,CAAA,KAAY;MAC7BD,OAAO,CAACE,KAAK,GAAG,IAAI;MACpB,IAAI;QACF,MAAMC,GAAG,GAAG,MAAMb,GAAG,CAACc,GAAG,CAAC,iBAAiB,CAAC,CAAC,CAAC;QAC9CL,SAAS,CAACG,KAAK,GAAGC,GAAG,CAACE,IAAI,CAACA,IAAI,IAAI,EAAE;MACvC,CAAC,CAAC,OAAOC,GAAG,EAAE;QACZC,OAAO,CAACC,KAAK,CAAC,uBAAuB,EAAEF,GAAG,CAAC;MAC7C;MACAN,OAAO,CAACE,KAAK,GAAG,KAAK;IACvB,CAAC;IAED,MAAMO,YAAY,GAAIP,KAAK,IAAK,IAAIQ,IAAI,CAACC,YAAY,CAAC,OAAO,CAAC,CAACC,MAAM,CAACV,KAAK,CAAC;IAE5Ed,SAAS,CAACa,UAAU,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}