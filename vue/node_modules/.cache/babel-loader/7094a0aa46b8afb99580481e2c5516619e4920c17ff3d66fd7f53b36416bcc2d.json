alue, toDate.value); // ðŸ‘ˆ debug log\n\n  const token = localStorage.getItem(\"token\");\n  if (!token) {\n    alert(\"Authentication token not found. Please log in again.\");\n    return;\n  }\n\n  try {\n    const params = {\n      type: \"purchases\",\n    };\n\n    if (fromDate.value) params.from = fromDate.value;\n    if (toDate.value) params.to = toDate.value;\n\n    console.log(\">>> Params dikirim ke API:\", params); // ðŸ‘ˆ cek params\n\n    const response = await axios.get(API_URL, {\n      headers: {\n        Authorization: `Bearer ${token}`,\n      },\n      params,\n    });\n\n    console.log(\">>> Raw backend data:\", response.data);\n\n    const purchases = response.data.data || [];\n    reportData.value = purchases.map((p) => ({\n      date: p.period ? new Date(p.period).toLocaleDateString(\"id-ID\") : \"-\",\n      trx: p.trx || 0,\n      total: p.total || 0,\n    }));\n  } catch (error) {\n    console.error(\"Failed to fetch report data:\", error);\n    alert(\"Failed to fetch report data: \" + error.message);\n  }\n};\n\n// Export\nconst exportReport = async (format) => {\n  const token = localStorage.getItem(\"token\");\n  if (!token) {\n    alert(\"Authentication token not found. Please log in again.\");\n    return;\n  }\n\n  try {\n    const params = {\n      type: \"purchases\",\n      export: format,\n    };\n    if (fromDate.value) params.from = fromDate.value;\n    if (toDate.value) params.to = toDate.value;\n\n    const response = await axios.get(API_URL, {\n      headers: {\n        Authorization: `Bearer ${token}`,\n      },\n      params,\n      responseType: \"blob\",\n    });\n\n    const url = window.URL.createObjectURL(new Blob([response.data]));\n    const link = document.createElement(\"a\");\n    link.href = url;\n    link.setAttribute(\"download\", `purchase_report.${format}`);\n    document.body.appendChild(link);\n    link.click();\n    document.body.removeChild(link);\n  } catch (error) {\n    console.error(\"Export failed:\", error);\n    alert(\"Export failed: \" + error.message);\n  }\n};\n\n// Format Rupiah\nconst formatCurrency = (num) => {\n  if (num === null || num === undefined) return \"Rp 0\";\n  return new Intl.NumberFormat(\"id-ID\", {\n    style: \"currency\",\n    currency: \"IDR\",\n  }).format(num);\n};\n\n// Load default report pas halaman dibuka\nonMounted(fetchReport);\n</script>\n"],"mappings":";;;;;AAgIA,OAAOA,YAAY,MAAM,gCAAgC;AACzD,SAASC,UAAU,QAAQ,YAAY;AACvC,OAAOC,KAAK,MAAM,OAAO;AACzB,SAASC,GAAG,EAAEC,SAAS,QAAQ,KAAK;AAEpC,MAAMC,OAAO,GAAG,4CAA4C;;;;;;;IAE5D,MAAMC,UAAU,GAAGH,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMI,QAAQ,GAAGJ,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMK,MAAM,GAAGL,GAAG,CAAC,EAAE,CAAC;;IAEtB;IACA,MAAMM,WAAW,GAAG,MAAAA,CAAA,KAAY;MAC9BC,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAEJ,QAAQ,CAACK,KAAK,EAAEJ,MAAM,CAACI,KAAK,CAAC,CAAC,CAAC;;MAEhE,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAC3C,IAAI,CAACF,KAAK,EAAE;QACVG,KAAK,CAAC,sDAAsD,CAAC;QAC7D;MACF;MAEA,IAAI;QACF,MAAMC,MAAM,GAAG;UACbC,IAAI,EAAE;QACR,CAAC;QAED,IAAIX,QAAQ,CAACK,KAAK,EAAEK,MAAM,CAACE,IAAI,GAAGZ,QAAQ,CAACK,KAAK;QAChD,IAAIJ,MAAM,CAACI,KAAK,EAAEK,MAAM,CAACG,EAAE,GAAGZ,MAAM,CAACI,KAAK;QAE1CF,OAAO,CAACC,GAAG,CAAC,4BAA4B,EAAEM,MAAM,CAAC,CAAC,CAAC;;QAEnD,MAAMI,QAAQ,GAAG,MAAMnB,KAAK,CAACoB,GAAG,CAACjB,OAAO,EAAE;UACxCkB,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUX,KAAK;UAChC,CAAC;UACDI;QACF,CAAC,CAAC;QAEFP,OAAO,CAACC,GAAG,CAAC,uBAAuB,EAAEU,QAAQ,CAACI,IAAI,CAAC;QAEnD,MAAMC,SAAS,GAAGL,QAAQ,CAACI,IAAI,CAACA,IAAI,IAAI,EAAE;QAC1CnB,UAAU,CAACM,KAAK,GAAGc,SAAS,CAACC,GAAG,CAAEC,CAAC,KAAM;UACvCC,IAAI,EAAED,CAAC,CAACE,MAAM,GAAG,IAAIC,IAAI,CAACH,CAAC,CAACE,MAAM,CAAC,CAACE,kBAAkB,CAAC,OAAO,CAAC,GAAG,GAAG;UACrEC,GAAG,EAAEL,CAAC,CAACK,GAAG,IAAI,CAAC;UACfC,KAAK,EAAEN,CAAC,CAACM,KAAK,IAAI;QACpB,CAAC,CAAC,CAAC;MACL,CAAC,CAAC,OAAOC,KAAK,EAAE;QACdzB,OAAO,CAACyB,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;QACpDnB,KAAK,CAAC,+BAA+B,GAAGmB,KAAK,CAACC,OAAO,CAAC;MACxD;IACF,CAAC;;IAED;IACA,MAAMC,YAAY,GAAG,MAAOC,MAAM,IAAK;MACrC,MAAMzB,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAA