0];\n    todayTransactions.value = recentSales.value.filter((s) => {\n      const saleDate = s.date.split(\"T\")[0];\n      return saleDate === today;\n    }).length;\n  } catch (err) {\n    console.error(\"Failed to fetch additional data\", err);\n    // Provide dummy data for testing\n    totalProducts.value = 24;\n    lowStockCount.value = 3;\n    todayTransactions.value = 5;\n    recentSales.value = [\n      {\n        id: 1,\n        transactionNumber: \"#TRX-001\",\n        total: 500000,\n        date: new Date().toISOString(),\n      },\n    ];\n  }\n};\n\nonMounted(() => {\n  fetchSummary();\n  fetchAdditionalData();\n});\n</script>\n\n<style scoped>\n/* Aturan container query dihapus */\n</style>\n"],"mappings":";;AA6OA,OAAOA,YAAY,MAAM,gCAAgC;AACzD,SAASC,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,GAAG,MAAM,gBAAgB;;;;;;;IAEhC,MAAMC,UAAU,GAAGH,GAAG,CAAC,IAAI,CAAC;IAC5B,MAAMI,cAAc,GAAGJ,GAAG,CAAC,IAAI,CAAC;IAChC,MAAMK,YAAY,GAAGL,GAAG,CAAC,IAAI,CAAC;IAE9B,MAAMM,iBAAiB,GAAGN,GAAG,CAAC,CAAC,CAAC;IAChC,MAAMO,aAAa,GAAGP,GAAG,CAAC,CAAC,CAAC;IAC5B,MAAMQ,aAAa,GAAGR,GAAG,CAAC,CAAC,CAAC;IAC5B,MAAMS,WAAW,GAAGT,GAAG,CAAC,EAAE,CAAC;IAE3B,MAAMU,cAAc,GAAIC,CAAC,IAAK;MAC5B,IAAIA,CAAC,IAAI,IAAI,EAAE,OAAO,GAAG;MACzB,OAAO,IAAIC,IAAI,CAACC,YAAY,CAAC,OAAO,EAAE;QACpCC,KAAK,EAAE,UAAU;QACjBC,QAAQ,EAAE,KAAK;QACfC,qBAAqB,EAAE;MACzB,CAAC,CAAC,CAACC,MAAM,CAACC,MAAM,CAACP,CAAC,CAAC,CAAC;IACtB,CAAC;IAED,MAAMQ,UAAU,GAAIC,OAAO,IAAK;MAC9B,IAAI,CAACA,OAAO,EAAE,OAAO,GAAG;MACxB,MAAMC,CAAC,GAAG,IAAIC,IAAI,CAACF,OAAO,CAAC;MAC3B,OAAO,IAAIR,IAAI,CAACW,cAAc,CAAC,OAAO,EAAE;QACtCC,GAAG,EAAE,SAAS;QACdC,KAAK,EAAE,OAAO;QACdC,IAAI,EAAE,SAAS;QACfC,MAAM,EAAE;MACV,CAAC,CAAC,CAACV,MAAM,CAACI,CAAC,CAAC;IACd,CAAC;IAED,MAAMO,YAAY,GAAG,MAAAA,CAAA,KAAY;MAC/B,IAAI;QACF,MAAMC,GAAG,GAAG,MAAM3B,GAAG,CAAC4B,GAAG,CAAC,0BAA0B,CAAC;QACrD,MAAMT,CAAC,GAAGQ,GAAG,CAACE,IAAI,IAAIF,GAAG,CAACE,IAAI,CAACA,IAAI,GAAGF,GAAG,CAACE,IAAI,CAACA,IAAI,GAAG,IAAI;QAC1D,IAAIV,CAAC,EAAE;UACLlB,UAAU,CAAC6B,KAAK,GAAGX,CAAC,CAACY,WAAW;UAChC7B,cAAc,CA