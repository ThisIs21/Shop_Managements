n\n        // Fetch recent sales (or use history API)\n        const salesRes = await api.get(\"/history/sales?limit=5\");\n        recentSales.value = salesRes.data || [];\n\n        // Count today's transactions\n        const today = new Date().toISOString().split(\"T\")[0];\n        todayTransactions.value = recentSales.value.filter(s => {\n          const saleDate = s.date.split(\"T\")[0];\n          return saleDate === today;\n        }).length;\n      } catch (err) {\n        console.error(\"Failed to fetch additional data\", err);\n        // Provide dummy data for testing\n        totalProducts.value = 24;\n        lowStockCount.value = 3;\n        todayTransactions.value = 5;\n        recentSales.value = [{\n          id: 1,\n          transactionNumber: \"#TRX-001\",\n          total: 500000,\n          date: new Date().toISOString()\n        }];\n      }\n    };\n    onMounted(() => {\n      fetchSummary();\n      fetchAdditionalData();\n    });\n    const __returned__ = {\n      totalSales,\n      totalPurchases,\n      totalReturns,\n      todayTransactions,\n      totalProducts,\n      lowStockCount,\n      recentSales,\n      formatCurrency,\n      formatDate,\n      fetchSummary,\n      fetchAdditionalData,\n      OwnerLayouts,\n      ref,\n      onMounted,\n      get api() {\n        return api;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["OwnerLayouts","ref","onMounted","api","totalSales","totalPurchases","totalReturns","todayTransactions","totalProducts","lowStockCount","recentSales","formatCurrency","v","Intl","NumberFormat","style","currency","maximumFractionDigits","format","Number","formatDate","dateStr","d","Date","DateTimeFormat","day","month","hour","minute","fetchSummary","res","get","data","value","total_sales","total_purchases","total_returns","err","console","error","fetchAdditionalDat