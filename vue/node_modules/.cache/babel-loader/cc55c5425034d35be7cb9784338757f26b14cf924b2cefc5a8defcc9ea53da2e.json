{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport OwnerLayouts from \"../components/OwnerLayouts.vue\";\nimport { RouterLink } from \"vue-router\";\nimport axios from \"axios\";\nimport { ref, onMounted } from \"vue\";\n\n// Base URLs\nconst API_URL = \"http://localhost:8081/api/v1/owner/reports\";\nconst ANALYTICS_URL = \"http://localhost:8081/api/v1/owner/product-analytics\"; // <--- Endpoint baru\n\n// --- State Variables ---\n\n// State for Sales Summary\n\nexport default {\n  __name: 'SalesSummaryReportOwners',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const reportData = ref([]);\n    const filterBy = ref(\"month\");\n    const loadingReport = ref(false);\n\n    // State for Product Analytics\n    const topSelling = ref([]);\n    const slowMoving = ref([]);\n    const loadingTop = ref(false);\n    const loadingSlow = ref(false);\n\n    // --- 1. Sales Summary Logic (Sudah Ada) ---\n\n    const fetchReport = async () => {\n      try {\n        loadingReport.value = true;\n        const token = localStorage.getItem(\"token\");\n        const res = await axios.get(API_URL, {\n          params: {\n            type: \"sales\",\n            by: filterBy.value\n          },\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        reportData.value = res.data.data || [];\n      } catch (err) {\n        console.error(\"Failed to fetch sales report:\", err.response?.data || err.message);\n        reportData.value = [];\n      } finally {\n        loadingReport.value = false;\n      }\n    };\n    const exportReport = async format => {\n      try {\n        loadingReport.value = true;\n        const token = localStorage.getItem(\"token\");\n        const res = await axios.get(API_URL, {\n          params: {\n            type: \"sales\",\n            by: filterBy.value,\n            export: format\n          },\n          headers: {\n            Authorization: `Bearer ${token}`\n          },\n          responseType: \"blob\"\n        });\n        const url = window.URL.createObjectURL(new Blob([res.data]));\n        const link = document.createElement(\"a\");\n        link.href = url;\n        link.setAttribute(\"download\", `sales_report_${filterBy.value}.${format}`);\n        document.body.appendChild(link);\n        link.click();\n        document.body.removeChild(link);\n      } catch (err) {\n        console.error(\"Export failed:\", err.response?.data || err.message);\n      } finally {\n        loadingReport.value = false;\n      }\n    };\n\n    // --- 2. Product Analytics Logic (Penambahan Baru) ---\n\n    const fetchAnalytics = async (type, dataRef, loadingRef) => {\n      loadingRef.value = true;\n      try {\n        const token = localStorage.getItem(\"token\");\n\n        // Mengambil data analitik dari endpoint baru\n        const res = await axios.get(ANALYTICS_URL, {\n          params: {\n            type,\n            limit: 10\n          },\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n\n        // Debugging: Cek respons dari backend\n        console.log(`[DEBUG] Analytics Response (${type}):`, res.data);\n\n        // Asumsi: Respons data dikembalikan sebagai { data: [...] } atau langsung array\n        const dataArray = Array.isArray(res.data) ? res.data : res.data.data;\n        dataRef.value = Array.isArray(dataArray) ? dataArray : [];\n      } catch (err) {\n        console.error(`Failed to fetch product analytics (${type}):`, err.response?.data || err);\n        dataRef.value = [];\n      } finally {\n        loadingRef.value = false;\n      }\n    };\n\n    // --- Helper Functions ---\n\n    const formatCurrency = num => {\n      if (num === undefined || num === null || isNaN(num)) return \"Rp 0\";\n      return new Intl.NumberFormat(\"id-ID\", {\n        style: \"currency\",\n        currency: \"IDR\",\n        minimumFractionDigits: 0\n      }).format(num);\n    };\n\n    // --- Initialization ---\n\n    onMounted(() => {\n      fetchReport(); // Load Sales Summary\n\n      // Panggil fungsi analytics yang baru dibuat\n      fetchAnalytics(\"top\", topSelling, loadingTop);\n      fetchAnalytics(\"slow\", slowMoving, loadingSlow);\n    });\n    const __returned__ = {\n      API_URL,\n      ANALYTICS_URL,\n      reportData,\n      filterBy,\n      loadingReport,\n      topSelling,\n      slowMoving,\n      loadingTop,\n      loadingSlow,\n      fetchReport,\n      exportReport,\n      fetchAnalytics,\n      formatCurrency,\n      OwnerLayouts,\n      get RouterLink() {\n        return RouterLink;\n      },\n      get axios() {\n        return axios;\n      },\n      ref,\n      onMounted\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["OwnerLayouts","RouterLink","axios","ref","onMounted","API_URL","ANALYTICS_URL","reportData","filterBy","loadingReport","topSelling","slowMoving","loadingTop","loadingSlow","fetchReport","value","token","localStorage","getItem","res","get","params","type","by","headers","Authorization","data","err","console","error","response","message","exportReport","format","export","responseType","url","window","URL","createObjectURL","Blob","link","document","createElement","href","setAttribute","body","appendChild","click","removeChild","fetchAnalytics","dataRef","loadingRef","limit","log","dataArray","Array","isArray","formatCurrency","num","undefined","isNaN","Intl","NumberFormat","style","currency","minimumFractionDigits"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/SalesSummaryReportOwners.vue"],"sourcesContent":["<template>\n  <OwnerLayouts>\n    <div class=\"flex flex-1 flex-col py-5 p-8\">\n      <div class=\"layout-content-container flex flex-col flex-1\">\n        <div class=\"flex items-center gap-4 p-4\">\n          <RouterLink\n            to=\"/report-owners\"\n            class=\"flex items-center justify-center h-10 w-10 text-[#111418] rounded-full hover:bg-gray-100 transition-colors\"\n          >\n            <svg\n              xmlns=\"http://www.w3.org/2000/svg\"\n              width=\"24px\"\n              height=\"24px\"\n              fill=\"currentColor\"\n              viewBox=\"0 0 256 256\"\n            >\n              <path\n                d=\"M165.66,202.34a8,8,0,0,1-11.32,11.32l-80-80a8,8,0,0,1,0-11.32l80-80a8,8,0,0,1,11.32,11.32L91.31,128Z\"\n              ></path>\n            </svg>\n          </RouterLink>\n          <div class=\"flex min-w-72 flex-col gap-1\">\n            <p class=\"text-[#111418] text-[32px] font-bold leading-tight\">\n              Sales Summary Report\n            </p>\n            <p class=\"text-[#60758a] text-sm font-normal leading-normal\">\n              Overview of sales performance across different periods and product\n              analysis.\n            </p>\n          </div>\n        </div>\n\n        <h3 class=\"text-[#111418] text-lg font-bold px-4 pb-2 pt-4\">Filters</h3>\n        <div class=\"flex max-w-[480px] flex-wrap items-end gap-4 px-4 py-3\">\n          <label class=\"flex flex-col min-w-40 flex-1\">\n            <select\n              v-model=\"filterBy\"\n              @change=\"fetchReport\"\n              class=\"form-input w-full h-14 rounded-lg border border-[#dbe0e6] bg-white p-[15px] text-base\"\n            >\n              <option value=\"day\">Daily</option>\n              <option value=\"month\">Monthly</option>\n            </select>\n          </label>\n        </div>\n\n        <div class=\"flex justify-stretch\">\n          <div class=\"flex flex-1 gap-3 flex-wrap px-4 py-3 justify-start\">\n            <button\n              @click=\"exportReport('csv')\"\n              class=\"flex items-center justify-center h-10 px-4 bg-[#f0f2f5] rounded-lg text-sm font-bold\"\n              :disabled=\"loadingReport\"\n            >\n              Export Sales CSV\n            </button>\n            <button\n              @click=\"exportReport('pdf')\"\n              class=\"flex items-center justify-center h-10 px-4 bg-[#f0f2f5] rounded-lg text-sm font-bold\"\n              :disabled=\"loadingReport\"\n            >\n              Export Sales PDF\n            </button>\n          </div>\n        </div>\n\n        <div\n          v-if=\"loadingReport || loadingTop || loadingSlow\"\n          class=\"p-4 text-center text-gray-500\"\n        >\n          Loading reports...\n        </div>\n\n        <h3 class=\"text-[#111418] text-lg font-bold px-4 pb-2 pt-4\">\n          1. Sales Summary Data\n        </h3>\n        <div class=\"px-4 py-3 @container\">\n          <div\n            class=\"flex overflow-hidden rounded-lg border border-[#dbe0e6] bg-white\"\n          >\n            <table class=\"flex-1\">\n              <thead>\n                <tr class=\"bg-white\">\n                  <th class=\"px-4 py-3 text-left w-[400px] text-sm font-medium\">\n                    Period\n                  </th>\n                  <th class=\"px-4 py-3 text-left w-[400px] text-sm font-medium\">\n                    Transactions\n                  </th>\n                  <th class=\"px-4 py-3 text-left w-[400px] text-sm font-medium\">\n                    Total Sales\n                  </th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr\n                  v-for=\"row in reportData\"\n                  :key=\"row.period\"\n                  class=\"border-t border-[#dbe0e6]\"\n                >\n                  <td class=\"px-4 py-2 text-sm\">{{ row.period }}</td>\n                  <td class=\"px-4 py-2 text-sm text-[#60758a]\">\n                    {{ row.trx }}\n                  </td>\n                  <td class=\"px-4 py-2 text-sm text-[#60758a]\">\n                    {{ formatCurrency(row.total) }}\n                  </td>\n                </tr>\n                <tr v-if=\"!reportData.length && !loadingReport\">\n                  <td colspan=\"3\" class=\"p-4 text-center text-gray-500\">\n                    No sales summary data available.\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n\n        <h3\n          class=\"text-[#111418] text-lg font-bold px-4 pb-2 pt-6 border-t mt-6\"\n        >\n          2. Product Sales Analytics (Top 10)\n        </h3>\n\n        <div class=\"grid grid-cols-1 lg:grid-cols-2 gap-6 p-4\">\n          <div class=\"border p-4 rounded-lg shadow-sm\">\n            <h4 class=\"text-base font-semibold mb-3 text-green-700\">\n              Top 10 Most Selling Products\n            </h4>\n            <div v-if=\"loadingTop\" class=\"text-center py-4\">\n              Loading Top Sellers...\n            </div>\n            <div\n              v-else-if=\"topSelling.length === 0 && !loadingTop\"\n              class=\"text-center py-4 text-gray-500\"\n            >\n              No top selling product data available.\n            </div>\n            <div v-else class=\"overflow-x-auto\">\n              <table class=\"min-w-full divide-y divide-gray-200\">\n                <thead class=\"bg-gray-50\">\n                  <tr>\n                    <th\n                      class=\"px-3 py-2 text-left text-xs font-medium text-gray-500 uppercase\"\n                    >\n                      Rank\n                    </th>\n                    <th\n                      class=\"px-3 py-2 text-left text-xs font-medium text-gray-500 uppercase\"\n                    >\n                      Product Name\n                    </th>\n                    <th\n                      class=\"px-3 py-2 text-right text-xs font-medium text-gray-500 uppercase\"\n                    >\n                      Qty Sold\n                    </th>\n                    <th\n                      class=\"px-3 py-2 text-right text-xs font-medium text-gray-500 uppercase\"\n                    >\n                      Revenue\n                    </th>\n                  </tr>\n                </thead>\n                <tbody class=\"bg-white divide-y divide-gray-200\">\n                  <tr\n                    v-for=\"(item, index) in topSelling\"\n                    :key=\"`top-${item.product_id}`\"\n                  >\n                    <td\n                      class=\"px-3 py-2 whitespace-nowrap text-sm font-medium text-gray-900\"\n                    >\n                      {{ index + 1 }}\n                    </td>\n                    <td\n                      class=\"px-3 py-2 text-sm text-gray-700 max-w-xs truncate\"\n                    >\n                      {{ item.product_name }}\n                    </td>\n                    <td\n                      class=\"px-3 py-2 whitespace-nowrap text-sm text-right text-gray-900\"\n                    >\n                      {{ item.total_quantity }}\n                    </td>\n                    <td\n                      class=\"px-3 py-2 whitespace-nowrap text-sm text-right text-gray-900\"\n                    >\n                      {{ formatCurrency(item.total_revenue) }}\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n\n          <div class=\"border p-4 rounded-lg shadow-sm\">\n            <h4 class=\"text-base font-semibold mb-3 text-red-700\">\n              Top 10 Slow Moving Products\n            </h4>\n            <div v-if=\"loadingSlow\" class=\"text-center py-4\">\n              Loading Slow Movers...\n            </div>\n            <div\n              v-else-if=\"slowMoving.length === 0 && !loadingSlow\"\n              class=\"text-center py-4 text-gray-500\"\n            >\n              No slow moving product data available.\n            </div>\n            <div v-else class=\"overflow-x-auto\">\n              <table class=\"min-w-full divide-y divide-gray-200\">\n                <thead class=\"bg-gray-50\">\n                  <tr>\n                    <th\n                      class=\"px-3 py-2 text-left text-xs font-medium text-gray-500 uppercase\"\n                    >\n                      Rank\n                    </th>\n                    <th\n                      class=\"px-3 py-2 text-left text-xs font-medium text-gray-500 uppercase\"\n                    >\n                      Product Name\n                    </th>\n                    <th\n                      class=\"px-3 py-2 text-right text-xs font-medium text-gray-500 uppercase\"\n                    >\n                      Qty Sold\n                    </th>\n                    <th\n                      class=\"px-3 py-2 text-right text-xs font-medium text-gray-500 uppercase\"\n                    >\n                      Revenue\n                    </th>\n                  </tr>\n                </thead>\n                <tbody class=\"bg-white divide-y divide-gray-200\">\n                  <tr\n                    v-for=\"(item, index) in slowMoving\"\n                    :key=\"`slow-${item.product_id}`\"\n                  >\n                    <td\n                      class=\"px-3 py-2 whitespace-nowrap text-sm font-medium text-gray-900\"\n                    >\n                      {{ index + 1 }}\n                    </td>\n                    <td\n                      class=\"px-3 py-2 text-sm text-gray-700 max-w-xs truncate\"\n                    >\n                      {{ item.product_name }}\n                    </td>\n                    <td\n                      class=\"px-3 py-2 whitespace-nowrap text-sm text-right text-gray-900\"\n                    >\n                      {{ item.total_quantity }}\n                    </td>\n                    <td\n                      class=\"px-3 py-2 whitespace-nowrap text-sm text-right text-gray-900\"\n                    >\n                      {{ formatCurrency(item.total_revenue) }}\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </OwnerLayouts>\n</template>\n\n<script setup>\nimport OwnerLayouts from \"../components/OwnerLayouts.vue\";\nimport { RouterLink } from \"vue-router\";\nimport axios from \"axios\";\nimport { ref, onMounted } from \"vue\";\n\n// Base URLs\nconst API_URL = \"http://localhost:8081/api/v1/owner/reports\";\nconst ANALYTICS_URL = \"http://localhost:8081/api/v1/owner/product-analytics\"; // <--- Endpoint baru\n\n// --- State Variables ---\n\n// State for Sales Summary\nconst reportData = ref([]);\nconst filterBy = ref(\"month\");\nconst loadingReport = ref(false);\n\n// State for Product Analytics\nconst topSelling = ref([]);\nconst slowMoving = ref([]);\nconst loadingTop = ref(false);\nconst loadingSlow = ref(false);\n\n// --- 1. Sales Summary Logic (Sudah Ada) ---\n\nconst fetchReport = async () => {\n  try {\n    loadingReport.value = true;\n    const token = localStorage.getItem(\"token\");\n\n    const res = await axios.get(API_URL, {\n      params: { type: \"sales\", by: filterBy.value },\n      headers: { Authorization: `Bearer ${token}` },\n    });\n\n    reportData.value = res.data.data || [];\n  } catch (err) {\n    console.error(\n      \"Failed to fetch sales report:\",\n      err.response?.data || err.message\n    );\n    reportData.value = [];\n  } finally {\n    loadingReport.value = false;\n  }\n};\n\nconst exportReport = async (format) => {\n  try {\n    loadingReport.value = true;\n    const token = localStorage.getItem(\"token\");\n    const res = await axios.get(API_URL, {\n      params: { type: \"sales\", by: filterBy.value, export: format },\n      headers: { Authorization: `B