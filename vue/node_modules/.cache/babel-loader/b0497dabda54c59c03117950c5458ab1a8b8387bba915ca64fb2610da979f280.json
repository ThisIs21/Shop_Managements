{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, onMounted } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport PurchasingSidebar from \"../components/PurchasingSidebar.vue\";\nimport api from \"/src/services/api.js\"; // Import the configured API instance\n\nexport default {\n  __name: 'ProductListPurchase',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const products = ref([]);\n    const searchQuery = ref(\"\"); // For search functionality\n    const selectedCategory = ref(\"\"); // For category filter\n    const categories = ref([]); // To store categories for filtering\n\n    // Fetch products from the backend\n    const fetchProducts = async () => {\n      try {\n        const response = await api.get(\"/api/v1/products\");\n        products.value = response.data.data.map(product => ({\n          id: product.id,\n          name: product.name,\n          code: product.code || `P-${product.id.toString().padStart(3, \"0\")}`,\n          // Generate code if not provided\n          purchasePrice: `$${product.cost_price}`,\n          // Format as currency\n          sellingPrice: `$${product.sell_price}`,\n          // Format as currency\n          stock: product.stock,\n          category: product.category?.name || \"Uncategorized\" // Handle category name\n        }));\n      } catch (err) {\n        console.error(\"Failed to fetch products:\", err);\n        alert(\"Failed to load products\");\n      }\n    };\n\n    // Fetch categories for the filter dropdown\n    const fetchCategories = async () => {\n      try {\n        const response = await api.get(\"/api/v1/categories\");\n        categories.value = response.data.data;\n      } catch (err) {\n        console.error(\"Failed to fetch categories:\", err);\n      }\n    };\n\n    // Filter products based on search and category\n    const filteredProducts = computed(() => {\n      let filtered = products.value;\n      if (searchQuery.value) {\n        filtered = filtered.filter(product => product.name.toLowerCase().includes(searchQuery.value.toLowerCase()));\n      }\n      if (selectedCategory.value) {\n        filtered = filtered.filter(product => product.category === selectedCategory.value);\n      }\n      return filtered;\n    });\n\n    // Navigate to the add product page\n    const goToAddProduct = () => {\n      router.push(\"/management-product\");\n    };\n\n    // Initialize data on component mount\n    onMounted(() => {\n      fetchProducts();\n      fetchCategories();\n    });\n    const __returned__ = {\n      router,\n      products,\n      searchQuery,\n      selectedCategory,\n      categories,\n      fetchProducts,\n      fetchCategories,\n      filteredProducts,\n      goToAddProduct,\n      ref,\n      onMounted,\n      get useRouter() {\n        return useRouter;\n      },\n      PurchasingSidebar,\n      get api() {\n        return api;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","PurchasingSidebar","api","router","products","searchQuery","selectedCategory","categories","fetchProducts","response","get","value","data","map","product","id","name","code","toString","padStart","purchasePrice","cost_price","sellingPrice","sell_price","stock","category","err","console","error","alert","fetchCategories","filteredProducts","computed","filtered","filter","toLowerCase","includes","goToAddProduct","push"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/ProductListPurchase.vue"],"sourcesContent":["<script setup>\nimport { ref, onMounted } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport PurchasingSidebar from \"../components/PurchasingSidebar.vue\";\nimport api from \"/src/services/api.js\"; // Import the configured API instance\n\nconst router = useRouter();\nconst products = ref([]);\nconst searchQuery = ref(\"\"); // For search functionality\nconst selectedCategory = ref(\"\"); // For category filter\nconst categories = ref([]); // To store categories for filtering\n\n// Fetch products from the backend\nconst fetchProducts = async () => {\n  try {\n    const response = await api.get(\"/api/v1/products\");\n    products.value = response.data.data.map((product) => ({\n      id: product.id,\n      name: product.name,\n      code: product.code || `P-${product.id.toString().padStart(3, \"0\")}`, // Generate code if not provided\n      purchasePrice: `$${product.cost_price}`, // Format as currency\n      sellingPrice: `$${product.sell_price}`, // Format as currency\n      stock: product.stock,\n      category: product.category?.name || \"Uncategorized\", // Handle category name\n    }));\n  } catch (err) {\n    console.error(\"Failed to fetch products:\", err);\n    alert(\"Failed to load products\");\n  }\n};\n\n// Fetch categories for the filter dropdown\nconst fetchCategories = async () => {\n  try {\n    const response = await api.get(\"/api/v1/categories\");\n    categories.value = response.data.data;\n  } catch (err) {\n    console.error(\"Failed to fetch categories:\", err);\n  }\n};\n\n// Filter products based on search and category\nconst filteredProducts = computed(() => {\n  let filtered = products.value;\n\n  if (searchQuery.value) {\n    filtered = filtered.filter((product) =>\n      product.name.toLowerCase().includes(searchQuery.value.toLowerCase())\n    );\n  }\n\n  if (selectedCategory.value) {\n    filtered = filtered.filter(\n      (product) => product.category === selectedCategory.value\n    );\n  }\n\n  return filtered;\n});\n\n// Navigate to the add product page\nconst goToAddProduct = () => {\n  router.push(\"/management-product\");\n};\n\n// Initialize data on component mount\nonMounted(() => {\n  fetchProducts();\n  fetchCategories();\n});\n</script>\n\n<template>\n  <div\n    class=\"relative flex size-full min-h-screen flex-col bg-white group/design-root overflow-x-hidden\"\n    style=\"font-family: Manrope, 'Noto Sans', sans-serif\"\n  >\n    <div class=\"layout-container flex h-full grow flex-col\">\n      <div class=\"flex flex-1 justify-center\">\n        <PurchasingSidebar />\n        <div\n          class=\"layout-content-container flex flex-col max-w-[960px] flex-1\"\n        >\n          <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n            <p\n              class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\"\n            >\n              Products\n            </p>\n            <button\n              @click=\"goToAddProduct\"\n              class=\"flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-lg h-8 px-4 bg-[#f0f2f5] text-[#111418] text-sm font-medium leading-normal\"\n            >\n              <span class=\"truncate\">Add Product</span>\n            </button>\n          </div>\n          <div class=\"px-4 py-3\">\n            <label class=\"flex flex-col min-w-40 h-12 w-full\">\n              <div class=\"flex w-full flex-1 items-stretch rounded-lg h-full\">\n                <div\n                  class=\"text-[#60758a] flex border-none bg-[#f0f2f5] items-center justify-center pl-4 rounded-l-lg border-r-0\"\n                  data-icon=\"MagnifyingGlass\"\n                  data-size=\"24px\"\n                  data-weight=\"regular\"\n                >\n                  <svg\n                    xmlns=\"http://www.w3.org/2000/svg\"\n                    width=\"24px\"\n                    height=\"24px\"\n                    fill=\"currentColor\"\n                    viewBox=\"0 0 256 256\"\n                  >\n                    <path\n                      d=\"M229.66,