{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport CashierLayouts from \"@/components/CashierLayouts.vue\";\n\n// Definisikan state reaktif\n\nexport default {\n  __name: 'DashboardCashier',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const userName = ref(\"Kasir\");\n    const totalTransactions = ref(0);\n    const totalSalesValue = ref(0);\n    const salesHistory = ref([]);\n    const loadingHistory = ref(true);\n\n    // Fungsi untuk mengambil data dashboard\n    const fetchDashboardData = async () => {\n      try {\n        const token = localStorage.getItem(\"token\");\n        const nameFromStorage = localStorage.getItem(\"userName\");\n        if (nameFromStorage) {\n          userName.value = nameFromStorage;\n        }\n\n        // Mengambil tanggal hari ini dalam format YYYY-MM-DD untuk filter\n        const today = new Date().toISOString().slice(0, 10);\n        const response = await axios.get(\"http://localhost:8081/api/v1/history/sales\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          },\n          params: {\n            from: today,\n            to: today\n          }\n        });\n        const data = response.data;\n\n        // Pastikan data memiliki properti 'data' dan 'total' sesuai dengan respons backend\n        if (data && data.data) {\n          salesHistory.value = data.data;\n          totalTransactions.value = data.total;\n\n          // Hitung total nilai penjualan dari data yang diterima\n          let sum = 0;\n          data.data.forEach(sale => {\n            sum += sale.total_amount;\n          });\n          totalSalesValue.value = sum;\n        } else {\n          console.warn(\"API response is missing 'data' or 'total' property.\");\n        }\n      } catch (error) {\n        console.error(\"Failed to fetch dashboard data:\", error);\n        if (error.response && error.response.status === 401) {\n          // Handle unauthorized access, e.g., redirect to login page\n          // this.$router.push('/login');\n        }\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n    onMounted(() => {\n      fetchDashboardData();\n    });\n    const __returned__ = {\n      userName,\n      totalTransactions,\n      totalSalesValue,\n      salesHistory,\n      loadingHistory,\n      fetchDashboardData,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      CashierLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","CashierLayouts","userName","totalTransactions","totalSalesValue","salesHistory","loadingHistory","fetchDashboardData","token","localStorage","getItem","nameFromStorage","value","today","Date","toISOString","slice","response","get","headers","Authorization","params","from","to","data","total","sum","forEach","sale","total_amount","console","warn","error","status"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/DashboardCashier.vue"],"sourcesContent":["<template>\n  <CashierLayouts>\n    <div>\n      <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n        <p\n          class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\"\n        >\n          Selamat datang, {{ userName }}\n        </p>\n      </div>\n\n      <div v-if=\"!loadingHistory\" class=\"flex flex-wrap gap-4 p-4\">\n        <div\n          class=\"flex min-w-[158px] flex-1 flex-col gap-2 rounded-lg p-6 bg-[#f0f2f5]\"\n        >\n          <p class=\"text-[#111418] text-base font-medium leading-normal\">\n            Total Transaksi\n          </p>\n          <p\n            class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\"\n          >\n            {{ totalTransactions }}\n          </p>\n        </div>\n        <div\n          class=\"flex min-w-[158px] flex-1 flex-col gap-2 rounded-lg p-6 bg-[#f0f2f5]\"\n        >\n          <p class=\"text-[#111418] text-base font-medium leading-normal\">\n            Nilai Penjualan\n          </p>\n          <p\n            class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\"\n          >\n            ${{ totalSalesValue.toLocaleString() }}\n          </p>\n        </div>\n      </div>\n      <div v-else class=\"p-4\">\n        <p class=\"text-gray-500\">Loading data...</p>\n      </div>\n\n      ---\n\n      <div class=\"p-4\">\n        <p\n          class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\"\n        >\n          History Penjualan Hari Ini\n        </p>\n        <div\n          class=\"mt-4 overflow-x-auto rounded-lg border border-[#dbe0e6] bg-white\"\n        >\n          <table class=\"min-w-full\">\n            <thead>\n              <tr class=\"bg-gray-100\">\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Tanggal\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Nomor Transaksi\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Total\n                </th>\n                <th\n                  class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\"\n                >\n                  Detail\n                </th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-if=\"loadingHistory\">\n                <td colspan=\"4\" class=\"text-center py-4\">Loading history...</td>\n              </tr>\n              <tr v-else-if=\"salesHistory.length === 0\">\n                <td colspan=\"4\" class=\"text-center py-4 text-gray-500\">\n                  Tidak ada data penjualan hari ini.\n                </td>\n              </tr>\n              <tr\n                v-else\n                v-for=\"sale in salesHistory\"\n                :key=\"sale.id\"\n                class=\"border-t border-t-[#dbe0e6]\"\n              >\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{ new Date(sale.date).toLocaleDateString() }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  {{ sale.transaction_number }}\n                </td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                  ${{ sale.total_amount?.toLocaleString() }}\n                </td>\n                <td\n                  class=\"px-6 py-4 whitespace-nowrap text-sm text-blue-500 hover:text-blue-700 cursor-pointer\"\n                >\n                  Lihat Detail\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  </CashierLayouts>\n</template>\n\n<script setup>\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport CashierLayouts from \"@/components/CashierLayouts.vue\";\n\n// Definisikan state reaktif\nconst userName = ref(\"Kasir\");\nconst totalTransactions = ref(0);\nconst totalSalesValue = ref(0);\nconst salesHistory = ref([]);\nconst loadingHistory = ref(true);\n\n// Fungsi untuk mengambil data dashboard\nconst fetchDashboardData = async () => {\n  try {\n    const token = localStorage.getItem(\"token\");\n    const nameFromStorage = localStorage.getItem(\"userName\");\n\n    if (nameFromStorage) {\n      userName.value = nameFromStorage;\n    }\n\n    // Mengambil tanggal hari ini dalam format YYYY-MM-DD untuk filter\n    const today = new Date().toISOString().slice(0, 10);\n\n    const response = await axios.get(\n      \"http://localhost:8081/api/v1/history/sales\",\n      {\n        headers: {\n          Authorization: `Bearer ${token}`,\n        },\n        params: {\n          from: today,\n          to: today,\n        },\n      }\n    );\n\n    const data = response.data;\n\n    // Pastikan data memiliki properti 'data' dan 'total' sesuai dengan respons backend\n    if (data && data.data) {\n      salesHistory.value = data.data;\n      totalTransactions.value = data.total;\n\n      // Hitung total nilai penjualan dari data yang diterima\n      let sum = 0;\n      data.data.forEach((sale) => {\n        sum += sale.total_amount;\n      });\n      totalSalesValue.value = sum;\n    } else {\n      console.warn(\"API response is missing 'data' or 'total' property.\");\n    }\n  } catch (error) {\n    console.error(\"Failed to fetch dashboard data:\", error);\n    if (error.response && error.response.status === 401) {\n      // Handle unauthorized access, e.g., redirect to login page\n      // this.$router.push('/login');\n    }\n  } finally {\n    loadingHistory.value = false;\n  }\n};\n\nonMounted(() => {\n  fetchDashboardData();\n});\n</script>\n\n<style scoped>\n/* Tambahkan style jika diperlukan */\n</style>\n"],"mappings":";;AAoHA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,cAAc,MAAM,iCAAiC;;AAE5D;;;;;;;;IACA,MAAMC,QAAQ,GAAGJ,GAAG,CAAC,OAAO,CAAC;IAC7B,MAAMK,iBAAiB,GAAGL,GAAG,CAAC,CAAC,CAAC;IAChC,MAAMM,eAAe,GAAGN,GAAG,CAAC,CAAC,CAAC;IAC9B,MAAMO,YAAY,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMQ,cAAc,GAAGR,GAAG,CAAC,IAAI,CAAC;;IAEhC;IACA,MAAMS,kBAAkB,GAAG,MAAAA,CAAA,KAAY;MACrC,IAAI;QACF,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,MAAMC,eAAe,GAAGF,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;QAExD,IAAIC,eAAe,EAAE;UACnBT,QAAQ,CAACU,KAAK,GAAGD,eAAe;QAClC;;QAEA;QACA,MAAME,KAAK,GAAG,IAAIC,IAAI,