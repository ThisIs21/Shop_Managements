{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport AppSidebar from \"../components/CashierSidebar.vue\";\n\n// State definitions\n\nexport default {\n  __name: 'ReturnPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const salesHistory = ref([]);\n    const loadingHistory = ref(true);\n    const selectedSaleId = ref(\"\");\n    const returnItems = ref([]);\n    const additionalNotes = ref(\"\");\n    const isSubmitting = ref(false);\n\n    // Axios instance\n    const api = axios.create({\n      baseURL: \"http://localhost:8081/api/v1\"\n    });\n    const token = localStorage.getItem(\"token\");\n    if (token) {\n      api.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n    }\n\n    // Load sales history data\n    const loadSalesHistory = async () => {\n      loadingHistory.value = true;\n      try {\n        const res = await api.get(\"/history/sales\", {\n          params: {\n            page: 1,\n            size: 20,\n            orderBy: \"date DESC\"\n          } // Ambil 20 transaksi terakhir\n        });\n        salesHistory.value = res.data.data || [];\n      } catch (err) {\n        console.error(\"Error loading sales history:\", err);\n        alert(\"Gagal memuat riwayat transaksi!\");\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n\n    // Load sale items when a transaction is selected\n    const loadSaleItems = async saleId => {\n      selectedSaleId.value = saleId;\n      returnItems.value = []; // Reset item retur sebelumnya\n      try {\n        const res = await api.get(`/kasir/sales/${saleId}`); // Asumsi endpoint ini mengembalikan detail transaksi termasuk itemnya\n\n        // Asumsi data item berada di res.data.items atau res.data.sale_items\n        const items = res.data.items || res.data.sale_items || [];\n\n        // Logika untuk mengisi returnItems\n        returnItems.value = items.map(item => ({\n          product: {\n            id: item.product_id,\n            name: item.product_name || \"Unknown Product\",\n            // Asumsi nama produk tersedia\n            sell_price: item.price || 0\n          },\n          qty: item.qty,\n          returnQty: 0,\n          reason: \"\"\n        }));\n      } catch (err) {\n        console.error(\"Error loading sale items:\", err);\n        alert(\"Gagal memuat item transaksi! Periksa endpoint API.\");\n        selectedSaleId.value = \"\";\n      }\n    };\n\n    // Format currency\n    const formatCurrency = num => new Intl.NumberFormat(\"id-ID\", {\n      style: \"currency\",\n      currency: \"IDR\"\n    }).format(num);\n\n    // Format date\n    const formatDate = dateStr => {\n      return new Date(dateStr).toLocaleString(\"id-ID\", {\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\",\n        hour: \"2-digit\",\n        minute: \"2-digit\"\n      });\n    };\n\n    // Submit return\n    const submitReturn = async () => {\n      if (!selectedSaleId.value) {\n        alert(\"Pilih transaksi terlebih dahulu!\");\n        return;\n      }\n      const itemsToReturn = returnItems.value.filter(item => item.returnQty > 0 && item.reason);\n      if (itemsToReturn.length === 0) {\n        alert(\"Masukkan jumlah retur dan alasan untuk setidaknya satu item!\");\n        return;\n      }\n      isSubmitting.value = true;\n      try {\n        const payload = {\n          sale_id: selectedSaleId.value,\n          date: new Date().toISOString(),\n          items: itemsToReturn.map(item => ({\n            product_id: item.product.id,\n            qty: item.returnQty,\n            reason: item.reason\n          })),\n          notes: additionalNotes.value || null\n        };\n        const res = await api.post(\"/kasir/sale-returns\", payload);\n        alert(`Retur berhasil! ID: ${res.data.id}`);\n\n        // Reset form setelah berhasil\n        selectedSaleId.value = \"\";\n        returnItems.value = [];\n        additionalNotes.value = \"\";\n        await loadSalesHistory(); // Refresh daftar riwayat penjualan\n      } catch (err) {\n        console.error(\"Error submitting return:\", err);\n        alert(\"Gagal mengirim retur: \" + (err.response?.data?.error || err.message));\n      } finally {\n        isSubmitting.value = false;\n      }\n    };\n\n    // Load data on mount\n    onMounted(() => {\n      loadSalesHistory();\n    });\n    const __returned__ = {\n      salesHistory,\n      loadingHistory,\n      selectedSaleId,\n      returnItems,\n      additionalNotes,\n      isSubmitting,\n      api,\n      token,\n      loadSalesHistory,\n      loadSaleItems,\n      formatCurrency,\n      formatDate,\n      submitReturn,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      AppSidebar\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","AppSidebar","salesHistory","loadingHistory","selectedSaleId","returnItems","additionalNotes","isSubmitting","api","create","baseURL","token","localStorage","getItem","defaults","headers","common","loadSalesHistory","value","res","get","params","page","size","orderBy","data","err","console","error","alert","loadSaleItems","saleId","items","sale_items","map","item","product","id","product_id","name","product_name","sell_price","price","qty","returnQty","reason","formatCurrency","num","Intl","NumberFormat","style","currency","format","formatDate","dateStr","Date","toLocaleString","year","month","day","hour","minute","submitReturn","itemsToReturn","filter","length","payload","sale_id","date","toISOString","notes","post","response","message"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/ReturnPage.vue"],"sourcesContent":["<template>\n  <div class=\"flex size-full min-h-screen\">\n    <AppSidebar />\n    <div class=\"flex h-full grow flex-col bg-white p-6\" style=\"font-family: Manrope, 'Noto Sans', sans-serif\">\n      <div class=\"layout-content-container flex flex-col flex-1 px-4 py-5\">\n        <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n          <div class=\"flex min-w-72 flex-col gap-3\">\n            <p class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight\">\n              Sales Return\n            </p>\n            <p class=\"text-[#60758a] text-sm font-normal leading-normal\">\n              Create a return for a previous sale\n            </p>\n          </div>\n        </div>\n\n        <h3 class=\"text-[#111418] text-lg font-bold leading-tight tracking-[-0.015em] px-4 pb-2 pt-4\">\n          Sales History (Click to Select)\n        </h3>\n        <div class=\"px-4 py-3\">\n          <div class=\"mt-4 overflow-x-auto rounded-lg border border-[#dbe0e6] bg-white\">\n            <table class=\"min-w-full\">\n              <thead>\n                <tr class=\"bg-gray-100\">\n                  <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">\n                    Tanggal\n                  </th>\n                  <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">\n                    Nomor Transaksi\n                  </th>\n                  <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">\n                    Total\n                  </th>\n                  <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">\n                    Aksi\n                  </th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-if=\"loadingHistory\">\n                  <td colspan=\"4\" class=\"text-center py-4\">Loading history...</td>\n                </tr>\n                <tr v-else-if=\"salesHistory.length === 0\">\n                  <td colspan=\"4\" class=\"text-center py-4 text-gray-500\">\n                    Tidak ada data penjualan.\n                  </td>\n                </tr>\n                <tr\n                  v-else\n                  v-for=\"sale in salesHistory\"\n                  :key=\"sale.ID\"\n                  class=\"border-t border-t-[#dbe0e6]\"\n                >\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                    {{ formatDate(sale.Date) || \"N/A\" }}\n                  </td>\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                    {{ sale.transaction_number || `TRX-${sale.ID || \"N/A\"}` }}\n                  </td>\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\n                    {{ formatCurrency(sale.Total) || \"Rp 0\" }}\n                  </td>\n                  <td class=\"px-6 py-4 whitespace-nowrap text-sm text-blue-500 hover:text-blue-700 cursor-pointer\">\n                    <span @click=\"loadSaleItems(sale.ID)\">Pilih</span>\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n        <hr class=\"my-4\" />\n\n        <h3 class=\"text-[#111418] text-lg font-bold leading-tight tracking-[-0.015em] px-4 pb-2 pt-4\">\n          Return Items for Sale ID: {{ selectedSaleId || 'Not Selected' }}\n        </h3>\n        <div class=\"px-4 py-3 @container\">\n          <div class=\"flex overflow-hidden rounded-lg border border-[#dbe0e6] bg-white\">\n            <table class=\"flex-1\">\n              <thead>\n                <tr class=\"bg-white\">\n                  <th class=\"table-column px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\">\n                    Product\n                  </th>\n                  <th class=\"table-column px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\">\n                    Quantity\n                  </th>\n                  <th class=\"table-column px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\">\n                    Return Quantity\n                  </th>\n                  <th class=\"table-column px-4 py-3 text-left text-[#111418] text-sm font-medium leading-normal\">\n                    Reason\n                  </th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr v-if=\"returnItems.length === 0\">\n                  <td colspan=\"4\" class=\"text-center text-[#60758a] py-4\">\n                    No items available for selected transaction\n                  </td>\n                </tr>\n                <tr\n                  v-else\n                  v-for=\"(item, index) in returnItems\"\n                  :key=\"index\"\n                  class=\"border-t border-t-[#dbe0e6]\"\n                >\n                  <td class=\"table-column h-[72px] px-4 py-2 text-[#111418] text-sm font-normal leading-normal\">\n                    {{ item.product.name }}\n                  </td>\n                  <td class=\"table-column h-[72px] px-4 py-2 text-[#60758a] text-sm font-normal leading-normal\">\n                    {{ item.qty }}\n                  </td>\n                  <td class=\"table-column h-[72px] px-4 py-2 text-[#60758a] text-sm font-normal leading-normal\">\n                    <input\n                      v-model.number=\"item.returnQty\"\n                      type=\"number\"\n                      min=\"0\"\n                      :max=\"item.qty\"\n                      class=\"w-full border rounded p-2\"\n                    />\n                  </td>\n                  <td class=\"table-column h-[72px] px-4 py-2 text-[#60758a] text-sm font-normal leading-normal\">\n                    <input\n                      v-model=\"item.reason\"\n                      type=\"text\"\n                      placeholder=\"Enter reason\"\n                      class=\"w-full border rounded p-2\"\n                    />\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n        <div class=\"flex max-w-[480px] flex-wrap items-end gap-4 px-4 py-3\">\n          <label class=\"flex flex-col min-w-40 flex-1\">\n            <textarea\n              v-model=\"additionalNotes\"\n              placeholder=\"Additional Notes\"\n              class=\"form-input flex w-full min-w-0 flex-1 resize-none overflow-hidden rounded-lg text-[#111418] focus:outline-0 focus:ring-0 border border-[#dbe0e6] bg-white focus:border-[#dbe0e6] min-h-36 placeholder:text-[#60758a] p-[15px] text-base font-normal leading-normal\"\n            ></textarea>\n          </label>\n        </div>\n        <div class=\"flex px-4 py-3 justify-end\">\n          <button\n            @click=\"submitReturn\"\n            :disabled=\"isSubmitting\"\n            class=\"flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-lg h-10 px-4 bg-[#0d80f2] text-white text-sm font-bold leading-normal tracking-[0.015em]\"\n          >\n            <span class=\"truncate\">{{\n              isSubmitting ? \"Submitting...\" : \"Submit Return\"\n            }}</span>\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport AppSidebar from \"../components/CashierSidebar.vue\";\n\n// State definitions\nconst salesHistory = ref([]);\nconst loadingHistory = ref(true);\nconst selectedSaleId = ref(\"\");\nconst returnItems = ref([]);\nconst additionalNotes = ref(\"\");\nconst isSubmitting = ref(false);\n\n// Axios instance\nconst api = axios.create({ baseURL: \"http://localhost:8081/api/v1\" });\nconst token = localStorage.getItem(\"token\");\nif (token) {\n  api.defaults.headers.common[\"Authorization\"] = `Bearer ${token}`;\n}\n\n// Load sales history data\nconst loadSalesHistory = async () => {\n  loadingHistory.value = true;\n  try {\n    const res = await api.get(\"/history/sales\", {\n      params: { page: 1, size: 20, orderBy: \"date DESC\" }, // Ambil 20 transaksi terakhir\n    });\n    salesHistory.value = res.data.data || [];\n  } catch (err) {\n    console.error(\"Error loading sales history:\", err);\n    alert(\"Gagal memuat riwayat transaksi!\");\n  } finally {\n    loadingHistory.value = false;\n  }\n};\n\n// Load sale items when a transaction is selected\nconst loadSaleItems = async (saleId) => {\n  selectedSaleId.value = saleId;\n  returnItems.value = []; // Reset item retur sebelumnya\n  try {\n    const res = await api.get(`/kasir/sales/${saleId}`); // Asumsi endpoint ini mengembalikan detail transaksi termasuk itemnya\n    \n    // Asumsi data item berada di res.data.items atau res.data.sale_items\n    const items = res.data.items || res.data.sale_items || [];\n    \n    // Logika untuk mengisi returnItems\n    returnItems.value = items.map((item) => ({\n      product: {\n        id: item.product_id,\n        name: item.product_name || \"Unknown Product\", // Asumsi nama produk tersedia\n        sell_price: item.price || 0,\n      },\n      qty: item.qty,\n      returnQty: 0,\n      reason: \"\",\n    }));\n  } catch (err) {\n    console.error(\"Error loading sale items:\", err);\n    alert(\"Gagal memuat item transaksi! Periksa endpoint API.\");\n    selectedSaleId.value = \"\";\n  }\n};\n\n// Format currency\nconst formatCurrency = (num) =>\n  new Intl.NumberFormat(\"id-ID\", { style: \"currency\", currency: \"IDR\" }).format(num);\n\n// Format date\nconst formatDate = (dateStr) => {\n  return new Date(dateStr).toLocaleString(\"id-ID\", {\n    year: \"numeric\",\n    month: \"long\",\n    day: \"numeric\",\n    hour: \"2-digit\",\n    minute: \"2-digit\",\n  });\n};\n\n// Submit return\nconst submitReturn = async () => {\n  if (!selectedSaleId.value) {\n    alert(\"Pilih transaksi terlebih dahulu!\");\n    return;\n  }\n  const itemsToReturn = returnItems.value.filter(\n    (item) => item.returnQty > 0 && item.reason\n  );\n  if (itemsToReturn.length === 0) {\n    alert(\"Masukkan jumlah retur dan alasan untuk setidaknya satu item!\");\n    return;\n  }\n\n  isSubmitting.value = true;\n  try {\n    const payload = {\n      sale_id: selectedSaleId.value,\n      date: new Date().toISOString(),\n      items: itemsToReturn.map((item) => ({\n        product_id: item.product.id,\n        qty: item.returnQty,\n        reason: item.reason,\n      })),\n      notes: additionalNotes.value || null,\n    };\n    const res = await api.post(\"/kasir/sale-returns\", payload);\n    alert(`Retur berhasil! ID: ${res.data.id}`);\n    \n    // Reset form setelah berhasil\n    selectedSaleId.value = \"\";\n    returnItems.value = [];\n    additionalNotes.value = \"\";\n    await loadSalesHistory(); // Refresh daftar riwayat penjualan\n  } catch (err) {\n    console.error(\"Error submitting return:\", err);\n    alert(\"Gagal mengirim retur: \" + (err.response?.data?.error || err.message));\n  } finally {\n    isSubmitting.value = false;\n  }\n};\n\n// Load data on mount\nonMounted(() => {\n  loadSalesHistory();\n});\n</script>\n\n<style scoped>\n/* Scoped styles tetap sama */\n.table-column {\n  width: 25%; /* Membagi lebar tabel menjadi 4 kolom yang sama */\n}\n</style>"],"mappings":";;;AAgKA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,UAAU,MAAM,kCAAkC;;AAEzD;;;;;;;;IACA,MAAMC,YAAY,GAAGJ,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMK,cAAc,GAAGL,GAAG,CAAC,IAAI,CAAC;IAChC,MAAMM,cAAc,GAAGN,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMO,WAAW,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMQ,eAAe,GAAGR,GAAG,CAAC,EAAE,CAAC;IAC/B,MAAMS,YAAY,GAAGT,GAAG,CAAC,KAAK,CAAC;;IAE/B;IACA,MAAMU,GAAG,GAAGR,KAAK,CAACS,MAAM,CAAC;MAAEC,OAAO