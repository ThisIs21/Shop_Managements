{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, onMounted } from 'vue';\nimport axios from 'axios';\nimport AppSidebar from '../components/CashierSidebar.vue';\n\n// State definitions\n\nexport default {\n  __name: 'ReturnPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const sales = ref([]);\n    const selectedSale = ref(null); // Transaksi yang dipilih\n    const returnItems = ref([]); // Item retur\n    const additionalNotes = ref('');\n    const isSubmitting = ref(false);\n    const fromDate = ref('');\n    const toDate = ref('');\n    const loading = ref(true);\n    const returnDate = ref(''); // Tambah state untuk tanggal retur\n\n    // Axios instance\n    const api = axios.create({\n      baseURL: 'http://localhost:8081/api/v1'\n    });\n    const token = localStorage.getItem('token');\n    if (token) {\n      api.defaults.headers.common['Authorization'] = `Bearer ${token}`;\n    } else {\n      console.error('No token found in localStorage');\n      alert('Silakan login kembali!');\n    }\n\n    // Load sales data\n    const fetchSales = async () => {\n      loading.value = true;\n      try {\n        const params = {\n          page: 1,\n          size: 10,\n          orderBy: \"Date DESC\" // Sesuaikan dengan dashboard\n        };\n        if (fromDate.value) params.start_date = fromDate.value;\n        if (toDate.value) params.end_date = toDate.value;\n        const res = await api.get('/history/sales', {\n          params\n        }); // Sesuaikan endpoint dengan dashboard\n        sales.value = res.data.data || [];\n        if (sales.value.length === 0) {\n          console.warn('No sales data returned from API');\n        }\n      } catch (err) {\n        console.error('Error loading sales:', err);\n        const errorMsg = err.response?.data?.error || err.message || 'Gagal memuat daftar transaksi!';\n        alert(errorMsg);\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // Load sale details and items when transaction is selected\n    const selectSaleForReturn = async id => {\n      try {\n        const res = await api.get(`/history/sales/${id}`); // Sesuaikan endpoint\n        selectedSale.value = res.data; // Full sale object\n        returnItems.value = res.data.items.map(item => ({\n          product: item.product,\n          Qty: item.Qty,\n          returnQty: 0,\n          reason: ''\n        }));\n      } catch (err) {\n        console.error('Error loading sale details:', err);\n        const errorMsg = err.response?.data?.error || err.message || 'Gagal memuat detail transaksi!';\n        alert(errorMsg);\n      }\n    };\n\n    // Reset filter\n    const resetFilter = () => {\n      fromDate.value = \"\";\n      toDate.value = \"\";\n      fetchSales();\n    };\n\n    // Submit return\n    const submitReturn = async () => {\n      if (!selectedSale.value) {\n        alert('Pilih transaksi terlebih dahulu!');\n        return;\n      }\n      const itemsToReturn = returnItems.value.filter(item => item.returnQty > 0 && item.reason);\n      if (itemsToReturn.length === 0) {\n        alert('Masukkan jumlah retur dan alasan untuk setidaknya satu item!');\n        return;\n      }\n      isSubmitting.value = true;\n      try {\n        const payload = {\n          sale_id: selectedSale.value.ID,\n          date: returnDate.value || new Date().toISOString(),\n          items: itemsToReturn.map(item => ({\n            product_id: item.product.ID,\n            qty: item.returnQty,\n            price: item.product.sell_price\n          })),\n          additional_notes: additionalNotes.value || null\n        };\n        const res = await api.post('/kasir/sale-returns', payload);\n        alert(`Retur berhasil! ID: ${res.data.id}`);\n        selectedSale.value = null;\n        returnItems.value = [];\n        additionalNotes.value = '';\n        fetchSales(); // Refresh list\n      } catch (err) {\n        console.error('Error submitting return:', err);\n        const errorMsg = err.response?.data?.error || err.message || 'Gagal mengirim retur!';\n        alert(errorMsg);\n      } finally {\n        isSubmitting.value = false;\n      }\n    };\n\n    // Format currency\n    const formatCurrency = amount => {\n      return amount !== undefined && amount !== null ? new Intl.NumberFormat('id-ID', {\n        style: 'currency',\n        currency: 'IDR',\n        minimumFractionDigits: 0\n      }).format(amount) : \"Rp 0\";\n    };\n\n    // Format date\n    const formatDate = dateStr => {\n      if (!dateStr) return \"N/A\";\n      const date = new Date(dateStr);\n      if (isNaN(date.getTime())) {\n        const sliced = dateStr.split(\"T\")[0]; // Ambil tanggal saja\n        return sliced || \"Invalid Date\";\n      }\n      return date.toLocaleDateString('id-ID', {\n        day: '2-digit',\n        month: '2-digit',\n        year: 'numeric'\n      });\n    };\n\n    // Load data on mount\n    onMounted(() => {\n      if (!token) {\n        alert('Silakan login kembali!');\n        return;\n      }\n      fetchSales();\n    });\n    const __returned__ = {\n      sales,\n      selectedSale,\n      returnItems,\n      additionalNotes,\n      isSubmitting,\n      fromDate,\n      toDate,\n      loading,\n      returnDate,\n      api,\n      token,\n      fetchSales,\n      selectSaleForReturn,\n      resetFilter,\n      submitReturn,\n      formatCurrency,\n      formatDate,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      AppSidebar\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","AppSidebar","sales","selectedSale","returnItems","additionalNotes","isSubmitting","fromDate","toDate",