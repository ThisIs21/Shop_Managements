{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport { ref, computed, onMounted } from \"vue\";\nimport axios from \"@/utils/axios\"; // gunakan helper axios proyekmu\n\nexport default {\n  __name: 'StockSnapshotOwners',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const search = ref(\"\");\n    const stocks = ref([]);\n    const loading = ref(true);\n    const error = ref(\"\");\n\n    /**\n     * Fetch products from backend.\n     * Endpoint sesuai routing backend: GET /api/v1/owner/products\n     * Response shape: either array directly or { data: [...] }\n     */\n    async function fetchProducts() {\n      loading.value = true;\n      error.value = \"\";\n      try {\n        const res = await axios.get(\"/api/v1/owner/products\");\n        // fleksibel: some handlers return { data: [...] } some return [...]\n        stocks.value = res.data?.data ?? res.data ?? [];\n      } catch (err) {\n        console.error(\"Gagal mengambil data stok:\", err);\n        error.value = \"Gagal mengambil data stok dari server.\";\n      } finally {\n        loading.value = false;\n      }\n    }\n    onMounted(() => {\n      fetchProducts();\n    });\n\n    // filter search: sesuai permintaan (nama atau kode)\n    const filteredStocks = computed(() => {\n      const q = search.value?.toString().toLowerCase().trim();\n      if (!q) return stocks.value;\n      return stocks.value.filter(item => {\n        const name = (item.name || \"\").toString().toLowerCase();\n        const code = formatProductCode(item.id).toString().toLowerCase();\n        return name.includes(q) || code.includes(q);\n      });\n    });\n\n    // Format product code -> gunakan ID karena tidak ada field code di model.\n    // Format: PRD-XXXX (padding 4 digit). Jika ingin hanya ID, ubah fungsi ini.\n    function formatProductCode(id) {\n      if (id === undefined || id === null) return \"-\";\n      const num = Number(id);\n      if (Number.isNaN(num)) return String(id);\n      return \"PRD-\" + String(num).padStart(4, \"0\");\n    }\n\n    // Format money (2 decimal, tanpa mengubah desain)\n    function formatMoney(v) {\n      if (v === undefined || v === null) return \"-\";\n      const n = Number(v);\n      if (Number.isNaN(n)) return \"-\";\n      // localized with 2 decimals\n      return n.toFixed(2);\n    }\n\n    /** Export CSV dari filteredStocks */\n    function exportCSV() {\n      const rows = filteredStocks.value.map(it => ({\n        name: it.name ?? \"\",\n        code: formatProductCode(it.id),\n        category: it.Category?.name ?? \"\",\n        purchase_price: (it.cost_price ?? it.CostPrice ?? it.costPrice ?? \"\").toString(),\n        selling_price: (it.sell_price ?? it.SellPrice ?? it.sellPrice ?? \"\").toString(),\n        stock: it.stock ?? 0,\n        location: it.location ?? \"\"\n      }));\n      const header = [\"Product Name\", \"Product Code\", \"Category\", \"Purchase Price\", \"Selling Price\", \"Stock Quantity\", \"Location\"];\n      const csv = [header.join(\",\")].concat(rows.map(r => [escapeCsv(r.name), escapeCsv(r.code), escapeCsv(r.category), escapeCsv(r.purchase_price), escapeCsv(r.selling_price), escapeCsv(String(r.stock)), escapeCsv(r.location)].join(\",\"))).join(\"\\n\");\n      const blob = new Blob([csv], {\n        type: \"text/csv;charset=utf-8;\"\n      });\n      const url = URL.createObjectURL(blob);\n      const a = document.createElement(\"a\");\n      a.href = url;\n      a.download = `stock_snapshot_${new Date().toISOString().slice(0, 10)}.csv`;\n      document.body.appendChild(a);\n      a.click();\n      a.remove();\n      URL.revokeObjectURL(url);\n    }\n    function escapeCsv(s) {\n      if (s === undefined || s === null) return \"\";\n      const str = String(s);\n      // jika mengandung koma atau kutip atau newline, wrap di quotes dan escape quotes\n      if (/[,\"\\n]/.test(str)) {\n        return '\"' + str.replace(/\"/g, '\"\"') + '\"';\n      }\n      return str;\n    }\n\n    /** Export PDF -> tampilkan halaman print-friendly dan panggil print.\n     *  Implementasi sederhana: buka window baru dengan tabel HTML dan print.\n     */\n    function exportPDF() {\n      // buat HTML sederhana yang mempertahankan table (styling minimal supaya rapi saat print)\n      const rowsHtml = filteredStocks.value.map(it => `\n    <tr>\n      <td style=\"padding:8px;border:1px solid #ddd\">${escapeHtml(it.name)}</td>\n      <td style=\"padding:8px;border:1px solid #ddd\">${escapeHtml(formatProductCode(it.id))}</td>\n      <td style=\"padding:8px;border:1px solid #ddd\">${escapeHtml(it.Category?.name ?? \"-\")}</td>\n      <td style=\"padding:8px;border:1px solid #ddd\">${escapeHtml(formatMoney(it.cost_price ?? it.CostPrice ?? it.costPrice))}</td>\n      <td style=\"padding:8px;border:1px solid #ddd\">${escapeHtml(formatMoney(it.sell_price ?? it.SellPrice ?? it.sellPrice))}</td>\n      <td style=\"padding:8px;border:1px solid #ddd\">${escapeHtml(String(it.stock ?? 0))}</td>\n      <td style=\"padding:8px;border:1px solid #ddd\">${escapeHtml(it.location ?? \"-\")}</td>\n    </tr>\n  `).join(\"\");\n      const html = `\n    <html>\n      <head>\n        <title>Stock Snapshot</title>\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\n        <style>\n          body { font-family: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial; padding: 20px; color: #111418; }\n          table { border-collapse: collapse; width: 100%; }\n          th, td { border: 1px solid #ddd; padding: 8px; text-align: left; }\n          th { background: #f8fafc; font-weight: 600; }\n        </style>\n      </head>\n      <body>\n        <h2>Stock Snapshot</h2>\n        <table>\n          <thead>\n            <tr>\n              <th>Product Name</th><th>Product Code</th><th>Category</th><th>Purchase Price</th><th>Selling Price</th><th>Stock Quantity</th><th>Location</th>\n            </tr>\n          </thead>\n          <tbody>\n            ${rowsHtml}\n          </tbody>\n        </table>\n      </body>\n    </html>\n  `;\n      const w = window.open(\"\", \"_blank\");\n      if (!w) {\n        alert(\"Popup blocked. Please allow popups for this site to export PDF/print.\");\n        return;\n      }\n      w.document.write(html);\n      w.document.close();\n      // delay sebentar sampai konten ter-render lalu panggil print\n      setTimeout(() => {\n        w.print();\n      }, 500);\n    }\n    function escapeHtml(s) {\n      if (s === undefined || s === null) return \"\";\n      return String(s).replace(/&/g, \"&amp;\").replace(/</g, \"&lt;\").replace(/>/g, \"&gt;\").replace(/\"/g, \"&quot;\").replace(/'/g, \"&#039;\");\n    }\n    const __returned__ = {\n      search,\n      stocks,\n      loading,\n      error,\n      fetchProducts,\n      filteredStocks,\n      formatProductCode,\n      formatMoney,\n      exportCSV,\n      escapeCsv,\n      exportPDF,\n      escapeHtml,\n      ref,\n      computed,\n      onMounted,\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","axios","search","stocks","loading","error","fetchProducts","value","res","get","data","err","console","filteredStocks","q","toString","toLowerCase","trim","filter","item","name","code","formatProductCode","id","includes","undefined","num","Number","isNaN","String","padStart","formatMoney","v","n","toFixed","exportCSV","rows","map","it","category","Category","purchase_price","cost_price","CostPrice","costPrice","selling_price","sell_price","SellPrice","sellPrice","stock","location","header","csv","join","concat","r","escapeCsv","blob","Blob","type","url","URL","createObjectURL","a","document","createElement","href","download","Date","toISOString","slice","body","appendChild","click","remove","revokeObjectURL","s","str","test","replace","exportPDF","rowsHtml","escapeHtml","html","w","window","open","alert","write","close","setTimeout","print"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/StockSnapshotOwners.vue"],"sourcesContent":["<template>\n  <OwnerLayouts>\n    <div class=\"layout-content-container flex flex-col max-w-[960px] flex-1\">\n      <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n        <div class=\"flex min-w-72 flex-col gap-3\">\n          <p\n            class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight\"\n          >\n            Stock Snapshot\n          </p>\n          <p class=\"text-[#60758a] text-sm font-normal leading-normal\">\n            View current stock levels for all products\n          </p>\n        </div>\n        <div class=\"flex items-center\">\n          <router-link to=\"/report-owners\">\n            <button\n              class=\"flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-lg h-10 px-4 bg-[#f0f2f5] text-[#111418] text-sm font-bold leading-normal tracking-[0.015em]\"\n            >\n              <span class=\"truncate\">Kembali ke Report Owners</span>\n            </button>\n          </router-link>\n        </div>\n      </div>\n\n      <div class=\"px-4 py-3\">\n        <label class=\"flex flex-col min-w-40 h-12 w-full\">\n          <div class=\"flex w-full flex-1 items-stretch rounded-lg h-full\">\n            <div\n              class=\"text-[#60758a] flex border-none bg-[#f0f2f5] items-center justify-center pl-4 rounded-l-lg border-r-0\"\n              data-icon=\"MagnifyingGlass\"\n              data-size=\"24px\"\n              data-weight=\"regular\"\n            >\n              <svg\n                xmlns=\"http://www.w3.org/2000/svg\"\n                width=\"24px\"\n                height=\"24px\"\n                fill=\"currentColor\"\n                viewBox=\"0 0 256 256\"\n              >\n                <path\n                  d=\"M229.66,218.34l-50.07-50.06a88.11,88.11,0,1,0-11.31,11.31l50.06,50.07a8,8,0,0,0,11.32-11.32ZM40,112a72,72,0,1,1,72,72A72.08,72.08,0,0,1,40,112Z\"\n                ></path>\n              </svg>\n            </div>\n            <input\n              v-model=\"search\"\n              placeholder=\"Search products by name or code\"\n              class=\"form-input flex w-full min-w-0 flex-1 resize-none overflow-hidden rounded-lg text-[#111418] focus:outline-0 focus:ring-0 border-none bg-[#f0f2f5] focus:border-none h-full placeholder:text-[#60758a] px-4 rounded-l-none border-l-0 pl-2 text-base font-normal leading-normal\"\n            />\n          </div>\n        </label>\n      </div>\n\n      <div class=\"flex gap-3 p-3 flex-wrap pr-4\">\n        <button\n          class=\"flex h-8 shrink-0 items-center justify-center gap-x-2 rounded-lg bg-[#f0f2f5] pl-4 pr-2\"\n        >\n          <p class=\"text-[#111418] text-sm font-medium leading-normal\">\n            Category\n          </p>\n          <div\n            class=\"text-[#111418]\"\n            data-icon=\"CaretDown\"\n            data-size=\"20px\"\n            data-weight=\"regular\"\n          >\n            <svg\n              xmlns=\"http://www.w3.org/2000/svg\"\n              width=\"20px\"\n              height=\"20px\"\n              fill=\"currentColor\"\n              viewBox=\"0 0 256 256\"\n            >\n              <path\n                d=\"M213.66,101.66l-80,80a8,8,0,0,1-11.32,0l-80-80A8,8,0,0,1,53.66,90.34L128,164.69l74.34-74.35a8,8,0,0,1,11.32,11.32Z\"\n              ></path>\n            </svg>\n          </div>\n        </button>\n        <button\n          class=\"flex h-8 shrink-0 items-center justify-center gap-x-2 rounded-lg bg-[#f0f2f5] pl-4 pr-2\"\n        >\n          <p class=\"text-[#111418] text-sm font-medium leading-normal\">\n            Location\n          </p>\n          <div\n            class=\"text-[#111418]\"\n            data-icon=\"CaretDown\"\n            data-size=\"20px\"\n            data-weight=\"regular\"\n          >\n            <svg\n              xmlns=\"http://www.w3.org/2000/svg\"\n              width=\"20px\"\n              height=\"20px\"\n              fill=\"currentColor\"\n              viewBox=\"0 0 256 256\"\n            >\n              <path\n                d=\"M213.66,101.66l-80,80a8,8,0,0,1-11.32,0l-80-80A8,8,0,0,1,53.66,90.34L128,164.69l74.34-74.35a8,8,0,0,1,11.32,11.32Z\"\n              ></path>\n            </svg>\n          </div>\n        </button>\n        <button\n          class=\"flex h-8 shrink-0 items-center justify-center gap-x-2 rounded-lg bg-[#f0f2f5] pl-4 pr-2\"\n        >\n          <p class=\"text-[#111418] text-sm font-medium leading-normal\">\n            Low Stock\n          </p>\n          <div\n            class=\"text-[#111418]\"\n            data-icon=\"CaretDown\"\n            data-size=\"20px\"\n            data-weight=\"regular\"\n          >\n            <svg\n              xmlns=\"http://www.w3.org/2000/svg\"\n              width=\"20px\"\n              height=\"20px\"\n              fill=\"currentColor\"\n              viewBox=\"0 0 256 256\"\n            >\n              <path\n                d=\"M213.66,101.66l-80,80a8,8,0,0,1-11.32,0l-80-80A8,8,0,0,1,53.66,90.34L128,164.69l74.34-74.35a8,8,0,0,1,11.32,11.32Z\"\n              ></path>\n            </svg>\n          </div>\n        </button>\n      </div>\n\n      <div class=\"px-4 py-3 @container\">\n        <div\n          class=\"flex overflow-hidden rounded-lg border border-[#dbe0e6] bg-white\"\n        >\n          <table class=\"flex-1\">\n            <thead>\n              <tr class=\"bg-white\">\n                <th\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-120 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                >\n                  Product Name\n                </th>\n                <th\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-240 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                >\n                  Product Code\n                </th>\n                <th\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-360 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                >\n                  Category\n                </th>\n                <th\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-480 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                >\n                  Purchase Price\n                </th>\n                <th\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-600 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                >\n                  Selling Price\n                </th>\n                <th\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-720 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                >\n                  Stock Quantity\n                </th>\n                <th\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-840 px-4 py-3 text-left text-[#111418] w-[400px] text-sm font-medium leading-normal\"\n                >\n                  Location\n                </th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr\n                v-for=\"(item) in filteredStocks\"\n                :key=\"item.id\"\n                class=\"border-t border-t-[#dbe0e6]\"\n              >\n                <td\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-120 h-[72px] px-4 py-2 w-[400px] text-[#111418] text-sm font-normal leading-normal\"\n                >\n                  {{ item.name }}\n                </td>\n                <td\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-240 h-[72px] px-4 py-2 w-[400px] text-[#60758a] text-sm font-normal leading-normal\"\n                >\n                  {{ formatProductCode(item.id) }}\n                </td>\n                <td\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-360 h-[72px] px-4 py-2 w-[400px] text-[#60758a] text-sm font-normal leading-normal\"\n                >\n                  {{ item.Category?.name || \"-\" }}\n                </td>\n                <td\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-480 h-[72px] px-4 py-2 w-[400px] text-[#60758a] text-sm font-normal leading-normal\"\n                >\n                  {{\n                    formatMoney(\n                      item.cost_price ??\n                        item.CostPrice ??\n                        item.costPrice ??\n                        item.CostPrice\n                    )\n                  }}\n                </td>\n                <td\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-600 h-[72px] px-4 py-2 w-[400px] text-[#60758a] text-sm font-normal leading-normal\"\n                >\n                  {{\n                    formatMoney(\n                      item.sell_price ?? item.SellPrice ?? item.sellPrice\n                    )\n                  }}\n                </td>\n                <td\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-720 h-[72px] px-4 py-2 w-[400px] text-[#60758a] text-sm font-normal leading-normal\"\n                >\n                  {{ item.stock }}\n                </td>\n                <td\n                  class=\"table-3b7bcaaf-95b2-443e-96c3-a9b755615db1-column-840 h-[72px] px-4 py-2 w-[400px] text-[#60758a] text-sm font-normal leading-normal\"\n                >\n                  {{ item.location || \"-\" }}\n                </td>\n              </tr>\n\n              <tr v-if=\"!loading && filteredStocks.length === 0\">\n                <td colspan=\"7\" class=\"text-center py-4 text-[#60758a]\">\n                  No data found\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n\n      <div class=\"flex justify-stretch\">\n        <div class=\"flex flex-1 gap-3 flex-wrap px-4 py-3 justify-end\">\n          <button\n            @click=\"exportCSV\"\n            :disabled=\"loading || stocks.length === 0\"\n            class=\"flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-lg h-10 px-4 bg-[#f0f2f5] text-[#111418] text-sm font-bold leading-normal tracking-[0.015em]\"\n          >\n            <span class=\"truncate\">Export to CSV</span>\n          </button>\n          <button\n            @click=\"exportPDF\"\n            :disabled=\"loading || stocks.length === 0\"\n            class=\"flex min-w-[84px] max-w-[480px] cursor-pointer items-center justify-center overflow-hidden rounded-lg h-10 px-4 bg-[#0d80f2] text-white text-sm font-bold leading-normal tracking-[0.015em]\"\n          >\n            <span class=\"truncate\">Export to PDF</span>\n          </button>\n        </div>\n      </div>\n    </div>\n  </OwnerLayouts>\n</template>\n\n<script setup>\nimport { ref, computed, onMounted } from \"vue\";\nimport axios from \"@/utils/axios\"; // gunakan helper axios proyekmu\n\nconst search = ref(\"\");\nconst stocks = ref([]);\nconst loading = ref(true);\nconst error = ref(\"\");\n\n/**\n * Fetch products from backend.\n * Endpoint sesuai routing backend: GET /api/v1/owner/products\n * Response shape: either array directly or { data: [...] }\n */\nasync function fetchProducts() {\n  loading.value = true;\n  error.value = \"\";\n  try {\n    const res = await axios.get(\"/api/v1/owner/products\");\n    // fleksibel: some handlers return { data: [...] } some return [...]\n    stocks.value = res.data?.data ?? res.data ?? [];\n  } catch (err) {\n    console.error(\"Gagal mengambil data stok:\", err);\n    error.value = \"Gagal mengambil data stok dari server.\";\n  } finally {\n    loading.value = false;\n  }\n}\n\nonMounted(() => {\n  fetchProducts();\n});\n\n// filter search: sesuai permintaan (nama atau kode)\nconst filteredStocks = computed(() => {\n  const q = search.value?.toString().toLowerCase().trim();\n  if (!q) return stocks.value;\n  return stocks.value.filter((item) => {\n    const name = (item.name || \"\").toString().toLowerCase();\n    const code = formatProductCode(item.id).toString().toLowerCase();\n    return name.includes(q) || code.includes(q);\n  });\n});\n\n// Format product code -> gunakan ID karena tidak ada field code di model.\n// Format: PRD-XXXX (padding 4 digit). Jika ingin hanya ID, ubah fungsi ini.\nfunction formatProductCode(id) {\n  if (id === undefined || id === null) return \"-\";\n  const num = Number(id);\n  if (Number.isNaN(num)) return String(id);\n  return \"PRD-\" + String(num).padStart(4, \"0\");\n}\n\n// Format money (2 decimal, tanpa mengubah desain)\nfunction formatMoney(v) {\n  if (v === undefined || v === null) return \"-\";\n  const n = Number(v);\n  if (Number.isNaN(n)) return \"-\";\n  // localized with 2 decimals\n  return n.toFixed(2);\n}\n\n/** Export CSV dari filteredStocks */\nfunction exportCSV() {\n  const rows = filteredStocks.value.map((it) => ({\n    name: it.name ?? \"\",\n    code: formatProductCode(it.id),\n    category: it.Category?.name ?? \"\",\n    purchase_price: (\n      it.cost_price ??\n      it.CostPrice ??\n      it.costPrice ??\n      \"\"\n    ).toString(),\n    selling_price: (\n      it.sell_price ??\n      it.SellPrice ??\n      it.sellPrice ??\n      \"\"\n    ).toString(),\n    stock: it.stock ?? 0,\n    location: it.location ?? \"\",\n  }));\n\n  const header = [\n    \"Product Name\",\n    \"Product Code\",\n    \"Category\",\n    \"Purchase Price\",\n    \"Selling Price\",\n    \"Stock Quantity\",\n    \"Location\",\n  ];\n  const csv = [header.join(\",\")]\n    .concat(\n      rows.map((r) =>\n        [\n          escapeCsv(r.name),\n          escapeCsv(r.code),\n          escapeCsv(r.category),\n          escapeCsv(r.purchase_price),\n          escapeCsv(r.selling_price),\n          escapeCsv(String(r.stock)),\n          escapeCsv(r.location),\n        ].join(\",\")\n      )\n    )\n    .join(\"\\n\");\n\n  const blob = new Blob([csv], { type: \"text/csv;charset=utf-8;\" });\n  const url = URL.createObjectURL(blob);\n  const a = document.createElement(\"a\");\n  a.href = url;\n  a.download = `stock_snapshot_${new Date().toISOString().slice(0, 10)}.csv`;\n  document.body.appendChild(a);\n  a.click();\n  a.remove();\n  URL.revokeObjectURL(url);\n}\n\nfunction escapeCsv(s) {\n  if (s === undefined || s === null) return \"\";\n  const str = String(s);\n  // jika mengandung koma atau kutip atau newline, wrap di quotes dan escape quotes\n  if (/[,\"\\n]/.test(str)) {\n    return '\"' + str.replace(/\"/g, '\"\"') + '\"';\n  }\n  return str;\n}\n\n/** Export PDF -> tampilkan halaman print-friendly dan panggil print.\n *  Implementasi sederhana: buka window baru dengan tabel HTML dan print.\n */\nfunction exportPDF() {\n  // buat HTML sederhana yang mempertahankan table (styling minimal supaya rapi saat print)\n  const rowsHtml = filteredStocks.value\n    .map(\n      (it) => `\n    <tr>\n      <td style=\"padding:8px;border:1px solid #ddd\">${escapeHtml(it.name)}</td>\n      <td style=\"padding:8px;border:1px solid #ddd\">${escapeHtml(\n        formatProductCode(it.id)\n      )}</td>\n      <td style=\"padding:8px;bor