se;\n  }\n};\n\n/* -------------------------\n   Reset filters\n   ------------------------- */\nconst resetPurchaseFilter = () => {\n  fromDate.value = \"\";\n  toDate.value = \"\";\n  fetchPurchases();\n};\n\nconst resetReturnFilter = () => {\n  returnFromDate.value = \"\";\n  returnToDate.value = \"\";\n  returnStatusFilter.value = \"\";\n  fetchPurchaseReturns();\n};\n\n/* -------------------------\n   Lifecycle\n   ------------------------- */\nonMounted(() => {\n  fetchPurchases();\n  fetchPurchaseReturns();\n});\n</script>\n\n<style scoped>\n/* keep styles minimal — you can reuse style from your project */\n</style>\n"],"mappings":";;;;AA+LA,SAASA,GAAG,EAAEC,SAAS,EAAEC,QAAQ,QAAQ,KAAK;AAC9C,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,cAAc,MAAM,iCAAiC;;AAE5D;AACA;AACA;AACA,MAAMC,QAAQ,GAAG,8BAA8B;AAC/C,MAAMC,yBAAyB,GAAG,2BAA2B,CAAC,CAAC;AAC/D,MAAMC,+BAA+B,GAAG,6BAA6B,CAAC,CAAC;AACvE,MAAMC,8BAA8B,GAAG,2BAA2B,CAAC,CAAC;;AAEpE;;;;;;;;IACA,MAAMC,GAAG,GAAGN,KAAK,CAACO,MAAM,CAAC;MAAEC,OAAO,EAAEN;IAAS,CAAC,CAAC;IAC/C,MAAMO,UAAU,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;IAChD,IAAIF,UAAU,EAAEH,GAAG,CAACM,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,CAAC,GAAG,UAAUL,UAAU,EAAE;;IAErF;AACA;AACA;IACA,MAAMM,SAAS,GAAGlB,GAAG,CAAC,EAAE,CAAC;IACzB,MAAMmB,gBAAgB,GAAGnB,GAAG,CAAC,KAAK,CAAC;IACnC,MAAMoB,QAAQ,GAAGpB,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMqB,MAAM,GAAGrB,GAAG,CAAC,EAAE,CAAC;IAEtB,MAAMsB,eAAe,GAAGtB,GAAG,CAAC,EAAE,CAAC;IAC/B,MAAMuB,cAAc,GAAGvB,GAAG,CAAC,KAAK,CAAC;IACjC,MAAMwB,cAAc,GAAGxB,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMyB,YAAY,GAAGzB,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAM0B,kBAAkB,GAAG1B,GAAG,CAAC,EAAE,CAAC;;IAElC;IACA,MAAM2B,SAAS,GAAG3B,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAM4B,gBAAgB,GAAG5B,GAAG,CAAC,IAAI,CAAC;IAClC,MAAM6B,WAAW,GAAG7B,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAM8B,UAAU,GAAG9B,GAAG,CAAC,KAAK,CAAC;;IAE7B;IACA,MAAM+B,UAAU,GAAG/B,GAAG,CAAC;MAAEgC,KAAK,EAAE;IAAG,CAAC,CAAC;;IAErC;AACA;AACA;IACA,MAAMC,UAAU,GAAIC,OAAO,IAAK;MAC9B,IAAI,CAACA,OAAO,EAAE,OAAO,KAAK;MAC1B,MAAMC,CAAC,GAAG,IAAIC,IAAI,CAACF,OAAO,CAAC;MAC3B,IAAIG,KAAK