{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport CashierLayouts from \"@/components/CashierLayouts.vue\";\n\n/* -------------------------\n   Configuration / API\n   ------------------------- */\nconst API_BASE = \"http://localhost:8081/api/v1\";\nconst LATEST_PURCHASES_ENDPOINT = \"/history/purchases/latest\"; // -> GET ?limit=10 OR with start_date/end_date\nconst CREATE_PURCHASE_RETURN_ENDPOINT = \"/pembelian/purchase-returns\"; // POST\nconst LIST_PURCHASE_RETURNS_ENDPOINT = \"/history/purchase-returns\"; // GET (history view)\n\n/* Axios instance with token */\n\nexport default {\n  __name: 'PurchaseReturn',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const api = axios.create({\n      baseURL: API_BASE\n    });\n    const savedToken = localStorage.getItem(\"token\");\n    if (savedToken) api.defaults.headers.common[\"Authorization\"] = `Bearer ${savedToken}`;\n\n    /* -------------------------\n       State\n       ------------------------- */\n    const purchases = ref([]);\n    const loadingPurchases = ref(false);\n    const fromDate = ref(\"\");\n    const toDate = ref(\"\");\n    const purchaseReturns = ref([]);\n    const loadingReturns = ref(false);\n    const returnFromDate = ref(\"\");\n    const returnToDate = ref(\"\");\n    const returnStatusFilter = ref(\"\");\n\n    /* Modal state */\n    const showModal = ref(false);\n    const selectedPurchase = ref(null);\n    const returnNotes = ref(\"\");\n    const submitting = ref(false);\n\n    /* returnForm will mirror selectedPurchase.Items but with qty & reason */\n    const returnForm = ref({\n      items: []\n    });\n\n    /* -------------------------\n       Helpers: formatters\n       ------------------------- */\n    const formatDate = dateStr => {\n      if (!dateStr) return \"N/A\";\n      const d = new Date(dateStr);\n      if (isNaN(d.getTime())) {\n        const sliced = dateStr.split(\"T\")[0];\n        return sliced || \"Invalid Date\";\n      }\n      return d.toLocaleDateString(\"id-ID\", {\n        day: \"2-digit\",\n        month: \"2-digit\",\n        year: \"numeric\"\n      });\n    };\n    const formatCurrency = amount => {\n      return amount !== undefined && amount !== null ? new Intl.NumberFormat(\"id-ID\", {\n        style: \"currency\",\n        currency: \"IDR\",\n        minimumFractionDigits: 0\n      }).format(amount) : \"Rp 0\";\n    };\n    const formatPONumber = id => `PO-${String(id || \"\").padStart(3, \"0\")}`;\n    const formatReturnNumber = id => `2025-${String(id || \"\").padStart(3, \"0\")}`;\n    const getStatusClass = status => {\n      switch (status) {\n        case \"PENDING\":\n          return \"bg-yellow-100 text-yellow-800\";\n        case \"APPROVED\":\n          return \"bg-green-100 text-green-800\";\n        case \"REJECTED\":\n          return \"bg-red-100 text-red-800\";\n        default:\n          return \"bg-gray-100 text-gray-800\";\n      }\n    };\n    const getStatusText = status => {\n      switch (status) {\n        case \"PENDING\":\n          return \"Pending\";\n        case \"APPROVED\":\n          return \"Approved\";\n        case \"REJECTED\":\n          return \"Rejected\";\n        default:\n          return status || \"-\";\n      }\n    };\n\n    /* -------------------------\n       Computed\n       ------------------------- */\n    const hasValidItems = computed(() => {\n      return returnForm.value.items.some(item => item.qty > 0 && item.reason && item.reason.trim().length > 0);\n    });\n\n    /* -------------------------\n       Fetch purchases (latest or filtered)\n       ------------------------- */\n    const fetchPurchases = async () => {\n      loadingPurchases.value = true;\n      try {\n        const params = {};\n        if (fromDate.value) params.start_date = fromDate.value;\n        if (toDate.value) params.end_date = toDate.value;\n        let url = LATEST_PURCHASES_ENDPOINT;\n        if (fromDate.value || toDate.value) {\n          url = \"/history/purchases\";\n          params.page = 1;\n          params.size = 10;\n        } else {\n          params.limit = 10;\n        }\n        const res = await api.get(url, {\n          params\n        });\n\n        // Normalisasi response dengan cara bertingkat — hindari duplicate else-if\n        const body = res && res.data ? res.data : null;\n        if (Array.isArray(body)) {\n          purchases.value = body;\n        } else if (body && body.data) {\n          if (Array.isArray(body.data)) {\n            purchases.value = body.data;\n          } else if (body.data && Array.isArray(body.data.data)) {\n            purchases.value = body.data.data;\n          } else if (body.data && Array.isArray(body.data.rows)) {\n            purchases.value = body.data.rows;\n          } else {\n            purchases.value = body.data || [];\n          }\n        } else if (res && res.data && res.data.purchases && Array.isArray(res.data.purchases)) {\n          purchases.value = res.data.purchases;\n        } else {\n          purchases.value = [];\n        }\n      } catch (err) {\n        console.error(\"Error fetching purchases:\", err);\n        alert(`Gagal mengambil data pembelian: ${err.response?.status || err.message}`);\n        purchases.value = [];\n      } finally {\n        loadingPurchases.value = false;\n      }\n    };\n\n    /* -------------------------\n       Fetch purchase returns (history)\n       ------------------------- */\n    const fetchPurchaseReturns = async () => {\n      loadingReturns.value = true;\n      try {\n        const params = {\n          page: 1,\n          size: 10\n        };\n        if (returnFromDate.value) params.start_date = returnFromDate.value;\n        if (returnToDate.value) params.end_date = returnToDate.value;\n        if (returnStatusFilter.value) params.status = returnStatusFilter.value;\n        const res = await api.get(LIST_PURCHASE_RETURNS_ENDPOINT, {\n          params\n        });\n        purchaseReturns.value = res.data.data || res.data || [];\n      } catch (err) {\n        console.error(\"Error fetching purchase returns:\", err);\n        alert(`Gagal mengambil daftar retur: ${err.response?.data?.error || err.message}`);\n        purchaseReturns.value = [];\n      } finally {\n        loadingReturns.value = false;\n      }\n    };\n\n    /* -------------------------\n       Modal: open purchase detail & prepare returnForm\n       ------------------------- */\n    const openReturnModal = async purchaseId => {\n      try {\n        const res = await api.get(`/pembelian/purchases/${purchaseId}`);\n        const payload = res.data.data || res.data;\n        selectedPurchase.value = payload;\n        returnForm.value.items = (payload.Items || []).map(item => ({\n          product_id: item.ProductID || item.Product?.ID || item.Product?.id,\n          purchase_item_id: item.ID,\n          qty: 0,\n          price: item.Price || 0,\n          reason: \"\"\n        }));\n        returnNotes.value = \"\";\n        showModal.value = true;\n      } catch (err) {\n        console.error(\"Error fetching purchase detail:\", err);\n        alert(`Gagal memuat detail pembelian: ${err.response?.data?.error || err.message}`);\n      }\n    };\n    const closeModal = () => {\n      showModal.value = false;\n      selectedPurchase.value = null;\n      returnForm.value = {\n        items: []\n      };\n      returnNotes.value = \"\";\n    };\n\n    /* -------------------------\n       Submit return\n       ------------------------- */\n    const submitReturn = async () => {\n      if (!hasValidItems.value) {\n        alert(\"Pilih minimal satu item dengan qty > 0 dan alasan.\");\n        return;\n      }\n      submitting.value = true;\n      try {\n        const validItems = returnForm.value.items.filter(it => it.qty > 0 && it.reason && it.reason.trim().length > 0).map(it => ({\n          product_id: it.product_id,\n          qty: it.qty,\n          price: it.price\n        }));\n        const dto = {\n          purchase_id: selectedPurchase.value.ID,\n          date: new Date().toISOString(),\n          items: validItems,\n          notes: returnNotes.value\n        };\n        const res = await api.post(CREATE_PURCHASE_RETURN_ENDPOINT, dto);\n        if (res.status === 201 || res.status === 200) {\n          alert(\"Retur pembelian berhasil dibuat (status: PENDING).\");\n          closeModal();\n          fetchPurchaseReturns();\n          fetchPurchases();\n        } else {\n          alert(\"Server merespon, tetapi tidak 201. Periksa response di console.\");\n          console.log(\"Create purchase return response:\", res);\n        }\n      } catch (err) {\n        console.error(\"Error creating purchase return:\", err);\n        alert(`Gagal membuat retur: ${err.response?.data?.error || err.message}`);\n      } finally {\n        submitting.value = false;\n      }\n    };\n\n    /* -------------------------\n       Reset filters\n       ------------------------- */\n    const resetPurchaseFilter = () => {\n      fromDate.value = \"\";\n      toDate.value = \"\";\n      fetchPurchases();\n    };\n    const resetReturnFilter = () => {\n      returnFromDate.value = \"\";\n      returnToDate.value = \"\";\n      returnStatusFilter.value = \"\";\n      fetchPurchaseReturns();\n    };\n\n    /* -------------------------\n       Lifecycle\n       ------------------------- */\n    onMounted(() => {\n      fetchPurchases();\n      fetchPurchaseReturns();\n    });\n    const __returned__ = {\n      API_BASE,\n      LATEST_PURCHASES_ENDPOINT,\n      CREATE_PURCHASE_RETURN_ENDPOINT,\n      LIST_PURCHASE_RETURNS_ENDPOINT,\n      api,\n      savedToken,\n      purchases,\n      loadingPurchases,\n      fromDate,\n      toDate,\n      purchaseReturns,\n      loadingReturns,\n      returnFromDate,\n      returnToDate,\n      returnStatusFilter,\n      showModal,\n      selectedPurchase,\n      returnNotes,\n      submitting,\n      returnForm,\n      formatDate,\n      formatCurrency,\n      formatPONumber,\n      formatReturnNumber,\n      getStatusClass,\n      getStatusText,\n      hasValidItems,\n      fetchPurchases,\n      fetchPurchaseReturns,\n      openReturnModal,\n      closeModal,\n      submitReturn,\n      resetPurchaseFilter,\n      resetReturnFilter,\n      ref,\n      onMounted,\n      computed,\n      get axios() {\n        return axios;\n      },\n      CashierLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","axios","CashierLayouts","API_BASE","LATEST_PURCHASES_ENDPOINT","CREATE_PURCHASE_RETURN_ENDPOINT","LIST_PURCHASE_RETURNS_ENDPOINT","api","create","baseURL","savedToken","localStorage","getItem","defaults","headers","common","purchases","loadingPurchases","fromDate","toDate","purchaseReturns","loadingReturns","returnFromDate","returnToDate","returnStatusFilter","showModal","selectedPurchase","returnNotes","submitting","returnForm","items","formatDate","dateStr","d","Date","isNaN","getTime","sliced","split","toLocaleDateString","day","month","year","formatCurrency","amount","undefined","Intl","NumberFormat","style","currency","minimumFractionDigits","format","formatPONumber","id","String","padStart","formatReturnNumber","getStatusClass","status","getStatusText","hasValidItems","value","some","item","qty","reason","trim","length","fetchPurchases","params","start_date","end_date","url","page","size","limit","res","get","body","data","Array","isArray","rows","err","console","error","alert","response","message","fetchPurchaseReturns","openReturnModal","purchaseId","payload","Items","map","product_id","ProductID","Product","ID","purchase_item_id","price","Price","closeModal","submitReturn","validItems","filter","it","dto","purchase_id","date","toISOString","notes","post","log","resetPurchaseFilter","resetReturnFilter"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/PurchaseReturn.vue"],"sourcesContent":["<template>\n  <CashierLayouts>\n    <div>\n      <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n        <p class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\">\n          Retur Pembelian\n        </p>\n      </div>\n\n      <!-- Filter Tanggal untuk memilih pembelian (Latest / Date range) -->\n      <div class=\"p-4 bg-[#f0f2f5] rounded-lg\">\n        <div class=\"flex flex-wrap gap-4 items-end\">\n          <div>\n            <label class=\"block text-sm font-medium text-gray-700 mb-1\">Dari Tanggal</label>\n            <input type=\"date\" v-model=\"fromDate\" class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\" />\n          </div>\n          <div>\n            <label class=\"block text-sm font-medium text-gray-700 mb-1\">Sampai Tanggal</label>\n            <input type=\"date\" v-model=\"toDate\" class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\" />\n          </div>\n\n          <button @click=\"fetchPurchases\" :disabled=\"loadingPurchases\" class=\"px-4 py-2 bg-blue-500 text-white rounded-md hover:bg-blue-600 disabled:opacity-50\">\n            {{ loadingPurchases ? \"Loading...\" : (fromDate || toDate ? \"Filter\" : \"Load Latest (10)\") }}\n          </button>\n\n          <button @click=\"resetPurchaseFilter\" class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\">\n            Reset (10 Terakhir)\n          </button>\n        </div>\n        <p v-if=\"fromDate || toDate\" class=\"text-sm text-gray-600 mt-2\">\n          Filter aktif: {{ fromDate || \"N/A\" }} s/d {{ toDate || \"N/A\" }}\n        </p>\n      </div>\n\n      <hr class=\"my-4\" />\n\n      <!-- Table: Purchases (for returns) -->\n      <div class=\"p-4\">\n        <p class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\">\n          Daftar Pembelian (untuk Retur) ({{ fromDate || toDate ? \"Filtered\" : \"10 Terakhir\" }})\n        </p>\n\n        <div class=\"mt-4 overflow-x-auto rounded-lg border border-[#dbe0e6] bg-white\">\n          <table class=\"min-w-full\">\n            <thead>\n              <tr class=\"bg-gray-100\">\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Tanggal</th>\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Nomor PO</th>\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Supplier</th>\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Total</th>\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Aksi</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-if=\"loadingPurchases\">\n                <td colspan=\"5\" class=\"text-center py-4\">Loading purchases...</td>\n              </tr>\n              <tr v-else-if=\"purchases.length === 0\">\n                <td colspan=\"5\" class=\"text-center py-4 text-gray-500\">Tidak ada data pembelian.</td>\n              </tr>\n              <tr v-else v-for=\"po in purchases\" :key=\"po.ID\" class=\"border-t border-t-[#dbe0e6]\">\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">{{ formatDate(po.created_at) || \"N/A\" }}</td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">{{ formatPONumber(po.ID) }}</td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">{{ po.Supplier?.name || \"-\" }}</td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">{{ formatCurrency(po.Total) || \"Rp 0\" }}</td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm\">\n                  <button @click=\"openReturnModal(po.ID)\" class=\"px-3 py-1 bg-green-500 text-white rounded text-sm hover:bg-green-600\">\n                    ADD\n                  </button>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n\n      <!-- My Purchase Returns List -->\n      <div class=\"p-4 mt-8\">\n        <p class=\"text-[#111418] tracking-light text-2xl font-bold leading-tight\">Daftar Retur Saya</p>\n\n        <div class=\"p-4 bg-[#f0f2f5] rounded-lg mt-4\">\n          <div class=\"flex flex-wrap gap-4 items-end\">\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Dari Tanggal</label>\n              <input type=\"date\" v-model=\"returnFromDate\" class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\" />\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Sampai Tanggal</label>\n              <input type=\"date\" v-model=\"returnToDate\" class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\" />\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Status</label>\n              <select v-model=\"returnStatusFilter\" class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\">\n                <option value=\"\">Semua</option>\n                <option value=\"PENDING\">Pending</option>\n                <option value=\"APPROVED\">Approved</option>\n                <option value=\"REJECTED\">Rejected</option>\n              </select>\n            </div>\n            <button @click=\"fetchPurchaseReturns\" :disabled=\"loadingReturns\" class=\"px-4 py-2 bg-blue-500 text-white rounded-md hover:bg-blue-600 disabled:opacity-50\">\n              {{ loadingReturns ? \"Loading...\" : \"Filter\" }}\n            </button>\n            <button @click=\"resetReturnFilter\" class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\">Reset (10 Terakhir)</button>\n          </div>\n\n          <p v-if=\"returnFromDate || returnToDate || returnStatusFilter\" class=\"text-sm text-gray-600 mt-2\">\n            Filter aktif: {{ returnFromDate || \"N/A\" }} s/d {{ returnToDate || \"N/A\" }} (Status: {{ returnStatusFilter || \"Semua\" }})\n          </p>\n        </div>\n\n        <div class=\"mt-4 overflow-x-auto rounded-lg border border-[#dbe0e6] bg-white\">\n          <table class=\"min-w-full\">\n            <thead>\n              <tr class=\"bg-gray-100\">\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Return No.</th>\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">PO Number</th>\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Tanggal</th>\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Total</th>\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Status</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-if=\"loadingReturns\">\n                <td colspan=\"5\" class=\"text-center py-4\">Loading returns...</td>\n              </tr>\n              <tr v-else-if=\"purchaseReturns.length === 0\">\n                <td colspan=\"5\" class=\"text-center py-4 text-gray-500\">Tidak ada retur yang dibuat.</td>\n              </tr>\n              <tr v-else v-for=\"ret in purchaseReturns\" :key=\"ret.id\" class=\"border-t border-t-[#dbe0e6]\">\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">RTN-{{ formatReturnNumber(ret.id) }}</td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">{{ ret.purchase_id ? formatPONumber(ret.purchase_id) : \"-\" }}</td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">{{ formatDate(ret.date) || \"N/A\" }}</td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">{{ formatCurrency(ret.total) || \"Rp 0\" }}</td>\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm font-medium\">\n                  <span :class=\"getStatusClass(ret.status)\" class=\"px-2 py-1 rounded text-xs\">{{ getStatusText(ret.status) }}</span>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n\n      <!-- Modal untuk Buat Retur Pembelian -->\n      <div v-if=\"showModal\" class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50 p-4\">\n        <div class=\"bg-white rounded-lg p-6 max-w-4xl w-full max-h-[90vh] overflow-y-auto\">\n          <h2 class=\"text-xl font-bold mb-4\">Buat Retur Pembelian</h2>\n\n          <div v-if=\"selectedPurchase\" class=\"mb-4 p-4 bg-gray-100 rounded\">\n            <p><strong>Nomor PO:</strong> {{ formatPONumber(selectedPurchase.ID) }}</p>\n            <p><strong>Tanggal:</strong> {{ formatDate(selectedPurchase.created_at) }}</p>\n            <p><strong>Total:</strong> {{ formatCurrency(selectedPurchase.Total) }}</p>\n          </div>\n\n          <div class=\"mb-4\">\n            <h3 class=\"text-lg font-semibold mb-2\">Items untuk Diretur</h3>\n            <div v-for=\"(item, index) in returnForm.items\" :key=\"index\" class=\"border-b py-3 last:border-b-0\">\n              <div class=\"flex flex-wrap gap-4 items-start\">\n                <div class=\"flex-1\">\n                  <p class=\"font-medium\">{{ item.Product?.name || \"N/A\" }}</p>\n                  <p class=\"text-sm text-gray-600\">Qty Original: {{ selectedPurchase?.Items[index]?.Qty || 0 }} | Harga: {{ formatCurrency(selectedPurchase?.Items[index]?.Price) }}</p>\n                </div>\n                <div class=\"w-32\">\n                  <label class=\"block text-sm font-medium text-gray-700 mb-1\">Qty Retur</label>\n                  <input type=\"number\" v-model.number=\"item.qty\" :min=\"0\" :max=\"selectedPurchase?.Items[index]?.Qty || 0\" class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\" />\n                </div>\n                <div class=\"flex-1\">\n                  <label class=\"block text-sm font-medium text-gray-700 mb-1\">Alasan</label>\n                  <textarea v-model=\"item.reason\" placeholder=\"Masukkan alasan retur item ini\" rows=\"2\" class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"></textarea>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"mb-4\">\n            <label class=\"block text-sm font-medium text-gray-700 mb-1\">Catatan Tambahan</label>\n            <textarea v-model=\"returnNotes\" placeholder=\"Catatan opsional untuk retur ini\" rows=\"3\" class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"></textarea>\n          </div>\n\n          <div class=\"flex justify-end gap-2\">\n            <button @click=\"closeModal\" class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\">Batal</button>\n            <button @click=\"submitReturn\" :disabled=\"submitting || !hasValidItems\" class=\"px-4 py-2 bg-green-500 text-white rounded-md hover:bg-green-600 disabled:opacity-50\">\n              {{ submitting ? \"Menyimpan...\" : \"Submit Retur\" }}\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </CashierLayouts>\n</template>\n\n<script setup>\nimport { ref, onMounted, computed } from \"vue\";\nimport axios from \"axios\";\nimport CashierLayouts from \"@/components/CashierLayouts.vue\";\n\n/* -------------------------\n   Configuration / API\n   ------------------------- */\nconst API_BASE = \"http://localhost:8081/api/v1\";\nconst LATEST_PURCHASES_ENDPOINT = \"/history/purchases/latest\"; // -> GET ?limit=10 OR with start_date/end_date\nconst CREATE_PURCHASE_RETURN_ENDPOINT = \"/pembelian/purchase-returns\"; // POST\nconst LIST_PURCHASE_RETURNS_ENDPOINT = \"/history/purchase-returns\"; // GET (history view)\n\n/* Axios instance with token */\nconst api = axios.create({ baseURL: API_BASE });\nconst savedToken = localStorage.getItem(\"token\");\nif (savedToken) api.defaults.headers.common[\"Authorization\"] = `Bearer ${savedToken}`;\n\n/* -------------------------\n   State\n   ------------------------- */\nconst purchases = ref([]);\nconst loadingPurchases = ref(false);\nconst fromDate = ref(\"\");\nconst toDate = ref(\"\");\n\nconst purchaseReturns = ref([]);\nconst loadingReturns = ref(false);\nconst returnFromDate = ref(\"\");\nconst returnToDate = ref(\"\");\nconst returnStatusFilter = ref(\"\");\n\n/* Modal state */\nconst showModal = ref(false);\nconst selectedPurchase = ref(null);\nconst returnNotes = ref(\"\");\nconst submitting = ref(false);\n\n/* returnForm will mirror selectedPurchase.Items but with qty & reason */\nconst returnForm = ref({ items: [] });\n\n/* -------------------------\n   Helpers: formatters\n   ------------------------- */\nconst formatDate = (dateStr) => {\n  if (!dateStr) return \"N/A\";\n  const d = new Date(dateStr);\n  if (isNaN(d.getTime())) {\n    const sliced = dateStr.split(\"T\")[0];\n    return sliced || \"Invalid Date\";\n  }\n  return d.toLocaleDateString(\"id-ID\", {\n    day: \"2-digit\",\n    month: \"2-digit\",\n    year: \"numeric\",\n  });\n};\n\nconst formatCurrency = (amount) => {\n  return amount !== undefined && amount !== null\n    ? new Intl.NumberFormat(\"id-ID\", { style: \"currency\", currency: \"IDR\", minimumFractionDigits: 0 }).format(amount)\n    : \"Rp 0\";\n};\n\nconst formatPONumber = (id) => `PO-${String(id || \"\").padStart(3, \"0\")}`;\nconst formatReturnNumber = (id) => `2025-${String(id || \"\").padStart(3, \"0\")}`;\n\nconst getStatusClass = (status) => {\n  switch (status) {\n    case \"PENDING\": return \"bg-yellow-100 text-yellow-800\";\n    case \"APPROVED\": return \"bg-green-100 text-green-800\";\n    case \"REJECTED\": return \"bg-red-100 text-red-800\";\n    default: return \"bg-gray-100 text-gray-800\";\n  }\n};\n\nconst getStatusText = (status) => {\n  switch (status) {\n    case \"PENDING\": return \"Pending\";\n    case \"APPROVED\": return \"Approved\";\n    case \"REJECTED\": return \"Rejected\";\n    default: return status || \"-\";\n  }\n};\n\n/* -------------------------\n   Computed\n   ------------------------- */\nconst hasValidItems = computed(() => {\n  return returnForm.value.items.some(item => item.qty > 0 && item.reason && item.reason.trim().length > 0);\n});\n\n/* -------------------------\n   Fetch purchases (latest or filtered)\n   ------------------------- */\nconst fetchPurchases = async () => {\n  loadingPurchases.value = true;\n  try {\n    const params = {};\n    if (fromDate.value) params.start_date = fromDate.value;\n    if (toDate.value) params.end_date = toDate.value;\n\n    let url = LATEST_PURCHASES_ENDPOINT;\n    if (fromDate.value || toDate.value) {\n      url = \"/history/purchases\";\n      params.page = 1;\n      params.size = 10;\n    } else {\n      params.limit = 10;\n    }\n\n    const res = await api.get(url, { params });\n\n    // Normalisasi response dengan cara bertingkat — hindari duplicate else-if\n    const body = res && res.data ? res.data : null;\n\n    if (Array.isArray(body)) {\n      purchases.value = body;\n    } else if (body && body.data) {\n      if (Array.isArray(body.data)) {\n        purchases.value = body.data;\n      } else if (body.data && Array.isArray(body.data.data)) {\n        purchases.value = body.data.data;\n      } else if (body.data && Array.isArray(body.data.rows)) {\n        purchases.value = body.data.rows;\n      } else {\n        purchases.value = body.data || [];\n      }\n    } else if (res && res.data && res.data.purchases && Array.isArray(res.data.purchases)) {\n      purchases.value = res.data.purchases;\n    } else {\n      purchases.value = [];\n    }\n\n  } catch (err) {\n    console.error(\"Error fetching purchases:\", err);\n    alert(`Gagal mengambil data pembelian: ${err.response?.status || err.message}`);\n    purchases.value = [];\n  } finally {\n    loadingPurchases.value = false;\n  }\n};\n\n/* -------------------------\n   Fetch purchase returns (history)\n   ------------------------- */\nconst fetchPurchaseReturns = async () => {\n  loadingReturns.value = true;\n  try {\n    const params = { page: 1, size: 10 };\n    if (returnFromDate.value) params.start_date = returnFromDate.value;\n    if (returnToDate.value) params.end_date = returnToDate.value;\n    if (returnStatusFilter.value) params.status = returnStatusFilter.value;\n\n    const res = await api.get(LIST_PURCHASE_RETURNS_ENDPOINT, { params });\n    purchaseReturns.value = res.data.data || res.data || [];\n  } catch (err) {\n    console.error(\"Error fetching purchase returns:\", err);\n    alert(`Gagal mengambil daftar retur: ${err.response?.data?.error || err.message}`);\n    purchaseReturns.value = [];\n  } finally {\n    loadingReturns.value = false;\n  }\n};\n\n/* -------------------------\n   Modal: open purchase detail & prepare returnForm\n   ------------------------- */\nconst openReturnModal = async (purchaseId) => {\n  try {\n    const res = await api.get(`/pembelian/purchases/${purchaseId}`);\n    const payload = res.data.data || res.data;\n    selectedPurchase.value =