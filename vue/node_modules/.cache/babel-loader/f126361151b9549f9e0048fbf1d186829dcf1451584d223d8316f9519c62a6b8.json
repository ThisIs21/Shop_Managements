 (dateStr) => {\n  if (!dateStr) return \"N/A\";\n  const date = new Date(dateStr);\n  if (isNaN(date.getTime())) {\n    // Fallback: Slice YYYY-MM-DD dari ISO string\n    const sliced = dateStr ? dateStr.split(\"T\")[0] : \"\";\n    return sliced || \"Invalid Date\";\n  }\n  return date.toLocaleDateString(\"id-ID\"); // DD/MM/YYYY\n};\n\nconst formatCurrency = (amount) => {\n  return amount !== undefined && amount !== null\n    ? `$${Number(amount).toLocaleString()}`\n    : \"$0\";\n};\n\nonMounted(() => {\n  fetchDashboardData(); // Default: 10 terakhir\n});\n</script>\n"],"mappings":"AAgKA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,cAAc,MAAM,iCAAiC;;;;;;;IAE5D,MAAMC,QAAQ,GAAGJ,GAAG,CAAC,OAAO,CAAC;IAC7B,MAAMK,iBAAiB,GAAGL,GAAG,CAAC,CAAC,CAAC;IAChC,MAAMM,eAAe,GAAGN,GAAG,CAAC,CAAC,CAAC;IAC9B,MAAMO,YAAY,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMQ,cAAc,GAAGR,GAAG,CAAC,IAAI,CAAC;;IAEhC;IACA,MAAMS,QAAQ,GAAGT,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMU,MAAM,GAAGV,GAAG,CAAC,EAAE,CAAC;IAEtB,MAAMW,kBAAkB,GAAG,MAAAA,CAAA,KAAY;MACrCH,cAAc,CAACI,KAAK,GAAG,IAAI;MAC3B,IAAI;QACF,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QAC3C,MAAMC,eAAe,GAAGF,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;QAExD,IAAIC,eAAe,EAAE;UACnBZ,QAAQ,CAACQ,KAAK,GAAGI,eAAe;QAClC;;QAEA;QACA,MAAMC,MAAM,GAAG;UACbC,IAAI,EAAE,CAAC;UACPC,IAAI,EAAE,EAAE;UACRC,OAAO,EAAE;QACX,CAAC;;QAED;QACA,IAAIX,QAAQ,CAACG,KAAK,EAAEK,MAAM,CAACI,IAAI,GAAGZ,QAAQ,CAACG,KAAK;QAChD,IAAIF,MAAM,CAACE,KAAK,EAAEK,MAAM,CAACK,EAAE,GAAGZ,MAAM,CAACE,KAAK;QAE1C,MAAMW,QAAQ,GAAG,MAAMrB,KAAK,CAACsB,GAAG,CAC9B,4CAA4C,EAC5C;UACEC,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAUb,KAAK;UAAG,CAAC;UAC7CI;QACF,CACF,CAAC;QAED,MAAMU,IAAI,GAAGJ,QAAQ,CAACI,IAAI;QAE1B,IAAIA,IAAI,IAAIA,IAAI,CAACA,IAAI,EAAE;UACrBpB,YAAY,CAACK,KAAK,GAAGe,IAAI,CAACA,IAAI;UAC9BtB,iBAAiB,CAACO,KAAK,GAAGe,IAAI,CAACC,KAAK,IAAID,IAAI,CAACA,IAAI,CAACE,MAAM;UACxDvB,eAAe,CAACM,KAAK,GAAGe,IAAI,CAACG,WAAW,IAAI,CAAC;QAC/C,CAAC,MAAM;UACLC,OAAO,CAACC,IAAI,CAAC,qDAAqD,CAAC;UACnEzB,YAAY,CAACK,KAAK,GAAG,EAAE;UACvBP