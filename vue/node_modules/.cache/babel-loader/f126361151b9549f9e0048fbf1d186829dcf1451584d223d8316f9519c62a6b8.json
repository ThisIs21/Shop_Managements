{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nimport CashierLayouts from \"@/components/CashierLayouts.vue\";\nexport default {\n  __name: 'DashboardCashier',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const userName = ref(\"Kasir\");\n    const totalTransactions = ref(0);\n    const totalSalesValue = ref(0);\n    const salesHistory = ref([]);\n    const loadingHistory = ref(true);\n\n    // Filter dates\n    const fromDate = ref(\"\");\n    const toDate = ref(\"\");\n    const fetchDashboardData = async () => {\n      loadingHistory.value = true;\n      try {\n        const token = localStorage.getItem(\"token\");\n        const nameFromStorage = localStorage.getItem(\"userName\");\n        if (nameFromStorage) {\n          userName.value = nameFromStorage;\n        }\n\n        // Params untuk API (cocok backend: page, size, orderBy)\n        const params = {\n          page: 1,\n          size: 10,\n          orderBy: \"date DESC\"\n        };\n\n        // Tambah filter tanggal kalau diisi\n        if (fromDate.value) params.from = fromDate.value;\n        if (toDate.value) params.to = toDate.value;\n        const response = await axios.get(\"http://localhost:8081/api/v1/history/sales\", {\n          headers: {\n            Authorization: `Bearer ${token}`\n          },\n          params\n        });\n        const data = response.data;\n        if (data && data.data) {\n          salesHistory.value = data.data;\n          totalTransactions.value = data.total || data.data.length;\n          totalSalesValue.value = data.total_value || 0;\n        } else {\n          console.warn(\"API response is missing 'data' or 'total' property.\");\n          salesHistory.value = [];\n          totalTransactions.value = 0;\n          totalSalesValue.value = 0;\n        }\n      } catch (error) {\n        console.error(\"Failed to fetch dashboard data:\", error);\n        if (error.response) {\n          console.error(\"Status:\", error.response.status);\n        }\n        salesHistory.value = [];\n      } finally {\n        loadingHistory.value = false;\n      }\n    };\n    const resetFilter = () => {\n      fromDate.value = \"\";\n      toDate.value = \"\";\n      fetchDashboardData(); // Fetch 10 terakhir lagi\n    };\n\n    // Helper functions untuk format\n    const formatDate = dateStr => {\n      if (!dateStr) return \"N/A\";\n      const date = new Date(dateStr);\n      if (isNaN(date.getTime())) {\n        // Fallback: Slice YYYY-MM-DD dari ISO string\n        const sliced = dateStr ? dateStr.split(\"T\")[0] : \"\";\n        return sliced || \"Invalid Date\";\n      }\n      return date.toLocaleDateString(\"id-ID\"); // DD/MM/YYYY\n    };\n    const formatCurrency = amount => {\n      return amount !== undefined && amount !== null ? `$${Number(amount).toLocaleString()}` : \"$0\";\n    };\n    onMounted(() => {\n      fetchDashboardData(); // Default: 10 terakhir\n    });\n    const __returned__ = {\n      userName,\n      totalTransactions,\n      totalSalesValue,\n      salesHistory,\n      loadingHistory,\n      fromDate,\n      toDate,\n      fetchDashboardData,\n      resetFilter,\n      formatDate,\n      formatCurrency,\n      ref,\n      onMounted,\n      get axios() {\n        return axios;\n      },\n      CashierLayouts\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","CashierLayouts","userName","totalTransactions","totalSalesValue","salesHistory","loadingHistory","fromDate","toDate","fetchDashboardData","value","token","localStorage","getItem","nameFromStorage","params","page","size","orderBy","from","to","response","get","headers","Authorization","data","total","length","total_value","console","warn","error","status","resetFilter","formatDate","dateStr","date","Date","isNaN","getTime","sliced","split","toLocaleDateString","formatCurrency","amount","undefined","Number","toLocaleString"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/DashboardCashier.vue"],"sourcesContent":["<template>\n  <CashierLayouts>\n    <div>\n      <div class=\"flex flex-wrap justify-between gap-3 p-4\">\n        <p\n          class=\"text-[#111418] tracking-light text-[32px] font-bold leading-tight min-w-72\"\n        >\n          Selamat datang, {{ userName }}\n        </p>\n      </div>\n\n      <!-- Filter Tanggal -->\n      <div class=\"p-4 bg-[#f0f2f5] rounded-lg\">\n        <div class=\"flex flex-wrap gap-4 items-end\">\n          <div>\n            <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n              >Dari Tanggal</label\n            >\n            <input\n              type=\"date\"\n              v-model=\"fromDate\"\n              class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n            />\n          </div>\n          <div>\n            <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n              >Sampai Tanggal</label\n            >\n            <input\n              type=\"date\"\n              v-model=\"toDate\"\n              class=\"px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n            />\n          </div>\n          <button\n            @click=\"fetchDashboardData\"\n            :disabled=\"loadingHistory\"\n            class=\"px-4 py-2 bg-blue-500 text-white rounded-md hover:bg-blue-600 disabled:opacity-50\"\n          >\n            {{ loadingHistory ? \"Loading...\" : \"Filter\" }}\n          </button>\n          <button\n            @click=\"resetFilter\"\n            class=\"px-4 py-2 bg-gray-500 text-white rounded-md hover:bg-gray-600\"\n          >\n            Reset (10 Terakhir)\n          </button>\n        </div>\n        <p v-if=\"fromDate || toDate\" class=\