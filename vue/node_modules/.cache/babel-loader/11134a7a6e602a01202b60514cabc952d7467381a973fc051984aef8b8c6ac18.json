{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.every.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, computed, onMounted } from \"vue\";\nimport GudangLayouts from \"@/components/GudangLayouts.vue\";\nimport axios from \"axios\";\n\n// URL Base API (Penting: Ganti dengan variabel lingkungan jika ada)\nconst API_BASE = \"http://localhost:8081/api/v1\";\n\n// --- Variabel State ---\n\nexport default {\n  __name: 'DashboardGudang',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const stockOpnames = ref([]);\n    const products = ref([]);\n    const showModal = ref(false);\n    const loading = ref(false);\n    const submitting = ref(false);\n    const errorMessage = ref(\"\");\n    const token = localStorage.getItem(\"token\");\n    const role = localStorage.getItem(\"userRole\") || \"\";\n    const currentDate = new Date().toISOString().slice(0, 10); // Today's date for min attribute\n\n    const opnameForm = ref({\n      date: currentDate,\n      note: \"\",\n      items: [{\n        product_id: \"\",\n        qty_fisik: 0\n      }]\n    });\n\n    // --- Computed Properties ---\n    const isFormValid = computed(() => {\n      // Pastikan setidaknya ada satu item dan semua item memiliki product_id dan qty_fisik >= 0\n      if (opnameForm.value.items.length === 0) return false;\n      return opnameForm.value.items.every(item => item.product_id && item.qty_fisik >= 0);\n    });\n\n    // --- Functions ---\n\n    // Fetch Stock Opnames\n    const fetchStockOpnames = async () => {\n      if (!token) {\n        errorMessage.value = \"No token found. Please log in again.\";\n        return;\n      }\n      loading.value = true;\n      errorMessage.value = \"\";\n      try {\n        const res = await axios.get(`${API_BASE}/gudang/stock-opnames`, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        // Asumsi API mengembalikan array opname di res.data.data\n        stockOpnames.value = res.data.data || [];\n      } catch (err) {\n        console.error(\"Failed to fetch stock opnames:\", err);\n        errorMessage.value = err.response?.data?.error || err.response?.data?.message || \"Network error\";\n        stockOpnames.value = [];\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // Fetch Products for dropdown\n    const fetchProducts = async () => {\n      if (!token) return;\n      try {\n        const res = await axios.get(`${API_BASE}/gudang/products`, {\n          params: {\n            page: 1,\n            size: 100,\n            q: \"\"\n          },\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        products.value = res.data.data?.data || [];\n      } catch (err) {\n        console.error(\"Failed to fetch products:\", err);\n      }\n    };\n\n    // Add Item to Form\n    const addItem = () => {\n      opnameForm.value.items.push({\n        product_id: \"\",\n        qty_fisik: 0\n      });\n    };\n\n    // Remove Item from Form\n    const removeItem = index => {\n      if (opnameForm.value.items.length > 1) {\n        opnameForm.value.items.splice(index, 1);\n      }\n    };\n\n    // Format Tanggal\n    const formatDate = dateStr => {\n      if (!dateStr) return \"-\";\n      const date = new Date(dateStr.endsWith(\"Z\") ? dateStr : dateStr + \"Z\");\n      return date.toLocaleDateString(\"id-ID\", {\n        day: \"2-digit\",\n        month: \"short\",\n        year: \"numeric\",\n        hour: \"2-digit\",\n        minute: \"2-digit\"\n      });\n    };\n\n    // Submit Stock Opname\n    const submitOpname = async () => {\n      if (!isFormValid.value) {\n        errorMessage.value = \"Please fill in all required fields correctly.\";\n        return;\n      }\n      submitting.value = true;\n      errorMessage.value = \"\";\n      try {\n        // Transformasi data untuk API\n        const payload = {\n          date: opnameForm.value.date,\n          note: opnameForm.value.note,\n          items: opnameForm.value.items.map(item => ({\n            product_id: parseInt(item.product_id),\n            // Pastikan ini int\n            qty_fisik: parseInt(item.qty_fisik) // Pastikan ini int\n          }))\n        };\n        const res = await axios.post(`${API_BASE}/gudang/stock-opnames`, payload, {\n          headers: {\n            Authorization: `Bearer ${token}`\n          }\n        });\n        alert(\"Stock Opname submitted successfully!\");\n        showModal.value = false;\n        // Reset form\n        opnameForm.value = {\n          date: currentDate,\n          note: \"\",\n          items: [{\n            product_id: \"\",\n            qty_fisik: 0\n          }]\n        };\n        // Refresh list\n        await fetchStockOpnames();\n      } catch (err) {\n        console.error(\"Failed to submit stock opname:\", err);\n        errorMessage.value = err.response?.data?.error || err.response?.data?.message || \"Terjadi kesalahan saat submit.\";\n      } finally {\n        submitting.value = false;\n      }\n    };\n    onMounted(() => {\n      fetchStockOpnames();\n      fetchProducts(); // Muat daftar produk saat komponen dimuat\n    });\n    const __returned__ = {\n      API_BASE,\n      stockOpnames,\n      products,\n      showModal,\n      loading,\n      submitting,\n      errorMessage,\n      token,\n      role,\n      currentDate,\n      opnameForm,\n      isFormValid,\n      fetchStockOpnames,\n      fetchProducts,\n      addItem,\n      removeItem,\n      formatDate,\n      submitOpname,\n      ref,\n      computed,\n      onMounted,\n      GudangLayouts,\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","GudangLayouts","axios","API_BASE","stockOpnames","products","showModal","loading","submitting","errorMessage","token","localStorage","getItem","role","currentDate","Date","toISOString","slice","opnameForm","date","note","items","product_id","qty_fisik","isFormValid","value","length","every","item","fetchStockOpnames","res","get","headers","Authorization","data","err","console","error","response","message","fetchProducts","params","page","size","q","addItem","push","removeItem","index","splice","formatDate","dateStr","endsWith","toLocaleDateString","day","month","year","hour","minute","submitOpname","payload","map","parseInt","post","alert"],"sources":["C:/Users/Personal/Documents/PKL/aplikasi/aplikasi/src/views/DashboardGudang.vue"],"sourcesContent":["<template>\n  <GudangLayouts>\n    <div class=\"space-y-6\">\n      <div>\n        <h1 class=\"text-3xl font-bold text-gray-900\">\n          Stock Opname Management\n        </h1>\n        <p class=\"text-lg text-gray-600 mt-1\">\n          Monitor and record physical stock counts.\n        </p>\n      </div>\n\n      <div class=\"flex justify-end\">\n        <button\n          @click=\"showModal = true\"\n          class=\"px-5 py-2 bg-blue-600 text-white font-medium rounded-lg shadow-md hover:bg-blue-700 transition-colors\"\n        >\n          + Add New Opname\n        </button>\n      </div>\n\n      <div class=\"bg-white rounded-xl shadow-lg p-6 border border-gray-100\">\n        <div class=\"flex justify-between items-center mb-4 border-b pb-3\">\n          <h2 class=\"text-xl font-semibold text-gray-800\">\n            Riwayat Stock Opname Terbaru\n          </h2>\n          <button\n            @click=\"fetchStockOpnames\"\n            :disabled=\"loading\"\n            class=\"px-4 py-1.5 text-sm bg-gray-200 text-gray-700 rounded-md hover:bg-gray-300 disabled:opacity-50 transition-colors\"\n          >\n            {{ loading ? \"Loading...\" : \"Refresh Data\" }}\n          </button>\n        </div>\n\n        <div class=\"overflow-x-auto\">\n          <table class=\"min-w-full divide-y divide-gray-200\">\n            <thead class=\"bg-gray-50\">\n              <tr>\n                <th\n                  class=\"px-4 py-2 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider\"\n                >\n                  Date\n                </th>\n                <th\n                  class=\"px-4 py-2 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider\"\n                >\n                  Status\n                </th>\n                <th\n                  class=\"px-4 py-2 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider\"\n                >\n                  Items Count\n                </th>\n                <th\n                  class=\"px-4 py-2 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider\"\n                >\n                  Staff\n                </th>\n              </tr>\n            </thead>\n            <tbody class=\"bg-white divide-y divide-gray-100\">\n              <tr v-if=\"loading\">\n                <td\n                  colspan=\"4\"\n                  class=\"px-4 py-4 text-center text-sm text-gray-500\"\n                >\n                  Loading...\n                </td>\n              </tr>\n              <tr v-else-if=\"stockOpnames.length === 0\">\n                <td\n                  colspan=\"4\"\n                  class=\"px-4 py-4 text-center text-sm text-gray-500\"\n                >\n                  No data available.\n                </td>\n              </tr>\n              <tr\n                v-else\n                v-for=\"opname in stockOpnames\"\n                :key=\"opname.ID\"\n                class=\"hover:bg-gray-50 transition-colors\"\n              >\n                <td class=\"px-4 py-3 whitespace-nowrap text-sm text-gray-700\">\n                  {{ formatDate(opname.Date) }}\n                </td>\n                <td class=\"px-4 py-3 whitespace-nowrap text-sm\">\n                  <span\n                    class=\"inline-flex items-center rounded-full px-3 py-0.5 text-xs font-medium\"\n                    :class=\"{\n                      'bg-green-100 text-green-800':\n                        opname.Status === 'Completed',\n                      'bg-yellow-100 text-yellow-800':\n                        opname.Status === 'Pending',\n                    }\"\n                  >\n                    {{ opname.Status || \"Unknown\" }}\n                  </span>\n                </td>\n                <td class=\"px-4 py-3 whitespace-nowrap text-sm text-gray-700\">\n                  {{ opname.ProductsCount || 0 }}\n                </td>\n                <td class=\"px-4 py-3 whitespace-nowrap text-sm text-gray-700\">\n                  {{ opname.StaffName || \"Unknown\" }}\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n        <p\n          v-if=\"errorMessage && !loading\"\n          class=\"text-red-500 text-sm mt-4 p-3 bg-red-50 rounded-lg border border-red-200\"\n        >\n          **Error:** {{ errorMessage }}\n        </p>\n      </div>\n    </div>\n\n    <div\n      v-if=\"showModal\"\n      class=\"fixed inset-0 z-50 overflow-y-auto bg-gray-900 bg-opacity-50 flex justify-center items-center\"\n      @click.self=\"showModal = false\"\n    >\n      <div\n        class=\"bg-white rounded-xl shadow-2xl w-full max-w-2xl p-6 relative transform transition-all\"\n      >\n        <h3 class=\"text-2xl font-bold text-gray-800 mb-4 border-b pb-2\">\n          Create New Stock Opname\n        </h3>\n        <button\n          @click=\"showModal = false\"\n          class=\"absolute top-4 right-4 text-gray-400 hover:text-gray-600 text-3xl leading-none\"\n        >\n          &times;\n        </button>\n\n        <form\n          @submit.prevent=\"submitOpname\"\n          class=\"space-y-4 max-h-[70vh] overflow-y-auto pr-2\"\n        >\n          <div class=\"grid grid-cols-2 gap-4\">\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Date</label\n              >\n              <input\n                v-model=\"opnameForm.date\"\n                type=\"date\"\n                required\n                class=\"block w-full border-gray-300 rounded-lg shadow-sm focus:ring-blue-500 focus:border-blue-500\"\n                :min=\"currentDate\"\n              />\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\"\n                >Note (Optional)</label\n              >\n              <textarea\n                v-model=\"opnameForm.note\"\n                class=\"block w-full border-gray-300 rounded-lg shadow-sm focus:ring-blue-500 focus:border-blue-500\"\n                rows=\"3\"\n                placeholder=\"Enter any notes...\"\n              ></textarea>\n            </div>\n          </div>\n\n          <div class=\"border p-3 rounded-lg bg-gray-50\">\n            <label class=\"block text-base font-semibold text-gray-800 mb-3\"\n              >Opname Items</label\n            >\n            <div\n              v-for=\"(item, index) in opnameForm.items\"\n              :key=\"index\"\n              class=\"flex space-x-2 mb-3 items-center\"\n            >\n              <select\n                v-model=\"item.product_id\"\n                required\n                class=\"w-2/3 border-gray-300 rounded-lg shadow-sm focus:ring-blue-500 focus:border-blue-500 text-sm py-2\"\n              >\n                <option value=\"\" disabled>Select Product</option>\n                <option\n                  v-for=\"product in products\"\n                  :key=\"product.id\"\n                  :value=\"product.id\"\n                >\n                  {{ product.name }}\n                </option>\n              </select>\n              <input\n                v-model.number=\"item.qty_fisik\"\n                type=\"number\"\n                min=\"0\"\n                required\n                class=\"w-1/4 border-gray-300 rounded-lg shadow-sm focus:ring-blue-500 focus:border-blue-500 text-sm py-2 text-center\"\n                placeholder=\"Qty Fisik\"\n              />\n              <button\n                @click.prevent=\"removeItem(index)\"\n                type=\"button\"\n                :disabled=\"opnameForm.items.length === 1\"\n                class=\"text-red-500 hover:text-red-700 text-xl disabled:opacity-30 disabled:cursor-not-allowed\"\n                title=\"Remove Item\"\n              >\n                &times;\n              </button>\n            </div>\n            <button\n              @click.prevent=\"addItem\"\n              type=\"button\"\n              class=\"mt-2 px-3 py-1.5 text-sm bg-green-500 text-white rounded-lg hover:bg-green-600 transition-colors\"\n            >\n              + Add Item\n            </button>\n          </div>\n\n          <div class=\"flex justify-end pt-3\">\n            <button\n              type=\"button\"\n              @click=\"showModal = false\"\n              class=\"px-6 py-2 border border-gray-300 text-gray-700 rounded-lg shadow-sm hover:bg-gray-100 mr-3\"\n            >\n              Cancel\n            </button>\n            <button\n              type=\"submit\"\n              :disabled=\"submitting || !isFormValid\"\n              class=\"px-6 py-2 bg-blue-600 text-white font-medium rounded-lg shadow-md hover:bg-blue-700 disabled:opacity-50 transition-colors\"\n            >\n              {{ submitting ? \"Submitting...\" : \"Submit Opname\" }}\n            </button>\n          </div>\n\n          <p v-if=\"errorMessage\" class=\"text-red-500 text-sm pt-2\">\n            {{ errorMessage }}\n          </p>\n        </form>\n      </div>\n    </div>\n  </GudangLayouts>\n</template>\n\n<script setup>\nimport { ref, computed, onMounted } from \"vue\";\nimport GudangLayouts from \"@/components/GudangLayouts.vue\";\nimport axios from \"axios\";\n\n// URL Base API (Penting: Ganti dengan variabel lingkungan jika ada)\nconst API_BASE = \"http://localhost:8081/api/v1\";\n\n// --- Variabel State ---\nconst stockOpnames = ref([]);\nconst products = ref([]);\nconst showModal = ref(false);\nconst loading = ref(false);\nconst submitting = ref(false);\nconst errorMessage = ref(\"\");\nconst token = localStorage.getItem(\"token\");\nconst role = localStorage.getItem(\"userRole\") || \"\";\nconst currentDate = new Date().toISOString().slice(0, 10); // Today's date for min attribute\n\nconst opnameForm = ref({\n  date: currentDate,\n  note: \"\",\n  items: [\n    {\n      product_id: \"\",\n      qty_fisik: 0,\n    },\n  ],\n});\n\n// --- Computed Properties ---\nconst isFormValid = computed(() => {\n  // Pastikan setidaknya ada satu item dan semua item memiliki product_id dan qty_fisik >= 0\n  if (opnameForm.value.items.length === 0) return false;\n  return opnameForm.value.items.every(\n    (item) => item.product_id && item.qty_fisik >= 0\n  );\n});\n\n// --- Functions ---\n\n// Fetch Stock Opnames\nconst fetchStockOpnames = async () => {\n  if (!token) {\n    errorMessage.value = \"No token found. Please log in again.\";\n    return;\n  }\n  loading.value = true;\n  errorMessage.value = \"\";\n  try {\n    const res = await axios.get(`${API_BASE}/gudang/stock-opnames`, {\n      headers: { Authorization: `Bearer ${token}` },\n    });\n    // Asumsi API mengembalikan array opname di res.data.data\n    stockOpnames.value = res.data.data || [];\n  } catch (err) {\n    console.error(\"Failed to fetch stock opnames:\", err);\n    errorMessage.value =\n      err.response?.data?.error ||\n      err.response?.data?.message ||\n      \"Network error\";\n    stockOpnames.value = [];\n  } finally {\n    loading.value = false;\n  }\n};\n\n// Fetch Products for dropdown\nconst fetchProducts = async () => {\n 